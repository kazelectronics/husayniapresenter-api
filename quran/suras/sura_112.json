{
  "index": 112,
  "name": "الإخلاص",
  "arabic_name": "الإخْلَاص",
  "english_name": "Al-Ikhlāṣ",
  "english_meaning": "The Sincerity",
  "ayas": [
    {
      "index": 1,
      "text": "قُلْ هُوَ ٱللَّهُ أَحَدٌ",
      "bismillah": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ"
    },
    {
      "index": 2,
      "text": "ٱللَّهُ ٱلصَّمَدُ"
    },
    {
      "index": 3,
      "text": "لَمْ يَلِدْ وَلَمْ يُولَدْ"
    },
    {
      "index": 4,
      "text": "وَلَمْ يَكُن لَّهُۥ كُفُوًا أَحَدٌۢ"
    }
  ]
}