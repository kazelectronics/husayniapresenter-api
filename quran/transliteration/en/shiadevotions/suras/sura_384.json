{
  "index": 384,
  "name": "Sura 384",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma innī aslamtu nafsī ilayk",
      "bismillah": "bismillāh"
    },
    {
      "index": 2,
      "text": "wawajjahtu wajhī ilayk"
    },
    {
      "index": 3,
      "text": "wafawwaḍtu amrī ilayk"
    },
    {
      "index": 4,
      "text": "wa-alja-tu ẓahrī ilayk"
    },
    {
      "index": 5,
      "text": "watawakkaltu `alayka rahbatan minka waraghbatan ilayk"
    },
    {
      "index": 6,
      "text": "lā malja-a walā manjā minka illā ilayk"
    },
    {
      "index": 7,
      "text": "āmantu bikitābikal ladhī anzalt"
    },
    {
      "index": 8,
      "text": "wabirasūlikal ladhī ar-salt"
    }
  ]
}