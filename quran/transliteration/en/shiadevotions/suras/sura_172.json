{
  "index": 172,
  "name": "Sura 172",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma inna hādhash shahral mubārak"
    },
    {
      "index": 3,
      "text": "al ladhī unzila fīhil qur-ān"
    },
    {
      "index": 4,
      "text": "waju`ila hudan linnās"
    },
    {
      "index": 5,
      "text": "wabayyinātin minal hudā walfur-qān"
    },
    {
      "index": 6,
      "text": "qad ḥaḍar"
    },
    {
      "index": 7,
      "text": "fasallimnā fīh"
    },
    {
      "index": 8,
      "text": "wasallimhu lanā"
    },
    {
      "index": 9,
      "text": "watasallamhu minnā fī yusrin minka wa`āfiyah"
    },
    {
      "index": 10,
      "text": "yā man akhadhal qalīla washakaral kathīr"
    },
    {
      "index": 11,
      "text": "iqbal minnil yasīr"
    },
    {
      "index": 12,
      "text": "allāhumma innī as-aluk"
    },
    {
      "index": 13,
      "text": "an taj`ala lī ilā kulli khayrin sabīlā"
    },
    {
      "index": 14,
      "text": "wamin kulli mā lātuḥibbu māni`ā"
    },
    {
      "index": 15,
      "text": "yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 16,
      "text": "yā man `afā `annī wa`ammā khalawtu bihi minas sayyi-āt"
    },
    {
      "index": 17,
      "text": "yā man lam yu-khidhnī bir-tikābil ma`āṣī"
    },
    {
      "index": 18,
      "text": "`afwaka `afwaka `afwaka yā karīm"
    },
    {
      "index": 19,
      "text": "ilāhī wa`aẓtanī falam atta`iẓ"
    },
    {
      "index": 20,
      "text": "wazajar-tanī `an maḥārimika falam anzajir"
    },
    {
      "index": 21,
      "text": "famā `udhrī"
    },
    {
      "index": 22,
      "text": "fa`fu `annī yā karīm"
    },
    {
      "index": 23,
      "text": "`afwaka `afwak"
    },
    {
      "index": 24,
      "text": "allāhumma innī as-aluk"
    },
    {
      "index": 25,
      "text": "ar rāḥata `indal mawt"
    },
    {
      "index": 26,
      "text": "wal`afwa `indal ḥisāb"
    },
    {
      "index": 27,
      "text": "`aẓumadh dhanbu min `abdik"
    },
    {
      "index": 28,
      "text": "falyaḥsunit tajāwuzu min `indik"
    },
    {
      "index": 29,
      "text": "yā ahlat taqwā wayā ahlal maghfirah"
    },
    {
      "index": 30,
      "text": "`afwaka `afwak"
    },
    {
      "index": 31,
      "text": "allāhumma innī `abdukabnu `abdikabnu amatik"
    },
    {
      "index": 32,
      "text": "ḍa`īfun faqīrun ilā raḥmatik"
    },
    {
      "index": 33,
      "text": "wa-anta munzilul ghinā walbarakati `alal `ibād"
    },
    {
      "index": 34,
      "text": "qāhirun muqtadir"
    },
    {
      "index": 35,
      "text": "aḥṣayta a`mālahum"
    },
    {
      "index": 36,
      "text": "waqasamta ar-zāqahum"
    },
    {
      "index": 37,
      "text": "waja`altahum mukhtalifatan alsinatuhum wa-alwānuhum"
    },
    {
      "index": 38,
      "text": "khalqan min ba`di khalq"
    },
    {
      "index": 39,
      "text": "walā ya`lamul `ibādu `ilmak"
    },
    {
      "index": 40,
      "text": "walā yaqdirul `ibādu qadrak"
    },
    {
      "index": 41,
      "text": "wakullunā faqīrun ilā raḥmatik"
    },
    {
      "index": 42,
      "text": "falā taṣrif `annī wajhak"
    },
    {
      "index": 43,
      "text": "waj`alnī min ṣāliḥī khalqik"
    },
    {
      "index": 44,
      "text": "fil `amali wal-amal"
    },
    {
      "index": 45,
      "text": "walqaḍā-i walqadar"
    },
    {
      "index": 46,
      "text": "allāhumma abqinī khayral baqā"
    },
    {
      "index": 47,
      "text": "wa-afninī khayral fanā-i `alā muwālāti awliyā-ik"
    },
    {
      "index": 48,
      "text": "wamu`ādāti a`dā-ik"
    },
    {
      "index": 49,
      "text": "war raghbati ilayka war rahbati mink"
    },
    {
      "index": 50,
      "text": "walkhushū`i walwafā-i wat taslīmi lak"
    },
    {
      "index": 51,
      "text": "wat taṣdīqi bikitābik"
    },
    {
      "index": 52,
      "text": "wattibā`i sunnati rasūlik"
    },
    {
      "index": 53,
      "text": "allāhumma mā kāna fī qalbī"
    },
    {
      "index": 54,
      "text": "min shakkin aw rībah"
    },
    {
      "index": 55,
      "text": "aw juḥūdin aw qunūṭ"
    },
    {
      "index": 56,
      "text": "aw faraḥin aw badhakhin aw baṭar"
    },
    {
      "index": 57,
      "text": "aw khuyalā-a aw riyā-in aw sum`ah"
    },
    {
      "index": 58,
      "text": "aw shiqāqin aw nifāq"
    },
    {
      "index": 59,
      "text": "aw kufrin aw fusūqin aw `iṣyān"
    },
    {
      "index": 60,
      "text": "aw `aẓamatin aw shay-in lātuḥibb"
    },
    {
      "index": 61,
      "text": "fa-as-aluka yā rabbi an tubaddilanī makānahu īmānan biwa`dik"
    },
    {
      "index": 62,
      "text": "wawafā-an bi`ahdik"
    },
    {
      "index": 63,
      "text": "wariḍan biqaḍā-ik"
    },
    {
      "index": 64,
      "text": "wazuhdan fid dunyā"
    },
    {
      "index": 65,
      "text": "waraghbatan fīmā `indak"
    },
    {
      "index": 66,
      "text": "wa-atharatan waṭuma-nīnah"
    },
    {
      "index": 67,
      "text": "watawbatan naṣūḥā"
    },
    {
      "index": 68,
      "text": "as-aluka dhālika yā rabbal `ālamīn"
    },
    {
      "index": 69,
      "text": "ilāhī anta min ḥilmika tu`ṣā faka-annaka lam tar"
    },
    {
      "index": 70,
      "text": "wamin karamika wajūdika tuṭā`u faka-annaka lam tu`ṣ"
    },
    {
      "index": 71,
      "text": "wa-ana waman lam ya`ṣika sukkānu ar-ḍik"
    },
    {
      "index": 72,
      "text": "fakun `alaynā bil faḍli jawādā"
    },
    {
      "index": 73,
      "text": "wabil khayri `awwādā"
    },
    {
      "index": 74,
      "text": "yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 75,
      "text": "waṣallallāhu `alā muḥammadin wa ālihi ṣalātan dā-imah"
    },
    {
      "index": 76,
      "text": "lātuḥṣā walā tu`add"
    },
    {
      "index": 77,
      "text": "walā yaqdiru qadrahā ghayruk"
    },
    {
      "index": 78,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}