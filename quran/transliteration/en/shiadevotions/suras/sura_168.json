{
  "index": 168,
  "name": "Sura 168",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī ta`arraḍa laka fī hādhal laylil muta`arriḍūn"
    },
    {
      "index": 3,
      "text": "waqaṣadakal qāṣidūn"
    },
    {
      "index": 4,
      "text": "wa-ammala faḍlaka wama`rūfakaṭ ṭālibūn"
    },
    {
      "index": 5,
      "text": "walaka fī hādhal layli nafaḥātun wajawā-iz"
    },
    {
      "index": 6,
      "text": "wa`aṭāyā wamawāhib"
    },
    {
      "index": 7,
      "text": "tamunnu bihā `alā man tashā-u min `ibādik"
    },
    {
      "index": 8,
      "text": "watamna`uhā man lam tasbiq lahul `ināyatu mink"
    },
    {
      "index": 9,
      "text": "wahā ana dhā `ubaydukal faqīru ilayk"
    },
    {
      "index": 10,
      "text": "al mu-ammilu faḍlaka wama`rūfak"
    },
    {
      "index": 11,
      "text": "fa-in kunta yā mawlāya tafaḍḍalta fī hādhihil laylati `alā aḥadin min khalqik"
    },
    {
      "index": 12,
      "text": "wa`udta `alayhi bi`ā-idatin min `aṭfik"
    },
    {
      "index": 13,
      "text": "faṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 14,
      "text": "aṭ ṭayyibīnaṭ ṭāhirīnal khayyirīnal fāḍilīn"
    },
    {
      "index": 15,
      "text": "wajud `alayya biṭawlika wama`rūfik"
    },
    {
      "index": 16,
      "text": "yā rabbal `ālamīn"
    },
    {
      "index": 17,
      "text": "waṣallallāhu `alā muḥammadin khātamin nabiyyīn"
    },
    {
      "index": 18,
      "text": "wa ālihiṭ ṭāhirīn"
    },
    {
      "index": 19,
      "text": "wasallama taslīmā"
    },
    {
      "index": 20,
      "text": "innallāha ḥamīdun majīd"
    },
    {
      "index": 21,
      "text": "allāhumma innī ad`ūka kamā amar-t"
    },
    {
      "index": 22,
      "text": "fastajib lī kamā wa`adt"
    },
    {
      "index": 23,
      "text": "innaka lātukhliful mī`ād"
    }
  ]
}