{
  "index": 197,
  "name": "Sura 197",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluka biḥaqqi muḥammadin nabiyyik"
    },
    {
      "index": 3,
      "text": "wa`aliyyin waliyyik"
    },
    {
      "index": 4,
      "text": "wash sha-ni walqadril ladhī khaṣaṣtahumā bihi dūna khalqik"
    },
    {
      "index": 5,
      "text": "an tuṣalliya `alā muḥammadin wa`aliyy"
    },
    {
      "index": 6,
      "text": "wa-an tabda-a bihimā fī kulli khayrin `ājil"
    },
    {
      "index": 7,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 8,
      "text": "al a-immatil qādah"
    },
    {
      "index": 9,
      "text": "wad du`ātis sādah"
    },
    {
      "index": 10,
      "text": "wan nujūmiz zāhirah"
    },
    {
      "index": 11,
      "text": "wal-a`lāmil bāhirah"
    },
    {
      "index": 12,
      "text": "wasāsatil `ibād"
    },
    {
      "index": 13,
      "text": "wa-ar-kānil bilād"
    },
    {
      "index": 14,
      "text": "wan nāqatil mur-salah"
    },
    {
      "index": 15,
      "text": "was safīnatin nājiyah"
    },
    {
      "index": 16,
      "text": "al jāriyati fil lujajil ghāmirah"
    },
    {
      "index": 17,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 18,
      "text": "khuzzāni `ilmik"
    },
    {
      "index": 19,
      "text": "wa-ar-kāni tawḥīdik"
    },
    {
      "index": 20,
      "text": "wada`ā-imi dīnik"
    },
    {
      "index": 21,
      "text": "wama`ādini karāmatik"
    },
    {
      "index": 22,
      "text": "waṣafwatika min bariyyatik"
    },
    {
      "index": 23,
      "text": "wakhiyaratika min khalqik"
    },
    {
      "index": 24,
      "text": "al atqiyā-il anqiyā"
    },
    {
      "index": 25,
      "text": "an nujabā-il abrār"
    },
    {
      "index": 26,
      "text": "walbābil mubtalā bihin nās"
    },
    {
      "index": 27,
      "text": "man atāhu najā"
    },
    {
      "index": 28,
      "text": "waman abāhu hawā"
    },
    {
      "index": 29,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 30,
      "text": "ahlidh dhikril ladhīna amar-ta bimas-alatihim"
    },
    {
      "index": 31,
      "text": "wadhawil qur-bal ladhīna amar-ta bimawaddatihim"
    },
    {
      "index": 32,
      "text": "wafaraḍta ḥaqqahum"
    },
    {
      "index": 33,
      "text": "waja`altal jannata ma`āda maniqtaṣṣa-āthārahum"
    },
    {
      "index": 34,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 35,
      "text": "kamā amarū biṭā`atik"
    },
    {
      "index": 36,
      "text": "wanahū `an ma`ṣiyatik"
    },
    {
      "index": 37,
      "text": "wadallū `ibādaka `alā waḥdāniyyatik"
    },
    {
      "index": 38,
      "text": "allāhumma innī as-aluka biḥaqqi muḥammad"
    },
    {
      "index": 39,
      "text": "nabiyyika wanajībik"
    },
    {
      "index": 40,
      "text": "waṣafwatika wa-amīnik"
    },
    {
      "index": 41,
      "text": "warasūlika ilā khalqik"
    },
    {
      "index": 42,
      "text": "wabiḥaqqi amīril mu-minīn"
    },
    {
      "index": 43,
      "text": "waya`sūbid dīn"
    },
    {
      "index": 44,
      "text": "waqā-idil ghurril muḥajjalīn"
    },
    {
      "index": 45,
      "text": "al waṣiyyil wafiyy"
    },
    {
      "index": 46,
      "text": "waṣ ṣiddīqil akbar"
    },
    {
      "index": 47,
      "text": "walfārūqi baynal ḥaqqi walbāṭil"
    },
    {
      "index": 48,
      "text": "wash shāhidi lak"
    },
    {
      "index": 49,
      "text": "wad dālli `alayk"
    },
    {
      "index": 50,
      "text": "waṣ ṣādi`i bi-amrik"
    },
    {
      "index": 51,
      "text": "walmujāhidi fī sabīlik"
    },
    {
      "index": 52,
      "text": "lam ta-khudh-hu fīka lawmatu lā-im"
    },
    {
      "index": 53,
      "text": "an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 54,
      "text": "wa-an taj`alanī fī hādhal yawm"
    },
    {
      "index": 55,
      "text": "al ladhī `aqadta fīhi liwaliyyikal `ahda fī a`nāqi khalqik"
    },
    {
      "index": 56,
      "text": "wa-akmalta lahumud dīn"
    },
    {
      "index": 57,
      "text": "minal `ārifīna biḥur-matih"
    },
    {
      "index": 58,
      "text": "walmuqirrīna bifaḍlih"
    },
    {
      "index": 59,
      "text": "min `utaqā-ika waṭulaqā-ika minan nār"
    },
    {
      "index": 60,
      "text": "walā tushmit bī ḥāsidin ni`am"
    },
    {
      "index": 61,
      "text": "allāhumma fakamā ja`altahu `īdakal akbar"
    },
    {
      "index": 62,
      "text": "wasammaytahu fis samā-i yawmal `ahdil ma`hūd"
    },
    {
      "index": 63,
      "text": "wafil ar-ḍi yawmal mīthāqil ma-khūdhi waljam`il mas-ūl"
    },
    {
      "index": 64,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 65,
      "text": "wa-aqrir bihi `uyūnanā"
    },
    {
      "index": 66,
      "text": "wajma` bihi shamlanā"
    },
    {
      "index": 67,
      "text": "walā tuḍillanā ba`da idh hadaytanā"
    },
    {
      "index": 68,
      "text": "waj`alnā li-an`umika minash shākirīn"
    },
    {
      "index": 69,
      "text": "yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 70,
      "text": "al ḥamdu lillāhil ladhī `arrafanā faḍla hādhal yawm"
    },
    {
      "index": 71,
      "text": "wabaṣṣaranā ḥur-matah"
    },
    {
      "index": 72,
      "text": "wakarramanā bih"
    },
    {
      "index": 73,
      "text": "washarrafanā bima`rifatih"
    },
    {
      "index": 74,
      "text": "wahadānā binūrih"
    },
    {
      "index": 75,
      "text": "yā rasūlallāh"
    },
    {
      "index": 76,
      "text": "yā amīralmu-minīn"
    },
    {
      "index": 77,
      "text": "`alaykumā wa`alā `itratikumā"
    },
    {
      "index": 78,
      "text": "wa`alā muḥibbīkumā minnī afḍalus salām"
    },
    {
      "index": 79,
      "text": "mā baqiyal laylu wan nahār"
    },
    {
      "index": 80,
      "text": "wabikumā atawajjahu ilallāhi rabbī warabbikumā"
    },
    {
      "index": 81,
      "text": "fī najāḥi ṭalibtī"
    },
    {
      "index": 82,
      "text": "waqaḍā-i ḥawā-ijī"
    },
    {
      "index": 83,
      "text": "wataysīri umūrī"
    },
    {
      "index": 84,
      "text": "allāhumma innī as-aluka biḥaqqi muḥammadin wa āli muḥammad"
    },
    {
      "index": 85,
      "text": "an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 86,
      "text": "wa-an tal`ana man jaḥada ḥaqqa hādhal yawm"
    },
    {
      "index": 87,
      "text": "wa-ankara ḥur-matah"
    },
    {
      "index": 88,
      "text": "faṣadda `an sabīlika li-iṭfā-i nūrik"
    },
    {
      "index": 89,
      "text": "fa-aballāhu illā an yutimma nūrah"
    },
    {
      "index": 90,
      "text": "allāhumma farrij `an ahli bayti muḥammadin nabiyyik"
    },
    {
      "index": 91,
      "text": "wakshif `anhum wabihim `anil mu-minīnal kurubāt"
    },
    {
      "index": 92,
      "text": "allāhummamla-al ar-ḍa bihim `adlā"
    },
    {
      "index": 93,
      "text": "kamā muli-at ẓulman wajawrā"
    },
    {
      "index": 94,
      "text": "wa-anjiz lahum mā wa`adtahum"
    },
    {
      "index": 95,
      "text": "innaka lātukhliful mī`ād"
    }
  ]
}