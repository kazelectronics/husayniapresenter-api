{
  "index": 75,
  "name": "Sura 75",
  "ayas": [
    {
      "index": 1,
      "text": "lā uqsimu biyawmil qiyāmah",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "walā uqsimu bin nafsillawwāmah"
    },
    {
      "index": 3,
      "text": "ayaḥsabul-insānu allan najma`a `iẓāmah"
    },
    {
      "index": 4,
      "text": "balā qādirīna `alā an nusawwiya banānah"
    },
    {
      "index": 5,
      "text": "bal yurīdul-insānu liyafjura amāmah"
    },
    {
      "index": 6,
      "text": "yas-alu ayyāna yawmul qiyāmah"
    },
    {
      "index": 7,
      "text": "fa-idhā bariqal baṣar"
    },
    {
      "index": 8,
      "text": "wakhasafal qamar"
    },
    {
      "index": 9,
      "text": "wajumi`ash shamsu wal qamar"
    },
    {
      "index": 10,
      "text": "yaqūlul-insānu yawma-idhin aynal mafarr"
    },
    {
      "index": 11,
      "text": "kallā lā wazar"
    },
    {
      "index": 12,
      "text": "ilā rabbika yawma-idhinil-mustaqarr"
    },
    {
      "index": 13,
      "text": "yunabba-ul-insānu yawma-idhin bimā qaddama wa-akhhar"
    },
    {
      "index": 14,
      "text": "balil-insānu `alā nafsihi baṣīrah"
    },
    {
      "index": 15,
      "text": "walaw alqā ma`ādhīrah"
    },
    {
      "index": 16,
      "text": "lā tuḥarrik bihi lisānaka lita`jala bih"
    },
    {
      "index": 17,
      "text": "inna `alaynā jam`ahu waqur-ānah"
    },
    {
      "index": 18,
      "text": "fa-idhā qara-nāhu fattabi` qur-ānah"
    },
    {
      "index": 19,
      "text": "thumma inna `alaynā bayānah"
    },
    {
      "index": 20,
      "text": "kallā bal tuḥibbūnal `ājilah"
    },
    {
      "index": 21,
      "text": "watadharūnal-ākhirah"
    },
    {
      "index": 22,
      "text": "wujūhun yawma-idhin nāḍirah"
    },
    {
      "index": 23,
      "text": "ilā rabbihā nāẓirah"
    },
    {
      "index": 24,
      "text": "wawujūhun yawma-idhin bāsirah"
    },
    {
      "index": 25,
      "text": "taẓunnu an yuf`ala bihā fāqirah"
    },
    {
      "index": 26,
      "text": "kallā idhā balaghatit tarāqiy"
    },
    {
      "index": 27,
      "text": "waqīla man ۜ rāq"
    },
    {
      "index": 28,
      "text": "waẓanna annahul firāq"
    },
    {
      "index": 29,
      "text": "wal taffatis sāqu bis sāq"
    },
    {
      "index": 30,
      "text": "ilā rabbika yawma-idhinil-masāq"
    },
    {
      "index": 31,
      "text": "falā ṣaddaqa walā ṣallā"
    },
    {
      "index": 32,
      "text": "walākin kadhhaba watawallā"
    },
    {
      "index": 33,
      "text": "thumma dhahaba ilā ahlihi yatamaṭṭā"
    },
    {
      "index": 34,
      "text": "awlā laka fa-awlā"
    },
    {
      "index": 35,
      "text": "thumma awlā laka fa-awlā"
    },
    {
      "index": 36,
      "text": "ayaḥsabul-insānu an yutraka sudā"
    },
    {
      "index": 37,
      "text": "alam yaku nuṭfatan min maniyyin yumnā"
    },
    {
      "index": 38,
      "text": "thumma kāna `alaqatan fakhalaqa fasawwā"
    },
    {
      "index": 39,
      "text": "faja`ala minhuz zawjaynidh dhakara wal-unthā"
    },
    {
      "index": 40,
      "text": "alaysa dhālika biqādirin `alā an yuḥ-yiyal mawtā"
    }
  ]
}