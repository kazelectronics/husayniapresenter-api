{
  "index": 409,
  "name": "Sura 409",
  "ayas": [
    {
      "index": 1,
      "text": "wasta`mil bil qur-āni badanī",
      "bismillah": "allāhummashraḥ bil qur-āni ṣadrī"
    },
    {
      "index": 2,
      "text": "wanawwir bil qur-āni baṣarī"
    },
    {
      "index": 3,
      "text": "wa-aṭliq bil qur-āni lisānī"
    },
    {
      "index": 4,
      "text": "wa-a`innī `alayhi mā abqaytanī"
    },
    {
      "index": 5,
      "text": "fa-innahu lā ḥawla walā quwwata illā bik"
    }
  ]
}