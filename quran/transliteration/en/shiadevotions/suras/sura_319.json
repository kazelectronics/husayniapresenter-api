{
  "index": 319,
  "name": "Sura 319",
  "ayas": [
    {
      "index": 1,
      "text": "al latī lā ya`lamuhā illā ant",
      "bismillah": "allāhumma innī as-aluka bimafātiḥil ghayb"
    },
    {
      "index": 2,
      "text": "an tuṣalliya `alā muḥammadin wa-ālihi wa an"
    },
    {
      "index": 3,
      "text": ""
    },
    {
      "index": 4,
      "text": "allāhumma anta waliyyu ni`matī"
    },
    {
      "index": 5,
      "text": "wal qādiru `alā ṭalibatī"
    },
    {
      "index": 6,
      "text": "ta`lamu ḥājatī"
    },
    {
      "index": 7,
      "text": "fa-as-aluka biḥaqqi muḥammadin wa-ālih"
    },
    {
      "index": 8,
      "text": "`alayhi wa `alayhimus salām"
    },
    {
      "index": 9,
      "text": "lammā qaḍaytahā lī"
    }
  ]
}