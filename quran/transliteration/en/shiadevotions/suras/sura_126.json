{
  "index": 126,
  "name": "Sura 126",
  "ayas": [
    {
      "index": 1,
      "text": "wawā`adnā mūsā thalāthīna laylah",
      "bismillah": ""
    },
    {
      "index": 2,
      "text": "wa-atmamnāhā bi`ashr"
    },
    {
      "index": 3,
      "text": "fatamma mīqātu rabbihi ar-ba`īna laylah"
    },
    {
      "index": 4,
      "text": "waqāla mūsā li-akhīhi hārūnakhlufnī fī qawmī"
    },
    {
      "index": 5,
      "text": "wa-aṣliḥ walā tattabi` sabīlal mufsidīn"
    }
  ]
}