{
  "index": 188,
  "name": "Sura 188",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma yā ajwada man a`ṭā"
    },
    {
      "index": 3,
      "text": "wayā khayra man su-il"
    },
    {
      "index": 4,
      "text": "wayā ar-ḥama manistur-ḥim"
    },
    {
      "index": 5,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālihi fil awwalīn"
    },
    {
      "index": 6,
      "text": "waṣalli `alā muḥammadin wa ālihi fil-ākhirīn"
    },
    {
      "index": 7,
      "text": "waṣalli `alā muḥammadin wa ālihi fil mala-al a`lā"
    },
    {
      "index": 8,
      "text": "waṣalli `alā muḥammadin wa ālihi fil mur-salīn"
    },
    {
      "index": 9,
      "text": "allāhumma a`ṭi muḥammadan wa ālah"
    },
    {
      "index": 10,
      "text": "al wasīlata walfaḍīlah"
    },
    {
      "index": 11,
      "text": "wash sharafa war rif`ah"
    },
    {
      "index": 12,
      "text": "wad darajatal kabīrah"
    },
    {
      "index": 13,
      "text": "allāhumma innī-āmantu bimuḥammad"
    },
    {
      "index": 14,
      "text": "ṣallallāhu `alayhi wa ālihi walam arah"
    },
    {
      "index": 15,
      "text": "falā taḥrimnī fil qiyāmati ru-yatah"
    },
    {
      "index": 16,
      "text": "war-zuqnī ṣuḥbatah"
    },
    {
      "index": 17,
      "text": "watawaffanī `alā millatih"
    },
    {
      "index": 18,
      "text": "wasqinī min ḥawḍih"
    },
    {
      "index": 19,
      "text": "mashraban rawiyyā"
    },
    {
      "index": 20,
      "text": "sā-ighan hanī-ā"
    },
    {
      "index": 21,
      "text": "lā-aẓma-u ba`dahu abadā"
    },
    {
      "index": 22,
      "text": "innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 23,
      "text": "allāhumma innī-āmantu bimuḥammad"
    },
    {
      "index": 24,
      "text": "ṣallallāhu `alayhi wa ālihi walam arah"
    },
    {
      "index": 25,
      "text": "fa`arrifnī fil jināni wajhah"
    },
    {
      "index": 26,
      "text": "allāhumma balligh muḥammadā"
    },
    {
      "index": 27,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 28,
      "text": "minnī taḥiyyatan kathīratan wasalāmā"
    }
  ]
}