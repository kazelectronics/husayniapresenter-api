{
  "index": 192,
  "name": "Sura 192",
  "ayas": [
    {
      "index": 1,
      "text": "khātamin nabiyyīn",
      "bismillah": "as salāmu `alā muḥammadin rasūlillāh"
    },
    {
      "index": 2,
      "text": "wasayyidil mur-salīn"
    },
    {
      "index": 3,
      "text": "waṣafwati rabbil `ālamīn"
    },
    {
      "index": 4,
      "text": "amīnillāhi `alā waḥyih"
    },
    {
      "index": 5,
      "text": "wa`azā-imi amrih"
    },
    {
      "index": 6,
      "text": "walkhātimi limā sabaq"
    },
    {
      "index": 7,
      "text": "walfātiḥi limastuqbil"
    },
    {
      "index": 8,
      "text": "walmuhaymini `alā dhālika kullih"
    },
    {
      "index": 9,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 10,
      "text": "waṣalawātuhu wataḥiyyātuh"
    },
    {
      "index": 11,
      "text": "as salāmu `alā anbiyā-illāhi warusulih"
    },
    {
      "index": 12,
      "text": "wamalā-ikatihil muqarrabīn"
    },
    {
      "index": 13,
      "text": "wa`ibādihiṣ ṣāliḥīn"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka yā amīral mu-minīn"
    },
    {
      "index": 15,
      "text": "wasayyidal waṣiyyīn"
    },
    {
      "index": 16,
      "text": "wawāritha `ilmin nabiyyīn"
    },
    {
      "index": 17,
      "text": "wawaliyya rabbil `ālamīn"
    },
    {
      "index": 18,
      "text": "wamawlāya wamawlal mu-minīn"
    },
    {
      "index": 19,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka yā mawlāya yā amīral mu-minīn"
    },
    {
      "index": 21,
      "text": "yā amīnallāhi fī ar-ḍih"
    },
    {
      "index": 22,
      "text": "wasafīrahu fī khalqih"
    },
    {
      "index": 23,
      "text": "waḥujjatahul bālighata `alā `ibādih"
    },
    {
      "index": 24,
      "text": "as salāmu `alayka yā dīnallāhil qawīm"
    },
    {
      "index": 25,
      "text": "waṣirāṭahul mustaqīm"
    },
    {
      "index": 26,
      "text": "as salāmu `alayka ayyuhan naba-ul `aẓīm"
    },
    {
      "index": 27,
      "text": "al ladhī hum fīhi mukhtalifūn"
    },
    {
      "index": 28,
      "text": "wa`anhu yus-alūn"
    },
    {
      "index": 29,
      "text": "as salāmu `alayka yā amīral mu-minīn"
    },
    {
      "index": 30,
      "text": "āmanta billāhi wahum mushrikūn"
    },
    {
      "index": 31,
      "text": "waṣaddaqta bil ḥaqqi wahum mukadhhibūn"
    },
    {
      "index": 32,
      "text": "wajāhadta fillāhi wahum muḥjimūn"
    },
    {
      "index": 33,
      "text": "wa`abadtallāha mukhliṣan lahud dīn"
    },
    {
      "index": 34,
      "text": "ṣābiran muḥtasiban ḥattā atākal yaqīn"
    },
    {
      "index": 35,
      "text": "alā la`natullāhi `alaẓ ẓālimīn"
    },
    {
      "index": 36,
      "text": "as salāmu `alayka yā sayyidal muslimīn"
    },
    {
      "index": 37,
      "text": "waya`sūbal mu-minīn"
    },
    {
      "index": 38,
      "text": "wa-imāmal muttaqīn"
    },
    {
      "index": 39,
      "text": "waqā-idal ghurril muḥajjalīn"
    },
    {
      "index": 40,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 41,
      "text": "ash-hadu annaka akhū rasūlillāh"
    },
    {
      "index": 42,
      "text": "wawaṣiyyuhu wawārithu `ilmih"
    },
    {
      "index": 43,
      "text": "wa-amīnuhu `alā shar-`ih"
    },
    {
      "index": 44,
      "text": "wakhalīfatuhu fī ummatih"
    },
    {
      "index": 45,
      "text": "wa-awwalu man-āmana billāh"
    },
    {
      "index": 46,
      "text": "waṣaddaqa bimā unzila `alā nabiyyih"
    },
    {
      "index": 47,
      "text": "wa-ash-hadu annahu qad ballagha `anillāhi mā anzalahu fīk"
    },
    {
      "index": 48,
      "text": "faṣada`a bi-amrih"
    },
    {
      "index": 49,
      "text": "wa-awjaba `alā ummatihi far-ḍa ṭā`atika wawilāyatik"
    },
    {
      "index": 50,
      "text": "wa`aqada `alayhimul bay`ata lak"
    },
    {
      "index": 51,
      "text": "waja`alaka awlā bil mu-minīna min anfusihim"
    },
    {
      "index": 52,
      "text": "kamā ja`alahullāhu kadhālik"
    },
    {
      "index": 53,
      "text": "thumma ash-hadallāha ta`ālā `alayhim"
    },
    {
      "index": 54,
      "text": "faqāla alastu qad ballaght"
    },
    {
      "index": 55,
      "text": "faqālū allāhumma balā"
    },
    {
      "index": 56,
      "text": "faqāla allāhummash-had"
    },
    {
      "index": 57,
      "text": "wakafā bika shahīdan waḥākiman baynal `ibād"
    },
    {
      "index": 58,
      "text": "fala`anallāhu jāḥida wilāyatika ba`dal iqrār"
    },
    {
      "index": 59,
      "text": "wanākitha `ahdika ba`dal mīthāq"
    },
    {
      "index": 60,
      "text": "wa-ash-hadu annaka wafayta bi`ahdillāhi ta`ālā"
    },
    {
      "index": 61,
      "text": "wa-annallāha ta`ālā mūfin laka bi`ahdih"
    },
    {
      "index": 62,
      "text": "waman awfā bimā `āhada `alayhullāha fasayu-tīhi ajran `aẓīmā"
    },
    {
      "index": 63,
      "text": "wa-ash-hadu annaka amīrul mu-minīnal ḥaqq"
    },
    {
      "index": 64,
      "text": "al ladhī naṭaqa biwilāyatikat tanzīl"
    },
    {
      "index": 65,
      "text": "wa-akhadha lakal `ahda `alal ummati bidhālikar rasūl"
    },
    {
      "index": 66,
      "text": "wa-ash-hadu annaka wa`ammaka wa-akhāk"
    },
    {
      "index": 67,
      "text": "al ladhīna tājar-tumullāha binufūsikum"
    },
    {
      "index": 68,
      "text": "fa-anzalallāhu fīkum"
    },
    {
      "index": 69,
      "text": "innallāhashtarā minal mu-minīna anfusahum wa-amwālahum"
    },
    {
      "index": 70,
      "text": "bi-annna lahumul jannah"
    },
    {
      "index": 71,
      "text": "yuqātilūna fī sabīlillāhi fayaqtulūna wayuqtalūn"
    },
    {
      "index": 72,
      "text": "wa`dan `alayhi ḥaqqan fit tawrāti wal-injīli walqur-ān"
    },
    {
      "index": 73,
      "text": "waman awfā bi`ahdihi minallāh"
    },
    {
      "index": 74,
      "text": "fastabshirū bibay`ikumul ladhī bāya`tum bih"
    },
    {
      "index": 75,
      "text": "wadhālika huwal fawzul `aẓīm"
    },
    {
      "index": 76,
      "text": "at tā-ibūnal `ābidūn"
    },
    {
      "index": 77,
      "text": "al ḥāmidūnas sā-iḥūn"
    },
    {
      "index": 78,
      "text": "ar rāki`ūnas sājidūn"
    },
    {
      "index": 79,
      "text": "al-āmirūna bil ma`rūfi wan nāhūna `anil munkar"
    },
    {
      "index": 80,
      "text": "walḥāfiẓūna liḥudūdillāhi wabashhiril mu-minīn"
    },
    {
      "index": 81,
      "text": "ash-hadu yā amīral mu-minīn"
    },
    {
      "index": 82,
      "text": "annash shākka fīka mā-āmana birrasūlil amīn"
    },
    {
      "index": 83,
      "text": "wa-annal `ādila bika ghayraka `ānidun `anid dīnil qawīm"
    },
    {
      "index": 84,
      "text": "al ladhir-taḍāhu lanā rabbul `ālamīn"
    },
    {
      "index": 85,
      "text": "wa-akmalahu biwilāyatika yawmal ghadīr"
    },
    {
      "index": 86,
      "text": "wa-ash-hadu annakal ma`niyyu biqawlil `azīzir raḥīm"
    },
    {
      "index": 87,
      "text": "wa-anna hādhā ṣirāṭī mustaqīman fāttibi`ūh"
    },
    {
      "index": 88,
      "text": "walā tattabi`us subula fatafarraqa bikum `an sabīlih"
    },
    {
      "index": 89,
      "text": "ḍalla wallāhi wa-aḍalla manittaba`a siwāk"
    },
    {
      "index": 90,
      "text": "wa`anada `anil ḥaqqi man `ādāk"
    },
    {
      "index": 91,
      "text": "allāhumma sami`nā li-amrik"
    },
    {
      "index": 92,
      "text": "wa-aṭa`nā wattaba`nā ṣirāṭakal mustaqīm"
    },
    {
      "index": 93,
      "text": "fahdinā rabbanā"
    },
    {
      "index": 94,
      "text": "walā tuzigh qulūbanā ba`da idh hadaytanā ilā ṭā`atik"
    },
    {
      "index": 95,
      "text": "waj`alnā minash shākirīna li-an`umik"
    },
    {
      "index": 96,
      "text": "wa-ash-hadu annaka lam tazal lilhawā mukhālifā"
    },
    {
      "index": 97,
      "text": "walittuqā muḥālifā"
    },
    {
      "index": 98,
      "text": "wa`alā kaẓmil ghayẓi qādirā"
    },
    {
      "index": 99,
      "text": "wa`anin nāsi `āfiyan ghāfirā"
    },
    {
      "index": 100,
      "text": "wa-idhā `uṣiyallāhu sākhiṭā"
    },
    {
      "index": 101,
      "text": "wa-idhā uṭī`allāhu rāḍiyā"
    },
    {
      "index": 102,
      "text": "wabimā `ahida ilayka `āmilā"
    },
    {
      "index": 103,
      "text": "rā`iyan limastuḥfiẓt"
    },
    {
      "index": 104,
      "text": "ḥāfiẓan limastūdi`t"
    },
    {
      "index": 105,
      "text": "muballighan mā ḥummilt"
    },
    {
      "index": 106,
      "text": "muntaẓiran mā wu`idt"
    },
    {
      "index": 107,
      "text": "wa-ash-hadu annaka mattaqayta ḍāri`ā"
    },
    {
      "index": 108,
      "text": "walā amsakta `an ḥaqqika jāzi`ā"
    },
    {
      "index": 109,
      "text": "walā aḥjamta `an mujāhadati ghāṣibīka nākilā"
    },
    {
      "index": 110,
      "text": "walā aẓhar-tar riḍā bikhilāfi mā yur-ḍillāha mudāhinā"
    },
    {
      "index": 111,
      "text": "walā wahanta limā aṣābaka fī sabīlillāh"
    },
    {
      "index": 112,
      "text": "walā ḍa`ufta walastakanta `an ṭalabi ḥaqqika murāqibā"
    },
    {
      "index": 113,
      "text": "ma`ādhallāhi an takūna kadhālik"
    },
    {
      "index": 114,
      "text": "bal idh ẓulimtaḥtasabta rabbak"
    },
    {
      "index": 115,
      "text": "wafawwaḍta ilayhi amrak"
    },
    {
      "index": 116,
      "text": "wadhakkar-tahum famāddakarū"
    },
    {
      "index": 117,
      "text": "wawa`aẓtahum famatta`aẓuwā"
    },
    {
      "index": 118,
      "text": "wakhawwaftahumullāha famā takhawwafuwā"
    },
    {
      "index": 119,
      "text": "wa-ash-hadu annaka yā amīral mu-minīn"
    },
    {
      "index": 120,
      "text": "jāhadta fillāhi ḥaqqa jihādih"
    },
    {
      "index": 121,
      "text": "ḥattā da`ākallāhu ilā jiwārih"
    },
    {
      "index": 122,
      "text": "waqabaḍaka ilayhi bikhtiyārih"
    },
    {
      "index": 123,
      "text": "wa-alzama a`dā-akal ḥujjata biqatlihim iyyāk"
    },
    {
      "index": 124,
      "text": "litakūnal ḥujjatu laka `alayhim"
    },
    {
      "index": 125,
      "text": "ma`a mā laka minal ḥujajil bālighati `alā jamī`i khalqih"
    },
    {
      "index": 126,
      "text": "as salāmu `alayka yā amīral mu-minīn"
    },
    {
      "index": 127,
      "text": "`abadtallāha mukhliṣā"
    },
    {
      "index": 128,
      "text": "wajāhadta fillāhi ṣābirā"
    },
    {
      "index": 129,
      "text": "wajudta binafsika muḥtasibā"
    },
    {
      "index": 130,
      "text": "wa`amilta bikitābih"
    },
    {
      "index": 131,
      "text": "wattaba`ta sunnata nabiyyih"
    },
    {
      "index": 132,
      "text": "wa-aqamtaṣ ṣalāh"
    },
    {
      "index": 133,
      "text": "wa ātaytaz zakāh"
    },
    {
      "index": 134,
      "text": "wa-amar-ta bil ma`rūf"
    },
    {
      "index": 135,
      "text": "wanahayta `anil munkari mastaṭa`t"
    },
    {
      "index": 136,
      "text": "mubtaghiyan mā `indallāh"
    },
    {
      "index": 137,
      "text": "rāghiban fī mā wa`adallāh"
    },
    {
      "index": 138,
      "text": "lā taḥfilu binnawā-ib"
    },
    {
      "index": 139,
      "text": "walā tahinu `indash shadā-id"
    },
    {
      "index": 140,
      "text": "walā taḥjimu `an muḥārib"
    },
    {
      "index": 141,
      "text": "afika man nasaba ghayra dhālika ilayk"
    },
    {
      "index": 142,
      "text": "waftarā bāṭilan `alayk"
    },
    {
      "index": 143,
      "text": "wa-awlā liman `anada `ank"
    },
    {
      "index": 144,
      "text": "laqad jāhadta fillāhi ḥaqqal jihād"
    },
    {
      "index": 145,
      "text": "waṣabar-ta `alal adhā ṣabraḥtisāb"
    },
    {
      "index": 146,
      "text": "wa-anta awwalu man-āmana billāh"
    },
    {
      "index": 147,
      "text": "waṣallā lahu wajāhad"
    },
    {
      "index": 148,
      "text": "wa-abdā ṣafḥatahu fī dārish shir-k"
    },
    {
      "index": 149,
      "text": "wal-ar-ḍu mash-ḥūnatun ḍalālah"
    },
    {
      "index": 150,
      "text": "wash shayṭānu yu`badu jahrah"
    },
    {
      "index": 151,
      "text": "wa-antal qā-ilu lā tazīdunī kathratun nāsi ḥawlī `izzah"
    },
    {
      "index": 152,
      "text": "walā tafarruquhum `annī waḥshah"
    },
    {
      "index": 153,
      "text": "walaw aslamaniyan nāsu jamī`an lam akun mutaḍarri`ā"
    },
    {
      "index": 154,
      "text": "i`taṣamta billāhi fa`azazt"
    },
    {
      "index": 155,
      "text": "wāthar-tal ākhirata `alal ūlā fazahidt"
    },
    {
      "index": 156,
      "text": "wa-ayyadakallāhu wahadāk"
    },
    {
      "index": 157,
      "text": "wa-akhlaṣaka wajtabāk"
    },
    {
      "index": 158,
      "text": "famā tanāqaḍat af`āluk"
    },
    {
      "index": 159,
      "text": "walakhtalafat aqwāluk"
    },
    {
      "index": 160,
      "text": "walā taqallabat aḥwāluk"
    },
    {
      "index": 161,
      "text": "walādda`ayta walaftarayta `alallāhi kadhibā"
    },
    {
      "index": 162,
      "text": "walā sharihta ilal ḥuṭām"
    },
    {
      "index": 163,
      "text": "walā dannasakal-āthām"
    },
    {
      "index": 164,
      "text": "walam tazal `alā bayyinatin min rabbik"
    },
    {
      "index": 165,
      "text": "wayaqīnin min amrik"
    },
    {
      "index": 166,
      "text": "tahdī ilal ḥaqqi wa-ilā ṣirāṭin mustaqīm"
    },
    {
      "index": 167,
      "text": "ash-hadu shahādata ḥaqq"
    },
    {
      "index": 168,
      "text": "wa-uqsimu billāhi qasama ṣidq"
    },
    {
      "index": 169,
      "text": "anna muḥammadan wa ālahu ṣalawātullāhi `alayhim sādātul khalq"
    },
    {
      "index": 170,
      "text": "wa-annaka mawlāya wamawlal mu-minīn"
    },
    {
      "index": 171,
      "text": "wa-annaka `abdullāhi wawaliyyuh"
    },
    {
      "index": 172,
      "text": "wa-akhur rasūli wawaṣiyyuhu wawārithuh"
    },
    {
      "index": 173,
      "text": "wa-annahul qā-ilu lak"
    },
    {
      "index": 174,
      "text": "walladhī ba`athanī bil ḥaqqi mā-āmana bī man kafara bik"
    },
    {
      "index": 175,
      "text": "walā aqarra billāhi man jaḥadak"
    },
    {
      "index": 176,
      "text": "waqad ḍalla man ṣadda `ank"
    },
    {
      "index": 177,
      "text": "walam yahtadi ilallāhi walā ilayya man lā yahtadī bik"
    },
    {
      "index": 178,
      "text": "wahuwa qawlu rabbī `azza wajall"
    },
    {
      "index": 179,
      "text": "wa-innī laghaffārun liman tāba wa āmana wa`amila ṣāliḥā"
    },
    {
      "index": 180,
      "text": "thummahtadā ilā wilāyatik"
    },
    {
      "index": 181,
      "text": "mawlāya faḍluka lā yakhfā"
    },
    {
      "index": 182,
      "text": "wanūruka lā yuṭfa-u"
    },
    {
      "index": 183,
      "text": "wa-anna man jaḥadakaẓ ẓalūmul ashqā"
    },
    {
      "index": 184,
      "text": "mawlāya antal ḥujjatu `alal `ibād"
    },
    {
      "index": 185,
      "text": "walhādī ilar rashād"
    },
    {
      "index": 186,
      "text": "wal`uddatu lilma`ād"
    },
    {
      "index": 187,
      "text": "mawlāya laqad rafa`allāhu fil ūlā manzilatak"
    },
    {
      "index": 188,
      "text": "wa-a`lā fil-ākhirati darajatak"
    },
    {
      "index": 189,
      "text": "wabaṣṣaraka mā `amiya `alā man khālafak"
    },
    {
      "index": 190,
      "text": "waḥāla baynaka wabayna mawāhibillāhi lak"
    },
    {
      "index": 191,
      "text": "fala`anallāhu mustaḥillil ḥur-mati mink"
    },
    {
      "index": 192,
      "text": "wadhā-idil ḥaqqi `ank"
    },
    {
      "index": 193,
      "text": "wa-ash-hadu annahumul akhsarūn"
    },
    {
      "index": 194,
      "text": "al ladhīna talfaḥu wujūhahumun nār"
    },
    {
      "index": 195,
      "text": "wahum fīhā kāliḥūn"
    },
    {
      "index": 196,
      "text": "wa-ash-hadu annaka mā aqdamta walā aḥjamt"
    },
    {
      "index": 197,
      "text": "walā naṭaqta walā amsakt"
    },
    {
      "index": 198,
      "text": "illā bi-amrin minallāhi warasūlih"
    },
    {
      "index": 199,
      "text": "qulta walladhī nafsī biyadih"
    },
    {
      "index": 200,
      "text": "laqad naẓara ilayya rasūlullāhi ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 201,
      "text": "aḍribu bissayfi qudman faqāl"
    },
    {
      "index": 202,
      "text": "yā `aliyyu anta minnī bimanzilati hārūna min mūsā"
    },
    {
      "index": 203,
      "text": "illā annahu lā nabiyya ba`dī"
    },
    {
      "index": 204,
      "text": "wa-u`limuka anna mawtaka waḥayātaka ma`ī wa`alā sunnatī"
    },
    {
      "index": 205,
      "text": "fawallāhi mā kadhibtu walā kudhhibt"
    },
    {
      "index": 206,
      "text": "walā ḍalaltu walā ḍulli bī"
    },
    {
      "index": 207,
      "text": "walā nasītu mā `ahida ilayya rabbī"
    },
    {
      "index": 208,
      "text": "wa-innī la`alā bayyinatin min rabbī bayyanahā linabiyyih"
    },
    {
      "index": 209,
      "text": "wabayyanahan nabiyyu lī"
    },
    {
      "index": 210,
      "text": "wa-innī la`alaṭ ṭarīqil wāḍiḥi alfiẓuhu lafẓā"
    },
    {
      "index": 211,
      "text": "ṣadaqta wallāhi waqultal ḥaqq"
    },
    {
      "index": 212,
      "text": "fala`anallāhu man sāwāka biman nāwāk"
    },
    {
      "index": 213,
      "text": "wallāhu jallasmuhu yaqūl"
    },
    {
      "index": 214,
      "text": "hal yastawil ladhīna ya`lamūna walladhīna lā ya`lamūn"
    },
    {
      "index": 215,
      "text": "fala`anallāhu man `adala bika man faraḍallāhu `alayhi wilāyatak"
    },
    {
      "index": 216,
      "text": "wa-anta waliyyullāh"
    },
    {
      "index": 217,
      "text": "wa-akhū rasūlih"
    },
    {
      "index": 218,
      "text": "wadh dhābbu `an dīnih"
    },
    {
      "index": 219,
      "text": "walladhī naṭaqal qur-ānu bitafḍīlih"
    },
    {
      "index": 220,
      "text": "qālallāhu ta`ālā"
    },
    {
      "index": 221,
      "text": "wafaḍḍalallāhul mujāhidīna `alal qā`idīna ajran `aẓīmā"
    },
    {
      "index": 222,
      "text": "darajātin minhu wamaghfiratan waraḥmah"
    },
    {
      "index": 223,
      "text": "wakānallāhu ghafūran raḥīmā"
    },
    {
      "index": 224,
      "text": "waqālallāhu ta`ālā"
    },
    {
      "index": 225,
      "text": "aja`altum siqāyatal ḥājji wa`imāratal masjidil ḥarām"
    },
    {
      "index": 226,
      "text": "kaman-āmana billāhi walyawmil-ākhiri wajāhada fī sabīlillāh"
    },
    {
      "index": 227,
      "text": "lā yastawūna `indallāh"
    },
    {
      "index": 228,
      "text": "wallāhu lā yahdil qawmaẓ ẓālimīn"
    },
    {
      "index": 229,
      "text": "al ladhīna-āmanū wahājarū"
    },
    {
      "index": 230,
      "text": "wajāhadū fī sabīlillāhi bi-amwālihim wa-anfusihim"
    },
    {
      "index": 231,
      "text": "a`ẓamu darajatan `indallāh"
    },
    {
      "index": 232,
      "text": "wa-ūlā-ika humul fā-izūn"
    },
    {
      "index": 233,
      "text": "yubashhiruhum rabbuhum biraḥmatin minhu wariḍwān"
    },
    {
      "index": 234,
      "text": "wajannātin lahum fīhā na`īmun muqīm"
    },
    {
      "index": 235,
      "text": "khālidīna fīhā abadā"
    },
    {
      "index": 236,
      "text": "innallāha `indahu ajrun `aẓīm"
    },
    {
      "index": 237,
      "text": "ash-hadu annakal makhṣūṣu bimid-ḥatillāh"
    },
    {
      "index": 238,
      "text": "al mukhliṣu liṭā`atillāh"
    },
    {
      "index": 239,
      "text": "lam tabghi bil hudā badalā"
    },
    {
      "index": 240,
      "text": "walam tushrik bi`ibādati rabbika aḥadā"
    },
    {
      "index": 241,
      "text": "wa-annallāha ta`ālā istajāba linabiyyih"
    },
    {
      "index": 242,
      "text": "ṣallallāhu `alayhi wa ālihi fīka da`watah"
    },
    {
      "index": 243,
      "text": "thumma amarahu bi-iẓhāri mā awlākal-ummatih"
    },
    {
      "index": 244,
      "text": "i`lā-an lisha-nik"
    },
    {
      "index": 245,
      "text": "wa-i`lānan libur-hānik"
    },
    {
      "index": 246,
      "text": "wadaḥḍan lil-abāṭīl"
    },
    {
      "index": 247,
      "text": "waqaṭ`an lilma`ādhīr"
    },
    {
      "index": 248,
      "text": "falammā ashfaqa min fitnatil fāsiqīn"
    },
    {
      "index": 249,
      "text": "wattaqā fīkal munāfiqīn"
    },
    {
      "index": 250,
      "text": "awḥā ilayhi rabbul `ālamīn"
    },
    {
      "index": 251,
      "text": "yā ayyuhar rasūlu balligh mā unzila ilayka min rabbik"
    },
    {
      "index": 252,
      "text": "wa-in lam taf`al famā ballaghta risālatah"
    },
    {
      "index": 253,
      "text": "wallāhu ya`ṣimuka minan nās"
    },
    {
      "index": 254,
      "text": "fawaḍa`a `alā nafsihi awzāral masīr"
    },
    {
      "index": 255,
      "text": "wanahaḍa fī ramḍā-il hajīr"
    },
    {
      "index": 256,
      "text": "fakhaṭaba wa-asma`"
    },
    {
      "index": 257,
      "text": "wanādā fa-ablagh"
    },
    {
      "index": 258,
      "text": "thumma sa-alahum ajma`"
    },
    {
      "index": 259,
      "text": "faqāla hal ballaght"
    },
    {
      "index": 260,
      "text": "faqālū allāhumma balā"
    },
    {
      "index": 261,
      "text": "faqāla allāhummash-had"
    },
    {
      "index": 262,
      "text": "thumma qāla alastu awlā bil mu-minīna min anfusihim"
    },
    {
      "index": 263,
      "text": "faqālū balā"
    },
    {
      "index": 264,
      "text": "fa-akhadha biyadika waqāl"
    },
    {
      "index": 265,
      "text": "man kuntu mawlāhu fahādhā `aliyyun mawlāh"
    },
    {
      "index": 266,
      "text": "allāhumma wāli man wālāh"
    },
    {
      "index": 267,
      "text": "wa`ādi man `ādāh"
    },
    {
      "index": 268,
      "text": "wanṣur man naṣarah"
    },
    {
      "index": 269,
      "text": "wakhdhul man khadhalah"
    },
    {
      "index": 270,
      "text": "famā-āmana bimā anzalallāhu fīka `alā nabiyyihi illā qalīl"
    },
    {
      "index": 271,
      "text": "walā zāda aktharahum ghayra takhyīr"
    },
    {
      "index": 272,
      "text": "walaqad anzalallāhu ta`ālā fīka min qablu wahum kārihūn"
    },
    {
      "index": 273,
      "text": "yā ayyuhal ladhīna-āmanū"
    },
    {
      "index": 274,
      "text": "man yar-tadda minkum `an dīnih"
    },
    {
      "index": 275,
      "text": "fasawfa ya-tillāhu biqawmin yuḥibbuhum wayuḥibbūnah"
    },
    {
      "index": 276,
      "text": "adhillatin `alal mu-minīn"
    },
    {
      "index": 277,
      "text": "a`izzatin `alal kāfirīn"
    },
    {
      "index": 278,
      "text": "yujāhidūna fī sabīlillāh"
    },
    {
      "index": 279,
      "text": "walā yakhāfūna lawmata lā-im"
    },
    {
      "index": 280,
      "text": "dhālika faḍlullāhi yu-tīhi man yashā"
    },
    {
      "index": 281,
      "text": "wallāhu wāsi`un `alīm"
    },
    {
      "index": 282,
      "text": "innamā waliyyukumullāhu warasūluhu walladhīna-āmanū"
    },
    {
      "index": 283,
      "text": "al ladhīna yuqīmūnaṣ ṣalāh"
    },
    {
      "index": 284,
      "text": "wayu-tūnaz zakāta wahum rāki`ūn"
    },
    {
      "index": 285,
      "text": "waman yatawallallāha warasūlahu walladhīna-āmanū"
    },
    {
      "index": 286,
      "text": "fa-inna ḥizballāhi humul ghālibūn"
    },
    {
      "index": 287,
      "text": "rabbanā-āmannā bimā anzalta wattaba`nar rasūl"
    },
    {
      "index": 288,
      "text": "faktubnā ma`ash shāhidīn"
    },
    {
      "index": 289,
      "text": "rabbanā lā tuzigh qulūbanā ba`da idh hadaytanā"
    },
    {
      "index": 290,
      "text": "wahab lanā min ladunka raḥmah"
    },
    {
      "index": 291,
      "text": "innaka antal wahhāb"
    },
    {
      "index": 292,
      "text": "allāhumma innā na`lamu anna hādhā huwal ḥaqqu min `indik"
    },
    {
      "index": 293,
      "text": "fal`an man `āraḍahu wastakbar"
    },
    {
      "index": 294,
      "text": "wakadhhaba bihi wakafar"
    },
    {
      "index": 295,
      "text": "wasaya`lamul ladhīna ẓalamū ayya munqalabin yanqalibūn"
    },
    {
      "index": 296,
      "text": "as salāmu `alayka yā amīral mu-minīn"
    },
    {
      "index": 297,
      "text": "wasayyidal waṣiyyīn"
    },
    {
      "index": 298,
      "text": "wa-awwalal `ābidīn"
    },
    {
      "index": 299,
      "text": "wa-azhadaz zāhidīn"
    },
    {
      "index": 300,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 301,
      "text": "waṣalawātuhu wataḥiyyātuh"
    },
    {
      "index": 302,
      "text": "anta muṭ`imuṭ ṭa`āmi `alā ḥubbihi miskīnan wayatīman wa-asīrā"
    },
    {
      "index": 303,
      "text": "liwajhillāhi lā turīdu minhum jazā-an walā shukūrā"
    },
    {
      "index": 304,
      "text": "wafīka anzalallāhu ta`ālā"
    },
    {
      "index": 305,
      "text": "wayu-thirūna `alā anfusihim walaw kāna bihim khaṣāṣah"
    },
    {
      "index": 306,
      "text": "waman yūqa shuḥḥa nafsihi fa-ūlā-ika humul mufliḥūn"
    },
    {
      "index": 307,
      "text": "wa-antal kāẓimu lilghayẓ"
    },
    {
      "index": 308,
      "text": "wal`afī `anin nās"
    },
    {
      "index": 309,
      "text": "wallāhu yuḥibbul muḥsinīn"
    },
    {
      "index": 310,
      "text": "wa-antaṣ ṣābiru fil ba-sā-i waḍ ḍarrā-i waḥīnal ba-s"
    },
    {
      "index": 311,
      "text": "wa-antal qāsimu bissawiyyah"
    },
    {
      "index": 312,
      "text": "wal`ādilu fir ra`iyyah"
    },
    {
      "index": 313,
      "text": "wal`ālimu biḥudūdillāhi min jamī`il bariyyah"
    },
    {
      "index": 314,
      "text": "wallāhu ta`ālā akhbara `ammā awlāka min faḍlihi biqawlih"
    },
    {
      "index": 315,
      "text": "afaman kāna mu-minan kaman kāna fāsiqā"
    },
    {
      "index": 316,
      "text": "lā yastawūn"
    },
    {
      "index": 317,
      "text": "ammal ladhīna-āmanū wa`amiluṣ ṣāliḥāt"
    },
    {
      "index": 318,
      "text": "falahum jannātul ma-wā nuzulan bimā kānū ya`malūn"
    },
    {
      "index": 319,
      "text": "wa-antal makhṣūṣu bi`ilmit tanzīl"
    },
    {
      "index": 320,
      "text": "waḥukmit ta-wīl"
    },
    {
      "index": 321,
      "text": "wanaṣṣir rasūl"
    },
    {
      "index": 322,
      "text": "walakal mawāqiful mash-hūdah"
    },
    {
      "index": 323,
      "text": "walmaqāmātul mash-hūrah"
    },
    {
      "index": 324,
      "text": "wal-ayyāmul madhkūrah"
    },
    {
      "index": 325,
      "text": "yawma badrin wayawmal aḥzāb"
    },
    {
      "index": 326,
      "text": "idh zāghatil abṣāru wabalaghatil qulūbul ḥanājir"
    },
    {
      "index": 327,
      "text": "wataẓunnuna billāhiẓ ẓnūnā"
    },
    {
      "index": 328,
      "text": "hunālikabtuliyal mu-minūna wazulzilū zilzālan shadīdā"
    },
    {
      "index": 329,
      "text": "wa-idh yaqūlul munāfiqūna walladhīna fī qulūbihim maraḍ"
    },
    {
      "index": 330,
      "text": "mā wa`adanallāhu warasūluhu illā ghurūrā"
    },
    {
      "index": 331,
      "text": "wa-idh qālat ṭā-ifatun minhum"
    },
    {
      "index": 332,
      "text": "yā ahla yathriba lā muqāma lakum far-ji`ū"
    },
    {
      "index": 333,
      "text": "wayasta-dhinu farīqun minhumun nabiyy"
    },
    {
      "index": 334,
      "text": "yaqūlūna inna buyūtanā `awrah"
    },
    {
      "index": 335,
      "text": "wamā hiya bi`awratin in yurīdūna illā firārā"
    },
    {
      "index": 336,
      "text": "waqālallāhu ta`ālā"
    },
    {
      "index": 337,
      "text": "walammā ra-al mu-minūnal aḥzāba qālū"
    },
    {
      "index": 338,
      "text": "hādhā mā wa`adanallāhu warasūluh"
    },
    {
      "index": 339,
      "text": "waṣadaqallāhu warasūluh"
    },
    {
      "index": 340,
      "text": "wamā zādahum illā īmānan wataslīmā"
    },
    {
      "index": 341,
      "text": "faqatalta `amrahum"
    },
    {
      "index": 342,
      "text": "wahazamta jam`ahum"
    },
    {
      "index": 343,
      "text": "waraddallāhul ladhīna kafarū bighayẓihim"
    },
    {
      "index": 344,
      "text": "lam yanālū khayrā"
    },
    {
      "index": 345,
      "text": "wakafallāhul mu-minīnal qitāl"
    },
    {
      "index": 346,
      "text": "wakānallāhu qawiyyan `azīzā"
    },
    {
      "index": 347,
      "text": "wayawma uḥudin idh yuṣ`idūna walā yalwūna `alā aḥad"
    },
    {
      "index": 348,
      "text": "war rasūlu yad`ūhum fī ukhrāhum"
    },
    {
      "index": 349,
      "text": "wa-anta tadhūdu bihimul mushrikīna `anin nabiyy"
    },
    {
      "index": 350,
      "text": "dhātal yamīni wadhātash shimāl"
    },
    {
      "index": 351,
      "text": "ḥattā raddahumullāhu ta`ālā `ankumā khā-ifīn"
    },
    {
      "index": 352,
      "text": "wanaṣara bikal khādhilīn"
    },
    {
      "index": 353,
      "text": "wayawma ḥunaynin `alā mā naṭaqa bihit tanzīl"
    },
    {
      "index": 354,
      "text": "idh a`jabatkum kathratukum falam tughni `ankum shay-ā"
    },
    {
      "index": 355,
      "text": "waḍāqat `alaykumul ar-ḍu bimā raḥubat"
    },
    {
      "index": 356,
      "text": "thumma wallaytum mudbirīn"
    },
    {
      "index": 357,
      "text": "thumma anzalallāhu sakīnatahu `alā rasūlihi wa`alal mu-minīn"
    },
    {
      "index": 358,
      "text": "walmu-minūna anta waman yalīk"
    },
    {
      "index": 359,
      "text": "wa`ammukal `abbāsu yunādil munhazimīn"
    },
    {
      "index": 360,
      "text": "yā aṣ-ḥāba sūratil baqarah"
    },
    {
      "index": 361,
      "text": "yā ahla bay`atish shajarah"
    },
    {
      "index": 362,
      "text": "ḥattastajāba lahu qawmun qad kafaytahumul ma-ūnah"
    },
    {
      "index": 363,
      "text": "watakaffalta dūnahumul ma`ūnah"
    },
    {
      "index": 364,
      "text": "fa`ādū-āyisīna minal mathūbah"
    },
    {
      "index": 365,
      "text": "rājīna wa`dallāhi ta`ālā bittawbah"
    },
    {
      "index": 366,
      "text": "wadhālika qawlullāhi jalla dhikruh"
    },
    {
      "index": 367,
      "text": "thumma yatūbullāhu min ba`di dhālika `alā man yashā"
    },
    {
      "index": 368,
      "text": "wa-anta ḥā-izun darajataṣ ṣabr"
    },
    {
      "index": 369,
      "text": "fā-izun bi`aẓīmil ajr"
    },
    {
      "index": 370,
      "text": "wayawma khaybara idh aẓharallāhu khawaral munāfiqīn"
    },
    {
      "index": 371,
      "text": "waqaṭa`a dābiral kāfirīn"
    },
    {
      "index": 372,
      "text": "walḥamdu lillāhi rabbil `ālamīn"
    },
    {
      "index": 373,
      "text": "walaqad kānū `āhadullāha min qablu lā yuwallūnal adbār"
    },
    {
      "index": 374,
      "text": "wakāna `ahdullāhi mas-ūlā"
    },
    {
      "index": 375,
      "text": "mawlāya antal ḥujjatul bālighah"
    },
    {
      "index": 376,
      "text": "walmaḥajjatul wāḍiḥah"
    },
    {
      "index": 377,
      "text": "wan ni`matus sābighah"
    },
    {
      "index": 378,
      "text": "walbur-hānul munīr"
    },
    {
      "index": 379,
      "text": "fahanī-an laka bimā-ātākallāhu min faḍl"
    },
    {
      "index": 380,
      "text": "watabban lishāni-ika dhil jahl"
    },
    {
      "index": 381,
      "text": "shahidta ma`an nabiyyi ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 382,
      "text": "jamī`a ḥurūbihi wamaghāzīh"
    },
    {
      "index": 383,
      "text": "taḥmilur rāyata amāmah"
    },
    {
      "index": 384,
      "text": "wataḍribu bissayfi quddāmah"
    },
    {
      "index": 385,
      "text": "thumma liḥazmikal mash-hūr"
    },
    {
      "index": 386,
      "text": "wabaṣīratika fil umūr"
    },
    {
      "index": 387,
      "text": "ammaraka fil mawāṭin"
    },
    {
      "index": 388,
      "text": "walam yakun `alayka amīr"
    },
    {
      "index": 389,
      "text": "wakam min amrin ṣaddaka `an imḍā-i `azmika fīhit tuqā"
    },
    {
      "index": 390,
      "text": "wattaba`a ghayruka fī mithlihil hawā"
    },
    {
      "index": 391,
      "text": "faẓannal jāhilūna annaka `ajazta `ammā ilayhintahā"
    },
    {
      "index": 392,
      "text": "ḍalla wallāhiẓ ẓānnu lidhālika wamahtadā"
    },
    {
      "index": 393,
      "text": "walaqad awḍaḥta mā ashkala min dhālika liman tawahhama wamtarā"
    },
    {
      "index": 394,
      "text": "biqawlika ṣallallāhu `alayk"
    },
    {
      "index": 395,
      "text": "qad yaral ḥuwwalul qullabu wajhal ḥīlah"
    },
    {
      "index": 396,
      "text": "wadūnahā ḥājizun min taqwallāh"
    },
    {
      "index": 397,
      "text": "fayada`uhā ra-yal `ayn"
    },
    {
      "index": 398,
      "text": "wayantahizu fur-ṣatahā man lā ḥarījata lahu fid dīn"
    },
    {
      "index": 399,
      "text": "ṣadaqta wallāhi wakhasiral mubṭilūn"
    },
    {
      "index": 400,
      "text": "wa-idh mākarakan nākithān"
    },
    {
      "index": 401,
      "text": "faqālā nurīdul `umrah"
    },
    {
      "index": 402,
      "text": "faqulta lahumā la`amrukumā mā turīdānil `umrah"
    },
    {
      "index": 403,
      "text": "lākin turīdānil ghadrah"
    },
    {
      "index": 404,
      "text": "fa-akhadhtal bay`ata `alayhimā"
    },
    {
      "index": 405,
      "text": "wajaddadtal mīthāq"
    },
    {
      "index": 406,
      "text": "fajaddā fin nifāq"
    },
    {
      "index": 407,
      "text": "falammā nabbahtahumā `alā fi`lihimā aghfalā wa`ādā wamantafa`ā"
    },
    {
      "index": 408,
      "text": "wakāna `āqibatu amrihimā khusrā"
    },
    {
      "index": 409,
      "text": "thumma talāhumā ahlush shām"
    },
    {
      "index": 410,
      "text": "fasir-ta ilayhim ba`dal i`dhār"
    },
    {
      "index": 411,
      "text": "wahum lā yadīnūna dīnal ḥaqq"
    },
    {
      "index": 412,
      "text": "walā yatadabbarūnal qur-ān"
    },
    {
      "index": 413,
      "text": "hamajun ru`ā`un ḍāllūn"
    },
    {
      "index": 414,
      "text": "wabilladhī unzila `alā muḥammadin fīka kāfirūn"
    },
    {
      "index": 415,
      "text": "wal-ahlil khilāfi `alayka nāṣirūn"
    },
    {
      "index": 416,
      "text": "waqad amarallāhu ta`ālā bittibā`ik"
    },
    {
      "index": 417,
      "text": "wanadabal mu-minīna ilā naṣrik"
    },
    {
      "index": 418,
      "text": "waqāla `azza wajall"
    },
    {
      "index": 419,
      "text": "yā ayyuhal ladhīna-āmanū"
    },
    {
      "index": 420,
      "text": "ittaqullāha wakūnū ma`aṣ ṣādiqīn"
    },
    {
      "index": 421,
      "text": "mawlāya bika ẓaharal ḥaqq"
    },
    {
      "index": 422,
      "text": "waqad nabadhahul khalq"
    },
    {
      "index": 423,
      "text": "wa-awḍaḥtas sunana ba`dad durūsi waṭ ṭams"
    },
    {
      "index": 424,
      "text": "falaka sābiqatul jihādi `alā taṣdīqit tanzīl"
    },
    {
      "index": 425,
      "text": "walaka faḍīlatul jihādi `alā taḥqīqit ta-wīl"
    },
    {
      "index": 426,
      "text": "wa`aduwwuka `aduwwullāhi jāḥidun lirasūlillāh"
    },
    {
      "index": 427,
      "text": "yad`ū bāṭilan wayaḥkumu jā-irā"
    },
    {
      "index": 428,
      "text": "wayata-ammaru ghāṣiban wayad`ū ḥizbahu ilan nār"
    },
    {
      "index": 429,
      "text": "wa`ammārun yujāhidu wayunādī baynaṣ ṣaffayn"
    },
    {
      "index": 430,
      "text": "ar rawāḥar rawāḥa ilal jannah"
    },
    {
      "index": 431,
      "text": "walammastasqā fasuqiyal labana kabbara waqāl"
    },
    {
      "index": 432,
      "text": "qāla lī rasūlullāhi ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 433,
      "text": "ākhiru sharābika minad dunyā ḍayāḥun min laban"
    },
    {
      "index": 434,
      "text": "wataqtulukal fi-atul bāghiyah"
    },
    {
      "index": 435,
      "text": "fa`taraḍahu abul `ādiyatil fazāriyyu faqatalah"
    },
    {
      "index": 436,
      "text": "fa`alā abil `ādiyati la`natullāh"
    },
    {
      "index": 437,
      "text": "wala`natu mala-a-ikatihi warusulihi ajma`īn"
    },
    {
      "index": 438,
      "text": "wa`alā man salla sayfahu `alayk"
    },
    {
      "index": 439,
      "text": "wasalalta sayfaka `alayh"
    },
    {
      "index": 440,
      "text": "yā amīral mu-minīn"
    },
    {
      "index": 441,
      "text": "minal mushrikīna walmunāfiqīna ilā yawmid dīn"
    },
    {
      "index": 442,
      "text": "wa`alā man raḍiya bimā sā-ak"
    },
    {
      "index": 443,
      "text": "walam yakrah-hu wa-aghmaḍa `aynahu walam yunkir"
    },
    {
      "index": 444,
      "text": "aw a`āna `alayka biyadin aw lisān"
    },
    {
      "index": 445,
      "text": "aw qa`ada `an naṣrik"
    },
    {
      "index": 446,
      "text": "aw khadhala `anil jihādi ma`ak"
    },
    {
      "index": 447,
      "text": "aw ghamaṭa faḍlak"
    },
    {
      "index": 448,
      "text": "wajaḥada ḥaqqak"
    },
    {
      "index": 449,
      "text": "aw `adala bika man ja`alakallāhu awlā bihi min nafsih"
    },
    {
      "index": 450,
      "text": "waṣalawātullāhi `alayka waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 451,
      "text": "wasalāmuhu wataḥiyyātuh"
    },
    {
      "index": 452,
      "text": "wa`alal a-immati min-ālikaṭ ṭāhirīn"
    },
    {
      "index": 453,
      "text": "innahu ḥamīdun majīd"
    },
    {
      "index": 454,
      "text": "wal-amrul a`jab"
    },
    {
      "index": 455,
      "text": "walkhaṭbul afẓa`u ba`da jaḥdika ḥaqqak"
    },
    {
      "index": 456,
      "text": "ghaṣbuṣ ṣiddīqatiṭ ṭāhiratiz zahrā-i sayyidatin nisā-i fadakā"
    },
    {
      "index": 457,
      "text": "waraddu shahādatika washahādatis sayyidayni sulālatik"
    },
    {
      "index": 458,
      "text": "wa`itratil muṣṭafā ṣallallāhu `alaykum"
    },
    {
      "index": 459,
      "text": "waqad a`lallāhu ta`ālā `alal ummati darajatakum"
    },
    {
      "index": 460,
      "text": "warafa`a manzilatakum"
    },
    {
      "index": 461,
      "text": "wa-abāna faḍlakum washarafakum `alal `ālamīn"
    },
    {
      "index": 462,
      "text": "fa-adh-haba `ankumur rijsa waṭahharakum taṭ-hīrā"
    },
    {
      "index": 463,
      "text": "qālallāhu `azza wajall"
    },
    {
      "index": 464,
      "text": "innal insāna khuliqa halū`ā"
    },
    {
      "index": 465,
      "text": "idhā massahush sharru jazū`ā"
    },
    {
      "index": 466,
      "text": "wa-idhā massahul khayru manū`ā"
    },
    {
      "index": 467,
      "text": "illal muṣallīn"
    },
    {
      "index": 468,
      "text": "fastathnallāhu ta`ālā nabiyyahul muṣṭafā"
    },
    {
      "index": 469,
      "text": "wa-anta yā sayyidal awṣiyā-i min jamī`il khalq"
    },
    {
      "index": 470,
      "text": "famā a`maha man ẓalamaka `anil ḥaqq"
    },
    {
      "index": 471,
      "text": "thumma afraḍūka sahma dhawil qur-bā makrā"
    },
    {
      "index": 472,
      "text": "wa-aḥādūhu `an ahlihi jawrā"
    },
    {
      "index": 473,
      "text": "falammā-ālal amru ilayka ajraytahum `alā mā ajrayā"
    },
    {
      "index": 474,
      "text": "raghbatan `anhumā bimā `indallāhi lak"
    },
    {
      "index": 475,
      "text": "fa-ashbahat miḥnatuka bihimā miḥanal anbiyā-i `alayhimus salām"
    },
    {
      "index": 476,
      "text": "`indal waḥdati wa`adamil anṣār"
    },
    {
      "index": 477,
      "text": "wa-ashbahta fil bayāti `alal firāshidh dhabīḥa `alayhis salām"
    },
    {
      "index": 478,
      "text": "idh ajabta kamā ajāb"
    },
    {
      "index": 479,
      "text": "wa-aṭa`ta kamā aṭā`a ismā`īlu ṣābiran muḥtasibā"
    },
    {
      "index": 480,
      "text": "idh qāla lahu yā bunayya innī arā fil manāmi annī adhbaḥuk"
    },
    {
      "index": 481,
      "text": "fanẓur mādhā tarā"
    },
    {
      "index": 482,
      "text": "qāla yā abatif`al mā tu-mar"
    },
    {
      "index": 483,
      "text": "satajidunī in shā-allāhu minaṣ ṣābirīn"
    },
    {
      "index": 484,
      "text": "wakadhālika anta lammā abātakan nabiyyu ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 485,
      "text": "wa-amaraka an taḍja`a fī mar-qadih"
    },
    {
      "index": 486,
      "text": "wāqiyan lahu binafsik"
    },
    {
      "index": 487,
      "text": "asra`ta ilā ijābatihi muṭī`ā"
    },
    {
      "index": 488,
      "text": "walinafsika `alal qatli muwaṭṭinā"
    },
    {
      "index": 489,
      "text": "fashakarallāhu ta`ālā ṭā`atak"
    },
    {
      "index": 490,
      "text": "wa-abāna `an jamīli fi`lika biqawlihi jalla dhikruh"
    },
    {
      "index": 491,
      "text": "waminan nāsi man yashrī nafsahubtighā-a mar-ḍātillāh"
    },
    {
      "index": 492,
      "text": "thumma miḥnatuka yawma ṣiffīn"
    },
    {
      "index": 493,
      "text": "waqad rufi`atil maṣāḥifu ḥīlatan wamakrā"
    },
    {
      "index": 494,
      "text": "fa-a`raḍash shakk"
    },
    {
      "index": 495,
      "text": "wa`uzifal ḥaqq"
    },
    {
      "index": 496,
      "text": "wattubi`aẓ ẓann"
    },
    {
      "index": 497,
      "text": "ashbahat miḥnata hārūna idh ammarahu mūsā `alā qawmihi fatafarraqū `anh"
    },
    {
      "index": 498,
      "text": "wahārūnu yunādī bihim wayaqūl"
    },
    {
      "index": 499,
      "text": "yā qawmi innamā futintum bihi wa-inna rabbakumur raḥmān"
    },
    {
      "index": 500,
      "text": "fāttabi`ūnī wa-aṭī`ū amrī"
    },
    {
      "index": 501,
      "text": "qālū lan nabraḥa `alayhi `ākifīna ḥattā yar-ji`a ilaynā mūsā"
    },
    {
      "index": 502,
      "text": "wakadhālika anta lammā rufi`atil maṣāḥifu qult"
    },
    {
      "index": 503,
      "text": "yā qawmi innamā futintum bihā wakhudi`tum"
    },
    {
      "index": 504,
      "text": "fa`aṣawka wakhālafū `alayk"
    },
    {
      "index": 505,
      "text": "wastad`ū naṣbal ḥakamayn"
    },
    {
      "index": 506,
      "text": "fa-abayta `alayhim watabarra-ta ilallāhi min fi`lihim wafawwaḍtahu ilayhim"
    },
    {
      "index": 507,
      "text": "falammā asfaral ḥaqq"
    },
    {
      "index": 508,
      "text": "wasafihal munkar"
    },
    {
      "index": 509,
      "text": "wa`tarafū bizzalali waljawri `anil qaṣd"
    },
    {
      "index": 510,
      "text": "ikhtalafū min ba`dih"
    },
    {
      "index": 511,
      "text": "wa-alzamūka `alā safahit taḥkīmil ladhī abaytahu wa-aḥabbūh"
    },
    {
      "index": 512,
      "text": "waḥaẓar-tahu wa-abāḥū dhanbahumul ladhiqtarafūh"
    },
    {
      "index": 513,
      "text": "wa-anta `alā nahji baṣīratin wahudā"
    },
    {
      "index": 514,
      "text": "wahum `alā sunani ḍalālatin wa`amā"
    },
    {
      "index": 515,
      "text": "famā zālū `alan nifāqi muṣirrīn"
    },
    {
      "index": 516,
      "text": "wafil ghayyi mutaraddidīn"
    },
    {
      "index": 517,
      "text": "ḥattā adhāqahumullāhu wabāla amrihim"
    },
    {
      "index": 518,
      "text": "fa-amāta bisayfika man `ānadaka fashaqiya wahawā"
    },
    {
      "index": 519,
      "text": "wa-aḥyā biḥujjatika man sa`ada fahudiy"
    },
    {
      "index": 520,
      "text": "ṣalawātullāhi `alayka ghādiyatan warā-iḥah"
    },
    {
      "index": 521,
      "text": "wa`ākifatan wadhāhibah"
    },
    {
      "index": 522,
      "text": "famā yuḥīṭul mādiḥu waṣfak"
    },
    {
      "index": 523,
      "text": "walā yuḥbiṭuṭ ṭā`inu faḍlak"
    },
    {
      "index": 524,
      "text": "anta aḥsanul khalqi `ibādah"
    },
    {
      "index": 525,
      "text": "wa-akhlaṣuhum zahādah"
    },
    {
      "index": 526,
      "text": "wa-adhabbuhum `anid dīn"
    },
    {
      "index": 527,
      "text": "aqamta ḥudūdallāhi bijuhdik"
    },
    {
      "index": 528,
      "text": "wafalalta `asākiral māriqīna bisayfik"
    },
    {
      "index": 529,
      "text": "tukhmidu lahabal ḥurūbi bibanānik"
    },
    {
      "index": 530,
      "text": "watahtiku sutūrash shubahi bibayānik"
    },
    {
      "index": 531,
      "text": "watakshifu labsal bāṭili `an ṣarīḥil ḥaqq"
    },
    {
      "index": 532,
      "text": "lā ta-khudhuka fillāhi lawmatu lā-im"
    },
    {
      "index": 533,
      "text": "wafī mad-ḥillāhi ta`ālā laka ghinā"
    },
    {
      "index": 534,
      "text": "`an mad-ḥil mādiḥīna wataqrīẓil wāṣifīn"
    },
    {
      "index": 535,
      "text": "qālallāhu ta`ālā"
    },
    {
      "index": 536,
      "text": "minal mu-minīna rijālun ṣadaqū mā `āhadullāha `alayh"
    },
    {
      "index": 537,
      "text": "faminhum man qaḍā naḥbahu waminhum man yantaẓiru wamā baddalū tabdīlā"
    },
    {
      "index": 538,
      "text": "walammā ra-ayta an qataltan nākithīna walqāsiṭīna walmāriqīn"
    },
    {
      "index": 539,
      "text": "waṣadaqaka rasūlullāhi ṣallallāhu `alayhi wa ālihi wa`dah"
    },
    {
      "index": 540,
      "text": "fa-awfayta bi`ahdihi qult"
    },
    {
      "index": 541,
      "text": "amā-āna an tukhḍaba hādhihi min hādhih"
    },
    {
      "index": 542,
      "text": "am matā yub`athu ashqāhā"
    },
    {
      "index": 543,
      "text": "wāthiqan bi-annaka `alā bayyinatin min rabbik"
    },
    {
      "index": 544,
      "text": "wabaṣīratin min amrik"
    },
    {
      "index": 545,
      "text": "qādimun `alallāh"
    },
    {
      "index": 546,
      "text": "mustabshirun bibay`ikal ladhī bāya`tahu bih"
    },
    {
      "index": 547,
      "text": "wadhālika huwal fawzul `aẓīm"
    },
    {
      "index": 548,
      "text": "allāhummal `an qatalata anbiyā-ika wa-awṣiyā-i anbiyā-ik"
    },
    {
      "index": 549,
      "text": "bijamī`i la`anātik"
    },
    {
      "index": 550,
      "text": "wa-aṣlihim ḥarra nārik"
    },
    {
      "index": 551,
      "text": "wal`an man ghaṣaba waliyyaka ḥaqqah"
    },
    {
      "index": 552,
      "text": "wa-ankara `ahdah"
    },
    {
      "index": 553,
      "text": "wajaḥadahu ba`dal yaqīni wal-iqrāri bil wilāyati lah"
    },
    {
      "index": 554,
      "text": "yawma akmalta lahud dīn"
    },
    {
      "index": 555,
      "text": "allāhummal `an qatalata amīril mu-minīn"
    },
    {
      "index": 556,
      "text": "waman ẓalamahu wa-ashyā`ahum wa-anṣārahum"
    },
    {
      "index": 557,
      "text": "allāhummal `an ẓālimil ḥusayni waqātilīh"
    },
    {
      "index": 558,
      "text": "walmutābi`īna `aduwwahu wanāṣirīh"
    },
    {
      "index": 559,
      "text": "war rāḍīna biqatlihi wakhādhilīhi la`nan wabīlā"
    },
    {
      "index": 560,
      "text": "allāhummal `an awwala ẓālimin ẓalama-āla muḥammad"
    },
    {
      "index": 561,
      "text": "wamāni`īhim ḥuqūqahum"
    },
    {
      "index": 562,
      "text": "allāhumma khuṣṣa awwala ẓālimin waghāṣibin li-āli muḥammadin billla`n"
    },
    {
      "index": 563,
      "text": "wakulla mustannin bimā sanna ilā yawmil qiyāmah"
    },
    {
      "index": 564,
      "text": "allāhumma ṣalli `alā muḥammadin khātamin nabiyyīn"
    },
    {
      "index": 565,
      "text": "wa`alā `aliyyin sayyidil waṣiyyīna wa ālihiṭ ṭāhirīn"
    },
    {
      "index": 566,
      "text": "waj`alnā bihim mutamassikīn"
    },
    {
      "index": 567,
      "text": "wabiwilāyatihim minal fā-izīnal-āminīn"
    },
    {
      "index": 568,
      "text": "al ladhīna lā khawfun `alayhim walā hum yaḥzanūn"
    }
  ]
}