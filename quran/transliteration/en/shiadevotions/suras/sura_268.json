{
  "index": 268,
  "name": "Sura 268",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī in kāna qalla zādī fil masīri ilayk"
    },
    {
      "index": 3,
      "text": "falaqad ḥasuna ẓannī bittawakkuli `alayk"
    },
    {
      "index": 4,
      "text": "wa-in kāna jur-mī qad akhāfanī min `uqūbatik"
    },
    {
      "index": 5,
      "text": "fa-inna rajā-ī qad ash`aranī bil amni min naqimatik"
    },
    {
      "index": 6,
      "text": "wa-in kāna dhanbī qad `arraḍanī li`iqābik"
    },
    {
      "index": 7,
      "text": "faqad-ādhananī ḥusnu thiqatī bithawābik"
    },
    {
      "index": 8,
      "text": "wa-in anāmatnil ghaflatu `anil i-asti`dādi liliqā-ik"
    },
    {
      "index": 9,
      "text": "faqad nabbahatnil ma`rifatu bikaramika wa ālā-ik"
    },
    {
      "index": 10,
      "text": "wa-in awḥasha mā baynī wabaynaka far-ṭul `iṣyāni waṭ ṭughyān"
    },
    {
      "index": 11,
      "text": "faqad-ānasanī bushral ghufrāni war riḍwān"
    },
    {
      "index": 12,
      "text": "as-aluka bisubuḥāti wajhika wabi-anwāri qudsik"
    },
    {
      "index": 13,
      "text": "wa-abtahilu ilayka bi`awāṭifi raḥmatik"
    },
    {
      "index": 14,
      "text": "walaṭā-ifi birrik"
    },
    {
      "index": 15,
      "text": "an tuḥaqqiqa ẓannī bimā u-ammiluhu min jazīli ikrāmik"
    },
    {
      "index": 16,
      "text": "wajamīli in`āmika fil qur-bā mink"
    },
    {
      "index": 17,
      "text": "waz zulfā ladayk"
    },
    {
      "index": 18,
      "text": "wat tamattu`i binnaẓari ilayk"
    },
    {
      "index": 19,
      "text": "wahā ana muta`arriḍun linafaḥāti rawḥika wa`aṭfik"
    },
    {
      "index": 20,
      "text": "wamuntaji`un ghaytha jūdika waluṭfik"
    },
    {
      "index": 21,
      "text": "fārru min sakhaṭika ilā riḍāk"
    },
    {
      "index": 22,
      "text": "hāribun minka ilayk"
    },
    {
      "index": 23,
      "text": "rājin aḥsana mā ladayk"
    },
    {
      "index": 24,
      "text": "mu`awwilun `alā mawāhibik"
    },
    {
      "index": 25,
      "text": "muftaqirun ilā ri`āyatik"
    },
    {
      "index": 26,
      "text": "ilāhī mā bada-ta bihi min faḍlika fatammimh"
    },
    {
      "index": 27,
      "text": "wamā wahabta lī min karamika falā taslubh"
    },
    {
      "index": 28,
      "text": "wamā satar-tahu `alayya biḥilmika falā tahtikh"
    },
    {
      "index": 29,
      "text": "wamā `alimtahu min qabīḥi fi`lī faghfir-h"
    },
    {
      "index": 30,
      "text": "ilāhistashfa`tu bika ilayk"
    },
    {
      "index": 31,
      "text": "wastajar-tu bika mink"
    },
    {
      "index": 32,
      "text": "ataytuka ṭāmi`an fī iḥsānik"
    },
    {
      "index": 33,
      "text": "rāghiban fimtinānik"
    },
    {
      "index": 34,
      "text": "mustasqiyan wābila ṭawlik"
    },
    {
      "index": 35,
      "text": "mustamṭiran ghamāma faḍlik"
    },
    {
      "index": 36,
      "text": "ṭāliban mar-ḍātak"
    },
    {
      "index": 37,
      "text": "qāṣidan janābak"
    },
    {
      "index": 38,
      "text": "wāridan sharī`ata rifdik"
    },
    {
      "index": 39,
      "text": "multamisan saniyyal khayrāti min `indik"
    },
    {
      "index": 40,
      "text": "wāfidan ilā ḥaḍrati jamālik"
    },
    {
      "index": 41,
      "text": "murīdan wajhak"
    },
    {
      "index": 42,
      "text": "ṭāriqan bābak"
    },
    {
      "index": 43,
      "text": "mustakīnan li`aẓamatika wajalālik"
    },
    {
      "index": 44,
      "text": "faf`al bī mā anta ahluhu minal maghfirati war raḥmah"
    },
    {
      "index": 45,
      "text": "walā taf`al bī mā ana ahluhu minal `adhābi wan niqmah"
    },
    {
      "index": 46,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    }
  ]
}