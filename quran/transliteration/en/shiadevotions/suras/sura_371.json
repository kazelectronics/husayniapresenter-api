{
  "index": 371,
  "name": "Sura 371",
  "ayas": [
    {
      "index": 1,
      "text": "allāhummar-zuqnī fīhi ḥusna `ibādatik ",
      "bismillah": "allāhummaj`alhu thawba yumnin watuqan wabarakah"
    },
    {
      "index": 2,
      "text": "wa`amalan biṭā`atik"
    },
    {
      "index": 3,
      "text": "wa-adā-a shukri ni`matik"
    },
    {
      "index": 4,
      "text": "al ḥamdu lillāhil ladhī kasānī mā uwārī bihi `awratī"
    },
    {
      "index": 5,
      "text": "wa atajammalu bihi fin nās"
    }
  ]
}