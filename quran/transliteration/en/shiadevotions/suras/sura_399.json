{
  "index": 399,
  "name": "Sura 399",
  "ayas": [
    {
      "index": 1,
      "text": "waqad amar-tal muḥsin",
      "bismillah": "yā muḥsinu qad atākal musī"
    },
    {
      "index": 2,
      "text": "an yatajāwaza `anil musī"
    },
    {
      "index": 3,
      "text": "wa-antal muḥsinu wa-anal musī"
    },
    {
      "index": 4,
      "text": "faṣalli `alā muḥammadiw wa-āli muḥammad"
    },
    {
      "index": 5,
      "text": "watajāwaza `an qabīḥi mā ta`lamu minnī"
    }
  ]
}