{
  "index": 276,
  "name": "Sura 276",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī lawlal wājibu min qabūli amrik"
    },
    {
      "index": 3,
      "text": "lanazzahtuka min dhikrī iyyāk"
    },
    {
      "index": 4,
      "text": "`alā anna dhikrī laka biqadrī lā biqadrik"
    },
    {
      "index": 5,
      "text": "wamā `asā an yablugha miqdārī ḥattā uj`ala maḥallan litaqdīsik"
    },
    {
      "index": 6,
      "text": "wamin a`ẓamin ni`ami `alaynā jarayānu dhikrika `alā alsinatinā"
    },
    {
      "index": 7,
      "text": "wa-idhnuka lanā bidu`ā-ika watanzīhika watasbīḥik"
    },
    {
      "index": 8,
      "text": "ilāhī fa-alhimnā dhikraka fil khalā-i walmalā"
    },
    {
      "index": 9,
      "text": "wal layli wan nahār"
    },
    {
      "index": 10,
      "text": "wal-i`lāni wal-isrār"
    },
    {
      "index": 11,
      "text": "wafis sarrā-i waḍ ḍarrā"
    },
    {
      "index": 12,
      "text": "wa ānisnā bidhhikril khafiyy"
    },
    {
      "index": 13,
      "text": "wasta`milnā bil `amaliz zakiyy"
    },
    {
      "index": 14,
      "text": "was sa`yil mar-ḍiyy"
    },
    {
      "index": 15,
      "text": "wajāzinā bil mīzānil wafiyy"
    },
    {
      "index": 16,
      "text": "ilāhī bika hāmatil qulūbul wālihah"
    },
    {
      "index": 17,
      "text": "wa`alā ma`rifatika jumi`atil `uqūlul mutabāyinah"
    },
    {
      "index": 18,
      "text": "falā taṭma-innul qulūbu illā bidhikrāk"
    },
    {
      "index": 19,
      "text": "walā taskunun nufūsu illā `inda ru-yāk"
    },
    {
      "index": 20,
      "text": "antal musabbaḥu fī kulli makān"
    },
    {
      "index": 21,
      "text": "walma`būdu fī kulli zamān"
    },
    {
      "index": 22,
      "text": "walmawjūdu fī kulli awān"
    },
    {
      "index": 23,
      "text": "walmad`uwwu bikulli lisān"
    },
    {
      "index": 24,
      "text": "walmu`aẓẓamu fī kulli janān"
    },
    {
      "index": 25,
      "text": "wa-astaghfiruka min kulli ladhhatin bighayri dhikrik"
    },
    {
      "index": 26,
      "text": "wamin kulli rāḥatin bighayri unsik"
    },
    {
      "index": 27,
      "text": "wamin kulli surūrin bighayri qur-bik"
    },
    {
      "index": 28,
      "text": "wamin kulli shughlin bighayri ṭā`atik"
    },
    {
      "index": 29,
      "text": "ilāhī anta qulta waqawlukal ḥaqq"
    },
    {
      "index": 30,
      "text": "yā ayyuhal ladhīna-āmanudhkurullāha dhikran kathīrā"
    },
    {
      "index": 31,
      "text": "wasabbiḥūhu bukratan wa-aṣīlā"
    },
    {
      "index": 32,
      "text": "waqulta waqawlukal ḥaqq"
    },
    {
      "index": 33,
      "text": "fadhkurūnī adhkur-kum"
    },
    {
      "index": 34,
      "text": "fa-amar-tanā bidhikrik"
    },
    {
      "index": 35,
      "text": "wawa`adtanā `alayhi an tadhkuranā"
    },
    {
      "index": 36,
      "text": "tashrīfan lanā watafkhīman wa-i`ẓāmā"
    },
    {
      "index": 37,
      "text": "wahā naḥnu dhākirūka kamā amar-tanā"
    },
    {
      "index": 38,
      "text": "fa-anjiz lanā mā wa`adtanā"
    },
    {
      "index": 39,
      "text": "yā dhākiradh dhākirīn"
    },
    {
      "index": 40,
      "text": "wayā ar-ḥamar rāḥimīn"
    }
  ]
}