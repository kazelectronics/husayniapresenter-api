{
  "index": 94,
  "name": "Sura 94",
  "ayas": [
    {
      "index": 1,
      "text": "alam nashraḥ laka ṣadrak",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wawaḍa`nā `anka wizrak"
    },
    {
      "index": 3,
      "text": "alladhī anqaḍa ẓahrak"
    },
    {
      "index": 4,
      "text": "warafa`nā laka dhikrak"
    },
    {
      "index": 5,
      "text": "fa-inna ma`al `usri yusrā"
    },
    {
      "index": 6,
      "text": "inna ma`al `usri yusrā"
    },
    {
      "index": 7,
      "text": "fa-idhā faraghta fanṣab"
    },
    {
      "index": 8,
      "text": "wa ilā rabbika farghab"
    }
  ]
}