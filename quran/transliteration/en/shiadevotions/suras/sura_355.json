{
  "index": 355,
  "name": "Sura 355",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "biḥaqqi yāsīn wal qur-ānil ḥakīm"
    },
    {
      "index": 3,
      "text": "wa biḥaqqi ṭā-hā wal qur-ānil `aẓīm"
    },
    {
      "index": 4,
      "text": "yā man yaqdiru `alā ḥawā-ijis sā-ilīn"
    },
    {
      "index": 5,
      "text": "yā man ya`lamu mā fiḍ ḍamīr"
    },
    {
      "index": 6,
      "text": "yā munaffisa `anil makrūbīn"
    },
    {
      "index": 7,
      "text": "yā mufarrijā `anil maghmūmīn"
    },
    {
      "index": 8,
      "text": "yā rāḥimash shaykhil kabīr"
    },
    {
      "index": 9,
      "text": "yā rāziqaṭ ṭifliṣ ṣaghīr"
    },
    {
      "index": 10,
      "text": "yā man lā yaḥtāju ilat tafsīr"
    },
    {
      "index": 11,
      "text": "ṣalli `alā muḥammadin wa āli muḥammadin waf`al bī"
    },
    {
      "index": 12,
      "text": ""
    }
  ]
}