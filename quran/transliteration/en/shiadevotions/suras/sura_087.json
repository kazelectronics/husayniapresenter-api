{
  "index": 87,
  "name": "Sura 87",
  "ayas": [
    {
      "index": 1,
      "text": "sabbiḥis-ma rabbikal-a`lā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "alladhī khalaqa fasawwā"
    },
    {
      "index": 3,
      "text": "wal ladhī qaddara fahadā"
    },
    {
      "index": 4,
      "text": "wal ladhī akhrajal mar`ā"
    },
    {
      "index": 5,
      "text": "faja`alahu ghuthā-an aḥwā"
    },
    {
      "index": 6,
      "text": "sanuqri-uka falā tansā"
    },
    {
      "index": 7,
      "text": "illā mā shā-allāh ۚ innahu ya`lamul jahra wa mā yakhfā"
    },
    {
      "index": 8,
      "text": "wanuyassiruka lilyusrā"
    },
    {
      "index": 9,
      "text": "fadhakkir in nafa`atidh dhikrā"
    },
    {
      "index": 10,
      "text": "sayadhhakkaru man yakhshā"
    },
    {
      "index": 11,
      "text": "wayatajannabuhal-ashqā"
    },
    {
      "index": 12,
      "text": "alladhī yaṣlan nāral kubrā"
    },
    {
      "index": 13,
      "text": "thumma lā yamūtu fīhā walā yaḥyā"
    },
    {
      "index": 14,
      "text": "qad aflaḥa man tazakkā"
    },
    {
      "index": 15,
      "text": "wadhakaras-ma rabbihi faṣallā"
    },
    {
      "index": 16,
      "text": "bal tu-thirūnal ḥayātad dunyā"
    },
    {
      "index": 17,
      "text": "wal-ākhiratu khayrun wa-abqā"
    },
    {
      "index": 18,
      "text": "inna hādhā lafiṣ ṣuḥufil-ūlā"
    },
    {
      "index": 19,
      "text": "ṣuḥufi ibrāhīma wa mūsā"
    }
  ]
}