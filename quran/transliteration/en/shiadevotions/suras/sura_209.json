{
  "index": 209,
  "name": "Sura 209",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "yā man tuḥallu bihi `uqadul makārih"
    },
    {
      "index": 3,
      "text": "wayā man yuftha-u bihi ḥaddush shadā-id"
    },
    {
      "index": 4,
      "text": "wayā man yultamasu minhul makhraju ilā rawḥil faraj"
    },
    {
      "index": 5,
      "text": "dhallat liqudratikaṣ ṣi`āb"
    },
    {
      "index": 6,
      "text": "watasabbabat biluṭfikal asbāb"
    },
    {
      "index": 7,
      "text": "wajarā biqudratikal qaḍā"
    },
    {
      "index": 8,
      "text": "wamaḍat `alā irādatikal ashyā"
    },
    {
      "index": 9,
      "text": "fahiya bimashiyyatika dūna qawlika mu-tamirah"
    },
    {
      "index": 10,
      "text": "wabi-irādatika dūna nahyika munzajirah"
    },
    {
      "index": 11,
      "text": "antal mad`uwwu lilmuhimmāt"
    },
    {
      "index": 12,
      "text": "wa-antal mafza`u fil mulimmāt"
    },
    {
      "index": 13,
      "text": "lā yandafi`u minhā illā mā dafa`t"
    },
    {
      "index": 14,
      "text": "walā yankashifu minhā illā mā kashaft"
    },
    {
      "index": 15,
      "text": "waqad nazala bī yā rabbi mā qad taka-adanī thiqluh"
    },
    {
      "index": 16,
      "text": "wa-alamma bī mā qad bahaẓanī ḥamluh"
    },
    {
      "index": 17,
      "text": "wabiqudratika awradtahu `alayy"
    },
    {
      "index": 18,
      "text": "wabisulṭānika wajjahtahu ilayy"
    },
    {
      "index": 19,
      "text": "falā muṣdira limā awradt"
    },
    {
      "index": 20,
      "text": "walā ṣārifa limā wajjaht"
    },
    {
      "index": 21,
      "text": "walā fātiḥa limā aghlaqt"
    },
    {
      "index": 22,
      "text": "walā mughliqa limā fataḥt"
    },
    {
      "index": 23,
      "text": "walā muyassira limā `assar-t"
    },
    {
      "index": 24,
      "text": "walā nāṣira liman khadhalt"
    },
    {
      "index": 25,
      "text": "faṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 26,
      "text": "waftaḥ lī yā rabbi bābal faraji biṭawlik"
    },
    {
      "index": 27,
      "text": "waksir `annī sulṭānal hammi biḥawlik"
    },
    {
      "index": 28,
      "text": "wa-anilnī ḥusnan naẓari fīmā shakawt"
    },
    {
      "index": 29,
      "text": "wa-adhiqnī ḥalāwataṣ ṣun`i fīmā sa-alt"
    },
    {
      "index": 30,
      "text": "wahab lī min ladunka raḥmatan wafarajan hanī-ā"
    },
    {
      "index": 31,
      "text": "waj`al lī min `indika makhrajan waḥiyyā"
    },
    {
      "index": 32,
      "text": "walā tashghalnī bil ihtimāmi `an ta`āhudi furūḍika wasti`māli sunnatik"
    },
    {
      "index": 33,
      "text": "faqad ḍiqtu limā nazala bī yā rabbi dhar-`ā"
    },
    {
      "index": 34,
      "text": "wamtala-tu biḥamli mā ḥadatha `alayya hammā"
    },
    {
      "index": 35,
      "text": "wa-antal qādiru `alā kashfi mā munītu bih"
    },
    {
      "index": 36,
      "text": "wadaf`i mā waqa`tu fīh"
    },
    {
      "index": 37,
      "text": "faf`al bī dhālika wa-in lam astawjibhu mink"
    },
    {
      "index": 38,
      "text": "yā dhal `ar-shil `aẓīm"
    }
  ]
}