{
  "index": 161,
  "name": "Sura 161",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wasma` du`ā-ī idhā da`awtuk"
    },
    {
      "index": 3,
      "text": "wasma` nidā-ī idhā nādaytuk"
    },
    {
      "index": 4,
      "text": "wa-aqbil `alayya idhā nājaytuk"
    },
    {
      "index": 5,
      "text": "faqad harabtu ilayk"
    },
    {
      "index": 6,
      "text": "wawaqaftu bayna yadayka mustakīnan lak"
    },
    {
      "index": 7,
      "text": "mutaḍarri`an ilayk"
    },
    {
      "index": 8,
      "text": "rājiyan limā ladayka thawābī"
    },
    {
      "index": 9,
      "text": "wata`lamu mā fī nafsī"
    },
    {
      "index": 10,
      "text": "watakhburu ḥājatī"
    },
    {
      "index": 11,
      "text": "wata`rifu ḍamīrī"
    },
    {
      "index": 12,
      "text": "walā yakhfā `alayka amru munqalabī wamathwāy"
    },
    {
      "index": 13,
      "text": "wamā urīdu an ubdi-a bihi min manṭiqī"
    },
    {
      "index": 14,
      "text": "wa-atafawwahu bihi min ṭalibatī"
    },
    {
      "index": 15,
      "text": "wa-ar-jūhu li`āqibatī"
    },
    {
      "index": 16,
      "text": "waqad jarat maqādīruka `alayya yā sayyidī"
    },
    {
      "index": 17,
      "text": "fīmā yakūnu minnī ilā-ākhiri `umrī"
    },
    {
      "index": 18,
      "text": "min sarīratī wa`alāniyatī"
    },
    {
      "index": 19,
      "text": "wabiyadika lābiyadi ghayrika ziyādatī wanaqṣī"
    },
    {
      "index": 20,
      "text": "wanaf`ī waḍarrī"
    },
    {
      "index": 21,
      "text": "ilāhī in ḥaramtanī faman dhal ladhī yar-zuqunī"
    },
    {
      "index": 22,
      "text": "wa-in khadhaltanī faman dhal ladhī yanṣurunī"
    },
    {
      "index": 23,
      "text": "ilāhī a`ūdhu bika min ghaḍabika waḥulūli sakhaṭik"
    },
    {
      "index": 24,
      "text": "ilāhī in kuntu ghayra musta-hilin liraḥmatik"
    },
    {
      "index": 25,
      "text": "fa-anta ahlun an tajūda `alayya bifaḍli sa`atik"
    },
    {
      "index": 26,
      "text": "ilāhī ka-annī binafsī wāqifatun bayna yadayk"
    },
    {
      "index": 27,
      "text": "waqad aẓallahā ḥusnu tawakkulī `alayk"
    },
    {
      "index": 28,
      "text": "faqulta mā anta ahluhu wataghammadtanī bi`afwik"
    },
    {
      "index": 29,
      "text": "ilāhī in `afawta faman awlā minka bidhālik"
    },
    {
      "index": 30,
      "text": "wa-in kāna qad danā ajalī walam yudninī minka `amalī"
    },
    {
      "index": 31,
      "text": "faqad ja`altul iqrāra bidhhanbi ilayka wasīlatī"
    },
    {
      "index": 32,
      "text": "ilāhī qad jur-tu `alā nafsī fin naẓari lahā"
    },
    {
      "index": 33,
      "text": "falahal waylu in lam taghfir lahā"
    },
    {
      "index": 34,
      "text": "ilāhī lam yazal birruka `alayya ayyāma ḥayātī"
    },
    {
      "index": 35,
      "text": "falā taqṭa` birraka `annī fī mamātī"
    },
    {
      "index": 36,
      "text": "ilāhī kayfa-āyasu min ḥusni naẓarika lī ba`da mamātī"
    },
    {
      "index": 37,
      "text": "wa-anta lam tuwallinī illal jamīla fī ḥayātī"
    },
    {
      "index": 38,
      "text": "ilāhī tawalla min amrī mā anta ahluh"
    },
    {
      "index": 39,
      "text": "wa`ud `alayya bifaḍlika `alā mudhnibin qad ghamarahu jahluh"
    },
    {
      "index": 40,
      "text": "ilāhī qad satar-ta `alayya dhunūban fid dunyā"
    },
    {
      "index": 41,
      "text": "wa-ana aḥwaju ilā satrihā `alayya minka fil ukhrā"
    },
    {
      "index": 42,
      "text": "idh lam tuẓhir-hā li-aḥadin min `ibādikaṣ ṣāliḥīn"
    },
    {
      "index": 43,
      "text": "falā tafḍaḥnī yawmal qiyāmati `alā ru-ūsil ash-hād"
    },
    {
      "index": 44,
      "text": "ilāhī jūduka basaṭa amalī"
    },
    {
      "index": 45,
      "text": "wa`afwuka afḍalu min `amalī"
    },
    {
      "index": 46,
      "text": "ilāhī fasurranī biliqā-ika yawma taqḍī fīhi bayna `ibādik"
    },
    {
      "index": 47,
      "text": "ilāhi`tidhārī ilayka`tidhāru man lam yastaghni `an qabūli `udhrih"
    },
    {
      "index": 48,
      "text": "faqbal `udhrī yā akrama mani`tadhara ilayhil musī-ūn"
    },
    {
      "index": 49,
      "text": "ilāhī lātarudda ḥājatī"
    },
    {
      "index": 50,
      "text": "walā tukhayyib ṭama`ī"
    },
    {
      "index": 51,
      "text": "walā taqṭa` minka rajā-ī wa-amalī"
    },
    {
      "index": 52,
      "text": "ilāhī law aradta hawānī lam tahdinī"
    },
    {
      "index": 53,
      "text": "walaw aradta faḍīḥatī lam tu`āfinī"
    },
    {
      "index": 54,
      "text": "ilāhī mā aẓunnuka taruddunī fī ḥājatin qad afnaytu `umrī fī ṭalabihā mink"
    },
    {
      "index": 55,
      "text": "ilāhī falakal ḥamdu abadan dā-iman sar-madā"
    },
    {
      "index": 56,
      "text": "yazīdu walā yabīdu kamā tuḥibbu watar-ḍā"
    },
    {
      "index": 57,
      "text": "ilāhī in akhadhtanī bijur-mī akhadhtuka bi`afwik"
    },
    {
      "index": 58,
      "text": "wa-in akhadhtanī bidhunūbī akhadhtuka bimaghfiratik"
    },
    {
      "index": 59,
      "text": "wa-in adkhaltanin nāra a`lamtu ahlahā annī uḥibbuk"
    },
    {
      "index": 60,
      "text": "ilāhī in kāna ṣaghura fī janbi ṭā`atika `amalī"
    },
    {
      "index": 61,
      "text": "faqad kabura fī janbi rajā-ika amalī"
    },
    {
      "index": 62,
      "text": "ilāhī kayfa anqalibu min `indika bil khaybati maḥrūmā"
    },
    {
      "index": 63,
      "text": "waqad kāna ḥusnu ẓannī bijūdika an taqlibanī binnajāti mar-ḥūmā"
    },
    {
      "index": 64,
      "text": "ilāhī waqad afnaytu `umrī fī shirratis sahwi `ank"
    },
    {
      "index": 65,
      "text": "wa-ablaytu shabābī fī sakratit tabā`udi mink"
    },
    {
      "index": 66,
      "text": "ilāhī falam astayqiẓ ayyāmaghtirārī bik"
    },
    {
      "index": 67,
      "text": "warukūnī ilā sabīli sakhaṭik"
    },
    {
      "index": 68,
      "text": "ilāhī wa-ana `abduka wabnu `abdika qā-imun bayna yadayk"
    },
    {
      "index": 69,
      "text": "mutawassilun bikaramika ilayk"
    },
    {
      "index": 70,
      "text": "ilāhī ana `abdun atanaṣṣalu ilayka mimmā kuntu uwājihuka bih"
    },
    {
      "index": 71,
      "text": "min qillatistiḥyā-ī min naẓarik"
    },
    {
      "index": 72,
      "text": "wa-aṭlubul `afwa minka idhil `afwu na`tun likaramik"
    },
    {
      "index": 73,
      "text": "ilāhī lam yakun lī ḥawlun fa-antaqila bihi `an ma`ṣiyatik"
    },
    {
      "index": 74,
      "text": "illā fī waqtin ayqaẓtanī limaḥabbatik"
    },
    {
      "index": 75,
      "text": "wakamā aradta an akūna kunt"
    },
    {
      "index": 76,
      "text": "fashakar-tuka bi-idkhālī fī karamik"
    },
    {
      "index": 77,
      "text": "walitaṭ-hīri qalbī min awsākhil ghaflati `ank"
    },
    {
      "index": 78,
      "text": "ilāhī unẓur ilayya naẓara man nādaytahu fa-ajābak"
    },
    {
      "index": 79,
      "text": "wasta`maltahu bima`ūnatika fa-aṭā`ak"
    },
    {
      "index": 80,
      "text": "yā qarīban lāyab`udu `anil mughtarri bih"
    },
    {
      "index": 81,
      "text": "wayā jawādan lāyabkhalu `amman rajā thawābah"
    },
    {
      "index": 82,
      "text": "ilāhī hab lī qalban yudnīhi minka shawquh"
    },
    {
      "index": 83,
      "text": "walisānan yur-fa`u ilayka ṣidquh"
    },
    {
      "index": 84,
      "text": "wanaẓaran yuqarribuhu minka ḥaqquh"
    },
    {
      "index": 85,
      "text": "ilāhī inna man ta`arrafa bika ghayru majhūl"
    },
    {
      "index": 86,
      "text": "waman lādhabika ghayru makhdhūl"
    },
    {
      "index": 87,
      "text": "waman aqbalta `alayhi ghayru mamlūk"
    },
    {
      "index": 88,
      "text": "ilāhī inna manintahaja bika lamustanīr"
    },
    {
      "index": 89,
      "text": "wa-inna mani`taṣama bika lamustajīr"
    },
    {
      "index": 90,
      "text": "waqad ludhtu bika yā ilāhī falā tukhayyib ẓannī min raḥmatik"
    },
    {
      "index": 91,
      "text": "walā taḥjubnī `an ra-fatik"
    },
    {
      "index": 92,
      "text": "ilāhī aqimnī fī ahli wilāyatik"
    },
    {
      "index": 93,
      "text": "muqāma man rajaz ziyādata min maḥabbatik"
    },
    {
      "index": 94,
      "text": "ilāhī wa-alhimnī walahan bidhikrika ilā dhikrik"
    },
    {
      "index": 95,
      "text": "wahimmatī fī rawḥi najāḥi asmā-ika wamaḥalli qudsik"
    },
    {
      "index": 96,
      "text": "ilāhī bika `alayka illā alḥaqtanī bimaḥalli ahli ṭā`atik"
    },
    {
      "index": 97,
      "text": "walmathwaṣ ṣāliḥi min mar-ḍātik"
    },
    {
      "index": 98,
      "text": "fa-innī lā-aqdiru linafsī daf`ā"
    },
    {
      "index": 99,
      "text": "walā amliku lahā naf`ā"
    },
    {
      "index": 100,
      "text": "ilāhī ana `abdukaḍ ḍa`īful mudhnib"
    },
    {
      "index": 101,
      "text": "wamamlūkukal munīb"
    },
    {
      "index": 102,
      "text": "falā taj`alnī mimman ṣarafta `anhu wajhak"
    },
    {
      "index": 103,
      "text": "waḥajabahu sahwuhu `an `afwik"
    },
    {
      "index": 104,
      "text": "ilāhī hab lī kamālal anqiṭā`i ilayk"
    },
    {
      "index": 105,
      "text": "wa-anir abṣāra qulūbinā biḍiyā-i naẓarihā ilayk"
    },
    {
      "index": 106,
      "text": "ḥattā takhriqa abṣārul qulūbi ḥujuban nūr"
    },
    {
      "index": 107,
      "text": "fataṣila ilā ma`dinil `aẓamah"
    },
    {
      "index": 108,
      "text": "wataṣīra ar-wāḥunā mu`allaqatan bi`izzi qudsik"
    },
    {
      "index": 109,
      "text": "ilāhī waj`alnī mimman nādaytahu fa-ajābak"
    },
    {
      "index": 110,
      "text": "walāḥaẓtahu faṣa`iqa lijalālik"
    },
    {
      "index": 111,
      "text": "fanājaytahu sirran wa`amila laka jahrā"
    },
    {
      "index": 112,
      "text": "ilāhī lam usalliṭ `alā ḥusni ẓannī qunūṭal ayās"
    },
    {
      "index": 113,
      "text": "walanqaṭa`a rajā-ī min jamīli karamik"
    },
    {
      "index": 114,
      "text": "ilāhī in kānatil khaṭāyā qad asqaṭatnī ladayk"
    },
    {
      "index": 115,
      "text": "faṣfaḥ `annī biḥusni tawakkulī `alayk"
    },
    {
      "index": 116,
      "text": "ilāhī in ḥaṭṭatnidh dhunūbu min makārimi luṭfik"
    },
    {
      "index": 117,
      "text": "faqad nabbahanil yaqīnu ilā karami `aṭfik"
    },
    {
      "index": 118,
      "text": "ilāhī in anāmatnil ghaflatu `anil asti`dādi liliqā-ik"
    },
    {
      "index": 119,
      "text": "faqad nabbahatnil ma`rifatu bikarami-ālā-ik"
    },
    {
      "index": 120,
      "text": "ilāhī in da`ānī ilan nāri `aẓīmu `iqābik"
    },
    {
      "index": 121,
      "text": "faqad da`ānī ilal jannati jazīlu thawābik"
    },
    {
      "index": 122,
      "text": "ilāhī falaka as-alu wa-ilayka abtahilu wa-ar-ghab"
    },
    {
      "index": 123,
      "text": "wa-as-aluka an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 124,
      "text": "wa-an taj`alanī mimman yudīmu dhikrak"
    },
    {
      "index": 125,
      "text": "walā yanquḍu `ahdak"
    },
    {
      "index": 126,
      "text": "walā yaghfulu `an shukrik"
    },
    {
      "index": 127,
      "text": "walā yastakhiffu bi-amrik"
    },
    {
      "index": 128,
      "text": "ilāhī wa-alḥiqnī binūri `izzikal abhaj"
    },
    {
      "index": 129,
      "text": "fa-akūna laka `ārifā"
    },
    {
      "index": 130,
      "text": "wa`an siwāka munḥarifā"
    },
    {
      "index": 131,
      "text": "waminka khā-ifan murāqibā"
    },
    {
      "index": 132,
      "text": "yā dhal jalāli wal-ikrām"
    },
    {
      "index": 133,
      "text": "waṣallallāhu `alā muḥammadin rasūlihi wa ālihiṭ ṭāhirīn"
    },
    {
      "index": 134,
      "text": "wasallama taslīman kathīrā"
    }
  ]
}