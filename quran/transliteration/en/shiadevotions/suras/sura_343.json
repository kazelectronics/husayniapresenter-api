{
  "index": 343,
  "name": "Sura 343",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilahī ghārat nujūmu samā-ik"
    },
    {
      "index": 3,
      "text": "wa nāmat `uyūnu anāmik"
    },
    {
      "index": 4,
      "text": "wa hada-at aṣwātu `ibādika wa an`āmik"
    },
    {
      "index": 5,
      "text": "wa ghallaqatil mulūku `alayhā abwābahā"
    },
    {
      "index": 6,
      "text": "wa ṭāfa `alayhā ḥurrāsuhā"
    },
    {
      "index": 7,
      "text": "waḥtajabū `amman yas-aluhum ḥājah"
    },
    {
      "index": 8,
      "text": "aw yantaji`u minhum fā-idah"
    },
    {
      "index": 9,
      "text": "wa anta ilāhī ḥayyun qayyūm"
    },
    {
      "index": 10,
      "text": "lā takhudhuka sinatun wa lā nawm"
    },
    {
      "index": 11,
      "text": "wa lā yashghaluka shay-un `an shay"
    },
    {
      "index": 12,
      "text": "abwābu samā-ika liman da`āka mufattaḥāt"
    },
    {
      "index": 13,
      "text": "wa khazā-inuka ghayru mughallaqāt"
    },
    {
      "index": 14,
      "text": "wa abwābu raḥmatika ghayru maḥjūbāt"
    },
    {
      "index": 15,
      "text": "wa fawā-iduka liman sa-alaka ghayru maḥẓūrāt"
    },
    {
      "index": 16,
      "text": "bal hiya mabdhūlāt"
    },
    {
      "index": 17,
      "text": "anta ilāhī alkarīm"
    },
    {
      "index": 18,
      "text": "al ladhī lā taruddu sā-ilan minal mu-minīna sa-alak"
    },
    {
      "index": 19,
      "text": "wa lā taḥtajibu `an aḥadin minhum arādak"
    },
    {
      "index": 20,
      "text": "lā wa `izzatika wa jalālik"
    },
    {
      "index": 21,
      "text": "wa lā tukhtazalu ḥawā-ijahum dūnak"
    },
    {
      "index": 22,
      "text": "wa lā yaqḍīhā aḥadun ghayruk"
    },
    {
      "index": 23,
      "text": "allāhumma wa qad tarānī wa wuqūfī"
    },
    {
      "index": 24,
      "text": "wa dhulla maqāmī bayna yadayk"
    },
    {
      "index": 25,
      "text": "wa ta`lamu sarīratī"
    },
    {
      "index": 26,
      "text": "wa taṭṭali`u `alā mā fī qalbī"
    },
    {
      "index": 27,
      "text": "wa mā yaṣliḥu bihi amru-ākhiratī wa dunyāy"
    },
    {
      "index": 28,
      "text": "allāhumma inna dhikral mawt"
    },
    {
      "index": 29,
      "text": "wa ahwālil muṭṭali`"
    },
    {
      "index": 30,
      "text": "walwuqūfa bayna yadayk"
    },
    {
      "index": 31,
      "text": "naghhaṣanī maṭ`amī wa mashrabī"
    },
    {
      "index": 32,
      "text": "wa aghaṣṣanī birīqī aqlaqanī `an wisādī"
    },
    {
      "index": 33,
      "text": "wa mana`anī ruqādī"
    },
    {
      "index": 34,
      "text": "kayfa yanāmu man yakhāfu malakal mawt"
    },
    {
      "index": 35,
      "text": "fī ṭawāriqil layli wa ṭawāriqin nahār"
    },
    {
      "index": 36,
      "text": "bal kayfa yanāmul `āqilu wa malakul mawti lā yanām"
    },
    {
      "index": 37,
      "text": "lā billayli wa lā binnahār"
    },
    {
      "index": 38,
      "text": "wa yaṭlubu rūḥahu bil bayāt"
    },
    {
      "index": 39,
      "text": "wa fī-ānā-is sā`āt"
    },
    {
      "index": 40,
      "text": "as-ālukar rawḥa war rāḥata `indal mawt"
    },
    {
      "index": 41,
      "text": "wal `afwa `annī ḥīna alqāk"
    }
  ]
}