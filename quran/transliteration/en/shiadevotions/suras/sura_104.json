{
  "index": 104,
  "name": "Sura 104",
  "ayas": [
    {
      "index": 1,
      "text": "waylul likulli humazatin lumazah",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "alladhī jama`a mālan wa`addadah"
    },
    {
      "index": 3,
      "text": "yaḥsabu anna mālahu akhladah"
    },
    {
      "index": 4,
      "text": "kallā ۖ layunbadhanna fil ḥuṭamah"
    },
    {
      "index": 5,
      "text": "wa mā adrāka mal ḥuṭamah"
    },
    {
      "index": 6,
      "text": "nārullāhil mūqadah"
    },
    {
      "index": 7,
      "text": "allatī taṭṭali`u `alal-af-idah"
    },
    {
      "index": 8,
      "text": "innahā `alayhim mu-ṣadah"
    },
    {
      "index": 9,
      "text": "fī `amadin mumaddadah"
    }
  ]
}