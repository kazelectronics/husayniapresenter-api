{
  "index": 273,
  "name": "Sura 273",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī laysa lī wasīlatun ilayka illā `awāṭifu ra-fatik"
    },
    {
      "index": 3,
      "text": "walā lī dharī`atun ilayka illā `awārifu raḥmatik"
    },
    {
      "index": 4,
      "text": "washafā`atu nabiyyika nabiyyir raḥmah"
    },
    {
      "index": 5,
      "text": "wamunqidhil ummati minal ghummah"
    },
    {
      "index": 6,
      "text": "faj`alhumā lī sababan ilā nayli ghufrānik"
    },
    {
      "index": 7,
      "text": "waṣayyir-humā lī wuṣlatan ilal fawzi biriḍwānik"
    },
    {
      "index": 8,
      "text": "waqad ḥalla rajā-ī biḥarami karamik"
    },
    {
      "index": 9,
      "text": "waḥaṭṭa ṭama`ī bifinā-i jūdik"
    },
    {
      "index": 10,
      "text": "faḥaqqiq fīka amalī"
    },
    {
      "index": 11,
      "text": "wakhtim bil khayri `amalī"
    },
    {
      "index": 12,
      "text": "waj`alnī min ṣafwatikal ladhīna aḥlaltahum buḥbūḥata jannatik"
    },
    {
      "index": 13,
      "text": "wabawwa-tahum dāra karāmatik"
    },
    {
      "index": 14,
      "text": "wa-aqrar-ta a`yunahum binnaẓari ilayka yawma liqā-ik"
    },
    {
      "index": 15,
      "text": "wa-awrathtahum manāzilaṣ ṣidqi fī jawārik"
    },
    {
      "index": 16,
      "text": "yā man lā yafidul wāfidūna `alā akrama minh"
    },
    {
      "index": 17,
      "text": "walā yajidul qāṣidūna ar-ḥama minh"
    },
    {
      "index": 18,
      "text": "yā khayra man khalā bihi waḥīd"
    },
    {
      "index": 19,
      "text": "wayā a`ṭafa man-āwā ilayhi ṭarīd"
    },
    {
      "index": 20,
      "text": "ilā sa`ati `afwika madadtu yadī"
    },
    {
      "index": 21,
      "text": "wabidhayli karamika a`laqtu kaffī"
    },
    {
      "index": 22,
      "text": "falā tūlinil ḥir-mān"
    },
    {
      "index": 23,
      "text": "walā tublinī bil khaybati walkhusrān"
    },
    {
      "index": 24,
      "text": "yā samī`ad du`ā"
    },
    {
      "index": 25,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}