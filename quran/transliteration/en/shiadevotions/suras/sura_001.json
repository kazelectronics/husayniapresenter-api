{
  "index": 1,
  "name": "Sura 1",
  "ayas": [
    {
      "index": 1,
      "text": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al-ḥamdu lillāhi rabbil `ālamīn"
    },
    {
      "index": 3,
      "text": "ar-raḥmānir raḥīm"
    },
    {
      "index": 4,
      "text": "māliki yawmid dīn"
    },
    {
      "index": 5,
      "text": "iyyāka na`budu wa iyyāka nasta`īn"
    },
    {
      "index": 6,
      "text": "ihdinaṣ ṣirāṭal mustaqīm"
    },
    {
      "index": 7,
      "text": "ṣirāṭal ladhīna an`amta `alayhim ghayril maghḍūbi `alayhim walaḍ ḍāllīn"
    }
  ]
}