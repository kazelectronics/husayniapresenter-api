{
  "index": 103,
  "name": "Sura 103",
  "ayas": [
    {
      "index": 1,
      "text": "wal `aṣr",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "innal-insāna lafī khusr"
    },
    {
      "index": 3,
      "text": "illal ladhīna āmanū wa`amiluṣ ṣāliḥāti watawāṣaw bil ḥaqqi watawāṣaw biṣ ṣabr"
    }
  ]
}