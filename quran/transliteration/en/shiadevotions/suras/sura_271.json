{
  "index": 271,
  "name": "Sura 271",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "subḥānak"
    },
    {
      "index": 3,
      "text": "mā aḍyaqaṭ ṭuruqa `alā man lam takun dalīlah"
    },
    {
      "index": 4,
      "text": "wamā awḍaḥal ḥaqqa `inda man hadaytahu sabīlah"
    },
    {
      "index": 5,
      "text": "ilāhī fasluk binā subulal wuṣūli ilayk"
    },
    {
      "index": 6,
      "text": "wasayyir-nā fī aqrabiṭ ṭuruqi lilwufūdi `alayk"
    },
    {
      "index": 7,
      "text": "qarrib `alaynal ba`īd"
    },
    {
      "index": 8,
      "text": "wasahhil `alaynal `asīrash shadīd"
    },
    {
      "index": 9,
      "text": "wa-alḥiqnā bi`ibādikal ladhīna hum bil bidāri ilayka yusāri`ūn"
    },
    {
      "index": 10,
      "text": "wabābaka `alad dawāmi yaṭruqūn"
    },
    {
      "index": 11,
      "text": "wa-iyyāka fil layli wan nahāri ya`budūn"
    },
    {
      "index": 12,
      "text": "wahum min haybatika mushfiqūn"
    },
    {
      "index": 13,
      "text": "al ladhīna ṣaffayta lahumul mashārib"
    },
    {
      "index": 14,
      "text": "waballaghtahumur raghā-ib"
    },
    {
      "index": 15,
      "text": "wa-anjaḥta lahumul maṭālib"
    },
    {
      "index": 16,
      "text": "waqaḍayta lahum min faḍlikal ma-ārib"
    },
    {
      "index": 17,
      "text": "wamala-ta lahum ḍamā-irahum min ḥubbik"
    },
    {
      "index": 18,
      "text": "warawwaytahum min ṣafī shir-bik"
    },
    {
      "index": 19,
      "text": "fabika ilā ladhīdhi munājātika waṣalū"
    },
    {
      "index": 20,
      "text": "waminka aqṣā maqāṣidihim ḥaṣṣalū"
    },
    {
      "index": 21,
      "text": "fayā man huwa `alal muqbilīna `alayhi muqbil"
    },
    {
      "index": 22,
      "text": "wabil `aṭfi `alayhim `ā-idun mufḍil"
    },
    {
      "index": 23,
      "text": "wabil ghāfilīna `an dhikrihi raḥīmun ra-ūf"
    },
    {
      "index": 24,
      "text": "wabijadhbihim ilā bābihi wadūdun `aṭūf"
    },
    {
      "index": 25,
      "text": "as-aluka an taj`alanī min awfarihim minka ḥaẓẓā"
    },
    {
      "index": 26,
      "text": "wa-a`lāhum `indaka manzilā"
    },
    {
      "index": 27,
      "text": "wa-ajzalihim min wuddika qismā"
    },
    {
      "index": 28,
      "text": "wa-afḍalihim fī ma`rifatika naṣībā"
    },
    {
      "index": 29,
      "text": "faqadinqaṭa`at ilayka himmatī"
    },
    {
      "index": 30,
      "text": "wanṣarafat naḥwaka raghbatī"
    },
    {
      "index": 31,
      "text": "fa-anta lā ghayruka murādī"
    },
    {
      "index": 32,
      "text": "walaka lā lisiwāka saharī wasuhādī"
    },
    {
      "index": 33,
      "text": "waliqa-uka qurratu `aynī"
    },
    {
      "index": 34,
      "text": "wawaṣluka munā nafsī"
    },
    {
      "index": 35,
      "text": "wa-ilayka shawqī"
    },
    {
      "index": 36,
      "text": "wafī maḥabbatika walahī"
    },
    {
      "index": 37,
      "text": "wa-ilā hawāka ṣabābatī"
    },
    {
      "index": 38,
      "text": "wariḍāka bughyatī"
    },
    {
      "index": 39,
      "text": "waru-yatuka ḥājatī"
    },
    {
      "index": 40,
      "text": "wajiwāruka ṭalibatī"
    },
    {
      "index": 41,
      "text": "waqur-buka ghāyatu su-lī"
    },
    {
      "index": 42,
      "text": "wafī munājātika rawḥī warāḥatī"
    },
    {
      "index": 43,
      "text": "wa`indaka dawā-u `illatī"
    },
    {
      "index": 44,
      "text": "washifā-u ghullatī"
    },
    {
      "index": 45,
      "text": "wabar-du law`atī"
    },
    {
      "index": 46,
      "text": "wakashfu kur-batī"
    },
    {
      "index": 47,
      "text": "fakun anīsī fī waḥshatī"
    },
    {
      "index": 48,
      "text": "wamuqīla `athratī"
    },
    {
      "index": 49,
      "text": "waghāfira zallatī"
    },
    {
      "index": 50,
      "text": "waqābila tawbatī"
    },
    {
      "index": 51,
      "text": "wamujība da`watī"
    },
    {
      "index": 52,
      "text": "wawaliyya `iṣmatī"
    },
    {
      "index": 53,
      "text": "wamughniya fāqatī"
    },
    {
      "index": 54,
      "text": "walā taqṭa`nī `ank"
    },
    {
      "index": 55,
      "text": "walā tub`idnī mink"
    },
    {
      "index": 56,
      "text": "yā na`īmī wajannatī"
    },
    {
      "index": 57,
      "text": "wayā dunyāya wa ākhiratī"
    },
    {
      "index": 58,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}