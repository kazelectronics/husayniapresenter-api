{
  "index": 89,
  "name": "Sura 89",
  "ayas": [
    {
      "index": 1,
      "text": "wal fajr",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "walayālin `ashr"
    },
    {
      "index": 3,
      "text": "wash shaf`i wal watr"
    },
    {
      "index": 4,
      "text": "wal layli idhā yasr"
    },
    {
      "index": 5,
      "text": "hal fī dhālika qasamul lidhī ḥijr"
    },
    {
      "index": 6,
      "text": "alam tara kayfa fa`ala rabbuka bi`ād"
    },
    {
      "index": 7,
      "text": "irama dhātil `imād"
    },
    {
      "index": 8,
      "text": "allatī lam yukhlaq mithluhā fil bilād"
    },
    {
      "index": 9,
      "text": "wathamūdal ladhīna jābuṣ ṣakhra bil wād"
    },
    {
      "index": 10,
      "text": "wafir`awna dhil-awtād"
    },
    {
      "index": 11,
      "text": "alladhīna ṭaghaw fil bilād"
    },
    {
      "index": 12,
      "text": "fa-aktharū fīhal fasād"
    },
    {
      "index": 13,
      "text": "faṣabba `alayhim rabbuka sawṭa `adhāb"
    },
    {
      "index": 14,
      "text": "inna rabbaka labil mirṣād"
    },
    {
      "index": 15,
      "text": "fa-ammal-insānu idhā mab-talāhu rabbuhu fa-akramahu wana``amahu fayaqūlu rabbī akraman"
    },
    {
      "index": 16,
      "text": "wa-ammā idhā mab-talāhu faqadara `alayhi rizqahu fayaqūlu rabbī ahānan"
    },
    {
      "index": 17,
      "text": "kallā ۖ bal lā tukrimūnal yatīm"
    },
    {
      "index": 18,
      "text": "walā taḥāḍḍūna `alā ṭa`āmil miskīn"
    },
    {
      "index": 19,
      "text": "wata-kulūnat turātha aklal lammā"
    },
    {
      "index": 20,
      "text": "watuḥibbūnal māla ḥubban jammā"
    },
    {
      "index": 21,
      "text": "kallā idhā dukkatil-arḍu dakkan dakkā"
    },
    {
      "index": 22,
      "text": "wajā-a rabbuka wal malaku ṣaffan ṣaffā"
    },
    {
      "index": 23,
      "text": "wajī-a yawma-idhin bijahannam ۚ yawma-idhiy yatadhakkarul-insānu wa-annā lahudh dhikrā"
    },
    {
      "index": 24,
      "text": "yaqūlu yālaytanī qaddamtu liḥayātī"
    },
    {
      "index": 25,
      "text": "fayawma-idhin lā yu`adhhibu `adhābahu aḥad"
    },
    {
      "index": 26,
      "text": "walā yūthiqu wathāqahu aḥad"
    },
    {
      "index": 27,
      "text": "yā-ayyatuhan nafsul muṭma-innah"
    },
    {
      "index": 28,
      "text": "irji`ī ilā rabbiki rāḍiyatan marḍiyyah"
    },
    {
      "index": 29,
      "text": "fadkhulī fī `ibādī"
    },
    {
      "index": 30,
      "text": "wadkhulī jannatī"
    }
  ]
}