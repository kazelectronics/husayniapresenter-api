      "text": "liwajhikad dā-imil bāqī"
    },
    {
      "index": 416,
      "text": "sajada wajhiyal faqīr"
    },
    {
      "index": 417,
      "text": "liwajhikal ghaniyyil kabīr"
    },
    {
      "index": 418,
      "text": "sajada wajhī wasam`ī wabaṣarī"
    },
    {
      "index": 419,
      "text": "walaḥmī wadamī wajildī"
    },
    {
      "index": 420,
      "text": "wa`aẓmī wamā aqallatil ar-ḍu minnī"
    },
    {
      "index": 421,
      "text": "lillāhi rabbil `ālamīn"
    },
    {
      "index": 422,
      "text": "allāhumma `ud `alā jahlī biḥilmik"
    },
    {
      "index": 423,
      "text": "wa`alā faqrī bighināk"
    },
    {
      "index": 424,
      "text": "wa`alā dhullī bi`izzika wasulṭānik"
    },
    {
      "index": 425,
      "text": "wa`alā ḍa`fī biquwwatik"
    },
    {
      "index": 426,
      "text": "wa`alā khawfī bi-amnik"
    },
    {
      "index": 427,
      "text": "wa`alā dhunūbī wakhaṭāyāya bi`afwika waraḥmatik"
    },
    {
      "index": 428,
      "text": "yā raḥmānu yā raḥīm"
    },
    {
      "index": 429,
      "text": "allāhumma innī adra-u bika fī naḥri..."
    },
    {
      "index": 430,
      "text": ""
    },
    {
      "index": 431,
      "text": "wa-a`ūdhu bika min sharrih"
    },
    {
      "index": 432,
      "text": "fakfinīhi bimā kafayta bihi anbiyā-ak"
    },
    {
      "index": 433,
      "text": "wa-awliyā-aka min khalqik"
    },
    {
      "index": 434,
      "text": "waṣāliḥi `ibādik"
    },
    {
      "index": 435,
      "text": "min farā`inati khalqik"
    },
    {
      "index": 436,
      "text": "waṭughāti `udātik"
    },
    {
      "index": 437,
      "text": "washarri jamī`i khalqik"
    },
    {
      "index": 438,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 439,
      "text": "innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 440,
      "text": "waḥasbunallāhu wani`mal wakīl"
    }
