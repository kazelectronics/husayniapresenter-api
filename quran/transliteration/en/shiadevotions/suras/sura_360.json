{
  "index": 360,
  "name": "Sura 360",
  "ayas": [
    {
      "index": 1,
      "text": "ilāhī kam min `aduww",
      "bismillah": "bismillāhir raḥmanir raḥīm"
    },
    {
      "index": 2,
      "text": "ntaḍā `alayya sayfa `adāwatih"
    },
    {
      "index": 3,
      "text": "washaḥadha lī ẓubata midyatih"
    },
    {
      "index": 4,
      "text": "wa-ar-hafa lī shabā ḥaddih"
    },
    {
      "index": 5,
      "text": "wadāfa lī qawātila sumūmih"
    },
    {
      "index": 6,
      "text": "wasaddada ilayya ṣawā-iba sihāmih"
    },
    {
      "index": 7,
      "text": "walam tanam `annī `aynu ḥirāsatih"
    },
    {
      "index": 8,
      "text": "wa-aḍmara an yasūmanil makrūh"
    },
    {
      "index": 9,
      "text": "wayujarri`anī dhu`āfa marāratih"
    },
    {
      "index": 10,
      "text": "fanaẓar-ta ilā ḍa`fī `aniḥtimālil fawādiḥ"
    },
    {
      "index": 11,
      "text": "wa`ajzī `anil intiṣāri mimman qaṣadanī bimuḥārabatih"
    },
    {
      "index": 12,
      "text": "wawaḥdatī fī kathīrin mimman nāwānī wa-ar-ṣada lī"
    },
    {
      "index": 13,
      "text": "fīmā lam u`mil fikrī fil ir-ṣādi lahum bimithlih"
    },
    {
      "index": 14,
      "text": "fa-ayyadtanī biquwwatik"
    },
    {
      "index": 15,
      "text": "washadadta azrī binuṣratik"
    },
    {
      "index": 16,
      "text": "wafalalta lī ḥaddah"
    },
    {
      "index": 17,
      "text": "wakhadhaltahu ba`da jam`i `adīdihi waḥashadih"
    },
    {
      "index": 18,
      "text": "wa-a`layta ka`bī `alayh"
    },
    {
      "index": 19,
      "text": "wawajjahta mā saddada ilayya min makā-idihi ilayh"
    },
    {
      "index": 20,
      "text": "waradadtahu `alayh"
    },
    {
      "index": 21,
      "text": "walam yashfi ghalīlah"
    },
    {
      "index": 22,
      "text": "walam tabrud ḥazāzātu ghayẓih"
    },
    {
      "index": 23,
      "text": "waqad `aḍḍa `alayya anāmilah"
    },
    {
      "index": 24,
      "text": "wa-adbara muwalliyan qad akhfaqat sarāyāh"
    },
    {
      "index": 25,
      "text": "falakal ḥamdu yā rabb"
    },
    {
      "index": 26,
      "text": "min muqtadirin lāyughlab"
    },
    {
      "index": 27,
      "text": "wadhī anātin lāya`jal"
    },
    {
      "index": 28,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 29,
      "text": "waj`alnī lina`mā-ika minash shākirīn"
    },
    {
      "index": 30,
      "text": "wali-ālā-ika minadh dhākirīn"
    },
    {
      "index": 31,
      "text": "ilāhī wakam min bāgh"
    },
    {
      "index": 32,
      "text": "baghānī bimakā-idih"
    },
    {
      "index": 33,
      "text": "wanaṣaba lī ashrāka maṣā-idih"
    },
    {
      "index": 34,
      "text": "wawakkala bī tafaqquda ri`āyatih"
    },
    {
      "index": 35,
      "text": "wa-aḍba-a ilayya iḍbā-as sabu`i liṭarīdatih"
    },
    {
      "index": 36,
      "text": "intiẓāran lintihāzi fur-ṣatih"
    },
    {
      "index": 37,
      "text": "wahuwa yuẓhiru bashāshatal malaq"
    },
    {
      "index": 38,
      "text": "wayabsuṭu wajhan ghayra ṭaliq"
    },
    {
      "index": 39,
      "text": "falammā ra-ayta daghala sarīratih"
    },
    {
      "index": 40,
      "text": "waqubḥa manṭawā `alayhi lisharīkihi fī millatih"
    },
    {
      "index": 41,
      "text": "wa-aṣbaḥa mujliban lī fī baghyih"
    },
    {
      "index": 42,
      "text": "ar-kastahu li-ummi ra-sih"
    },
    {
      "index": 43,
      "text": "wa-atayta bunyānahu min asāsih"
    },
    {
      "index": 44,
      "text": "faṣara`tahu fī zubyatih"
    },
    {
      "index": 45,
      "text": "waraddaytahu fī mahwā ḥufratih"
    },
    {
      "index": 46,
      "text": "waja`alta khaddahu ṭabaqan liturābi rijlih"
    },
    {
      "index": 47,
      "text": "washaghaltahu fī badanihi warizqih"
    },
    {
      "index": 48,
      "text": "waramaytahu biḥajarih"
