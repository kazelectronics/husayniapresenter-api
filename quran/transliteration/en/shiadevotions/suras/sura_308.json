      "text": "as salāmu `alayka yabna amīril mu-minīn"
    },
    {
      "index": 3,
      "text": "as salāmu `alayka yabna fāṭimataz zahrā"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka yā ḥabīballāh"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā ṣifwatallāh"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā amīnallāh"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā ḥujjatallāh"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yā nūrallāh"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka yā ṣirāṭallāh"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā bayāna ḥukmillāh"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka yā nāṣira dīnillāh"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka ayyuhas sayyiduz zakiyy"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka ayyuhal barrul wafiyy"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka ayyuhal qā-imul amīn"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka ayyuhal `ālimu bitta-wīl"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka ayyuhal hādil mahdiyy"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka ayyuhaṭ ṭāhiruz zakiyy"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka ayyuhat taqiyyun naqiyy"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka ayyuhal ḥaqqul ḥaqīq"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka ayyuhash shahīduṣ ṣiddīq"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka yā abā muḥammadinil ḥasanabna `aliyy"
    },
    {
      "index": 22,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 23,
      "text": ""
    },
    {
      "index": 24,
      "text": "as salāmu `alayka yabna rasūlillāh"
    },
    {
      "index": 25,
      "text": "as salāmu `alayka yabna amīril mu-minīn"
    },
    {
      "index": 26,
      "text": "as salāmu `alayka yabna sayyidati nisā-il `ālamīn"
    },
    {
      "index": 27,
      "text": "ash-hadu annaka aqamtaṣ ṣalāh"
    },
    {
      "index": 28,
      "text": "wa ātaytaz zakāh"
    },
    {
      "index": 29,
      "text": "wa-amar-ta bil ma`rūf"
    },
    {
      "index": 30,
      "text": "wanahayta `anil munkar"
    },
    {
      "index": 31,
      "text": "wa`abadtallāha mukhliṣā"
    },
    {
      "index": 32,
      "text": "wajāhadta fillāhi ḥaqqa jihādih"
    },
    {
      "index": 33,
      "text": "ḥattā atākal yaqīn"
    },
    {
      "index": 34,
      "text": "fa`alaykas salāmu minnī"
    },
    {
      "index": 35,
      "text": "mā baqītu wabaqiyal laylu wan nahār"
    },
    {
      "index": 36,
      "text": "wa`alā-āli baytikaṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 37,
      "text": "anā yā mawlāya mawlan laka wa li-āli baytik"
    },
    {
      "index": 38,
      "text": "silmun liman sālamakum"
    },
    {
      "index": 39,
      "text": "waḥar-bun liman ḥārabakum"
    },
    {
      "index": 40,
      "text": "mu-minun bisirrikum wajahrikum"
    },
    {
      "index": 41,
      "text": "waẓāhirikum wabāṭinikum"
    },
    {
      "index": 42,
      "text": "la`anallāhu a`dā-akum"
    },
    {
      "index": 43,
      "text": "minal awwalīna wal-ākhirīn"
    },
    {
      "index": 44,
      "text": "wa-ana abra-u ilallāhi ta`ālā minhum"
    },
    {
      "index": 45,
      "text": ""
    },
    {
      "index": 46,
      "text": "yā mawlāya yā abā muḥammad"
    },
    {
      "index": 47,
      "text": "yā mawlāya yā abā `abdillāh"
    },
    {
      "index": 48,
      "text": "hādhā yawmul ithnayn"
    },
    {
      "index": 49,
      "text": "wahuwa yawmukumā wabismikumā"
    },
    {
      "index": 50,
      "text": "wa-ana fīhi ḍayfukumā"
    },
    {
      "index": 51,
      "text": "fa-aḍīfānī wa-aḥsinā ḍiyāfatī"
    },
    {
      "index": 52,
      "text": "fani`ma manistuḍīfa bihi antumā"
    },
    {
      "index": 53,
      "text": "wa-ana fīhi min jiwārikumā fa-ajīrānī"
    },
    {
      "index": 54,
      "text": "fa-innakumā ma-mūrāni biḍ ḍiyāfati wal-ijārah"
    },
    {
      "index": 55,
      "text": "faṣallallāhu `alaykumā wa ālikumaṭ ṭayyibīn"
    }
  ]
