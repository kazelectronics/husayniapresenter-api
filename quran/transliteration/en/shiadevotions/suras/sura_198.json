{
  "index": 198,
  "name": "Sura 198",
  "ayas": [
    {
      "index": 1,
      "text": "waja`alanā minal mūfīna bi`ahdihi ilaynā",
      "bismillah": "al ḥamdu lillāhil ladhī akramanā bihādhal yawm"
    },
    {
      "index": 2,
      "text": "wamīthāqihil ladhī wāthaqanā bih"
    },
    {
      "index": 3,
      "text": "min wilāyati wulāti amrih"
    },
    {
      "index": 4,
      "text": "walquwwāmi biqisṭih"
    },
    {
      "index": 5,
      "text": "walam yaj`alnā minal jāḥidīn"
    },
    {
      "index": 6,
      "text": "walmukadhhibīna biyawmid dīn"
    }
  ]
}