{
  "index": 176,
  "name": "Sura 176",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "yā `azīzu yā ghaffār"
    },
    {
      "index": 3,
      "text": "ighfir lī dhunūbī"
    },
    {
      "index": 4,
      "text": "wadhunūba jamī`il mu-minīna walmu-mināt"
    },
    {
      "index": 5,
      "text": "fa-innahu lā yaghfirudh dhunūba illā ant"
    }
  ]
}