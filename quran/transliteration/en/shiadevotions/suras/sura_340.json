{
  "index": 340,
  "name": "Sura 340",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma antal ḥayyul qayyūm, al `aliyyul `aẓīm"
    },
    {
      "index": 3,
      "text": "al khāliqur rāziq, al muḥyil mumīt"
    },
    {
      "index": 4,
      "text": "al badī-ul badī`"
    },
    {
      "index": 5,
      "text": "lakal karamu wa lakal jūd"
    },
    {
      "index": 6,
      "text": "wa lakal mannu wa lakal amr"
    },
    {
      "index": 7,
      "text": "waḥdaka lā sharīka lak"
    },
    {
      "index": 8,
      "text": "yā khāliqu yā rāziq"
    },
    {
      "index": 9,
      "text": "yā muḥyī yā mumīt"
    },
    {
      "index": 10,
      "text": "yā badī`u yā rafī`"
    },
    {
      "index": 11,
      "text": "as-aluka an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 12,
      "text": "wa an tar-ḥama dhulli bayna yadayka wa taḍarru`ī ilayk"
    },
    {
      "index": 13,
      "text": "waḥshatī minan nāsi wa unsī bik"
    }
  ]
}