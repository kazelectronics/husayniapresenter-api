{
  "index": 114,
  "name": "Sura 114",
  "ayas": [
    {
      "index": 1,
      "text": "qul a`ūdhu birabbin nās",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "malikin nās"
    },
    {
      "index": 3,
      "text": "ilāhin nās"
    },
    {
      "index": 4,
      "text": "min sharril waswāsil khannās"
    },
    {
      "index": 5,
      "text": "alladhī yuwaswisu fī ṣudūrin nās"
    },
    {
      "index": 6,
      "text": "minal jinnati wan nās"
    }
  ]
}