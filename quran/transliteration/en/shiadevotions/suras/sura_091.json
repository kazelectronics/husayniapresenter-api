{
  "index": 91,
  "name": "Sura 91",
  "ayas": [
    {
      "index": 1,
      "text": "wash shamsi waḍuḥāhā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wal qamari idhā talāhā"
    },
    {
      "index": 3,
      "text": "wan nahāri idhā jallāhā"
    },
    {
      "index": 4,
      "text": "wal layli idhā yaghshāhā"
    },
    {
      "index": 5,
      "text": "was samā-i wa mā banāhā"
    },
    {
      "index": 6,
      "text": "wal-arḍi wa mā ṭaḥāhā"
    },
    {
      "index": 7,
      "text": "wanafsin wa mā sawwāhā"
    },
    {
      "index": 8,
      "text": "fa-alhamahā fujūrahā wataqwāhā"
    },
    {
      "index": 9,
      "text": "qad aflaḥa man zakkāhā"
    },
    {
      "index": 10,
      "text": "waqad khāba man dassāhā"
    },
    {
      "index": 11,
      "text": "kadhhabat thamūdu biṭaghwāhā"
    },
    {
      "index": 12,
      "text": "idhin-ba`atha ashqāhā"
    },
    {
      "index": 13,
      "text": "faqāla lahum rasūlullāhi nāqatallāhi wasuqyāhā"
    },
    {
      "index": 14,
      "text": "fakadhhabūhu fa`aqarūhā fadamdama `alayhim rabbuhum bidhanbihim fasawwāhā"
    },
    {
      "index": 15,
      "text": "walā yakhāfu `uqbāhā"
    }
  ]
}