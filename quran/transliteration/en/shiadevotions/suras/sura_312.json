{
  "index": 312,
  "name": "Sura 312",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alaykum yā ḥujajallāh",
      "bismillah": "as salāmu `alaykum yā awliyā-alla"
    },
    {
      "index": 2,
      "text": "as salāmu `alaykum yā nūrallāhi fī ẓulumātil ar-ḍ"
    },
    {
      "index": 3,
      "text": "as salāmu `alaykum"
    },
    {
      "index": 4,
      "text": "ṣalawātullāhi `alaykum"
    },
    {
      "index": 5,
      "text": "wa`alā-āli baytikumuṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 6,
      "text": "bi-abī antum wa-ummī"
    },
    {
      "index": 7,
      "text": "laqad `abadtumullāha mukhliṣīn"
    },
    {
      "index": 8,
      "text": "wajāhadtum fillāhi ḥaqqa jihādih"
    },
    {
      "index": 9,
      "text": "ḥattā atākumul yaqīn"
    },
    {
      "index": 10,
      "text": "fala`anallāhu a`dā-akum"
    },
    {
      "index": 11,
      "text": "minal jinni wal-insi ajma`īn"
    },
    {
      "index": 12,
      "text": "wa-ana abra-u ilallāhi wa-ilaykum minhum"
    },
    {
      "index": 13,
      "text": "yā mawlāya yā abā ibrāhīm"
    },
    {
      "index": 14,
      "text": "mūsabna ja`far"
    },
    {
      "index": 15,
      "text": "yā mawlāya yā abal ḥasan"
    },
    {
      "index": 16,
      "text": "`aliyyibna mūsā"
    },
    {
      "index": 17,
      "text": "yā mawlāya yā abā ja`far"
    },
    {
      "index": 18,
      "text": "muḥammadabna `aliyy"
    },
    {
      "index": 19,
      "text": "yā mawlāya yā abal ḥasan"
    },
    {
      "index": 20,
      "text": "`aliyyibna muḥammad"
    },
    {
      "index": 21,
      "text": "anā mawlan lakum"
    },
    {
      "index": 22,
      "text": "mu-minun bisirrikum wajahrikum"
    },
    {
      "index": 23,
      "text": "mutaḍayyifun bikum fī yawmikum hādhā"
    },
    {
      "index": 24,
      "text": "wahuwa yawmul ar-ba`ā"
    },
    {
      "index": 25,
      "text": "wamustajīrun bikum"
    },
    {
      "index": 26,
      "text": "fa-aḍīfūnī wa-ajīrūnī"
    },
    {
      "index": 27,
      "text": "bi-āli baytikumuṭ ṭayyibīnaṭ ṭāhirīn"
    }
  ]
}