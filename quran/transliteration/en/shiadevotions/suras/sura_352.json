{
  "index": 352,
  "name": "Sura 352",
  "ayas": [
    {
      "index": 1,
      "text": "min ahli lā ilāha illallāh",
      "bismillah": "as salāmu `alā ahli lā ilāha illallāh"
    },
    {
      "index": 2,
      "text": "yā ahli lā ilāha illallāh"
    },
    {
      "index": 3,
      "text": "biḥaqqi lā ilāha illallāh"
    },
    {
      "index": 4,
      "text": "kayfa wajadtum qawla lā ilāha illallāh"
    },
    {
      "index": 5,
      "text": "min lā ilāha illallāh"
    },
    {
      "index": 6,
      "text": "yā lā ilāha illallāh"
    },
    {
      "index": 7,
      "text": "biḥaqqi lā ilāha illallāh"
    },
    {
      "index": 8,
      "text": "ighfir liman qāla lā ilāha illallāh"
    },
    {
      "index": 9,
      "text": "waḥshur-nā fī zumrati man qāla lā ilāha illallāh"
    },
    {
      "index": 10,
      "text": "muḥammadun rasūlullāh"
    },
    {
      "index": 11,
      "text": "`aliyyun waliyyullāh"
    }
  ]
}