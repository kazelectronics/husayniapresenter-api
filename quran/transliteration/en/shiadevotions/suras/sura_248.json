{
  "index": 248,
  "name": "Sura 248",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilahī hadaytanī falahawt"
    },
    {
      "index": 3,
      "text": "wa wa`aẓta faqasawt"
    },
    {
      "index": 4,
      "text": "wa ablaytal jamīla fa`aṣayt"
    },
    {
      "index": 5,
      "text": "thumma `araftu mā aṣdar-t"
    },
    {
      "index": 6,
      "text": "idh `arraftanīh, fastaghfar-tu fa-aqalt, fa`udtu fasatar-t"
    },
    {
      "index": 7,
      "text": "falaka ilahil ḥamd"
    },
    {
      "index": 8,
      "text": "taqaḥḥamtu awdiyatal halāk"
    },
    {
      "index": 9,
      "text": "wa ḥalaltu shi`āba talafin ta`arraḍtu fīhā lisaṭawātik"
    },
    {
      "index": 10,
      "text": "wa biḥulūlihā `uqūbātik"
    },
    {
      "index": 11,
      "text": "wa wasīlatī ilaykat tawḥīd"
    },
    {
      "index": 12,
      "text": "wa dharī`atī annī lam ushrik bika shay-ā"
    },
    {
      "index": 13,
      "text": "wa lam attakhidh ma`aka ilahā"
    },
    {
      "index": 14,
      "text": "wa qad farar-tu ilayka binafsī"
    },
    {
      "index": 15,
      "text": "wa ilayka mafarrul musī"
    },
    {
      "index": 16,
      "text": "wa mafza`ul muḍayyi`i liḥaẓẓi nafsihil multajī"
    },
    {
      "index": 17,
      "text": "fakam min `aduwwinntaḍā `alayya sayfa `adāwatih"
    },
    {
      "index": 18,
      "text": "wa shaḥadha lī ẓubata mudyatih"
    },
    {
      "index": 19,
      "text": "wa ar-hafa lī shabā ḥaddih"
    },
    {
      "index": 20,
      "text": "wa dāfa lī qawātila sumūmih"
    },
    {
      "index": 21,
      "text": "wa saddada naḥwī ṣawā-iba sihāmih"
    },
    {
      "index": 22,
      "text": "wa lam tanam `annī `aynu ḥirāsatih"
    },
    {
      "index": 23,
      "text": "wa aḍmara an yasūmanil makrūha wa yujarri`anī zu`āqa marāratih"
    },
    {
      "index": 24,
      "text": "fanaẓar-ta yā ilahī ilā ḍa`fī `aniḥtimālil fawādiḥ"
    },
    {
      "index": 25,
      "text": "wa `ajzī `anil intiṣāri mimman qaṣadanī bimuḥārabatih"
    },
    {
      "index": 26,
      "text": "wa waḥdatī fī kathīri `adadi man nāwānī"
    },
    {
      "index": 27,
      "text": "wa ar-ṣada lī bil balā-i fīmā lam u`mil fīhi fikrī"
    },
    {
      "index": 28,
      "text": "fabtada-tanī binaṣrik"
    },
    {
      "index": 29,
      "text": "wa shadadta azrī biquwwatik"
    },
    {
      "index": 30,
      "text": "thumma falalta lī ḥaddah"
    },
    {
      "index": 31,
      "text": "wa ṣayyar-tahu min ba`di jam`in `adīdin waḥdah"
    },
    {
      "index": 32,
      "text": "wa a`layta ka`bī `alayh"
    },
    {
      "index": 33,
      "text": "wa ja`alta mā saddadahu mar-dūdan `alayh"
    },
    {
      "index": 34,
      "text": "faradadtahu lam yashfi ghayẓah"
    },
    {
      "index": 35,
      "text": "wa lam yaskun ghalīluh"
    },
    {
      "index": 36,
      "text": "qad `aḍḍa `alā shawāh"
    },
    {
      "index": 37,
      "text": "wa adbara muwalliyan qad akhlafat sarāyāh"
    },
    {
      "index": 38,
      "text": "wa kam min bāghin baghānī bimakāyidih"
    },
    {
      "index": 39,
      "text": "wa naṣaba lī sharaka maṣāyidih"
    },
    {
      "index": 40,
      "text": "wa wakkala bī tafaqquda ri`āyatih"
    },
    {
      "index": 41,
      "text": "wa aḍba-a ilayya iḍbā-as sabu`i liṭarīdatih"
    },
    {
      "index": 42,
      "text": "intiẓāran lintihāzil fur-ṣati lifarīsatih"
    },
    {
      "index": 43,
      "text": "wa huwa yuẓhiru lī bashāshatal malaq"
    },
    {
      "index": 44,
      "text": "wa yanẓurunī `alā shiddatil ḥanaq"
    },
    {
      "index": 45,
      "text": "falammā ra-ayta yā ilahī tabārakta wa ta`ālayta daghala sarīratih"
    },
    {
      "index": 46,
      "text": "wa qubḥa manṭawā `alayh, ar-kastahu li-ummi ra-sihi fī zubyatih"
    },
    {
      "index": 47,
      "text": "wa radadtahu fī mahwā ḥufratih"
    },
    {
      "index": 48,
      "text": "fanqama`a ba`dastiṭālatihi dhalīlan fī ribaqi ḥibālatih"
    },
    {
      "index": 49,
      "text": "al latī kāna yuqaddiru an yarānī fīhā"
    },
    {
      "index": 50,
      "text": "wa qad kāda an yaḥulla bī lawlā raḥmatuka mā ḥalla bisāḥatih"
    },
    {
      "index": 51,
      "text": "wa kam min ḥāsidin qad shariqa bī bighuṣṣatih"
    },
    {
      "index": 52,
      "text": "wa shajiya minnī bighayẓih, wa salaqanī biḥaddi lisānih"
    },
    {
      "index": 53,
      "text": "wa waḥaranī biqar-fi `uyūbih"
    },
    {
      "index": 54,
      "text": "wa ja`ala `ir-ḍī gharaḍan limarāmīh"
    },
    {
      "index": 55,
      "text": "wa qalladanī khilālan lam tazal fīh"
    },
    {
      "index": 56,
      "text": "wa waḥaranī bikaydih, wa qaṣadanī bimakīdatih"
    },
    {
      "index": 57,
      "text": "fanādaytuka yā ilahī mustaghīthan bik"
    },
    {
      "index": 58,
      "text": "wāthiqan bisur-`ati ijābatik"
    },
    {
      "index": 59,
      "text": "`āliman annahu lā yuḍṭahadu man awā ilā ẓilli kanafik"
    },
    {
      "index": 60,
      "text": "wa lā yafza`u man laja-a ilā ma`qilintiṣārik"
    },
    {
      "index": 61,
      "text": "faḥaṣṣantanī min ba-sihi biqudratik"
    },
    {
      "index": 62,
      "text": "wa kam min saḥā-ibi makrūhin jallaytahā `annī"
    },
    {
      "index": 63,
      "text": "wa saḥā-ibi ni`amin amṭar-tahā `alayy"
    },
    {
      "index": 64,
      "text": "wa jadāwili raḥmatin nashar-tahā"
    },
    {
      "index": 65,
      "text": "wa `āfiyatin albastahā, wa a`yuni aḥdāthin ṭamastahā"
    },
    {
      "index": 66,
      "text": "wa ghawāshiya kurubātin kashaftahā"
    },
    {
      "index": 67,
      "text": "wa kam min ẓannin ḥasanin ḥaqqaqt, wa `adamin jabar-t"
    },
    {
      "index": 68,
      "text": "wa ṣar-`atin an`ashta wa maskanatin ḥawwalt"
    },
    {
      "index": 69,
      "text": "kullu dhālika in`āman wa taṭawwulan mink"
    },
    {
      "index": 70,
      "text": "wa fī jamī`ihinhimākan minnī `alā ma`āṣīk"
    },
    {
      "index": 71,
      "text": "lam tamna`ka isā-atī `an itmāmi iḥsānik"
    },
    {
      "index": 72,
      "text": "wa lā ḥajaranī dhālika `anir-tikābi masākhiṭik"
    },
    {
      "index": 73,
      "text": "lā tus-alu `ammā taf`al, wa laqad su-ilta fa-a`ṭayt"
    },
    {
      "index": 74,
      "text": "wa lam tus-al fabtada-t, wastumīḥa faḍluka famā akdayt"
    },
    {
      "index": 75,
      "text": "abayta yā mawlāya illā iḥsānā"
    },
    {
      "index": 76,
      "text": "wamtinānan wa taṭawwulan wa in`āmā"
    },
    {
      "index": 77,
      "text": "wa abaytu illā taqaḥḥuman liḥurumātik"
    },
    {
      "index": 78,
      "text": "wa ta`addiyan liḥudūdik, wa ghaflatan `an wa`īdik"
    },
    {
      "index": 79,
      "text": "falakal ḥamdu ilahī min muqtadirin lā yughlab"
    },
    {
      "index": 80,
      "text": "wa dhī anātin lā ya`jal"
    },
    {
      "index": 81,
      "text": "hadhā maqāmu mani`tarafa bisubūghin ni`am"
    },
    {
      "index": 82,
      "text": "wa qābalahā bittaqṣīr, wa shahida `alā nafsihi bittaḍyī`"
    },
    {
      "index": 83,
      "text": "allāhumma fa-innī ataqarrabu ilayka bil muḥammadiyyatir rafī`ah"
    },
    {
      "index": 84,
      "text": "wal `alawiyyatil bayḍā-i, wa atawajjahu ilayka bihimā"
    },
    {
      "index": 85,
      "text": "an tu`īdhanī min sharri kadhā wa kadhā, fa-inna dhalik"
    },
    {
      "index": 86,
      "text": "lā yaḍīqu `alayka fī wujdik"
    },
    {
      "index": 87,
      "text": "wa lā yataka-aduka fī qudratik"
    },
    {
      "index": 88,
      "text": "wa anta `alā kulli shay-in qadīr"
    },
    {
      "index": 89,
      "text": "fahab lī yā ilahī min raḥmatika wa dawāmi tawfīqik"
    },
    {
      "index": 90,
      "text": "mā attakhidhuhu sullaman a`ruju bihi ilā riḍwānik"
    },
    {
      "index": 91,
      "text": "wa-āmanu bihi min `iqābik, yā ar-ḥamar rāḥimīn"
    }
  ]
}