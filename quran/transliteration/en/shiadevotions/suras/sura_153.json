{
  "index": 153,
  "name": "Sura 153",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al ḥamdu lillāhil ladhī lam yattakhidh waladā"
    },
    {
      "index": 3,
      "text": "walam yakun lahu sharīkun fil mulk"
    },
    {
      "index": 4,
      "text": "walam yakun lahu waliyyun minadh dhull"
    },
    {
      "index": 5,
      "text": "wakabbir-hu takbīrā"
    },
    {
      "index": 6,
      "text": "allāhumma innī as-aluka bima`āqidi `izzik"
    },
    {
      "index": 7,
      "text": "`alā ar-kāni `ar-shik"
    },
    {
      "index": 8,
      "text": "wamuntahar raḥmati min kitābik"
    },
    {
      "index": 9,
      "text": "wabismikal a`ẓamil a`ẓamil a`ẓam"
    },
    {
      "index": 10,
      "text": "wadhikrikal a`lal a`lal a`lā"
    },
    {
      "index": 11,
      "text": "wabikalimātikat tāmmāt"
    },
    {
      "index": 12,
      "text": "an tuṣalliya `alā muḥammadin wa ālih"
    },
    {
      "index": 13,
      "text": "wa-an taf`ala bī mā anta ahluh"
    }
  ]
}