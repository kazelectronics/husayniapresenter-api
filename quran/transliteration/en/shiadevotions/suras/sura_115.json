{
  "index": 115,
  "name": "Sura 115",
  "ayas": [
    {
      "index": 1,
      "text": "wa ash-hadu anna muḥammadan `abduhu warasūluh",
      "bismillah": "ash-hadu an lā ilāha illallāhu waḥdahu lā sharīka lah"
    },
    {
      "index": 2,
      "text": "wa annahu sayyidul awwalīna wal-ākhirīn"
    },
    {
      "index": 3,
      "text": "wa annahu sayyidul anbiyā-i walmur-salīn"
    },
    {
      "index": 4,
      "text": "allāhumma ṣalli `alayhi wa`alahli baytihil ā-imatiṭ ṭayyibīn"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā rasūlallāh"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā khalīlallāh"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā nabiyyallāh"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yā ṣafiyyallāh"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka yā raḥmatallāh"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā khiyaratallāh"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka yā ḥabīballāh"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka yā najīballāh"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka yā khātaman nabiyyīn"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka yā sayyidal mur-salīn"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka yā qā-iman bil qisṭ"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka yā fātiḥal khayr"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka yā ma`dinal waḥyi wat tanzīl"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka yā muballighan `anillāh"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka ayyuhas sirājul munīr"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka yā mubashhir"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka yā nadhīr"
    },
    {
      "index": 22,
      "text": "as salāmu `alayka yā mundhir"
    },
    {
      "index": 23,
      "text": "as salāmu `alayka yā nūrallāhil ladhī yustaḍā-u bih"
    },
    {
      "index": 24,
      "text": "as salāmu `alayka wa`alā ahli baytikaṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 25,
      "text": "al hādīnal mahdiyyīn"
    },
    {
      "index": 26,
      "text": "as salāmu `alayka wa`alā jaddika `abdil muṭṭalib"
    },
    {
      "index": 27,
      "text": "wa`alā abīka `abdillāh"
    },
    {
      "index": 28,
      "text": "as salāmu `alā ummika-āminata binti wahab"
    },
    {
      "index": 29,
      "text": "as salāmu `alā `ammika ḥamzata sayyidish shuhadā"
    },
    {
      "index": 30,
      "text": "as salāmu `alā `ammikal `abbāsibni `abdil muṭṭalib"
    },
    {
      "index": 31,
      "text": "as salāmu `alā `ammika wa kafīlika abī ṭālib"
    },
    {
      "index": 32,
      "text": "as salāmu `alabni `ammika ja`fariniṭ ṭayyāri fī jinānil khuld"
    },
    {
      "index": 33,
      "text": "as salāmu `alayka yā muḥammad"
    },
    {
      "index": 34,
      "text": "as salāmu `alayka yā aḥmad"
    },
    {
      "index": 35,
      "text": "as salāmu `alayka yā ḥujjatallāhi `alal awwalīna wal-ākhirīn"
    },
    {
      "index": 36,
      "text": "was sābiqa ilā ṭā`ati rabbil `ālamīn"
    },
    {
      "index": 37,
      "text": "walmuhaymina `alā rusulih"
    },
    {
      "index": 38,
      "text": "walkhātima li-anbiyā-ih"
    },
    {
      "index": 39,
      "text": "wash shāhida `alā khalqih"
    },
    {
      "index": 40,
      "text": "wash shafī`a ilayh"
    },
    {
      "index": 41,
      "text": "walmakīna ladayh"
    },
    {
      "index": 42,
      "text": "walmuṭā`a fī malakūtih"
    },
    {
      "index": 43,
      "text": "al aḥmada minal awṣāf"
    },
    {
      "index": 44,
      "text": "al muḥammada lisā-iril ashrāf"
    },
    {
      "index": 45,
      "text": "al karīma `indar rabb"
    },
    {
      "index": 46,
      "text": "wal mukallama min warā-il ḥujub"
    },
    {
      "index": 47,
      "text": "al fā-iza bissibāq"
    },
    {
      "index": 48,
      "text": "wal fā-ita `anil liḥāq"
    },
    {
      "index": 49,
      "text": "taslīma `ārifin biḥaqqik"
    },
    {
      "index": 50,
      "text": "mu`tarifin bittaqṣīri fī qiyāmihi biwājibik"
    },
    {
      "index": 51,
      "text": "ghayri munkirin mantahā ilayhi min faḍlik"
    },
    {
      "index": 52,
      "text": "mūqinin bil mazīdāti min rabbik"
    },
    {
      "index": 53,
      "text": "mu-minin bilkitābil munzali `alayk"
    },
    {
      "index": 54,
      "text": "muḥallilin ḥalālak"
    },
    {
      "index": 55,
      "text": "muḥarrimin ḥarāmak"
    },
    {
      "index": 56,
      "text": "ash-hadu yā rasūlallāhi ma`a kulli shāhid"
    },
    {
      "index": 57,
      "text": "wataḥammaluhā `an kulli jāḥid"
    },
    {
      "index": 58,
      "text": "annaka qad ballaghta risālāti rabbik"
    },
    {
      "index": 59,
      "text": "wanaṣaḥta li-ummatik"
    },
    {
      "index": 60,
      "text": "wajāhadta fī sabīli rabbik"
    },
    {
      "index": 61,
      "text": "waṣada`ta bimrih"
    },
    {
      "index": 62,
      "text": "waḥtamaltal adhā fī janbih"
    },
    {
      "index": 63,
      "text": "wada`awta ilā sabīlihi bil ḥikmati walmaw`iẓatil ḥasanatil jamīlah"
    },
    {
      "index": 64,
      "text": "waddaytal ḥaqqal ladhī kāna `alayk"
    },
    {
      "index": 65,
      "text": "wa annaka qad ra-ufta bil mu-minīn"
    },
    {
      "index": 66,
      "text": "waghaluẓta `alal kāfirīn"
    },
    {
      "index": 67,
      "text": "wa`abadtallāha mukhliṣan ḥattā atākal yaqīn"
    },
    {
      "index": 68,
      "text": "fabalaghallāhu bika ashrafa maḥallil mukarramīn"
    },
    {
      "index": 69,
      "text": "wa`lā manāzilil muqarrabīn"
    },
    {
      "index": 70,
      "text": "war-fa`a darajātil mur-salīna ḥaythu lā yalḥaquka lāḥiq"
    },
    {
      "index": 71,
      "text": "walā ifūquka fā-iq"
    },
    {
      "index": 72,
      "text": "walā yasbiquka sābiq"
    },
    {
      "index": 73,
      "text": "walā yaṭma`u fī idrākika ṭāmi`"
    },
    {
      "index": 74,
      "text": "al ḥamdu lillāhil ladhistanqadhanā bika minal halakah"
    },
    {
      "index": 75,
      "text": "wahadānā bika minaḍ ḍalālah"
    },
    {
      "index": 76,
      "text": "wanawwaranā bika minaẓ ẓulmah"
    },
    {
      "index": 77,
      "text": "fajazākallāhu yā rasūlallāhi min mab`ūthinifḍala mā jāzā nabiyyan `an ummatih"
    },
    {
      "index": 78,
      "text": "warasūlan `amman ar-sala ilayh"
    },
    {
      "index": 79,
      "text": "bi-abī anta wa ummī yā rasūlallāhi zur-tuka `ārifan biḥaqqik"
    },
    {
      "index": 80,
      "text": "muqirran bifaḍlik"
    },
    {
      "index": 81,
      "text": "mustabṣiran biḍalālati man khālafaka wakhālafa ahli baytik"
    },
    {
      "index": 82,
      "text": "`ārifan bil hudal ladhinta `alayh"
    },
    {
      "index": 83,
      "text": "bi-abī anta wa ummī wanafsī wa ahlī wamālī wawaladī"
    },
    {
      "index": 84,
      "text": "anā uṣallī `alayka kamā ṣallallāhu `alayk"
    },
    {
      "index": 85,
      "text": "waṣallā `alayka malā-ikatuhu wa anbiya-uhu warusuluh"
    },
    {
      "index": 86,
      "text": "ṣalātan mutatābi`atan wāfiratan mutawāṣilah"
    },
    {
      "index": 87,
      "text": "lā anqiṭā`a lahā walā amada walā ajal"
    },
    {
      "index": 88,
      "text": "ṣallallāhu `alayka wa`alā ahli baytikaṭ ṭayyibīnaṭ ṭāhirīna kamā antum ahluh"
    },
    {
      "index": 89,
      "text": "allāhummaj`al jawāmi`a ṣalawātik"
    },
    {
      "index": 90,
      "text": "wanawāmiya barakātik"
    },
    {
      "index": 91,
      "text": "wafawāḍila khayrātik"
    },
    {
      "index": 92,
      "text": "washarā-ifa taḥiyyātika wataslīmātik"
    },
    {
      "index": 93,
      "text": "wakarāmātika waraḥamātik"
    },
    {
      "index": 94,
      "text": "waṣalawāti malā-ikatikal muqarrabīn"
    },
    {
      "index": 95,
      "text": "wa anbiyā-ikal mur-salīn"
    },
    {
      "index": 96,
      "text": "wa a-immatikal muntajabīn"
    },
    {
      "index": 97,
      "text": "wa`ibādikaṣ ṣāliḥīn"
    },
    {
      "index": 98,
      "text": "wa ahlis samāwāti wālāraḍīn"
    },
    {
      "index": 99,
      "text": "waman sabbaḥa laka yā rabbal `ālamīn"
    },
    {
      "index": 100,
      "text": "minal āwwalīna wal-ākhirīn"
    },
    {
      "index": 101,
      "text": "`alā muḥammadin `abdika warasūlik"
    },
    {
      "index": 102,
      "text": "washāhidika wanabiyyik"
    },
    {
      "index": 103,
      "text": "wanadhīrika wa amīnik"
    },
    {
      "index": 104,
      "text": "wamakīnika wanajiyyik"
    },
    {
      "index": 105,
      "text": "wanajībika waḥabībik"
    },
    {
      "index": 106,
      "text": "wakhalīlika waṣafiyyik"
    },
    {
      "index": 107,
      "text": "waṣafwatika wakhāṣṣatik"
    },
    {
      "index": 108,
      "text": "wakhāliṣatika waraḥmatik"
    },
    {
      "index": 109,
      "text": "wakhayri khiyaratika min khalqik"
    },
    {
      "index": 110,
      "text": "nabiyyir raḥmah"
    },
    {
      "index": 111,
      "text": "wakhāzinil maghfirah"
    },
    {
      "index": 112,
      "text": "waqā-idil khayri wal barakah"
    },
    {
      "index": 113,
      "text": "wamunqidhil `ibādi minal halakati bi-idhnik"
    },
    {
      "index": 114,
      "text": "wadā`īhim ilā dīnik"
    },
    {
      "index": 115,
      "text": "al qayyimi bimrik"
    },
    {
      "index": 116,
      "text": "awwalin nabiyyīna mīthāqā"
    },
    {
      "index": 117,
      "text": "wa ākhirihim mab`athā"
    },
    {
      "index": 118,
      "text": "al ladhī ghamastahu fī baḥril faḍīlah"
    },
    {
      "index": 119,
      "text": "walmanzilatil jalīlah"
    },
    {
      "index": 120,
      "text": "wad darajatir rafī`ah"
    },
    {
      "index": 121,
      "text": "walmar-tabatil khaṭīrah"
    },
    {
      "index": 122,
      "text": "wawda`tahul aṣlābaṭ ṭāhirah"
    },
    {
      "index": 123,
      "text": "wanaqaltahu minhā ilal ar-ḥāmil muṭahharah"
    },
    {
      "index": 124,
      "text": "luṭfan minka lah"
    },
    {
      "index": 125,
      "text": "wataḥannunan minka `alayh"
    },
    {
      "index": 126,
      "text": "idh wakkalta liṣawnihi waḥirāsatih"
    },
    {
      "index": 127,
      "text": "waḥifẓihi waḥiyāṭatih"
    },
    {
      "index": 128,
      "text": "min qudratika `aynan `āṣimah"
    },
    {
      "index": 129,
      "text": "ḥajabta bihā `anhu madānisal `ahr"
    },
    {
      "index": 130,
      "text": "wama`ā-ibas sifāḥ"
    },
    {
      "index": 131,
      "text": "ḥattā rafa`ta bihi nawāẓiral `ibād"
    },
    {
      "index": 132,
      "text": "waḥyayta bihi maytal bilād"
    },
    {
      "index": 133,
      "text": "bin kashafta `an nūri wilādatihi ẓulamal astār"
    },
    {
      "index": 134,
      "text": "walbasta ḥaramaka bihi ḥulalal anwār"
    },
    {
      "index": 135,
      "text": "allāhumma fakamā khaṣaṣtahu bisharafi hādhihil mar-tabatil karīmah"
    },
    {
      "index": 136,
      "text": "wadhukhri hādhihil manqabatil `aẓīmah"
    },
    {
      "index": 137,
      "text": "ṣalli `alayhi kamā wafā bi`ahdik"
    },
    {
      "index": 138,
      "text": "waballagha risālātik"
    },
    {
      "index": 139,
      "text": "waqātalahlal juḥūdi `alā tawḥīdik"
    },
    {
      "index": 140,
      "text": "waqaṭa`a raḥimal kufri fī i`zāzi dīnik"
    },
    {
      "index": 141,
      "text": "walabisa thawbal balwā fī mujāhadati`dā-ik"
    },
    {
      "index": 142,
      "text": "wawjabta lahu bikullidhā massah"
    },
    {
      "index": 143,
      "text": "aw kaydinḥassa bih"
    },
    {
      "index": 144,
      "text": "minal fi-atil latī ḥāwalat qatlah"
    },
    {
      "index": 145,
      "text": "faḍīlatan tafūqul faḍā-il"
    },
    {
      "index": 146,
      "text": "wayamliku bihal jazīla min nawālik"
    },
    {
      "index": 147,
      "text": "waqadsarral ḥasrah"
    },
    {
      "index": 148,
      "text": "wakhfaz zafrah"
    },
    {
      "index": 149,
      "text": "watajarra`al ghuṣṣah"
    },
    {
      "index": 150,
      "text": "walam yatakhaṭṭa mā mathhala lahu waḥyuk"
    },
    {
      "index": 151,
      "text": "allāhumma ṣalli `alayhi wa`alā ahli baytihi ṣalātan tar-ḍāhā lahum"
    },
    {
      "index": 152,
      "text": "waballigh-hum minnā taḥiyyatan kathīratan wasalāmā"
    },
    {
      "index": 153,
      "text": "wa ātinā min ladunka fī muwālātihim faḍlan wa-iḥsānā"
    },
    {
      "index": 154,
      "text": "waraḥmatan waghufrānā"
    },
    {
      "index": 155,
      "text": "innaka dhul faḍlil `aẓīm"
    },
    {
      "index": 156,
      "text": "allāhumma innaka qulta linabiyyika muḥammad"
    },
    {
      "index": 157,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 158,
      "text": "walaw annahum idh ẓalamū anfusahum"
    },
    {
      "index": 159,
      "text": "ja-ūka fastaghfarullāh"
    },
    {
      "index": 160,
      "text": "wastaghfara lahumur rasūl"
    },
    {
      "index": 161,
      "text": "lawajadullāha tawwāban raḥīmā"
    },
    {
      "index": 162,
      "text": "walam aḥḍur zamāna rasūlika `alayhi wa ālihis salām"
    },
    {
      "index": 163,
      "text": "allāhumma waqad zur-tuhu rāghiban tā-iban min sayyi-i `amalī"
    },
    {
      "index": 164,
      "text": "wamustaghfiran laka min dhunūbī"
    },
    {
      "index": 165,
      "text": "wamuqirran laka bihā wa anta a`lamu bihā minnī"
    },
    {
      "index": 166,
      "text": "wamutawajjihan ilayka binabiyyika nabiyyir raḥmah"
    },
    {
      "index": 167,
      "text": "ṣalawātuka `alayhi wa ālih"
    },
    {
      "index": 168,
      "text": "faj`alnillāhumma bimuḥammadin wa ahli baytihi `indaka wajīhā"
    },
    {
      "index": 169,
      "text": "fid dunyā wal-ākhirati waminal muqarrabīn"
    },
    {
      "index": 170,
      "text": "yā muḥammad"
    },
    {
      "index": 171,
      "text": "yā rasūlallāh"
    },
    {
      "index": 172,
      "text": "bi-abī anta wa-ummī"
    },
    {
      "index": 173,
      "text": "yā nabiyyallāh"
    },
    {
      "index": 174,
      "text": "yā sayyida khalqillāh"
    },
    {
      "index": 175,
      "text": "innī atawajjahu bika ilallāhi rabbika warabbī"
    },
    {
      "index": 176,
      "text": "liyaghfira lī dhunūbī"
    },
    {
      "index": 177,
      "text": "wayataqabbala minnī `amalī"
    },
    {
      "index": 178,
      "text": "wayaqḍiya lī ḥawā-ijī"
    },
    {
      "index": 179,
      "text": "fakun lī shafī`an `inda rabbika warabbī"
    },
    {
      "index": 180,
      "text": "fani`mal mas-ūlul mawlā rabbī"
    },
    {
      "index": 181,
      "text": "wani`mash shafī`u ant"
    },
    {
      "index": 182,
      "text": "yā muḥammad"
    },
    {
      "index": 183,
      "text": "`alayka wa`alā ahli baytikas salām"
    },
    {
      "index": 184,
      "text": "allāhumma wa awjib lī minkal maghfirata war raḥmah"
    },
    {
      "index": 185,
      "text": "war rizqal wāsi`aṭ ṭayyiban nāfi`"
    },
    {
      "index": 186,
      "text": "kamā awjabta liman atā nabiyyika muḥammadā"
    },
    {
      "index": 187,
      "text": "ṣalawātuka `alayhi wa ālih"
    },
    {
      "index": 188,
      "text": "wahuwa ḥayyun fa-aqarra lahu bidhunūbih"
    },
    {
      "index": 189,
      "text": "wastaghfara lahu rasūluk"
    },
    {
      "index": 190,
      "text": "`alayhi wa ālihis salām"
    },
    {
      "index": 191,
      "text": "faghafar-ta lahu biraḥmatika yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 192,
      "text": "allāhumma waqadmmaltuka warajawtuk"
    },
    {
      "index": 193,
      "text": "waqumtu bayna yadayk"
    },
    {
      "index": 194,
      "text": "waraghibtu ilayka `amman siwāk"
    },
    {
      "index": 195,
      "text": "waqad ammaltu jazīla thawābik"
    },
    {
      "index": 196,
      "text": "wa-innī lamuqirrun ghayru munkir"
    },
    {
      "index": 197,
      "text": "watā-ibun ilayka mimmaqtaraft"
    },
    {
      "index": 198,
      "text": "wa`ā-idhun bika fī hādhal maqām"
    },
    {
      "index": 199,
      "text": "mimmā qaddamtu minal a`māl"
    },
    {
      "index": 200,
      "text": "al latī taqaddamta ilayya fīhā wanahaytanī `anhā"
    },
    {
      "index": 201,
      "text": "waw`adta `alayhal `iqāb"
    },
    {
      "index": 202,
      "text": "wā`ūdhu bikarami wajhik"
    },
    {
      "index": 203,
      "text": "an tuqīmanī maqāmal khizyi wadh dhull"
    },
    {
      "index": 204,
      "text": "yawma tuhtaku fīhil astār"
    },
    {
      "index": 205,
      "text": "watabdū fīhil asrāru walfaḍā-iḥ"
    },
    {
      "index": 206,
      "text": "watar-`adu fīhil farā-iṣ"
    },
    {
      "index": 207,
      "text": "yawmal ḥasrati wan nadāmah"
    },
    {
      "index": 208,
      "text": "yawmal-āfikah"
    },
    {
      "index": 209,
      "text": "yawmal-āzifah"
    },
    {
      "index": 210,
      "text": "yawmat taghābun"
    },
    {
      "index": 211,
      "text": "yawmal faṣl"
    },
    {
      "index": 212,
      "text": "yawmal jazā"
    },
    {
      "index": 213,
      "text": "yawman kāna miqdāruhu khamsīna alfa sanah"
    },
    {
      "index": 214,
      "text": "yawman nafkhah"
    },
    {
      "index": 215,
      "text": "yawma tar-jufur rājifah"
    },
    {
      "index": 216,
      "text": "tatba`uhar rādifah"
    },
    {
      "index": 217,
      "text": "yawman nashr"
    },
    {
      "index": 218,
      "text": "yawmal `ar-ḍ"
    },
    {
      "index": 219,
      "text": "yawma yaqūmun nāsu lirabbil `ālamīn"
    },
    {
      "index": 220,
      "text": "yawma yafirrul mar-u min akhīh"
    },
    {
      "index": 221,
      "text": "wa ummihi wa-abīh"
    },
    {
      "index": 222,
      "text": "waṣāḥibatihi wabanīh"
    },
    {
      "index": 223,
      "text": "yawma tashaqqaqul ar-ḍu waknāfus samā"
    },
    {
      "index": 224,
      "text": "yawma ta-tī kullu nafsin tujādilu `an nafsihā"
    },
    {
      "index": 225,
      "text": "yawma yuraddūna ilallāhi fayunabbi-uhum bimā `amilū"
    },
    {
      "index": 226,
      "text": "yawma lā yughnī mawlan `an mawlan shay-ā"
    },
    {
      "index": 227,
      "text": "walā hum yunṣarūn"
    },
    {
      "index": 228,
      "text": "illā man raḥimallāh"
    },
    {
      "index": 229,
      "text": "innahu huwal `azīzur raḥīm"
    },
    {
      "index": 230,
      "text": "yawma yuraddūna ilā `ālimil ghaybi wash shahādah"
    },
    {
      "index": 231,
      "text": "yawma yuraddūna ilallāhi mawlāhumul ḥaqq"
    },
    {
      "index": 232,
      "text": "yawma yakhrujūna minal ajdāthi sirā`ā"
    },
    {
      "index": 233,
      "text": "ka-annahum ilā nuṣubin yūfiḍūn"
    },
    {
      "index": 234,
      "text": "waka-annahum jarādun muntashir"
    },
    {
      "index": 235,
      "text": "muhṭi`īna ilad dā`ī"
    },
    {
      "index": 236,
      "text": "ilallāh"
    },
    {
      "index": 237,
      "text": "yawmal wāqi`ah"
    },
    {
      "index": 238,
      "text": "yawma turajjul ar-ḍu rajjā"
    },
    {
      "index": 239,
      "text": "yawma takūnus samā-u kalmuhl"
    },
    {
      "index": 240,
      "text": "watakūnul jibālu kal`ihn"
    },
    {
      "index": 241,
      "text": "walā yas-alu ḥamīmun ḥamīmā"
    },
    {
      "index": 242,
      "text": "yawmash shāhidi walmash-hūd"
    },
    {
      "index": 243,
      "text": "yawma takūnul malā-ikatu ṣaffan ṣaffā"
    },
    {
      "index": 244,
      "text": "allāhummar-ḥam mawqifī fī dhālikal yawmi bimawqifī fī hādhal yawm"
    },
    {
      "index": 245,
      "text": "walā tukhzinī fī dhālikal mawqifi bimā janaytu `alā nafsī"
    },
    {
      "index": 246,
      "text": "waj`al yā rabbi fī dhālikal yawmi ma`awliyā-ika munṭalaqī"
    },
    {
      "index": 247,
      "text": "wafī zumrati muḥammadin wa ahli baytihi `alayhimus salāmu maḥsharī"
    },
    {
      "index": 248,
      "text": "waj`al ḥawḍahu mawridī"
    },
    {
      "index": 249,
      "text": "wafil ghurril kirāmi maṣdarī"
    },
    {
      "index": 250,
      "text": "wa`ṭinī kitābī biyamīnī"
    },
    {
      "index": 251,
      "text": "ḥattā afūza biḥasanātī"
    },
    {
      "index": 252,
      "text": "watubayyiḍa bihi wajhī"
    },
    {
      "index": 253,
      "text": "watuyassira bihi ḥisābī"
    },
    {
      "index": 254,
      "text": "waturajjiḥa bihi mīzānī"
    },
    {
      "index": 255,
      "text": "wamḍiya ma`al fā-izīn"
    },
    {
      "index": 256,
      "text": "min `ibādikaṣ ṣāliḥīn"
    },
    {
      "index": 257,
      "text": "ilā riḍwānika wajinānik"
    },
    {
      "index": 258,
      "text": "ilāhal `ālamīn"
    },
    {
      "index": 259,
      "text": "allāhumma innī a`ūdhu bika min an tafḍaḥanī fī dhālikal yawm"
    },
    {
      "index": 260,
      "text": "bayna yadayal khalā-iqi bijarīratī"
    },
    {
      "index": 261,
      "text": "aw an alqal khizya wan nadāmata bikhaṭī-atī"
    },
    {
      "index": 262,
      "text": "aw an tuẓhira fīhi sayyi-ātī `alā ḥasanātī"
    },
    {
      "index": 263,
      "text": "aw an tunawwiha baynal khalā-iqi bismī"
    },
    {
      "index": 264,
      "text": "yā karīmu yā karīm"
    },
    {
      "index": 265,
      "text": "al `afwal `afw"
    },
    {
      "index": 266,
      "text": "as sitras sitr"
    },
    {
      "index": 267,
      "text": "allāhumma wa a`ūdhu bika min an yakūna fī dhālikal yawm"
    },
    {
      "index": 268,
      "text": "fī mawāqifil ashrāri mawqifī"
    },
    {
      "index": 269,
      "text": "aw fī maqāmil ashqiyā-i maqāmī"
    },
    {
      "index": 270,
      "text": "wa-idhā mayyazta bayna khalqik"
    },
    {
      "index": 271,
      "text": "fasuqta kullan bi-a`mālihim"
    },
    {
      "index": 272,
      "text": "zumaran ilā manāzilihim"
    },
    {
      "index": 273,
      "text": "fasuqnī biraḥmatika ma`a `ibādikaṣ ṣāliḥīn"
    },
    {
      "index": 274,
      "text": "wafī zumrati awliyā-ikal muttaqīn"
    },
    {
      "index": 275,
      "text": "ilā jannātika yā rabbal `ālamīn"
    },
    {
      "index": 276,
      "text": ""
    },
    {
      "index": 277,
      "text": "as salāmu `alayka yā rasūlallāh"
    },
    {
      "index": 278,
      "text": "as salāmu `alayka ayyuhal bashīrun nadhīr"
    },
    {
      "index": 279,
      "text": "as salāmu `alayka ayyuhas sirājul munīr"
    },
    {
      "index": 280,
      "text": "as salāmu `alayka ayyuhas safīru baynallāhi wabayna khalqih"
    },
    {
      "index": 281,
      "text": "ash-hadu yā rasūlallāhi annaka kunta nūran fil aṣlābish shāmikhah"
    },
    {
      "index": 282,
      "text": "wal-ar-ḥāmil muṭahharah"
    },
    {
      "index": 283,
      "text": "lam tunajjiskal jāhiliyyatu bi-anjāsihā"
    },
    {
      "index": 284,
      "text": "walam tulbiska min mudlahimmāti thiyābihā"
    },
    {
      "index": 285,
      "text": "wa-ash-hadu yā rasūlallāhi annī mu-minun bik"
    },
    {
      "index": 286,
      "text": "wabil a-immati min ahli baytik"
    },
    {
      "index": 287,
      "text": "mūqinun bijamī`i mā atayta bih"
    },
    {
      "index": 288,
      "text": "rāḍin mu-min"
    },
    {
      "index": 289,
      "text": "wa ash-hadu annal a-immata min ahli baytika a`lāmul hudā"
    },
    {
      "index": 290,
      "text": "wal`ur-watul wuthqā"
    },
    {
      "index": 291,
      "text": "walḥujjatu `alā ahlid dunyā"
    },
    {
      "index": 292,
      "text": "allāhumma lā taj`alhu-ākhiral `ahdi min ziyārati nabiyyik"
    },
    {
      "index": 293,
      "text": "`alayhi wa ālihis salām"
    },
    {
      "index": 294,
      "text": "wa-in tawaffaytanī fa-innī ash-hadu fī mamātī"
    },
    {
      "index": 295,
      "text": "`alā mā ash-hadu `alayhi fī ḥayātī"
    },
    {
      "index": 296,
      "text": "annaka antallāhu lā ilāha illā ant"
    },
    {
      "index": 297,
      "text": "waḥdaka lā sharīka lak"
    },
    {
      "index": 298,
      "text": "wa-anna muḥammadan `abduka warasūluk"
    },
    {
      "index": 299,
      "text": "wa annal a-immata min ahli baytihi awliya-uka wa-anṣāruk"
    },
    {
      "index": 300,
      "text": "waḥujajuka `alā khalqik"
    },
    {
      "index": 301,
      "text": "wakhulafa-uka fī `ibādik"
    },
    {
      "index": 302,
      "text": "wa a`lāmuka fī bilādik"
    },
    {
      "index": 303,
      "text": "wakhuzzānu `ilmik"
    },
    {
      "index": 304,
      "text": "waḥafaẓatu sirrik"
    },
    {
      "index": 305,
      "text": "watarājimatu waḥyik"
    },
    {
      "index": 306,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 307,
      "text": "waballigh rūḥa nabiyyika muḥammadin wa ālih"
    },
    {
      "index": 308,
      "text": "fī sā`atī hādhihi wafī kulli sā`ah"
    },
    {
      "index": 309,
      "text": "taḥiyyatan minnī wasalāmā"
    },
    {
      "index": 310,
      "text": "was salāmu `alayka yā rasūlallāhi waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 311,
      "text": "lā ja`alahullāhu-ākhira taslīmī `alayk"
    }
  ]
}