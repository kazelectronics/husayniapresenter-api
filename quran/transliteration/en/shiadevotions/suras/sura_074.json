{
  "index": 74,
  "name": "Sura 74",
  "ayas": [
    {
      "index": 1,
      "text": "yā-ayyuhal muddathhir",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "qum fa-andhir"
    },
    {
      "index": 3,
      "text": "warabbaka fakabbir"
    },
    {
      "index": 4,
      "text": "wathiyābaka faṭahhir"
    },
    {
      "index": 5,
      "text": "war rujza fahjur"
    },
    {
      "index": 6,
      "text": "walā tamnun tastakthir"
    },
    {
      "index": 7,
      "text": "walirabbika faṣbir"
    },
    {
      "index": 8,
      "text": "fa-idhā nuqira fin nāqūr"
    },
    {
      "index": 9,
      "text": "fadhālika yawma-idhiy yawmun `asīr"
    },
    {
      "index": 10,
      "text": "`alal-kāfirīna ghayru yasīr"
    },
    {
      "index": 11,
      "text": "dharnī wa man khalaqtu waḥīdā"
    },
    {
      "index": 12,
      "text": "waja`altu lahu mālan mamdūdā"
    },
    {
      "index": 13,
      "text": "wabanīna shuhūdā"
    },
    {
      "index": 14,
      "text": "wa mahhadtu lahu tamhīdā"
    },
    {
      "index": 15,
      "text": "thumma yaṭma`u an azīd"
    },
    {
      "index": 16,
      "text": "kallā ۖ innahu kāna li-āyātinā `anīdā"
    },
    {
      "index": 17,
      "text": "sa-urhiquhu ṣa`ūdā"
    },
    {
      "index": 18,
      "text": "innahu fakkara waqaddar"
    },
    {
      "index": 19,
      "text": "faqutila kayfa qaddar"
    },
    {
      "index": 20,
      "text": "thumma qutila kayfa qaddar"
    },
    {
      "index": 21,
      "text": "thumma naẓar"
    },
    {
      "index": 22,
      "text": "thumma `abasa wabasar"
    },
    {
      "index": 23,
      "text": "thumma adbara wastakbar"
    },
    {
      "index": 24,
      "text": "faqāla in hādhā illā siḥrun yu-thar"
    },
    {
      "index": 25,
      "text": "in hādhā illā qawlul bashar"
    },
    {
      "index": 26,
      "text": "sa-uṣlīhi saqar"
    },
    {
      "index": 27,
      "text": "wa mā adrāka mā saqar"
    },
    {
      "index": 28,
      "text": "lā tubqī walā tadhar"
    },
    {
      "index": 29,
      "text": "lawwāḥatun lilbashar"
    },
    {
      "index": 30,
      "text": "`alayhā tis`ata `ashar"
    },
    {
      "index": 31,
      "text": "wa mā ja`alnā aṣ-ḥāban nāri illā malā-ikatan wa mā ja`alnā `iddatahum illā fitnatan lilladhīna kafarū liyastayqinal ladhīna ūtul kitāba wayazdādal ladhīna āmanū-iymānan walā yartābal ladhīna ūtul kitāba wal mu-minūna waliyaqūlal ladhīna fī qulūbihim maraḍun wal kāfirūna mādhā arādallāhu bihādhā mathalā ۚ kadhālika yuḍillullāhu man yashā-u wayahdī man yashā-u ۚ wa mā ya`lamu junūda rabbika illā huw ۚ wa mā hiya illā dhikrā lilbashar"
    },
    {
      "index": 32,
      "text": "kallā wal qamar"
    },
    {
      "index": 33,
      "text": "wal layli idh adbar"
    },
    {
      "index": 34,
      "text": "waṣ ṣubḥi idhā asfar"
    },
    {
      "index": 35,
      "text": "innahā la-iḥdal kubar"
    },
    {
      "index": 36,
      "text": "nadhīral lilbashar"
    },
    {
      "index": 37,
      "text": "liman shā-a minkum an yataqaddama aw yata-akhhar"
    },
    {
      "index": 38,
      "text": "kullu nafsin bimā kasabat rahīnah"
    },
    {
      "index": 39,
      "text": "illā aṣ-ḥābal yamīn"
    },
    {
      "index": 40,
      "text": "fī jannātiy yatasā-alūn"
    },
    {
      "index": 41,
      "text": "`anil mujrimīn"
    },
    {
      "index": 42,
      "text": "mā salakakum fī saqar"
    },
    {
      "index": 43,
      "text": "qālū lam naku minal muṣallīn"
    },
    {
      "index": 44,
      "text": "walam naku nuṭ`imul miskīn"
    },
    {
      "index": 45,
      "text": "wakunnā nakhūḍu ma`al khā-iḍīn"
    },
    {
      "index": 46,
      "text": "wakunnā nukadhhibu biyawmid dīn"
    },
    {
      "index": 47,
      "text": "ḥattā atānal yaqīn"
    },
    {
      "index": 48,
      "text": "famā tanfa`uhum shafā`atush shhāfi`īn"
    },
    {
      "index": 49,
      "text": "famā lahum `anit tadhkirati mu`riḍīn"
    },
    {
      "index": 50,
      "text": "ka-annahum ḥumurun mustanfirah"
    },
    {
      "index": 51,
      "text": "farrat min qaswarah"
    },
    {
      "index": 52,
      "text": "bal yurīdu kullum-ri-in minhum an yu-tā ṣuḥufan munashharah"
    },
    {
      "index": 53,
      "text": "kallā ۖ bal lā yakhāfūnal-ākhirah"
    },
    {
      "index": 54,
      "text": "kallā innahu tadhkirah"
    },
    {
      "index": 55,
      "text": "faman shā-a dhakarah"
    },
    {
      "index": 56,
      "text": "wa mā yadhkurūna illā an yashā-allāh ۚ huwa ahlut taqwā wa-ahlul maghfirah"
    }
  ]
}