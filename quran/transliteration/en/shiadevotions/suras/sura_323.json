{
  "index": 323,
  "name": "Sura 323",
  "ayas": [
    {
      "index": 1,
      "text": "walmuslimīna walmuslimāt",
      "bismillah": "allāhummaghfir lilmu-minīna walmu-mināt"
    },
    {
      "index": 2,
      "text": "al aḥyā-i minhum wal-amwāt"
    },
    {
      "index": 3,
      "text": "tābi`illāhumma baynanā wa baynahum bil khayrāt"
    },
    {
      "index": 4,
      "text": "innaka mujībud da`awāt"
    },
    {
      "index": 5,
      "text": "innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 6,
      "text": "wa bil ijābati ḥaqqun jadīr"
    }
  ]
}