{
  "index": 102,
  "name": "Sura 102",
  "ayas": [
    {
      "index": 1,
      "text": "alhākumut takāthur",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ḥattā zurtumul maqābir"
    },
    {
      "index": 3,
      "text": "kallā sawfa ta`lamūn"
    },
    {
      "index": 4,
      "text": "thumma kallā sawfa ta`lamūn"
    },
    {
      "index": 5,
      "text": "kallā law ta`lamūna `il-mal yaqīn"
    },
    {
      "index": 6,
      "text": "latarawunnal jaḥīm"
    },
    {
      "index": 7,
      "text": "thumma latarawunnahā `aynal yaqīn"
    },
    {
      "index": 8,
      "text": "thumma latus-alunna yawma-idhin `anin na`īm"
    }
  ]
}