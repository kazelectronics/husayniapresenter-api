{
  "index": 356,
  "name": "Sura 356",
  "ayas": [
    {
      "index": 1,
      "text": "subḥānallāh",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "walḥamdu lillāh"
