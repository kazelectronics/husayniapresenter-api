{
  "index": 146,
  "name": "Sura 146",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alayka yabna khātamin nabiyyīn",
      "bismillah": "as salāmu `alayka yabna rasūlillāh"
    },
    {
      "index": 2,
      "text": "as salāmu `alayka yabna sayyidil mur-salīn"
    },
    {
      "index": 3,
      "text": "as salāmu `alayka yabna sayyidil waṣiyyīn"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka yā abā `abdillāh"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā ḥusaynabna `aliyy"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yabna fāṭimata sayyidati nisā-il `ālamīn"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā waliyyallāhi wabna waliyyih"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yā ṣafiyyallāhi wabna ṣafiyyih"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka yā ḥujjatallāhi wabna ḥujjatih"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā ḥabīballāhi wabna ḥabībih"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka yā safīrallāhi wabna safīrih"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka yā khāzinal kitābil masṭūr"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka yā wārithat tawrāti wal-injīli waz zabūr"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka yā amīnar raḥmān"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka yā sharīkal qur-ān"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka yā `amūdad dīn"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka yā bāba ḥikmati rabbil `ālamīn"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka yā bāba ḥiṭṭatinil ladhī man dakhalahu kāna minal-āminīn"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka yā `aybata `ilmillāh"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka yā mawḍi`a sirrillāh"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka yā thārallāhi wabna thārihi walwitral mawtūr"
    },
    {
      "index": 22,
      "text": "as salāmu `alayka wa`alal ar-wāḥil latī ḥallat bifinā-ika wa-anākhat biraḥlik"
    },
    {
      "index": 23,
      "text": "bi-abī anta wa-ummī wanafsī yā abā `abdillāh"
    },
    {
      "index": 24,
      "text": "laqad `aẓumatil muṣībatu wajallatir raziyyatu bika `alaynā wa`alā jamī`i ahlil islām"
    },
    {
      "index": 25,
      "text": "fala`anallāhu ummatan assasat asāsaẓ ẓulmi waljawri `alaykum ahlal bayt"
    },
    {
      "index": 26,
      "text": "wala`anallāhu ummatan dafa`atkum `an maqāmikum"
    },
    {
      "index": 27,
      "text": "wa-azālatkum `an marātibikumul latī rattabakumullāhu fīhā"
    },
    {
      "index": 28,
      "text": "bi-abī anta wa-ummī wanafsī yā abā `abdillāh"
    },
    {
      "index": 29,
      "text": "ash-hadu laqadiqsha`arrat lidimā-ikum aẓillatul `ar-shi ma`a aẓillatil khalā-iq"
    },
    {
      "index": 30,
      "text": "wabakatkumus samā-u wal-ar-ḍu wasukkānul jināni walbarri walbaḥr"
    },
    {
      "index": 31,
      "text": "ṣallallāhu `alayka `adada mā fī `ilmillāh"
    },
    {
      "index": 32,
      "text": "labbayka dā`iyallāh"
    },
    {
      "index": 33,
      "text": "in kāna lam yujibka badanī `indastighāthatika walisānī `indastinṣārik"
    },
    {
      "index": 34,
      "text": "faqad ajābaka qalbī wasam`ī wabaṣarī"
    },
    {
      "index": 35,
      "text": "subḥāna rabbinā in kāna wa`du rabbinā lamaf`ūlā"
    },
    {
      "index": 36,
      "text": "ash-hadu annaka ṭuhrun ṭāhirun muṭahharun min ṭuhrin ṭāhirin muṭahhar"
    },
    {
      "index": 37,
      "text": "ṭahur-ta waṭahurat bikal bilād"
    },
    {
      "index": 38,
      "text": "waṭahurat ar-ḍun anta bihā waṭahura ḥaramuk"
    },
    {
      "index": 39,
      "text": "ash-hadu annaka qad amar-ta bil qisṭi wal`adli wada`awta ilayhimā"
    },
    {
      "index": 40,
      "text": "wa-annaka ṣādiqun ṣiddīqun ṣadaqta fīmā da`awta ilayh"
    },
    {
      "index": 41,
      "text": "wa-annaka thārullāhi fil ar-ḍ"
    },
    {
      "index": 42,
      "text": "wa-ash-hadu annaka qad ballaghta `anillāh"
    },
    {
      "index": 43,
      "text": "wa`an jaddika rasūlillāh"
    },
    {
      "index": 44,
      "text": "wa`an abīka amīril mu-minīn"
    },
    {
      "index": 45,
      "text": "wa`an akhīkal ḥasan"
    },
    {
      "index": 46,
      "text": "wanaṣaḥta wajāhadta fī sabīlillāh"
    },
    {
      "index": 47,
      "text": "wa`abadtahu mukhliṣan ḥattā atākal yaqīn"
    },
    {
      "index": 48,
      "text": "fajazākallāhu khayra jazā-is sābiqīn"
    },
    {
      "index": 49,
      "text": "waṣallallāhu `alayka wasallama taslīmā"
    },
    {
      "index": 50,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammadin waṣalli `alal ḥusaynil maẓlūm"
    },
    {
      "index": 51,
      "text": "ash shahīdir rashīd"
    },
    {
      "index": 52,
      "text": "qatīlil `abarāt"
    },
    {
      "index": 53,
      "text": "wa-asīril kurubāt"
    },
    {
      "index": 54,
      "text": "ṣalātan nāmiyatan zākiyatan mubārakah"
    },
    {
      "index": 55,
      "text": "yaṣ`adu awwaluhā walā yanfadu-ākhiruhā"
    },
    {
      "index": 56,
      "text": "afḍala mā ṣallayta `alā aḥadin min awlādi anbiyā-ikal mur-salīn"
    },
    {
      "index": 57,
      "text": "yā ilāhal `ālamīn"
    }
  ]
}