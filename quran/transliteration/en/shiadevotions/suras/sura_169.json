{
  "index": 169,
  "name": "Sura 169",
  "ayas": [
    {
      "index": 1,
      "text": "wa āmana bika fu-ādī",
      "bismillah": "sajada laka sawādī wakhayālī"
    },
    {
      "index": 2,
      "text": "hādhihi yadāya wamā janaytuhu `alā nafsī"
    },
    {
      "index": 3,
      "text": "yā `aẓīmu tur-jā likulli `aẓīm"
    },
    {
      "index": 4,
      "text": "ighfir liyal `aẓīm"
    },
    {
      "index": 5,
      "text": "fa-innahu lā yaghfirudh dhanbal `aẓīma illar rabbul `aẓīm"
    }
  ]
}