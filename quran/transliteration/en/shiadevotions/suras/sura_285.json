{
  "index": 285,
  "name": "Sura 285",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "u`īdhu nafsy wadīnī"
    },
    {
      "index": 3,
      "text": "wa ahlī wamālī"
    },
    {
      "index": 4,
      "text": "wawaladī wa-ikhwānī fī dīnī"
    },
    {
      "index": 5,
      "text": "wamā razaqanī rabbī wakhawātīma `amalī"
    },
    {
      "index": 6,
      "text": "waman ya`nīnī amruh"
    },
    {
      "index": 7,
      "text": "billāhil wāḥid"
    },
    {
      "index": 8,
      "text": "al aḥadiṣ ṣamad"
    },
    {
      "index": 9,
      "text": "al ladhī lam yalid walam yūlad"
    },
    {
      "index": 10,
      "text": "walam yakun lahu kufuwan aḥad"
    },
    {
      "index": 11,
      "text": "wabirabbil falaq"
    },
    {
      "index": 12,
      "text": "min sharri mā khalaq"
    },
    {
      "index": 13,
      "text": "wamin sharri ghāsiqin idhā waqab"
    },
    {
      "index": 14,
      "text": "wamin sharrin naffāthāti fil `uqad"
    },
    {
      "index": 15,
      "text": "wamin sharri ḥāsidin idhā ḥasad"
    },
    {
      "index": 16,
      "text": "wabirabbin nās"
    },
    {
      "index": 17,
      "text": "malikin nās"
    },
    {
      "index": 18,
      "text": "ilāhin nās"
    },
    {
      "index": 19,
      "text": "min sharril waswāsil khannās"
    },
    {
      "index": 20,
      "text": "al ladhī yuwaswisu fī ṣudūrin nās"
    },
    {
      "index": 21,
      "text": "minal jinnati wan nās"
    }
  ]
}