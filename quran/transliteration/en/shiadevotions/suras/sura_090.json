{
  "index": 90,
  "name": "Sura 90",
  "ayas": [
    {
      "index": 1,
      "text": "lā uqsimu bihādhal balad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wa-anta ḥillun bihādhal balad"
    },
    {
      "index": 3,
      "text": "wawālidin wa mā walad"
    },
    {
      "index": 4,
      "text": "laqad khalaqnal-insāna fī kabad"
    },
    {
      "index": 5,
      "text": "ayaḥsabu an lan yaqdira `alayhi aḥad"
    },
    {
      "index": 6,
      "text": "yaqūlu ahlaktu mālal lubadā"
    },
    {
      "index": 7,
      "text": "ayaḥsabu an lam yarahu aḥad"
    },
    {
      "index": 8,
      "text": "alam naj`al lahu `aynayn"
    },
    {
      "index": 9,
      "text": "walisānan washafatayn"
    },
    {
      "index": 10,
      "text": "wahadaynāhun najdayn"
    },
    {
      "index": 11,
      "text": "falaq-taḥamal `aqabah"
    },
    {
      "index": 12,
      "text": "wa mā adrāka mal `aqabah"
    },
    {
      "index": 13,
      "text": "fakku raqabah"
    },
    {
      "index": 14,
      "text": "aw iṭ`āmun fī yawmin dhī masghabah"
    },
    {
      "index": 15,
      "text": "yatīman dhā maqrabah"
    },
    {
      "index": 16,
      "text": "aw miskīnan dhā matrabah"
    },
    {
      "index": 17,
      "text": "thumma kāna minal ladhīna āmanū watawāṣaw biṣ ṣabri watawāṣaw bil marḥamah"
    },
    {
      "index": 18,
      "text": "ulā-ika aṣ-ḥābul maymanah"
    },
    {
      "index": 19,
      "text": "wal ladhīna kafarū bi-āyātinā hum aṣ-ḥābul mash-amah"
    },
    {
      "index": 20,
      "text": "`alayhim nārun mu-ṣadah"
    }
  ]
}