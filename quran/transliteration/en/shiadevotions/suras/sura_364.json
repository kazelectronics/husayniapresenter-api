{
  "index": 364,
  "name": "Sura 364",
  "ayas": [
    {
      "index": 1,
      "text": "wa ṣafiyyih",
      "bismillah": "as salāmu `alayka yā waliyyallāh"
    },
    {
      "index": 2,
      "text": "as salāmu `alayka yā amīnallāh"
    },
    {
      "index": 3,
      "text": "wa ḥabībah"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka yā ḥujjatallāh"
    },
    {
      "index": 5,
      "text": "wa khalīfatah"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā nāṣira dīnillāh"
    },
    {
      "index": 7,
      "text": "wa khazīna `ilmih"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yabna rasūlillāh"
    },
    {
      "index": 9,
      "text": "wabna waṣiyyih "
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā imāmal mu-minīn"
    },
    {
      "index": 11,
      "text": "wa wārithil mur-salīn"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka yā zaynal `ābidīn"
    },
    {
      "index": 13,
      "text": "wa sulālatal waṣiyyīn"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka yā sayyidas sājidīn"
    },
    {
      "index": 15,
      "text": "wa khayrad dā`īn"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka yā abā muḥammad"
    },
    {
      "index": 17,
      "text": "`aliyyabnal ḥusayn"
    },
    {
      "index": 18,
      "text": "wa raḥmatullāhi wabarakātuh"
    }
  ]
}