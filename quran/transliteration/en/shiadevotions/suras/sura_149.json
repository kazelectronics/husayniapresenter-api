    {
      "index": 214,
      "text": "war-ḥam muḥammadan wa āla muḥammad"
    },
    {
      "index": 215,
      "text": "wabārik `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 216,
      "text": "kamā ṣallayta wabārakta waraḥimta `alā ibrāhīma wa āli ibrāhīm"
    },
    {
      "index": 217,
      "text": "innaka ḥamīdun majīd"
    },
    {
      "index": 218,
      "text": "war-ḥam dhullī wafāqatī"
    },
    {
      "index": 219,
      "text": "wafaqrī wanfirādī wawaḥdatī"
    },
    {
      "index": 220,
      "text": "wakhuḍū`ī bayna yadayk"
    },
    {
      "index": 221,
      "text": "wa`timādī `alayk"
    },
    {
      "index": 222,
      "text": "wataḍarru`ī ilayk"
    },
    {
      "index": 223,
      "text": "ad`ūka du`ā-al khāḍi`idh dhalīl"
    },
    {
      "index": 224,
      "text": "al khāshi`il khā-if"
    },
    {
      "index": 225,
      "text": "al mushfiqil bā-is"
    },
    {
      "index": 226,
      "text": "al mahīnil ḥaqīr"
    },
    {
      "index": 227,
      "text": "al jā-i`il faqīr"
    },
    {
      "index": 228,
      "text": "al `ā-idhil mustajīr"
    },
    {
      "index": 229,
      "text": "al muqirri bidhanbih"
    },
    {
      "index": 230,
      "text": "al mustaghfiri minh"
    },
    {
      "index": 231,
      "text": "al mustakīni lirabbih"
    },
    {
      "index": 232,
      "text": "du`ā-a man aslamat-hu thiqatuh"
    },
    {
      "index": 233,
      "text": "warafaḍat-hu aḥibbatuh"
    },
    {
      "index": 234,
      "text": "wa`aẓumat fajī`atuh"
    },
    {
      "index": 235,
      "text": "du`ā-a ḥariqin ḥazīn"
    },
    {
      "index": 236,
      "text": "ḍa`īfin mahīn"
    },
    {
      "index": 237,
      "text": "bā-isin mustakīn"
    },
    {
      "index": 238,
      "text": "bika mustajīr"
    },
    {
      "index": 239,
      "text": "allāhumma wa-as-aluk bi-annaka malīk"
    },
    {
      "index": 240,
      "text": "wa-annaka mā tashā-u min amrin yakūn"
    },
    {
      "index": 241,
      "text": "wa-annaka `alā mā tashā-u qadīr"
    },
    {
      "index": 242,
      "text": "wa-as-aluk biḥur-mati hādhash shahril ḥarām"
    },
    {
      "index": 243,
      "text": "walbaytil ḥarām"
    },
    {
      "index": 244,
      "text": "walbaladil ḥarām"
    },
    {
      "index": 245,
      "text": "war rukni walmaqām"
    },
    {
      "index": 246,
      "text": "walmashā`iril `iẓām"
    },
    {
      "index": 247,
      "text": "wabiḥaqqi nabiyyika muḥammadin `alayhi wa ālihis salām"
    },
    {
      "index": 248,
      "text": "yā man wahaba li-ādama shaythā"
    },
    {
      "index": 249,
      "text": "wali-ibrāhīma ismā`īla wa-is-ḥāq"
    },
    {
      "index": 250,
      "text": "wayā man radda yūsufa `alā ya`qūb"
    },
    {
      "index": 251,
      "text": "wayā man kashafa ba`dal balā-i ḍurra ayyūb"
    },
    {
      "index": 252,
      "text": "yā rādda mūsā `alā ummih"
    },
    {
      "index": 253,
      "text": "wazā-idal khiḍri fī `ilmih"
    },
    {
      "index": 254,
      "text": "wayā man wahaba lidāwuda sulaymān"
    },
    {
      "index": 255,
      "text": "walizakariyyā yaḥyā"
    },
    {
      "index": 256,
      "text": "walimar-yama `īsā"
    },
    {
      "index": 257,
      "text": "yā ḥāfiẓa binti shu`ayb"
    },
    {
      "index": 258,
      "text": "wayā kāfila waladi ummi mūsā"
    },
    {
      "index": 259,
      "text": "as-aluk-an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 260,
      "text": "wa-an taghfira lī dhunūbī kullahā"
    },
    {
      "index": 261,
      "text": "watujīranī min `adhābik"
    },
    {
      "index": 262,
      "text": "watūjiba lī riḍwānaka wa-amānak"
    },
    {
      "index": 263,
      "text": "wa-iḥsānaka waghufrānaka wajinānak"
    },
    {
      "index": 264,
      "text": "wa-as-aluk-an tafukka `annī kulla ḥalqah"
    },
    {
      "index": 265,
      "text": "baynī wabayna man yu-dhīnī"
    },
    {
      "index": 266,
      "text": "wataftaḥa lī kulla bāb"
    },
    {
      "index": 267,
      "text": "watulayyina lī kulla ṣa`b"
    },
    {
      "index": 268,
      "text": "watusahhila lī kulla `asīr"
    },
    {
      "index": 269,
      "text": "watukhrisa `annī kulla nāṭiqin bisharr"
    },
    {
      "index": 270,
      "text": "watakuffa `annī kulla bāgh"
    },
    {
      "index": 271,
      "text": "watakbita `annī kulla `aduwwin lī waḥāsid"
    },
    {
      "index": 272,
      "text": "watamna`a minnī kulla ẓālim"
    },
    {
      "index": 273,
      "text": "watakfiyanī kulla `ā-iqin yaḥūlu baynī wabayna ḥājatī"
    },
    {
      "index": 274,
      "text": "wayuḥāwilu an yufarriqa baynī wabayna ṭā`atik"
    },
    {
      "index": 275,
      "text": "wayuthabbiṭanī `an `ibādatik"
    },
    {
      "index": 276,
      "text": "yā man aljamal jinnal mutamarridīn"
    },
    {
      "index": 277,
      "text": "waqahara `utātash shayāṭīn"
    },
    {
      "index": 278,
      "text": "wa-adhalla riqābal mutajabbirīn"
    },
    {
      "index": 279,
      "text": "waradda kaydal mutasalliṭīna `anil mustaḍ`afīn"
    },
    {
      "index": 280,
      "text": "as-aluk biqudratika `alā mā tashā"
    },
    {
      "index": 281,
      "text": "watas-hīlika limā tashā-u kayfa tashā"
    },
    {
      "index": 282,
      "text": "an taj`ala qaḍā-a ḥājatī fīmā tashā"
    },
    {
      "index": 283,
      "text": ""
    },
    {
      "index": 284,
      "text": "allāhumma laka sajadt"
    },
    {
      "index": 285,
      "text": "wabika-āmant"
    },
    {
      "index": 286,
      "text": "far-ḥam dhullī wafāqatī"
    },
    {
      "index": 287,
      "text": "wajtihādī wataḍarru`ī"
    },
    {
      "index": 288,
      "text": "wamaskanatī wafaqrī ilayka yā rabb"
    },
    {
      "index": 289,
      "text": ""
    }
  ]
}