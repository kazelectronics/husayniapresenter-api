{
  "index": 339,
  "name": "Sura 339",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī atawajjahu ilayka binabiyyika nabiyyir raḥmati wa-ālih"
    },
    {
      "index": 3,
      "text": "wa uqaddimuhum bayna yaday ḥawa-ijī"
    },
    {
      "index": 4,
      "text": "faj`alnī bihim wajīhan fid dunyā wal-ākhirah"
    },
    {
      "index": 5,
      "text": "wa minal muqarrabīn"
    },
    {
      "index": 6,
      "text": "allāhummar-ḥamnī bihim wa lā tu`adhhibnī bihim"
    },
    {
      "index": 7,
      "text": "wahdinī bihim wa lā tuḍillanī bihim"
    },
    {
      "index": 8,
      "text": "war-zuqnī bihim wa lā taḥrimnī bihim"
    },
    {
      "index": 9,
      "text": "waqḍi lī ḥawā-ijad dunyā wal-ākhirah"
    },
    {
      "index": 10,
      "text": "innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 11,
      "text": "wa bikulli shay-in `alīm"
    }
  ]
}