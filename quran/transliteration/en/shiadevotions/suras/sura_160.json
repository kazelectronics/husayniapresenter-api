{
  "index": 160,
  "name": "Sura 160",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma anta muta`ālal makān"
    },
    {
      "index": 3,
      "text": "`aẓīmul jabarūt"
    },
    {
      "index": 4,
      "text": "shadīdul miḥāl"
    },
    {
      "index": 5,
      "text": "ghaniyyun `anil khalā-iq"
    },
    {
      "index": 6,
      "text": "`arīḍul kibriyā"
    },
    {
      "index": 7,
      "text": "qādirun `alā mā tashā"
    },
    {
      "index": 8,
      "text": "qarībur raḥmah"
    },
    {
      "index": 9,
      "text": "ṣādiqul wa`d"
    },
    {
      "index": 10,
      "text": "sābighun ni`mah"
    },
    {
      "index": 11,
      "text": "ḥasanul balā"
    },
    {
      "index": 12,
      "text": "qarībun idhā du`īt"
    },
    {
      "index": 13,
      "text": "muḥīṭun bimā khalaqt"
    },
    {
      "index": 14,
      "text": "qābilut tawbati liman tāba ilayk"
    },
    {
      "index": 15,
      "text": "qādirun `alā mā aradt"
    },
    {
      "index": 16,
      "text": "wamudrikun mā ṭalabt"
    },
    {
      "index": 17,
      "text": "washakūrun idhā shukir-t"
    },
    {
      "index": 18,
      "text": "wadhakūrun idhā dhukir-t"
    },
    {
      "index": 19,
      "text": "ad`ūka muḥtājā"
    },
    {
      "index": 20,
      "text": "wa-ar-ghabu ilayka faqīrā"
    },
    {
      "index": 21,
      "text": "wa-afza`u ilayka khā-ifā"
    },
    {
      "index": 22,
      "text": "wa-abkī ilayka makrūbā"
    },
    {
      "index": 23,
      "text": "wa-asta`īnu bika ḍa`īfā"
    },
    {
      "index": 24,
      "text": "wa-atawakkalu `alayka kāfiyā"
    },
    {
      "index": 25,
      "text": "aḥkum baynanā wabayna qawminā bil ḥaqq"
    },
    {
      "index": 26,
      "text": "fa-innahum gharrūnā wakhada`ūnā"
    },
    {
      "index": 27,
      "text": "wakhadhalūnā waghadarū binā waqatalūnā"
    },
    {
      "index": 28,
      "text": "wanaḥnu `itratu nabiyyik"
    },
    {
      "index": 29,
      "text": "wawaladu ḥabībik"
    },
    {
      "index": 30,
      "text": "muḥammadibni `abdillāh"
    },
    {
      "index": 31,
      "text": "al ladhiṣṭafaytahu birrisālah"
    },
    {
      "index": 32,
      "text": "wa-tamantahu `alā waḥyik"
    },
    {
      "index": 33,
      "text": "faj`al lanā min amrinā farajan wamakhrajā"
    },
    {
      "index": 34,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    }
  ]
}