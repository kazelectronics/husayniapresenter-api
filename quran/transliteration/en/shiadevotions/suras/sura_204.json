{
  "index": 204,
  "name": "Sura 204",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "walḥamdu lillāhil ladhī manna `alaynā"
    },
    {
      "index": 3,
      "text": "bimuḥammadin nabiyyihi ṣallallāhu `alayhi wa-ālih"
    },
    {
      "index": 4,
      "text": "dūnal umamil māḍiyati walqurūnis sālifah"
    },
    {
      "index": 5,
      "text": "biqudratihil latī lā ta`jizu `an shay-in wa in `aẓum"
    },
    {
      "index": 6,
      "text": "wa lā yafūtuhā shay-un wa in laṭuf"
    },
    {
      "index": 7,
      "text": "fakhatama binā `alā jamī`i man dhara-a"
    },
    {
      "index": 8,
      "text": "wa ja`alanā shuhadā-a `alā man jaḥad"
    },
    {
      "index": 9,
      "text": "wakathharanā bimannihi `alā man qall"
    },
    {
      "index": 10,
      "text": "allāhumma faṣalli `alā muḥammadin amīnika `alā waḥyik"
    },
    {
      "index": 11,
      "text": "wanajībika min khalqik"
    },
    {
      "index": 12,
      "text": "wa ṣafiyyika min `ibādik"
    },
    {
      "index": 13,
      "text": "imāmir raḥmati waqā-idil khayr"
    },
    {
      "index": 14,
      "text": "wa miftāḥil barakah"
    },
    {
      "index": 15,
      "text": "kamā naṣaba li-amrika nafsah"
    },
    {
      "index": 16,
      "text": "wa `arraḍa fīka lilmakrūhi badanah"
    },
    {
      "index": 17,
      "text": "wakāshafa fid du`ā-i ilayka ḥāmmatah"
    },
    {
      "index": 18,
      "text": "waḥāraba fī riḍāka usratah"
    },
    {
      "index": 19,
      "text": "waqaṭa`a fī iḥyā-i dīnika raḥimah"
    },
    {
      "index": 20,
      "text": "wa aqṣal adnayna `alā juḥūdihim"
    },
    {
      "index": 21,
      "text": "wa qarrabal aqṣayna `alastijābatihim lak"
    },
    {
      "index": 22,
      "text": "wa wālā fīkal ab`adīn"
    },
    {
      "index": 23,
      "text": "wa `ādā fīkal aqrabīn"
    },
    {
      "index": 24,
      "text": "wa ad-aba nafsahu fī tablīghi risālatik"
    },
    {
      "index": 25,
      "text": "wa-at`abahā bid du`ā-i ilā millatik"
    },
    {
      "index": 26,
      "text": "wa shaghalahā binnuṣ-ḥi li-ahli da`watik"
    },
    {
      "index": 27,
      "text": "wahājara ilā bilādil ghur-bah"
    },
    {
      "index": 28,
      "text": "wamaḥallin na-yi `an mawṭini raḥlih"
    },
    {
      "index": 29,
      "text": "wamawḍi`i rijlihi wa masqaṭi ra-sih"
    },
    {
      "index": 30,
      "text": "wama-nasi nafsihi irādatan minhu li-i`zāzi dīnik"
    },
    {
      "index": 31,
      "text": "wastinṣāran `alā ahlil kufri bik"
    },
    {
      "index": 32,
      "text": "ḥattastatabba lahu mā ḥāwala fī a`dā-ik"
    },
    {
      "index": 33,
      "text": "wastatamma lahu mā dabbara fī awliyā-ik"
    },
    {
      "index": 34,
      "text": "fanahada ilayhim mustaftiḥan bi`awnik"
    },
    {
      "index": 35,
      "text": "wamutaqawwiyan `alā ḍa`fihi binaṣrik"
    },
    {
      "index": 36,
      "text": "faghazāhum fī `uqri diyārihim"
    },
    {
      "index": 37,
      "text": "wahajama `alayhim fī buḥbūḥati qarārihim"
    },
    {
      "index": 38,
      "text": "ḥattā ẓahara amruk"
    },
    {
      "index": 39,
      "text": "wa`alat kalimatuka walaw karihal mushrikūn"
    },
    {
      "index": 40,
      "text": "allāhumma far-fa`hu bimā kadaḥa fīk"
    },
    {
      "index": 41,
      "text": "ilad darajatil `ulyā min jannatik"
    },
    {
      "index": 42,
      "text": "ḥattā lā yusāwā fī manzilah"
    },
    {
      "index": 43,
      "text": "walā yukāfa-a fī mar-tabah"
    },
    {
      "index": 44,
      "text": "walā yuwāziyahu ladayka malakun muqarrabun walā nabiyyun mur-sal"
    },
    {
      "index": 45,
      "text": "wa`arrifhu fī ahlihiṭ ṭāhirīn"
    },
    {
      "index": 46,
      "text": "wa-ummatihil mu-minīna min ḥusnish shafā`ah"
    },
    {
      "index": 47,
      "text": "ajalla mā wa`adtahu yā nāfidhal `idah"
    },
    {
      "index": 48,
      "text": "yā wāfiyal qawli yā mubaddilas sayyi-āti bi-aḍ`āfihā minal ḥasanāt"
    },
    {
      "index": 49,
      "text": "innaka dhul faḍlil `aẓīm"
    }
  ]
}