{
  "index": 347,
  "name": "Sura 347",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "sayyidī sayyidī"
    },
    {
      "index": 3,
      "text": "hādhihi yadāya qad madadtuhumā ilayk"
    },
    {
      "index": 4,
      "text": "bidhhunūbi mamlū-ah"
    },
    {
      "index": 5,
      "text": "wa `aynāya birrajā-i mamdūdah"
    },
    {
      "index": 6,
      "text": "waḥaqqun biman da`āka binnadami tadhallulā"
    },
    {
      "index": 7,
      "text": "an tujībahu bil karami tafaḍḍulā"
    },
    {
      "index": 8,
      "text": "sayyidī amin ahlish shaqā-i khalaqtanī"
    },
    {
      "index": 9,
      "text": "fa-aṭīlu bukā-ī"
    },
    {
      "index": 10,
      "text": "am min ahlis sa`ādati khalaqtanī"
    },
    {
      "index": 11,
      "text": "fa-ubashhiru rajā-ī"
    },
    {
      "index": 12,
      "text": "sayyidī law anna `abdan istaṭā`al harab"
    },
    {
      "index": 13,
      "text": "min mawlāh"
    },
    {
      "index": 14,
      "text": "lakuntu awwalal hāribīna mink"
    },
    {
      "index": 15,
      "text": "lakinnī a`lamu annī lā afūtuk"
    },
    {
      "index": 16,
      "text": "ilāhī wa sayyidī"
    },
    {
      "index": 17,
      "text": "ir-ḥamnī maṣrū`an `alal firāsh"
    },
    {
      "index": 18,
      "text": "tuqallibunī aydī aḥibbatī"
    },
    {
      "index": 19,
      "text": "war-ḥamnī maṭrūḥan `alal mughtasal"
    },
    {
      "index": 20,
      "text": "yughassilunī ṣāliḥu jīratī"
    },
    {
      "index": 21,
      "text": "war-ḥamnī maḥmūlā"
    },
    {
      "index": 22,
      "text": "qad tanāwalal aqribā-u aṭrāfa janāzatī"
    },
    {
      "index": 23,
      "text": "war-ḥam fī dhālikal baytil muẓlim"
    },
    {
      "index": 24,
      "text": "waḥshatī wa ghur-batī wa waḥdatī"
    }
  ]
}