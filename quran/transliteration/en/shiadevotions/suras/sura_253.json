{
  "index": 253,
  "name": "Sura 253",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "yā fārijal hammi wakāshifal ghamm"
    },
    {
      "index": 3,
      "text": "yā raḥmānad dunyā wal-ākhirati wa raḥīmahumā"
    },
    {
      "index": 4,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 5,
      "text": "wafruj hammī, wakshif ghammī"
    },
    {
      "index": 6,
      "text": "yā wāḥidu yā aḥadu yā ṣamad"
    },
    {
      "index": 7,
      "text": "yā man lam yalid walam yūlad"
    },
    {
      "index": 8,
      "text": "walam yakun lahu kufuwan aḥad"
    },
    {
      "index": 9,
      "text": "i`ṣimnī waṭahhir-nī, wadh-hab bibaliyyatī"
    },
    {
      "index": 10,
      "text": ""
    },
    {
      "index": 11,
      "text": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 12,
      "text": "allāhu lā ilāha illā huwal ḥayyul qayyūm"
    },
    {
      "index": 13,
      "text": "lā ta-khudhuhū sinatun walā nawm"
    },
    {
      "index": 14,
      "text": "lahu mā fis samāwāti wamā fil ar-ḍ"
    },
    {
      "index": 15,
      "text": "man dhal ladhī yashfa`u `indahū illā bi-idhnih"
    },
    {
      "index": 16,
      "text": "ya`lamu mā bayna aydīhim wamā khalfahum"
    },
    {
      "index": 17,
      "text": "walā yuḥīṭūna bishay-im min `ilmihī illā bimā shā-a"
    },
    {
      "index": 18,
      "text": "wasi`a kur-siyyuhus samāwāti wal-ar-ḍ"
    },
    {
      "index": 19,
      "text": "walā ya-ūduhu ḥifẓuhumā"
    },
    {
      "index": 20,
      "text": "wahuwal `aliyyul `aẓīm"
    },
    {
      "index": 21,
      "text": ""
    },
    {
      "index": 22,
      "text": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 23,
      "text": "qul a`ūdhu birabbin nās"
    },
    {
      "index": 24,
      "text": "malikin nās"
    },
    {
      "index": 25,
      "text": "ilāhin nās"
    },
    {
      "index": 26,
      "text": "min sharril waswāsil khannās"
    },
    {
      "index": 27,
      "text": "al ladhī yuwaswisu fī ṣudūrin nās"
    },
    {
      "index": 28,
      "text": "minal jinnati wan nās"
    },
    {
      "index": 29,
      "text": ""
    },
    {
      "index": 30,
      "text": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 31,
      "text": "qul a`ūdhu birabbil falaq"
    },
    {
      "index": 32,
      "text": "min sharri mā khalaq"
    },
    {
      "index": 33,
      "text": "wamin sharri ghāsiqin idhā waqab"
    },
    {
      "index": 34,
      "text": "wamin sharrin naffāthāti fil `uqad"
    },
    {
      "index": 35,
      "text": "wamin sharri ḥāsidin idhā ḥasad"
    },
    {
      "index": 36,
      "text": ""
    },
    {
      "index": 37,
      "text": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 38,
      "text": "qul huwallāhu aḥad"
    },
    {
      "index": 39,
      "text": "allāhuṣ ṣamad"
    },
    {
      "index": 40,
      "text": "lam yalid walam yūlad"
    },
    {
      "index": 41,
      "text": "walam yakun lahu kufuwan aḥad"
    },
    {
      "index": 42,
      "text": ""
    },
    {
      "index": 43,
      "text": "allāhumma innī as-aluka su-āla manishtaddat fāqatuh"
    },
    {
      "index": 44,
      "text": "waḍa`ufat quwwatuh, wakathurat dhunūbuh"
    },
    {
      "index": 45,
      "text": "su-āla man lā yajidu lifāqatihi mughīthā"
    },
    {
      "index": 46,
      "text": "walā liḍa`fihi muqawwiyā"
    },
    {
      "index": 47,
      "text": "walā lidhanbihi ghāfiran ghayrak"
    },
    {
      "index": 48,
      "text": "yā dhal jalāli wal-ikrām"
    },
    {
      "index": 49,
      "text": "as-aluka `amalan tuḥibbu bihi man `amila bih"
    },
    {
      "index": 50,
      "text": "wayaqīnan tanfa`u bihi manistayqana bih"
    },
    {
      "index": 51,
      "text": "ḥaqqal yaqīni fī nafādhi amrik"
    },
    {
      "index": 52,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 53,
      "text": "waqbiḍ `alaṣ ṣidqi nafsī"
    },
    {
      "index": 54,
      "text": "waqṭa` minad dunyā ḥājatī"
    },
    {
      "index": 55,
      "text": "waj`al fīmā `indaka raghbatī"
    },
    {
      "index": 56,
      "text": "shawqan ilā liqā-ik"
    },
    {
      "index": 57,
      "text": "wahab lī ṣidqat tawakkuli `alayk"
    },
    {
      "index": 58,
      "text": "as-aluka min khayri kitābin qad khalā wa-a`ūdhu bik"
    },
    {
      "index": 59,
      "text": "min sharri kitābin qad khalā"
    },
    {
      "index": 60,
      "text": "as-aluka khawfal `ābidīna lak"
    },
    {
      "index": 61,
      "text": "wa`ibādatal khāshi`īna lak"
    },
    {
      "index": 62,
      "text": "wayaqīnal mutawakkilīna `alayk"
    },
    {
      "index": 63,
      "text": "watawakkulal mu-minīna `alayk"
    },
    {
      "index": 64,
      "text": "allāhummaj`al raghbatī fī mas-alatī"
    },
    {
      "index": 65,
      "text": "mithla raghbati awliyā-ika fī masā-ilihim"
    },
    {
      "index": 66,
      "text": "warahbatī mithla rahbati awliyā-ik"
    },
    {
      "index": 67,
      "text": "wasta`milnī fī mar-ḍātik"
    },
    {
      "index": 68,
      "text": "`amalan lā atruku ma`ahu shay-an min dīnik"
    },
    {
      "index": 69,
      "text": "makhāfata aḥadin min khalqik"
    },
    {
      "index": 70,
      "text": "allāhumma hadhihi ḥājatī fa-a`ẓim fīhā raghbatī"
    },
    {
      "index": 71,
      "text": "wa-aẓhir fīhā `udhrī, walaqqinnī fīhā ḥujjatī, wa`āfi fīhā jasadī"
    },
    {
      "index": 72,
      "text": "allāhumma man aṣbaḥa lahu thiqatun aw rajā-un ghayruk"
    },
    {
      "index": 73,
      "text": "faqad aṣbaḥtu wa-anta thiqatī"
    },
    {
      "index": 74,
      "text": "warajā-ī fil umūri kullihā"
    },
    {
      "index": 75,
      "text": "faqḍi lī bikhayrihā `āqibah"
    },
    {
      "index": 76,
      "text": "wanajjinī min muḍillātil fitan"
    },
    {
      "index": 77,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 78,
      "text": "waṣallallāhu `alā sayyidinā muḥammadin rasūlillāhil muṣṭafā"
    },
    {
      "index": 79,
      "text": "wa`alā-ālihiṭ ṭāhirīn"
    }
  ]
}