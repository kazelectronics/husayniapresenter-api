{
  "index": 131,
  "name": "Sura 131",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "yā allāhu yā allāhu yā allāh"
    },
    {
      "index": 3,
      "text": "yā mujība da`watil muḍṭarrīn"
    },
    {
      "index": 4,
      "text": "yā kāshifa kurabil makrūbīn"
    },
    {
      "index": 5,
      "text": "yā ghiyāthal mustaghīthīn"
    },
    {
      "index": 6,
      "text": "yā ṣarīkhal mustaṣrikhīn"
    },
    {
      "index": 7,
      "text": "wayā man huwa aqrabu ilayya min ḥablil warīd"
    },
    {
      "index": 8,
      "text": "wayā man yaḥūlu baynal mar-i waqalbih"
    },
    {
      "index": 9,
      "text": "wayā man huwa bil manẓaril a`lā wabil ufuqil mubīn"
    },
    {
      "index": 10,
      "text": "wayā man huwar raḥmānur raḥīmu `alal `ar-shistawā"
    },
    {
      "index": 11,
      "text": "wayā man ya`lamu khā-inatal a`yuni wamā tukhfiṣ ṣudūr"
    },
    {
      "index": 12,
      "text": "wayā man lā yakhfā `alayhi khāfiyah"
    },
    {
      "index": 13,
      "text": "yā man lātashtabihu `alayhil aṣwāt"
    },
    {
      "index": 14,
      "text": "wayā man lātughalliṭuhul ḥājāt"
    },
    {
      "index": 15,
      "text": "wayā man lā yubrimuhu ilḥāḥul muliḥḥīn"
    },
    {
      "index": 16,
      "text": "yā mudrika kulli fawt"
    },
    {
      "index": 17,
      "text": "wayā jāmi`a kulli shaml"
    },
    {
      "index": 18,
      "text": "wayā bāri-an nufūsi ba`dal mawt"
    },
    {
      "index": 19,
      "text": "yā man huwa kulla yawmin fī sha-n"
    },
    {
      "index": 20,
      "text": "yā qāḍiyal ḥājāt"
    },
    {
      "index": 21,
      "text": "yā munaffisal kurubāt"
    },
    {
      "index": 22,
      "text": "yā mu`ṭiyas su-ulāt"
    },
    {
      "index": 23,
      "text": "yā waliyyar raghabāt"
    },
    {
      "index": 24,
      "text": "yā kāfiyal muhimmāt"
    },
    {
      "index": 25,
      "text": "yā man yakfī min kulli shay"
    },
    {
      "index": 26,
      "text": "walā yakfī minhu shay-un fis samāwāti wal-ar-ḍ"
    },
    {
      "index": 27,
      "text": "as-aluka biḥaqqi muḥammadin khātimin nabiyyīn"
    },
    {
      "index": 28,
      "text": "wa`aliyyin amīril mu-minīn"
    },
    {
      "index": 29,
      "text": "wabiḥaqqi fāṭimata binti nabiyyik"
    },
    {
      "index": 30,
      "text": "wabiḥaqqil ḥasani walḥusayn"
    },
    {
      "index": 31,
      "text": "fa-innī bihim atawajjahu ilayka fī maqāmī hādhā"
    },
    {
      "index": 32,
      "text": "wabihim atawassal"
    },
    {
      "index": 33,
      "text": "wabihim atashaffa`u ilayk"
    },
    {
      "index": 34,
      "text": "wabiḥaqqihim as-aluka wa-uqsimu wa-a`zimu `alayk"
    },
    {
      "index": 35,
      "text": "wabishha-nil ladhī lahum `indak"
