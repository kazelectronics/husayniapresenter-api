{
  "index": 80,
  "name": "Sura 80",
  "ayas": [
    {
      "index": 1,
      "text": "`abasa watawallā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "an jā-ahul-a`mā"
    },
    {
      "index": 3,
      "text": "wa mā yudrīka la`allahu yazzakkā"
    },
    {
      "index": 4,
      "text": "aw yadhhakkaru fatanfa`ahudh dhikrā"
    },
    {
      "index": 5,
      "text": "ammā manis-taghnā"
    },
    {
      "index": 6,
      "text": "fa-anta lahu taṣaddā"
    },
    {
      "index": 7,
      "text": "wa mā `alayka allā yazzakkā"
    },
    {
      "index": 8,
      "text": "wa-ammā man jā-aka yas`ā"
    },
    {
      "index": 9,
      "text": "wahuwa yakhshā"
    },
    {
      "index": 10,
      "text": "fa-anta `anhu talahhā"
    },
    {
      "index": 11,
      "text": "kallā innahā tadhkirah"
    },
    {
      "index": 12,
      "text": "faman shā-a dhakarah"
    },
    {
      "index": 13,
      "text": "fī ṣuḥufin mukarramah"
    },
    {
      "index": 14,
      "text": "marfū`atim muṭahharah"
    },
    {
      "index": 15,
      "text": "bi-aydī safarah"
    },
    {
      "index": 16,
      "text": "kirāmin bararah"
    },
    {
      "index": 17,
      "text": "qutilal-insānu mā akfarah"
    },
    {
      "index": 18,
      "text": "min ayyi shay-in khalaqah"
    },
    {
      "index": 19,
      "text": "min nuṭfatin khalaqahu faqaddarah"
    },
    {
      "index": 20,
      "text": "thummas sabīla yassarah"
    },
    {
      "index": 21,
      "text": "thumma amātahu fa-aqbarah"
    },
    {
      "index": 22,
      "text": "thumma idhā shā-a ansharah"
    },
    {
      "index": 23,
      "text": "kallā lammā yaqḍi mā amarah"
    },
    {
      "index": 24,
      "text": "falyanẓuril-insānu ilā ṭa`āmih"
    },
    {
      "index": 25,
      "text": "annā ṣababnal mā-a ṣabbā"
    },
    {
      "index": 26,
      "text": "thumma shaqaqnal-arḍa shaqqā"
    },
    {
      "index": 27,
      "text": "fa-anbatnā fīhā ḥabbā"
    },
    {
      "index": 28,
      "text": "wa`inaban waqaḍbā"
    },
    {
      "index": 29,
      "text": "wazaytūnan wanakhlā"
    },
    {
      "index": 30,
      "text": "waḥadā-iqa ghulbā"
    },
    {
      "index": 31,
      "text": "wafākihatan wa-abbā"
    },
    {
      "index": 32,
      "text": "matā`al lakum wali-an`āmikum"
    },
    {
      "index": 33,
      "text": "fa-idhā jā-atiṣ ṣākhhah"
    },
    {
      "index": 34,
      "text": "yawma yafirrul mar-u min akhīh"
    },
    {
      "index": 35,
      "text": "wa-ummihi wa-abīh"
    },
    {
      "index": 36,
      "text": "waṣāḥibatihi wabanīh"
    },
    {
      "index": 37,
      "text": "likullim-ri-in minhum yawma-idhin sha-nun yughnīh"
    },
    {
      "index": 38,
      "text": "wujūhun yawma-idhin musfirah"
    },
    {
      "index": 39,
      "text": "ḍāḥikatun mustabshirah"
    },
    {
      "index": 40,
      "text": "wawujūhun yawma-idhin `alayhā ghabarah"
    },
    {
      "index": 41,
      "text": "tarhaquhā qatarah"
    },
    {
      "index": 42,
      "text": "ulā-ika humul kafaratul fajarah"
    }
  ]
}