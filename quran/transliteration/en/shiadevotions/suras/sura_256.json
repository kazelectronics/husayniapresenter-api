{
  "index": 256,
  "name": "Sura 256",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma yāman"
    },
    {
      "index": 3,
      "text": "khaṣṣa muḥammadan wa ālahu bil karāmah"
    },
    {
      "index": 4,
      "text": "wa ḥabāhum birrisālah"
    },
    {
      "index": 5,
      "text": "wa khaṣṣaṣ-hum bil wasīlah"
    },
    {
      "index": 6,
      "text": "wa ja`alahum warathatal anbiyā"
    },
    {
      "index": 7,
      "text": "wa khatama bihimul awṣiyā-a wal a-immah"
    },
    {
      "index": 8,
      "text": "wa `allamahum `ilma mā kāna wa `ilma mā baqiy"
    },
    {
      "index": 9,
      "text": "wa ja`ala af-idatan minan nāsi tahwī ilayhim"
    },
    {
      "index": 10,
      "text": "faṣalli `alā muḥammadin wa ālihiṭ ṭāhirīn"
    },
    {
      "index": 11,
      "text": "waf`al binā mā anta ahluhu fid dīni wad dunyā wal-ākhirah"
    },
    {
      "index": 12,
      "text": "innaka `alā kulli shay-in qadīr"
    }
  ]
}