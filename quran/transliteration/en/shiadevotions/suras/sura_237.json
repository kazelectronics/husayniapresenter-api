{
  "index": 237,
  "name": "Sura 237",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma inna hadhayni-āyatāni min-āyātik"
    },
    {
      "index": 3,
      "text": "wa hadhayni `awnāni min a`wānik"
    },
    {
      "index": 4,
      "text": "yabtadirāni ṭā`ataka biraḥmatin nāfi`atin aw naqimatin ḍārrah"
    },
    {
      "index": 5,
      "text": "falā tumṭir-nā bihimā maṭaras saw"
    },
    {
      "index": 6,
      "text": "wa lā tulbisnā bihimā libāsal balā"
    },
    {
      "index": 7,
      "text": "ṣalli `alā muḥammadin wa-ālih"
    },
    {
      "index": 8,
      "text": "wa anzil `alaynā naf`a hadhihis saḥā-ibi wa barakatahā"
    },
    {
      "index": 9,
      "text": "waṣrif `annā adhāhā wa maḍarratahā"
    },
    {
      "index": 10,
      "text": "wa lā tuṣibnā fīhā bi-āfah"
    },
    {
      "index": 11,
      "text": "wa lā tur-sil `alā ma`āyishinā `āhah"
    },
    {
      "index": 12,
      "text": "allāhumma wa in kunta ba`athtahā niqmatan wa ar-saltahā sakhṭah"
    },
    {
      "index": 13,
      "text": "fa-innā nastajīruka min ghaḍabik"
    },
    {
      "index": 14,
      "text": "wa nabtahilu ilayka fī su-āli `afwik"
    },
    {
      "index": 15,
      "text": "famil bil ghaḍabi ilal mushrikīn"
    },
    {
      "index": 16,
      "text": "wa adir raḥā naqimatika `alal mulḥidīn"
    },
    {
      "index": 17,
      "text": "allāhumma adh-hib maḥla bilādinā bisuqyāk"
    },
    {
      "index": 18,
      "text": "wa akhrij waḥara ṣudūrinā birizqik"
    },
    {
      "index": 19,
      "text": "wa lā tashghalnā `anka bighayrik"
    },
    {
      "index": 20,
      "text": "wa lā taqṭa` `an kāffatinā māddata birrik"
    },
    {
      "index": 21,
      "text": "fa-innal ghaniyya man aghnayt"
    },
    {
      "index": 22,
      "text": "wa innas sālima man waqayt"
    },
    {
      "index": 23,
      "text": "mā `inda aḥadin dūnaka difā`"
    },
    {
      "index": 24,
      "text": "wa lā bi-aḥadin `an saṭwatikamtinā`"
    },
    {
      "index": 25,
      "text": "taḥkumu bimā shi-ta `alā man shi-t"
    },
    {
      "index": 26,
      "text": "wa taqḍī bimā aradta fīman aradt"
    },
    {
      "index": 27,
      "text": "falakal ḥamdu `alā mā waqaytanā minal balā"
    },
    {
      "index": 28,
      "text": "wa lakash shukru `alā mā khawwaltanā minan na`mā"
    },
    {
      "index": 29,
      "text": "ḥamdan yukhallifu ḥamdal ḥāmidīna warā-ah"
    },
    {
      "index": 30,
      "text": "ḥamdan yamla-u ar-ḍahu wa samā-ah"
    },
    {
      "index": 31,
      "text": "innakal mannānu bijasīmil minan"
    },
    {
      "index": 32,
      "text": "al wahhābu li`aẓīmin ni`am"
    },
    {
      "index": 33,
      "text": "al qābilu yasīral ḥamd"
    },
    {
      "index": 34,
      "text": "ash shākiru qalīlash shukr"
    },
    {
      "index": 35,
      "text": "al muḥsinul mujmilu dhuṭ ṭawl"
    },
    {
      "index": 36,
      "text": "lā ilaha illā anta ilaykal maṣīr"
    }
  ]
}