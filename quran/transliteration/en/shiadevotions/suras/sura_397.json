{
  "index": 397,
  "name": "Sura 397",
  "ayas": [
    {
      "index": 1,
      "text": "wa`ayshī qārrā",
      "bismillah": "allāhummaj`al qalbī bārrā"
    },
    {
      "index": 2,
      "text": "warizqī dārrā"
    },
    {
      "index": 3,
      "text": "waj`al lī `inda qabri rasūlik"
    },
    {
      "index": 4,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 5,
      "text": "mustaqarran waqarārā"
    }
  ]
}