{
  "index": 44,
  "name": "Sura 44",
  "ayas": [
    {
      "index": 1,
      "text": "ḥā mīm",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wal kitābil mubīn"
    },
    {
      "index": 3,
      "text": "innā anzalnāhu fī laylatim mubārakah ۚ innā kunnā mundhirīn"
    },
    {
      "index": 4,
      "text": "fīhā yufraqu kullu amrin ḥakīm"
    },
    {
      "index": 5,
      "text": "amran min `indinā ۚ innā kunnā mursilīn"
    },
    {
      "index": 6,
      "text": "raḥmatan min rabbik ۚ innahu huwas samī`ul `alīm"
    },
    {
      "index": 7,
      "text": "rabbis samāwāti wal-arḍi wa mā baynahumā ۖ in kuntum mūqinīn"
    },
    {
      "index": 8,
      "text": "lā ilāha illā huwa yuḥyī wayumīt ۖ rabbukum warabbu ābā-ikumul-awwalīn"
    },
    {
      "index": 9,
      "text": "bal hum fī shakkiy yal`abūn"
    },
    {
      "index": 10,
      "text": "fartaqib yawma ta-tis samā-u bidukhānin mubīn"
    },
    {
      "index": 11,
      "text": "yaghshan nās ۖ hādhā `adhābun alīm"
    },
    {
      "index": 12,
      "text": "rabbanak-shif `annal `adhāba innā mu-minūn"
    },
    {
      "index": 13,
      "text": "annā lahumudh dhikrā waqad jā-ahum rasūlun mubīn"
    },
    {
      "index": 14,
      "text": "thumma tawallaw `anhu waqālū mu`allamun majnūn"
    },
    {
      "index": 15,
      "text": "innā kāshiful `adhābi qalīlā ۚ innakum `ā-idūn"
    },
    {
      "index": 16,
      "text": "yawma nabṭishul baṭshatal kubrā innā muntaqimūn"
    },
    {
      "index": 17,
      "text": "walaqad fatannā qablahum qawma fir`awna wajā-ahum rasūlun karīm"
    },
    {
      "index": 18,
      "text": "an addū ilayya `ibādallāh ۖ innī lakum rasūlun amīn"
    },
    {
      "index": 19,
      "text": "wa-an lā ta`lū `alallāh ۖ innī ātīkum bisulṭānin mubīn"
    },
    {
      "index": 20,
      "text": "wa innī `udhtu birabbī warabbikum an tarjumūn"
    },
    {
      "index": 21,
      "text": "wa in lam tu-minū lī fa`tazilūn"
    },
    {
      "index": 22,
      "text": "fada`ā rabbahu anna hā-ulā-i qawmun mujrimūn"
    },
    {
      "index": 23,
      "text": "fa-asri bi`ibādī laylan innakum muttaba`ūn"
    },
    {
      "index": 24,
      "text": "wātrukil baḥra rahwā ۖ innahum jundun mughraqūn"
    },
    {
      "index": 25,
      "text": "kam tarakū min jannātin wa`uyūn"
    },
    {
      "index": 26,
      "text": "wazurū`in wa maqāmin karīm"
    },
    {
      "index": 27,
      "text": "wana`matin kānū fīhā fākihīn"
    },
    {
      "index": 28,
      "text": "kadhālik ۖ wa-awrathnāhā qawman ākharīn"
    },
    {
      "index": 29,
      "text": "famā bakat `alayhimus samā-u wal-arḍu wa mā kānū munẓarīn"
    },
    {
      "index": 30,
      "text": "walaqad najjaynā banī isrā-iyla minal `adhābil muhīn"
    },
    {
      "index": 31,
      "text": "min fir`awn ۚ innahu kāna `āliyan minal musrifīn"
    },
    {
      "index": 32,
      "text": "walaqadikh-tarnāhum `alā `il-min `alal-`ālamīn"
    },
    {
      "index": 33,
      "text": "wa-ātaynāhum minal-āyāti mā fīhi balā-un mubīn"
    },
    {
      "index": 34,
      "text": "inna hā-ulā-i layaqūlūn"
    },
    {
      "index": 35,
      "text": "in hiya illā mawtatunal-ūlā wa mā naḥnu bimunsharīn"
    },
    {
      "index": 36,
      "text": "fa-tū bi-ābā-inā in kuntum ṣādiqīn"
    },
    {
      "index": 37,
      "text": "ahum khayrun am qawmu tubba`in wal ladhīna min qablihim ۚ ahlaknāhum ۖ innahum kānū mujrimīn"
    },
    {
      "index": 38,
      "text": "wa mā khalaqnas samāwāti wal-arḍa wa mā baynahumā lā`ibīn"
    },
    {
      "index": 39,
      "text": "mā khalaqnāhumā illā bil ḥaqqi walākinna aktharahum lā ya`lamūn"
    },
    {
      "index": 40,
      "text": "inna yawmal faṣli mīqātuhum ajma`īn"
    },
    {
      "index": 41,
      "text": "yawma lā yughnī mawlan `an mawlan shay-an walā hum yunṣarūn"
    },
    {
      "index": 42,
      "text": "illā man raḥimallāh ۚ innahu huwal `azīzur raḥīm"
    },
    {
      "index": 43,
      "text": "inna shajarataz zaqqūm"
    },
    {
      "index": 44,
      "text": "ṭa`āmul-athīm"
    },
    {
      "index": 45,
      "text": "kal muhli yaghlī fil buṭūn"
    },
    {
      "index": 46,
      "text": "kaghalyil ḥamīm"
    },
    {
      "index": 47,
      "text": "khudhūhu fa`tilūhu ilā sawā-il jaḥīm"
    },
    {
      "index": 48,
      "text": "thumma ṣubbū fawqa ra-sihi min `adhābil ḥamīm"
    },
    {
      "index": 49,
      "text": "dhuq innaka antal `azīzul karīm"
    },
    {
      "index": 50,
      "text": "inna hādhā mā kuntum bihi tamtarūn"
    },
    {
      "index": 51,
      "text": "innal muttaqīna fī maqāmin amīn"
    },
    {
      "index": 52,
      "text": "fī jannātin wa`uyūn"
    },
    {
      "index": 53,
      "text": "yalbasūna min sundusin wa istabraqin mutaqābilīn"
    },
    {
      "index": 54,
      "text": "kadhālika wazawwajnāhum biḥūrin `īn"
    },
    {
      "index": 55,
      "text": "yad`ūna fīhā bikulli fākihatin āminīn"
    },
    {
      "index": 56,
      "text": "lā yadhūqūna fīhal mawta illal-mawtatal-ūlā ۖ wawaqāhum `adhābal jaḥīm"
    },
    {
      "index": 57,
      "text": "faḍlan min rabbik ۚ dhālika huwal fawzul `aẓīm"
    },
    {
      "index": 58,
      "text": "fa-innamā yassarnāhu bilisānika la`allahum yatadhakkarūn"
    },
    {
      "index": 59,
      "text": "fartaqib innahum murtaqibūn"
    }
  ]
}