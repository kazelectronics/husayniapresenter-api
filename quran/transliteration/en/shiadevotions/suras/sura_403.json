{
  "index": 403,
  "name": "Sura 403",
  "ayas": [
    {
      "index": 1,
      "text": "wabika-āmantu wa`alayka tawakkalt",
      "bismillah": "allāhumma laka sajadtu walaka aslamt"
    },
    {
      "index": 2,
      "text": "wa-anta rabbī"
    },
    {
      "index": 3,
      "text": "sajada wajhī lilladhī khalaqah"
    },
    {
      "index": 4,
      "text": "washaqqa sam`ahu wabaṣarah"
    },
    {
      "index": 5,
      "text": "al ḥamdu lillāhi rabbil `ālamīn"
    },
    {
      "index": 6,
      "text": "tabārakallāhu aḥsanal khāliqīn"
    }
  ]
}