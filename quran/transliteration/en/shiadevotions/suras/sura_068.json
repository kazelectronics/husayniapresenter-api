{
  "index": 68,
  "name": "Sura 68",
  "ayas": [
    {
      "index": 1,
      "text": "nūn ۚ wal qalami wa mā yasṭurūn",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "mā anta bini`mati rabbika bimajnūn"
    },
    {
      "index": 3,
      "text": "wa inna laka la-ajran ghayra mamnūn"
    },
    {
      "index": 4,
      "text": "wa innaka la`alā khuluqin `aẓīm"
    },
    {
      "index": 5,
      "text": "fasatubṣiru wayubṣirūn"
    },
    {
      "index": 6,
      "text": "bi-ayyikumul maftūn"
    },
    {
      "index": 7,
      "text": "inna rabbaka huwa a`lamu biman ḍalla `an sabīlihi wahuwa a`lamu bil muhtadīn"
    },
    {
      "index": 8,
      "text": "falā tuṭi`il mukadhhibīn"
    },
    {
      "index": 9,
      "text": "waddū law tud-hinu fayud-hinūn"
    },
    {
      "index": 10,
      "text": "walā tuṭi` kulla ḥallāfin mahīn"
    },
    {
      "index": 11,
      "text": "hammāzin mashhā-in binamīm"
    },
    {
      "index": 12,
      "text": "mannā`in lilkhayri mu`tadin athīm"
    },
    {
      "index": 13,
      "text": "`utullin ba`da dhālika zanīm"
    },
    {
      "index": 14,
      "text": "an kāna dhā mālin wabanīn"
    },
    {
      "index": 15,
      "text": "idhā tutlā `alayhi āyātunā qāla asāṭīrul-awwalīn"
    },
    {
      "index": 16,
      "text": "sanasimuhu `alal-khurṭūm"
    },
    {
      "index": 17,
      "text": "innā balawnāhum kamā balawnā aṣ-ḥābal jannati idh aqsamū layaṣrimunnahā muṣbiḥīn"
    },
    {
      "index": 18,
      "text": "walā yastathnūn"
    },
    {
      "index": 19,
      "text": "faṭāfa `alayhā ṭā-ifun min rabbika wahum nā-imūn"
    },
    {
      "index": 20,
      "text": "fa-aṣbaḥat kaṣ ṣarīm"
    },
    {
      "index": 21,
      "text": "fatanādaw muṣbiḥīn"
    },
    {
      "index": 22,
      "text": "anigh-dū `alā ḥarthikum in kuntum ṣārimīn"
    },
    {
      "index": 23,
      "text": "fanṭalaqū wahum yatakhāfatūn"
    },
    {
      "index": 24,
      "text": "an lā yadkhulannahal yawma `alaykum miskīn"
    },
    {
      "index": 25,
      "text": "waghadaw `alā ḥardin qādirīn"
    },
    {
      "index": 26,
      "text": "falammā ra-awhā qālū innā laḍāllūn"
    },
    {
      "index": 27,
      "text": "bal naḥnu maḥrūmūn"
    },
    {
      "index": 28,
      "text": "qāla awsaṭuhum alam aqul lakum lawlā tusabbiḥūn"
    },
    {
      "index": 29,
      "text": "qālū subḥāna rabbinā innā kunnā ẓālimīn"
    },
    {
      "index": 30,
      "text": "fa-aqbala ba`ḍuhum `alā ba`ḍiy yatalāwa mūn"
    },
    {
      "index": 31,
      "text": "qālū yāwaylanā innā kunnā ṭāghīn"
    },
    {
      "index": 32,
      "text": "`asā rabbunā an yubdilanā khayran minhā innā ilā rabbinā rāghibūn"
    },
    {
      "index": 33,
      "text": "kadhālikal `adhāb ۖ wala`adhābul-ākhirati akbar ۚ law kānū ya`lamūn"
    },
    {
      "index": 34,
      "text": "inna lil-muttaqīna `inda rabbihim jannātin na`īm"
    },
    {
      "index": 35,
      "text": "afanaj`alul muslimīna kal mujrimīn"
    },
    {
      "index": 36,
      "text": "mā lakum kayfa taḥkumūn"
    },
    {
      "index": 37,
      "text": "am lakum kitābun fīhi tadrusūn"
    },
    {
      "index": 38,
      "text": "inna lakum fīhi lamā takhayyarūn"
    },
    {
      "index": 39,
      "text": "am lakum aymānun `alaynā bālighatun ilā yawmil qiyāmati inna lakum lamā taḥkumūn"
    },
    {
      "index": 40,
      "text": "salhum ayyuhum bidhālika za`īm"
    },
    {
      "index": 41,
      "text": "am lahum shurakā-u falya-tū bishurakā-ihim in kānū ṣādiqīn"
    },
    {
      "index": 42,
      "text": "yawma yukshafu `an sāqin wayud`awna ilas sujūdi falā yastaṭī`ūn"
    },
    {
      "index": 43,
      "text": "khāshi`atan abṣāruhum tarhaquhum dhillah ۖ waqad kānū yud`awna ilas sujūdi wahum sālimūn"
    },
    {
      "index": 44,
      "text": "fadharnī wa man yukadhhibu bihādhal ḥadīth ۖ sanastadrijuhum min ḥaythu lā ya`lamūn"
    },
    {
      "index": 45,
      "text": "wa-umlī lahum ۚ inna kaydī matīn"
    },
    {
      "index": 46,
      "text": "am tas-aluhum ajran fahum min maghramin muthqalūn"
    },
    {
      "index": 47,
      "text": "am `indahumul ghaybu fahum yaktubūn"
    },
    {
      "index": 48,
      "text": "faṣbir liḥukmi rabbika walā takun kaṣāḥibil ḥūti idh nādā wahuwa makẓūm"
    },
    {
      "index": 49,
      "text": "lawlā an tadārakahu ni`matun min rabbihi lanubidha bil `arā-i wahuwa madhmūm"
    },
    {
      "index": 50,
      "text": "fajtabāhu rabbuhu faja`alahu minaṣ ṣāliḥīn"
    },
    {
      "index": 51,
      "text": "wa in yakādul ladhīna kafarū layuzliqūnaka bi-abṣārihim lammā sami`udh dhikra wayaqūlūna innahu lamajnūn"
    },
    {
      "index": 52,
      "text": "wa mā huwa illā dhikrul lil`ālamīn"
    }
  ]
}