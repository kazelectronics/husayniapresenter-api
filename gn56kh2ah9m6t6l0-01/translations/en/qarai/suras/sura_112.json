{
  "index": 112,
  "ayas": [
    {
      "index": 1,
      "text": "Say, ‘He is Allah, the One."
    },
    {
      "index": 2,
      "text": "Allah is the All-embracing."
    },
    {
      "index": 3,
      "text": "He neither begat, nor was begotten,"
    },
    {
      "index": 4,
      "text": "nor has He any equal.’"
    }
  ]
}