{
  "index": 112,
  "ayas": [
    {
      "index": 1,
      "text": "Say: He, Allah, is One."
    },
    {
      "index": 2,
      "text": "Allah is He on Whom all depend."
    },
    {
      "index": 3,
      "text": "He begets not, nor is He begotten."
    },
    {
      "index": 4,
      "text": "And none is like Him."
    }
  ]
}