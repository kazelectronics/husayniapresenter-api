{
  "index": 108,
  "name": "الكوثر",
  "arabic_name": "الكَوْثَر",
  "english_name": "Al-Kawthar",
  "english_meaning": "Abundance",
  "ayas": [
    {
      "index": 1,
      "text": "إِنَّآ أَعْطَيْنَـٰكَ ٱلْكَوْثَرَ",
      "bismillah": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ"
    },
    {
      "index": 2,
      "text": "فَصَلِّ لِرَبِّكَ وَٱنْحَرْ"
    },
    {
      "index": 3,
      "text": "إِنَّ شَانِئَكَ هُوَ ٱلْأَبْتَرُ"
    }
  ]
}