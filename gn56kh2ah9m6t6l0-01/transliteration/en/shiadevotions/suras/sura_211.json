{
  "index": 211,
  "name": "Sura 211",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma ṣalli `alā muḥammadin wa-ālih"
    },
    {
      "index": 3,
      "text": "waṣayyir-nā ilā maḥbūbika minat tawbah"
    },
    {
      "index": 4,
      "text": "wa-azilnā `an makrūhika minal iṣrār"
    },
    {
      "index": 5,
      "text": "allāhumma wa matā waqafnā bayna naqṣayni fī dīn"
    },
    {
      "index": 6,
      "text": "aw dunyā fa-awqi`in naqṣa bi-asra`ihimā fanā-ā"
    },
    {
      "index": 7,
      "text": "waj`alit tawbata fī aṭwalihimā baqā-ā"
    },
    {
      "index": 8,
      "text": "wa-idhā hamamnā bihammayni yur-ḍīka aḥaduhumā `annā"
    },
    {
      "index": 9,
      "text": "wayuskhiṭukal-ākharu `alaynā"
    },
    {
      "index": 10,
      "text": "famil binā ilā mā yur-ḍīka `annā"
    },
    {
      "index": 11,
      "text": "wa-awhin quwwatanā `ammā yuskhiṭuka `alaynā"
    },
    {
      "index": 12,
      "text": "walā tukhalli fī dhālika bayna nufūsinā wakhtiyārihā"
    },
    {
      "index": 13,
      "text": "fa-innahā mukhtāratun lilbāṭili illā mā waffaqta ammāratun bissū-i illā mā raḥimt"
    },
    {
      "index": 14,
      "text": "allāhumma wa-innaka minaḍ ḍu`fi khalaqtanā"
    },
    {
      "index": 15,
      "text": "wa`alal wahni banaytanā, wamin mā-in mahīninbtada-tanā"
    },
    {
      "index": 16,
      "text": "falā ḥawla lanā illā biquwwatika walā quwwata lanā illā bi`awnik"
    },
    {
      "index": 17,
      "text": "fa-ayyidnā bitawfīqika wasaddidnā bitasdīdik"
    },
    {
      "index": 18,
      "text": "wa-a`mi abṣāra qulūbinā `ammā khālafa maḥabbatak"
    },
    {
      "index": 19,
      "text": "walā taj`al lishay-in min jawāriḥinā nufūdhan fī ma`ṣiyatik"
    },
    {
      "index": 20,
      "text": "allāhumma faṣalli `alā muḥammadin wa-ālih"
    },
    {
      "index": 21,
      "text": "waj`al hamasāti qulūbinā waḥarakāti a`ḍā-inā"
    },
    {
      "index": 22,
      "text": "walamaḥāti a`yuninā, walahajāti alsinatinā fī mūjibāti thawābik"
    },
    {
      "index": 23,
      "text": "ḥattā lā tafūtanā ḥasanatun nastaḥiqqu bihā jazā-ak"
    },
    {
      "index": 24,
      "text": "walā tabqā lanā sayyi-atun nastawjibu bihā `iqābak"
    }
  ]
}