{
  "index": 346,
  "name": "Sura 346",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "lā ilāha illallāhul ḥalīmul karīm"
    },
    {
      "index": 3,
      "text": "lā ilāha illallāhul `aliyyul `aẓīm"
    },
    {
      "index": 4,
      "text": "subḥānallāhi rabbis samāwātis sab`"
    },
    {
      "index": 5,
      "text": "wa rabbil ar-ḍīnas sab`"
    },
    {
      "index": 6,
      "text": "wa mā fīhinna wa mā baynahunn"
    },
    {
      "index": 7,
      "text": "wa mā fawqahunna wa mā taḥtahunn"
    },
    {
      "index": 8,
      "text": "wa rabbil `ar-shil `aẓīm"
    },
    {
      "index": 9,
      "text": "allāhumma antallāhu nūrus samāwāti wal ar-ḍ"
    },
    {
      "index": 10,
      "text": "wa antallāhu zaynus samāwāti wal ar-ḍ"
    },
    {
      "index": 11,
      "text": "wa antallāhu jamālus samāwāti wal ar-ḍ"
    },
    {
      "index": 12,
      "text": "wa antallāhu `imādus samāwāti wal ar-ḍ"
    },
    {
      "index": 13,
      "text": "wa antallāhu qiwāmus samāwāti wal ar-ḍ"
    },
    {
      "index": 14,
      "text": "wa antallāhu ṣarīkhul mustaṣrikhīn"
    },
    {
      "index": 15,
      "text": "wa antullāhu ghiyāthul mustaghīthīn"
    },
    {
      "index": 16,
      "text": "wa antallāhul mufarriju `anil makrūbīn"
    },
    {
      "index": 17,
      "text": "wa antallāhul murawwiḥu `anil maghmūmīn"
    },
    {
      "index": 18,
      "text": "wa antallāhu mujību da`watil muḍṭarrīn"
    },
    {
      "index": 19,
      "text": "wa antallāhu ilahul `ālamīn"
    },
    {
      "index": 20,
      "text": "wa antallāhur raḥmānur raḥīm"
    }
  ]
}