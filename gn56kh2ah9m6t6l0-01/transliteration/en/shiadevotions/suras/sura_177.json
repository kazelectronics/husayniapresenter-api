{
  "index": 177,
  "name": "Sura 177",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma hādhihil ayyāmul latī faḍḍaltahā `alal ayyāmi washarraftahā"
    },
    {
      "index": 3,
      "text": "wa qad ballaghtanīhā bimannika waraḥmatik"
    },
    {
      "index": 4,
      "text": "fa-anzil `alaynā min barakātik"
    },
    {
      "index": 5,
      "text": "wa-awsi` `alaynā fīhā min na`mā-ik"
    },
    {
      "index": 6,
      "text": "allāhumma innī as-aluka an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 7,
      "text": "wa-an tahdiyanā fīhā lisabīlil hudā"
    },
    {
      "index": 8,
      "text": "wal`afāfi walghinā"
    },
    {
      "index": 9,
      "text": "wal`amali fīhā bimā tuḥibbu watar-ḍā"
    },
    {
      "index": 10,
      "text": "allāhumma innī as-aluka yā mawḍi`a kulli shakwā"
    },
    {
      "index": 11,
      "text": "wayā sāmi`a kulli najwā"
    },
    {
      "index": 12,
      "text": "wayā shāhida kulli malā"
    },
    {
      "index": 13,
      "text": "wayā `ālima kulli khafiyyah"
    },
    {
      "index": 14,
      "text": "an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 15,
      "text": "wa-an takshifa `annā fīhal balā"
    },
    {
      "index": 16,
      "text": "watastajība lanā fīhad du`ā"
    },
    {
      "index": 17,
      "text": "watuqawwiyanā fīhā watu`īnanā"
    },
    {
      "index": 18,
      "text": "watuwaffiqanā fīhā limā tuḥibbu rabbanā watar-ḍā"
    },
    {
      "index": 19,
      "text": "wa`alā maftaraḍta `alaynā min ṭā`atik"
    },
    {
      "index": 20,
      "text": "waṭā`ati rasūlika wa-ahli wilāyatik"
    },
    {
      "index": 21,
      "text": "allāhumma innī as-aluka yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 22,
      "text": "an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 23,
      "text": "wa-an tahaba lanā fīhar riḍā"
    },
    {
      "index": 24,
      "text": "innaka samī`ud du`ā"
    },
    {
      "index": 25,
      "text": "walā taḥrimnā khayra mā tunzilu fīhā minas samā"
    },
    {
      "index": 26,
      "text": "waṭahhir-nā minadh dhunūb"
    },
    {
      "index": 27,
      "text": "yā `allāmal ghuyūb"
    },
    {
      "index": 28,
      "text": "wa-awjib lanā fīhā dāral khulūd"
    },
    {
      "index": 29,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 30,
      "text": "walā tatruk lanā fīhā dhanban illā ghafar-tah"
    },
    {
      "index": 31,
      "text": "walā hamman illā farrajtah"
    },
    {
      "index": 32,
      "text": "walā daynan illā qaḍaytah"
    },
    {
      "index": 33,
      "text": "walā ghā-iban illā addaytah"
    },
    {
      "index": 34,
      "text": "walā ḥājatan min ḥawā-ijid dunyā wal-ākhirah"
    },
    {
      "index": 35,
      "text": "illā sahhaltahā wayassar-tahā"
    },
    {
      "index": 36,
      "text": "innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 37,
      "text": "allāhumma yā `ālimal khafiyyāt"
    },
    {
      "index": 38,
      "text": "yā rāḥimal `abarāt"
    },
    {
      "index": 39,
      "text": "yā mujībad da`awāt"
    },
    {
      "index": 40,
      "text": "yā rabbal araḍīna was samāwāt"
    },
    {
      "index": 41,
      "text": "yā man lātatashābahu `alayhil aṣwāt"
    },
    {
      "index": 42,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 43,
      "text": "waj`alnā fīhā min `utaqā-ika waṭulaqā-ika minan nār"
    },
    {
      "index": 44,
      "text": "wal fā-izīna bijannatik"
    },
    {
      "index": 45,
      "text": "wan nājīna biraḥmatik"
    },
    {
      "index": 46,
      "text": "yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 47,
      "text": "waṣallallāhu `alā sayyidinā muḥammadin wa ālihi ajma`īn"
    }
  ]
}