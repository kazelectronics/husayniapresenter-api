{
  "index": 155,
  "name": "Sura 155",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al ḥamdu lillāhil ladhī lam yattakhidh waladā"
    },
    {
      "index": 3,
      "text": "walam yakun lahu sharīkun fil mulk"
    },
    {
      "index": 4,
      "text": "walam yakun lahu waliyyun minadh dhull"
    },
    {
      "index": 5,
      "text": "wakabbir-hu takbīrā"
    },
    {
      "index": 6,
      "text": "yā `uddatī fī muddatī"
    },
    {
      "index": 7,
      "text": "yā ṣāḥibī fī shiddatī"
    },
    {
      "index": 8,
      "text": "yā waliyyī fī ni`matī"
    },
    {
      "index": 9,
      "text": "yā ghiyāthī fī raghbatī"
    },
    {
      "index": 10,
      "text": "yā najāḥī fī ḥājatī"
    },
    {
      "index": 11,
      "text": "yā ḥāfiẓī fī ghaybatī"
    },
    {
      "index": 12,
      "text": "yā kāfī fī waḥdatī"
    },
    {
      "index": 13,
      "text": "yā unsī fī waḥshatī"
    },
    {
      "index": 14,
      "text": "antas sātiru `awratī"
    },
    {
      "index": 15,
      "text": "falakal ḥamd"
    },
    {
      "index": 16,
      "text": "wa-antal muqīlu `athratī"
    },
    {
      "index": 17,
      "text": "falakal ḥamd"
    },
    {
      "index": 18,
      "text": "wa-antal mun`ishu ṣar-`atī"
    },
    {
      "index": 19,
      "text": "falakal ḥamd"
    },
    {
      "index": 20,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 21,
      "text": "wastur `awratī"
    },
    {
      "index": 22,
      "text": "wa āmin raw`atī"
    },
    {
      "index": 23,
      "text": "wa-aqilnī `athratī"
    },
    {
      "index": 24,
      "text": "waṣfaḥ `an jur-mī"
    },
    {
      "index": 25,
      "text": "watajāwaz `an sayyi-ātī"
    },
    {
      "index": 26,
      "text": "fī aṣ-ḥābil jannah"
    },
    {
      "index": 27,
      "text": "wa`daṣ ṣidqil ladhī kānū yū`adūn"
    }
  ]
}