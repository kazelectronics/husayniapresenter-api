{
  "index": 112,
  "name": "Sura 112",
  "ayas": [
    {
      "index": 1,
      "text": "qul huwallāhu aḥad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhuṣ ṣamad"
    },
    {
      "index": 3,
      "text": "lam yalid walam yūlad"
    },
    {
      "index": 4,
      "text": "walam yakun lahu kufuwan aḥad"
    }
  ]
}