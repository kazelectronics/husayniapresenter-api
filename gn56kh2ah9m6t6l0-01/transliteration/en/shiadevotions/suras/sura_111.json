{
  "index": 111,
  "name": "Sura 111",
  "ayas": [
    {
      "index": 1,
      "text": "tabbat yadā abī lahabin watabb",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "mā aghnā `anhu māluhu wa mā kasab"
    },
    {
      "index": 3,
      "text": "sayaṣlā nāran dhāta lahab"
    },
    {
      "index": 4,
      "text": "wamra-atuhu ḥammālatal ḥaṭab"
    },
    {
      "index": 5,
      "text": "fī jīdihā ḥablun min masad"
    }
  ]
}