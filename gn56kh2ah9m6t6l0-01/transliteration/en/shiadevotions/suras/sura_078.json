{
  "index": 78,
  "name": "Sura 78",
  "ayas": [
    {
      "index": 1,
      "text": "`amma yatasā-alūn",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "`anin naba-il `aẓīm"
    },
    {
      "index": 3,
      "text": "alladhī hum fīhi mukhtalifūn"
    },
    {
      "index": 4,
      "text": "kallā saya`lamūn"
    },
    {
      "index": 5,
      "text": "thumma kallā saya`lamūn"
    },
    {
      "index": 6,
      "text": "alam naj`alil-arḍa mihādā"
    },
    {
      "index": 7,
      "text": "wal jibāla awtādā"
    },
    {
      "index": 8,
      "text": "wakhalaqnākum azwājā"
    },
    {
      "index": 9,
      "text": "waja`alnā nawmakum subātā"
    },
    {
      "index": 10,
      "text": "waja`alnal layla libāsā"
    },
    {
      "index": 11,
      "text": "waja`alnan nahāra ma`āshā"
    },
    {
      "index": 12,
      "text": "wabanaynā fawqakum sab`an shidādā"
    },
    {
      "index": 13,
      "text": "waja`alnā sirājan wahhājā"
    },
    {
      "index": 14,
      "text": "wa-anzalnā minal mu`ṣirāti mā-an thajjājā"
    },
    {
      "index": 15,
      "text": "linukhrija bihi ḥabban wanabātā"
    },
    {
      "index": 16,
      "text": "wajannātin alfāfā"
    },
    {
      "index": 17,
      "text": "inna yawmal faṣli kāna mīqātā"
    },
    {
      "index": 18,
      "text": "yawma yunfakhu fiṣ ṣūri fata-tūna afwājā"
    },
    {
      "index": 19,
      "text": "wafutiḥatis samā-u fakānat abwābā"
    },
    {
      "index": 20,
      "text": "wasuyyiratil jibālu fakānat sarābā"
    },
    {
      "index": 21,
      "text": "inna jahannama kānat mirṣādā"
    },
    {
      "index": 22,
      "text": "liṭṭāghīna ma-ābā"
    },
    {
      "index": 23,
      "text": "lābithīna fīhā aḥqābā"
    },
    {
      "index": 24,
      "text": "lā yadhūqūna fīhā bardan walā sharābā"
    },
    {
      "index": 25,
      "text": "illā ḥamīman waghassāqā"
    },
    {
      "index": 26,
      "text": "jazā-an wifāqā"
    },
    {
      "index": 27,
      "text": "innahum kānū lā yarjūna ḥisābā"
    },
    {
      "index": 28,
      "text": "wakadhhabū bi-āyātinā kidhhābā"
    },
    {
      "index": 29,
      "text": "wakulla shay-in aḥṣaynāhu kitābā"
    },
    {
      "index": 30,
      "text": "fadhūqū falan nazīdakum illā `adhābā"
    },
    {
      "index": 31,
      "text": "inna lil-muttaqīna mafāzā"
    },
    {
      "index": 32,
      "text": "ḥadā-iqa wa-a`nābā"
    },
    {
      "index": 33,
      "text": "wakawā`iba atrābā"
    },
    {
      "index": 34,
      "text": "waka-san dihāqā"
    },
    {
      "index": 35,
      "text": "lā yasma`ūna fīhā laghwan walā kidhhābā"
    },
    {
      "index": 36,
      "text": "jazā-an min rabbika `aṭā-an ḥisābā"
    },
    {
      "index": 37,
      "text": "rabbis samāwāti wal-arḍi wa mā baynahumar raḥmān ۖ lā yamlikūna minhu khiṭābā"
    },
    {
      "index": 38,
      "text": "yawma yaqūmur rūḥu wal malā-ikatu ṣaffā ۖ lā yatakallamūna illā man adhina lahur raḥmānu waqāla ṣawābā"
    },
    {
      "index": 39,
      "text": "dhālikal yawmul ḥaqq ۖ faman shā-at-takhadha ilā rabbihi ma-ābā"
    },
    {
      "index": 40,
      "text": "innā andharnākum `adhāban qarīban yawma yanẓurul mar-u mā qaddamat yadāhu wayaqūlul kāfiru yālaytanī kuntu turābā"
    }
  ]
}