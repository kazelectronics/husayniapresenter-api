{
  "index": 190,
  "name": "Sura 190",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma hadhā yawmun mubārakun maymūn"
    },
    {
      "index": 3,
      "text": "wal muslimūna fīhi mujtami`ūna fī aqṭāri ar-ḍik"
    },
    {
      "index": 4,
      "text": "yash-hadus sā-ilu minhum waṭ ṭālibu war rāghibu war rāhib"
    },
    {
      "index": 5,
      "text": "wa antan nāẓiru fī ḥawā-ijihim"
    },
    {
      "index": 6,
      "text": "fa-as-aluka bijūdika wa karamika wa hawāni mā sa-altuka `alayk"
    },
    {
      "index": 7,
      "text": "an tuṣalliya `alā muḥammadin wa-ālih"
    },
    {
      "index": 8,
      "text": "wa as-alukallāhumma rabbanā bi-anna lakal mulka wa lakal ḥamd"
    },
    {
      "index": 9,
      "text": "lā ilaha illā ant,al ḥalīmul karīmul ḥannānul mannān"
    },
    {
      "index": 10,
      "text": "dhul jalāli wal ikrām, badī`us samāwāti wal ar-ḍ"
    },
    {
      "index": 11,
      "text": "mahmā qasamta bayna `ibādikal mu-minīn"
    },
    {
      "index": 12,
      "text": "min khayrin aw `āfiyatin aw barakatin aw hudan aw `amalin biṭā`atik"
    },
    {
      "index": 13,
      "text": "aw khayrin tamunnu bihi `alayhim tahdīhim bihi ilayk"
    },
    {
      "index": 14,
      "text": "aw tar-fa`u lahum `indaka darajah"
    },
    {
      "index": 15,
      "text": "aw tu`ṭīhim bihi khayran min khayrid dunyā wal-ākhirah"
    },
    {
      "index": 16,
      "text": "an tuwaffira ḥaẓẓī wa naṣībī minh"
    },
    {
      "index": 17,
      "text": "wa as-alukallāhumma bi-ann"
    },
    {
      "index": 18,
      "text": "lakal mulka wal ḥamd"
    },
    {
      "index": 19,
      "text": "lā ilaha illā ant"
    },
    {
      "index": 20,
      "text": "an tuṣalliya `alā muḥammad"
    },
    {
      "index": 21,
      "text": "`abdika wa rasūlik"
    },
    {
      "index": 22,
      "text": "wa ḥabībika wa ṣifwatik"
    },
    {
      "index": 23,
      "text": "wa khiyaratika min khalqik"
    },
    {
      "index": 24,
      "text": "wa `alā-āli muḥammadinil abrāriṭ ṭāhirīnal akhyār"
    },
    {
      "index": 25,
      "text": "ṣalātan lā yaqwā `alā iḥṣā-ihā illā ant"
    },
    {
      "index": 26,
      "text": "wa an tushrikanā fī"
    },
    {
      "index": 27,
      "text": "ṣāliḥi man da`āka fī hadhal yawm"
    },
    {
      "index": 28,
      "text": "min `ibādikal mu-minīn"
    },
    {
      "index": 29,
      "text": "yā rabbal `ālamīn"
    },
    {
      "index": 30,
      "text": "wa an taghfira lanā wa lahum"
    },
    {
      "index": 31,
      "text": "innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 32,
      "text": "allāhumma ilayka ta`ammadtu biḥājatī"
    },
    {
      "index": 33,
      "text": "wa bika anzaltul yawma faqrī"
    },
    {
      "index": 34,
      "text": "wa fāqatī wa maskanatī"
    },
    {
      "index": 35,
      "text": "wa innī bimaghfiratika wa raḥmatika awthaqu minnī bi`amalī"
    },
    {
      "index": 36,
      "text": "wa lamaghfiratuka wa raḥmatuka awsa`u min dhunūbī"
    },
    {
      "index": 37,
      "text": "faṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 38,
      "text": "wa tawalla qaḍā-a kulli ḥājatin hiya lī"
    },
    {
      "index": 39,
      "text": "biqudratika `alayhā"
    },
    {
      "index": 40,
      "text": "wa taysīri dhālika `alayk"
    },
    {
      "index": 41,
      "text": "wa bifaqrī ilayk"
    },
    {
      "index": 42,
      "text": "wa ghināka `annī"
    },
    {
      "index": 43,
      "text": "fa-innī lam uṣib khayran qaṭṭu illā mink"
    },
    {
      "index": 44,
      "text": "wa lam yaṣrif `annī sū-an qaṭṭu aḥadun ghayruk"
    },
    {
      "index": 45,
      "text": "wa lā ar-jū li-amr"
    },
    {
      "index": 46,
      "text": "ākhiratī wa dunyāya siwāk"
    },
    {
      "index": 47,
      "text": "allāhumma man tahayya-a wa ta`abba-a"
    },
    {
      "index": 48,
      "text": "wa a`adda wasta`add"
    },
    {
      "index": 49,
      "text": "liwifādatin ilā makhlūq"
    },
    {
      "index": 50,
      "text": "rajā-a rifdihi wa nawāfilihi wa ṭalaba naylihi wa jā-izatih"
    },
    {
      "index": 51,
      "text": "fa-ilayka yā mawlāya kānatil yawm"
    },
    {
      "index": 52,
      "text": "tahyi-atī wa ta`bi-atī"
    },
    {
      "index": 53,
      "text": "wa i`dādī wasti`dādī"
    },
    {
      "index": 54,
      "text": "rajā-a `afwika wa rifdik"
    },
    {
      "index": 55,
      "text": "wa ṭalaba naylika wa jā-izatik"
    },
    {
      "index": 56,
      "text": "allāhumma faṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 57,
      "text": "wa lā tukhayyibil yawma dhālika min rajā-ī"
    },
    {
      "index": 58,
      "text": "yā man lā yuḥfīhi sā-il"
    },
    {
      "index": 59,
      "text": "wa lā yanquṣuhu nā-il"
    },
    {
      "index": 60,
      "text": "fa-innī lam-ātika thiqah"
    },
    {
      "index": 61,
      "text": "minnī bi`amalin ṣāliḥin qaddamtuh"
    },
    {
      "index": 62,
      "text": "wa lā shafā`ati makhlūqin rajawtuh"
    },
    {
      "index": 63,
      "text": "illā shafā`ata muḥammadin wa ahli baytih"
    },
    {
      "index": 64,
      "text": "`alayhi wa `alayhim salāmuk"
    },
    {
      "index": 65,
      "text": "ataytuka muqirran bil jur-mi wal isā-ati ilā nafsī"
    },
    {
      "index": 66,
      "text": "ataytuka ar-jū `aẓīma `afwik"
    },
    {
      "index": 67,
      "text": "al ladhī `afawta bihi `anil khāṭi-īn"
    },
    {
      "index": 68,
      "text": "thumma lam yamna`ka ṭūlu `ukūfihim `alā `aẓīmil jur-m"
    },
    {
      "index": 69,
      "text": "an `udta `alayhim birraḥmati wal maghfirah"
    },
    {
      "index": 70,
      "text": "fayā man raḥmatuhu wāsi`ah"
    },
    {
      "index": 71,
      "text": "wa `afwuhu `aẓīm"
    },
    {
      "index": 72,
      "text": "yā `aẓīmu yā `aẓīm"
    },
    {
      "index": 73,
      "text": "yā karīmu yā karīm"
    },
    {
      "index": 74,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 75,
      "text": "wa `ud `alayya biraḥmatik"
    },
    {
      "index": 76,
      "text": "wa ta`aṭṭaf `alayya bifaḍlik"
    },
    {
      "index": 77,
      "text": "wa tawassa` `alayya bimaghfiratik"
    },
    {
      "index": 78,
      "text": "allāhumma inna hadhal maqāma likhulafā-ika wa aṣfiyā-ik"
    },
    {
      "index": 79,
      "text": "wa mawāḍi`a umanā-ik"
    },
    {
      "index": 80,
      "text": "fid darajatir rafī`atil latikhtaṣaṣtahum bihā"
    },
    {
      "index": 81,
      "text": "qadibtazzūhā"
    },
    {
      "index": 82,
      "text": "wa antal muqaddiru lidhalik"
    },
    {
      "index": 83,
      "text": "lā yughālabu amruk"
    },
    {
      "index": 84,
      "text": "wa lā yujāwazul maḥtūmu min tadbīrik"
    },
    {
      "index": 85,
      "text": "kayfa shi-ta wa annā shi-t"
    },
    {
      "index": 86,
      "text": "wa limā anta a`lamu bih"
    },
    {
      "index": 87,
      "text": "ghayru muttahamin `alā khalqika wa lā li-irādatik"
    },
    {
      "index": 88,
      "text": "ḥattā `āda ṣifwatuka wa khulafa-uk"
    },
    {
      "index": 89,
      "text": "maghlūbīna maqhūrīna mubtazzīn"
    },
    {
      "index": 90,
      "text": "yarawna ḥukmaka mubaddalā"
    },
    {
      "index": 91,
      "text": "wa kitābaka manbūdhā"
    },
    {
      "index": 92,
      "text": "wa farā-iḍaka muḥarrafatan `an jihāti ashrā`ik"
    },
    {
      "index": 93,
      "text": "wa sunana nabiyyika matrūkah"
    },
    {
      "index": 94,
      "text": "allāhummal `an a`dā-ahum minal awwalīna wal-ākhirīn"
    },
    {
      "index": 95,
      "text": "wa man raḍiya bifi`ālihim"
    },
    {
      "index": 96,
      "text": "wa ashyā`ahum wa atbā`ahum"
    },
    {
      "index": 97,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 98,
      "text": "innaka ḥamīdun majīd"
    },
    {
      "index": 99,
      "text": "kaṣalawātika wa barakātika wa taḥiyyātik"
    },
    {
      "index": 100,
      "text": "`alā aṣfiyā-ika ibrāhīma wa-āli ibrāhīm"
    },
    {
      "index": 101,
      "text": "wa `ajjilil faraj"
    },
    {
      "index": 102,
      "text": "war rawḥa wan nuṣrata wat tamkīna wat ta-yīda lahum"
    },
    {
      "index": 103,
      "text": "allāhumma waj`alnī"
    },
    {
      "index": 104,
      "text": "min ahlit tawḥīd"
    },
    {
      "index": 105,
      "text": "wal īmāni bik"
    },
    {
      "index": 106,
      "text": "wat taṣdīqi birasūlik"
    },
    {
      "index": 107,
      "text": "wal a-immatil ladhīna ḥatamta ṭā`atahum"
    },
    {
      "index": 108,
      "text": "mimman yajrī dhālika bihi wa `alā yadayh"
    },
    {
      "index": 109,
      "text": "āmīna rabbal `ālamīn"
    },
    {
      "index": 110,
      "text": "allāhumma laysa yaruddu ghaḍabaka illā ḥilmuk"
    },
    {
      "index": 111,
      "text": "wa lā yaruddu sakhaṭaka illā `afwuk"
    },
    {
      "index": 112,
      "text": "wa lā yujīru min `iqābika illā raḥmatuk"
    },
    {
      "index": 113,
      "text": "wa lā yunjīnī mink"
    },
    {
      "index": 114,
      "text": "illat taḍarru`u ilayka wa bayna yadayk"
    },
    {
      "index": 115,
      "text": "faṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 116,
      "text": "wa hab lanā yā ilahī min ladunka farajā"
    },
    {
      "index": 117,
      "text": "bil qudratil latī bihā tuḥyī amwātal `ibād"
    },
    {
      "index": 118,
      "text": "wa bihā tanshuru maytal bilād"
    },
    {
      "index": 119,
      "text": "wa lā tuhliknī yā ilahī ghammā"
    },
    {
      "index": 120,
      "text": "ḥattā tastajība lī"
    },
    {
      "index": 121,
      "text": "wa tu`arrifanil ijābata fī du`ā-ī"
    },
    {
      "index": 122,
      "text": "wa adhiqnī ṭa`mal `āfiyati ilā muntahā ajalī"
    },
    {
      "index": 123,
      "text": "wa lā tushmit bī `aduwwī"
    },
    {
      "index": 124,
      "text": "wa lā tumakkinhu min `unuqī"
    },
    {
      "index": 125,
      "text": "wa lā tusalliṭ-hu `alayy"
    },
    {
      "index": 126,
      "text": "ilahī in rafa`tanī"
    },
    {
      "index": 127,
      "text": "faman dhal ladhī yaḍa`unī"
    },
    {
      "index": 128,
      "text": "wa in waḍa`tanī"
    },
    {
      "index": 129,
      "text": "faman dhal ladhī yar-fa`unī"
    },
    {
      "index": 130,
      "text": "wa in akramtanī"
    },
    {
      "index": 131,
      "text": "faman dhal ladhī yuhīnunī"
    },
    {
      "index": 132,
      "text": "wa in ahantanī"
    },
    {
      "index": 133,
      "text": "faman dhal ladhī yukrimunī"
    },
    {
      "index": 134,
      "text": "wa in `adhhabtanī"
    },
    {
      "index": 135,
      "text": "faman dhal ladhī yar-ḥamunī"
    },
    {
      "index": 136,
      "text": "wa in ahlaktanī"
    },
    {
      "index": 137,
      "text": "faman dhal ladhī ya`riḍu laka fī `abdik"
    },
    {
      "index": 138,
      "text": "aw yas-aluka `an amrih"
    },
    {
      "index": 139,
      "text": "wa qad `alimtu annahu laysa fī ḥukmika ẓulm"
    },
    {
      "index": 140,
      "text": "wa lā fī naqimatika `ajalah"
    },
    {
      "index": 141,
      "text": "wa innamā ya`jalu man yakhāful fawt"
    },
    {
      "index": 142,
      "text": "wa innamā yaḥtāju ilaẓ ẓulmiḍ ḍa`īf"
    },
    {
      "index": 143,
      "text": "wa qad ta`ālayta yā ilahī"
    },
    {
      "index": 144,
      "text": "`an dhālika `uluwwan kabīrā"
    },
    {
      "index": 145,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 146,
      "text": "wa lā taj`alnī lilbalā-i gharaḍā"
    },
    {
      "index": 147,
      "text": "wa lā linaqimatika naṣabā"
    },
    {
      "index": 148,
      "text": "wa mahhilnī, wa naffisnī"
    },
    {
      "index": 149,
      "text": "wa aqilnī `athratī"
    },
    {
      "index": 150,
      "text": "wa lā tabtaliyannī bibala"
    },
    {
      "index": 151,
      "text": "`alā athari bala"
    },
    {
      "index": 152,
      "text": "faqad tarā ḍa`fī"
    },
    {
      "index": 153,
      "text": "wa qillata ḥīlatī"
    },
    {
      "index": 154,
      "text": "wa taḍarru`ī ilayk"
    },
    {
      "index": 155,
      "text": "a`ūdhu bikallāhummal yawma min ghaḍabik"
    },
    {
      "index": 156,
      "text": "faṣalli `alā muḥammadin wa-ālihi wa a`idhnī"
    },
    {
      "index": 157,
      "text": "wa astajīru bikal yawma min sakhaṭik"
    },
    {
      "index": 158,
      "text": "faṣalli `alā muḥammadin wa-ālihi wa ajir-nī"
    },
    {
      "index": 159,
      "text": "wa as-aluka amnan min `adhābik"
    },
    {
      "index": 160,
      "text": "faṣalli `alā muḥammadin wa-ālihi wa-āminnī"
    },
    {
      "index": 161,
      "text": "wa astahdīk"
    },
    {
      "index": 162,
      "text": "faṣalli `alā muḥammadin wa-ālihi wahdinī"
    },
    {
      "index": 163,
      "text": "wa astanṣiruk"
    },
    {
      "index": 164,
      "text": "faṣalli `alā muḥammadin wa-ālihi wanṣur-nī"
    },
    {
      "index": 165,
      "text": "wa astar-ḥimuk"
    },
    {
      "index": 166,
      "text": "faṣalli `alā muḥammadin wa-ālihi war-ḥamnī"
    },
    {
      "index": 167,
      "text": "wa astakfīk"
    },
    {
      "index": 168,
      "text": "faṣalli `alā muḥammadin wa-ālihi wakfinī"
    },
    {
      "index": 169,
      "text": "wa astar-ziquk"
    },
    {
      "index": 170,
      "text": "faṣalli `alā muḥammadin wa-ālihi war-zuqnī"
    },
    {
      "index": 171,
      "text": "wa asta`īnuk"
    },
    {
      "index": 172,
      "text": "faṣalli `alā muḥammadin wa-ālihi wa a`innī"
    },
    {
      "index": 173,
      "text": "wa astaghfiruka limā salafa min dhunūbī"
    },
    {
      "index": 174,
      "text": "faṣalli `alā muḥammadin wa-ālihi waghfir lī"
    },
    {
      "index": 175,
      "text": "wa asta`ṣimuk"
    },
    {
      "index": 176,
      "text": "faṣalli `alā muḥammadin wa-ālihi wa`ṣimnī"
    },
    {
      "index": 177,
      "text": "fa-innī lan a`ūda lishay-in karihtahu minnī in shi-ta dhalik"
    },
    {
      "index": 178,
      "text": "yā rabbi yā rabbi yā ḥannānu yā mannān"
    },
    {
      "index": 179,
      "text": "yā dhal jalāli wal ikrām"
    },
    {
      "index": 180,
      "text": "ṣalli `alā muḥammadin wa-ālih"
    },
    {
      "index": 181,
      "text": "wastajib lī jamī`"
    },
    {
      "index": 182,
      "text": "mā sa-altuka wa ṭalabtu ilayk"
    },
    {
      "index": 183,
      "text": "wa raghibtu fīhi ilayk"
    },
    {
      "index": 184,
      "text": "wa arid-hu wa qaddir-hu waqḍihi wa amḍih"
    },
    {
      "index": 185,
      "text": "wa khir lī fīmā taqḍī minh"
    },
    {
      "index": 186,
      "text": "wa bārik lī fī dhalik"
    },
    {
      "index": 187,
      "text": "wa tafaḍḍal `alayya bih"
    },
    {
      "index": 188,
      "text": "wa as`idnī bimā tu`ṭīnī minh"
    },
    {
      "index": 189,
      "text": "wa zidnī min faḍlik"
    },
    {
      "index": 190,
      "text": "wa sa`ati mā `indak"
    },
    {
      "index": 191,
      "text": "fa-innaka wāsi`un karīm"
    },
    {
      "index": 192,
      "text": "wa ṣil dhālika bikhayril-ākhirati wa na`īmihā"
    },
    {
      "index": 193,
      "text": "yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 194,
      "text": ""
    }
  ]
}