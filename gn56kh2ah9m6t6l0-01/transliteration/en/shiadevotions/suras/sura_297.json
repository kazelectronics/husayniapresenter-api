{
  "index": 297,
  "name": "Sura 297",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "lā ilāha illallāhul `aẓīmul ḥalīm"
    },
    {
      "index": 3,
      "text": "lā ilāha illallāhu rabbul `ar-shil karīm"
    },
    {
      "index": 4,
      "text": "al ḥamdu lillāhi rabbil `ālamīn"
    },
    {
      "index": 5,
      "text": "allāhumma innī as-aluka mūjibāti raḥmatik"
    },
    {
      "index": 6,
      "text": "wa `azā-ima maghfiratik"
    },
    {
      "index": 7,
      "text": "wal ghanīmata min kulli birr"
    },
    {
      "index": 8,
      "text": "was salāmata min kulli ithm"
    },
    {
      "index": 9,
      "text": "allāhumma lā tada` lī dhanban illā ghafar-tah"
    },
    {
      "index": 10,
      "text": "wa lā hamman illā farrajtah"
    },
    {
      "index": 11,
      "text": "wa lā suqman illā shafaytah"
    },
    {
      "index": 12,
      "text": "wa lā `ayban illā satar-tah"
    },
    {
      "index": 13,
      "text": "wa lā rizqan illā basaṭtah"
    },
    {
      "index": 14,
      "text": "wa lā khawfan illā-āmantah"
    },
    {
      "index": 15,
      "text": "wa lā sū-an illā ṣaraftah"
    },
    {
      "index": 16,
      "text": "wa lā ḥājatan hiya laka riḍan wa liya fīhā ṣalāḥun illā qaḍaytahā"
    },
    {
      "index": 17,
      "text": "yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 18,
      "text": "āmīna rabbal `ālamīn"
    }
  ]
}