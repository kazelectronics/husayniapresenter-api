{
  "index": 202,
  "name": "Sura 202",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma mā `amiltu fī hādhihis sanah"
    },
    {
      "index": 3,
      "text": "min `amalin nahaytanī `anhu walam tar-ḍah"
    },
    {
      "index": 4,
      "text": "wanasītuhu walam tansah"
    },
    {
      "index": 5,
      "text": "wada`awtanī ilat tawbati ba`dajtirā-ī `alayk"
    },
    {
      "index": 6,
      "text": "allāhumma fa-innī astaghfiruka minhu faghfir lī"
    },
    {
      "index": 7,
      "text": "wamā `amiltu min `amalin yuqarribunī ilayk"
    },
    {
      "index": 8,
      "text": "faqbalhu minnī walā taqṭa` rajā-ī minka yā karīm"
    }
  ]
}