{
  "index": 345,
  "name": "Sura 345",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluk"
    },
    {
      "index": 3,
      "text": "wa lam yus-al mithluk"
    },
    {
      "index": 4,
      "text": "anta mawḍi`u mas-alatis sā-ilīn"
    },
    {
      "index": 5,
      "text": "wa muntahā raghbatir rāghibīn"
    },
    {
      "index": 6,
      "text": "ad`ūka walam yud`a mithluk"
    },
    {
      "index": 7,
      "text": "wa ar-ghabu ilayk"
    },
    {
      "index": 8,
      "text": "wa lam yur-ghab ilā mithlik"
    },
    {
      "index": 9,
      "text": "anta mujību da`watil muḍṭarrīn"
    },
    {
      "index": 10,
      "text": "wa ar-ḥamur rāḥimīn"
    },
    {
      "index": 11,
      "text": "as-aluka bi-afḍalil masā-il"
    },
    {
      "index": 12,
      "text": "wa anjaḥihā wa a`ẓamihā"
    },
    {
      "index": 13,
      "text": "yā allāhu yā raḥmānu yā raḥīm"
    },
    {
      "index": 14,
      "text": "wa bi-asmā-ikal ḥusnā"
    },
    {
      "index": 15,
      "text": "wa amthālikal `ulyā"
    },
    {
      "index": 16,
      "text": "wa ni`matikal latī lā tuḥṣā"
    },
    {
      "index": 17,
      "text": "wa bi-akrami asmā-ik"
    },
    {
      "index": 18,
      "text": "wa aḥabbihā ilayk"
    },
    {
      "index": 19,
      "text": "wa aqrabihā minka wasīlah"
    },
    {
      "index": 20,
      "text": "wa ashrafihā `indaka manzilah"
    },
    {
      "index": 21,
      "text": "wa ajzalihā ladayka thawābā"
    },
    {
      "index": 22,
      "text": "wa asra`ihā fil umūri ijābah"
    },
    {
      "index": 23,
      "text": "wa bismikal maknūnil akbar"
    },
    {
      "index": 24,
      "text": "al a`azzil ajallil a`ẓamil akram"
    },
    {
      "index": 25,
      "text": "al ladhī tuḥibbuhu wa tahwāh"
    },
    {
      "index": 26,
      "text": "wa tar-ḍā bihi `amman da`āk"
    },
    {
      "index": 27,
      "text": "fastajabta lahu du`ā-ah"
    },
    {
      "index": 28,
      "text": "wa ḥaqqun `alayka an lā taḥrima sā-ilak"
    },
    {
      "index": 29,
      "text": "wa lā taruddah"
    },
    {
      "index": 30,
      "text": "wa bikullismin huwa laka fit tawrāti wal injīl"
    },
    {
      "index": 31,
      "text": "waz zabūri wal fur-qānil `aẓīm"
    },
    {
      "index": 32,
      "text": "wa bikullismin da`āka bihi ḥamalatu `ar-shika wa malā-ikatuk"
    },
    {
      "index": 33,
      "text": "wa anbiya-uka wa rusuluk"
    },
    {
      "index": 34,
      "text": "wa ahlu ṭā`atika min khalqik"
    },
    {
      "index": 35,
      "text": "an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 36,
      "text": "wa an tu`ajjila faraja waliyyik"
    },
    {
      "index": 37,
      "text": "wa tu`ajjila khizya a`dā-ih"
    },
    {
      "index": 38,
      "text": "wa an taf`ala bī"
    },
    {
      "index": 39,
      "text": ""
    }
  ]
}