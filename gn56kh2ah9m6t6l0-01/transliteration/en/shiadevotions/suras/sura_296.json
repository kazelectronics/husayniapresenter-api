{
  "index": 296,
  "name": "Sura 296",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "bismillāh"
    },
    {
      "index": 3,
      "text": "waṣallallāhu `alā muḥammadin wa ālih"
    },
    {
      "index": 4,
      "text": "wa-ufawwiḍu amrī ilallāh"
    },
    {
      "index": 5,
      "text": "innallāha baṣīrun bil `ibād"
    },
    {
      "index": 6,
      "text": "fawaqāhullāhu sayyi-āti mā makarū"
    },
    {
      "index": 7,
      "text": "la-ilāha illā anta subḥānak"
    },
    {
      "index": 8,
      "text": "innī kuntu minaẓ ẓālimīn"
    },
    {
      "index": 9,
      "text": "fastajabnā lahu wanajjaynāhu minal ghamm"
    },
    {
      "index": 10,
      "text": "wakadhalika nunjil mu-minīn"
    },
    {
      "index": 11,
      "text": "ḥasbunallāhu wani`mal wakīl"
    },
    {
      "index": 12,
      "text": "fanqalabū bini`matin minallāhi wafaḍl"
    },
    {
      "index": 13,
      "text": "lam yamsas-hum sū"
    },
    {
      "index": 14,
      "text": "mā shā-allāh"
    },
    {
      "index": 15,
      "text": "lā ḥawla walā quwwata illā billāh"
    },
    {
      "index": 16,
      "text": "mā shā-allāhu lā mā shā-an nās"
    },
    {
      "index": 17,
      "text": "mā shā-allāhu wa-in karihan nās"
    },
    {
      "index": 18,
      "text": "ḥasbiyar rabbu minal mar-būbīn"
    },
    {
      "index": 19,
      "text": "ḥasbiyal khāliqu minal makhlūqīn"
    },
    {
      "index": 20,
      "text": "ḥasbiyar rāziqu minal mar-zūqīn"
    },
    {
      "index": 21,
      "text": "ḥasbiyallāhu rabbul `ālamīn"
    },
    {
      "index": 22,
      "text": "ḥasbī man huwa ḥasbī"
    },
    {
      "index": 23,
      "text": "ḥasbī man lam yazal ḥasbī"
    },
    {
      "index": 24,
      "text": "ḥasbī man kāna mudh kuntu lam yazal ḥasbī"
    },
    {
      "index": 25,
      "text": "ḥasbiyallāhu la-ilāha illā huw"
    },
    {
      "index": 26,
      "text": "`alayhi tawakkalt"
    },
    {
      "index": 27,
      "text": "wahuwa rabbul `ar-shil `aẓīm"
    }
  ]
}