{
  "index": 106,
  "name": "Sura 106",
  "ayas": [
    {
      "index": 1,
      "text": "li-iylāfi quraysh",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "iylāfihim riḥlatash shitā-i waṣ ṣayf"
    },
    {
      "index": 3,
      "text": "falya`budū rabba hādhal bayt"
    },
    {
      "index": 4,
      "text": "alladhī aṭ`amahum min jū`in wa-āmanahum min khawf"
    }
  ]
}