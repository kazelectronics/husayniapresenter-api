{
  "index": 286,
  "name": "Sura 286",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī hādhihi ṣalātī ṣallaytuhā"
    },
    {
      "index": 3,
      "text": "lā liḥājatin minka ilayhā"
    },
    {
      "index": 4,
      "text": "walā raghbatin minka fīhā"
    },
    {
      "index": 5,
      "text": "illā ta`ẓīman waṭā`ah"
    },
    {
      "index": 6,
      "text": "wa-ijābatan laka ilā mā amar-tanī bih"
    },
    {
      "index": 7,
      "text": "ilāhī in kāna fīhā khalalun aw naqṣ"
    },
    {
      "index": 8,
      "text": "min rukū`ihā aw sujūdihā"
    },
    {
      "index": 9,
      "text": "falā tu-ākhidhnī"
    },
    {
      "index": 10,
      "text": "watafaḍḍal `alayya bil qabūli walghufrān"
    }
  ]
}