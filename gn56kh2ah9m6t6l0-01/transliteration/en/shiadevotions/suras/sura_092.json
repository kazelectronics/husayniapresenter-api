{
  "index": 92,
  "name": "Sura 92",
  "ayas": [
    {
      "index": 1,
      "text": "wal layli idhā yaghshā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wan nahāri idhā tajallā"
    },
    {
      "index": 3,
      "text": "wa mā khalaqadh dhakara wal-unthā"
    },
    {
      "index": 4,
      "text": "inna sa`yakum lashattā"
    },
    {
      "index": 5,
      "text": "fa-ammā man a`ṭā wattaqā"
    },
    {
      "index": 6,
      "text": "waṣaddaqa bil ḥusnā"
    },
    {
      "index": 7,
      "text": "fasanuyassiruhu lilyusrā"
    },
    {
      "index": 8,
      "text": "wa-ammā man bakhila wastaghnā"
    },
    {
      "index": 9,
      "text": "wakadhhaba bil ḥusnā"
    },
    {
      "index": 10,
      "text": "fasanuyassiruhu lil`usrā"
    },
    {
      "index": 11,
      "text": "wa mā yughnī `anhu māluhu idhā taraddā"
    },
    {
      "index": 12,
      "text": "inna `alaynā lalhudā"
    },
    {
      "index": 13,
      "text": "wa inna lanā lal-ākhirata wal-ūlā"
    },
    {
      "index": 14,
      "text": "fa-andhartukum nāran talaẓẓā"
    },
    {
      "index": 15,
      "text": "lā yaṣlāhā illal-ashqā"
    },
    {
      "index": 16,
      "text": "alladhī kadhhaba watawallā"
    },
    {
      "index": 17,
      "text": "wasayujannabuhal-atqā"
    },
    {
      "index": 18,
      "text": "alladhī yu-tī mālahu yatazakkā"
    },
    {
      "index": 19,
      "text": "wa mā li-aḥadin `indahu min ni`matin tujzā"
    },
    {
      "index": 20,
      "text": "illab-tighā-a wajhi rabbihil-a`lā"
    },
    {
      "index": 21,
      "text": "walasawfa yarḍā"
    }
  ]
}