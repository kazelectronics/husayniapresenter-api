{
  "index": 306,
  "name": "Sura 306",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alash shajaratin nabawiyyah",
      "bismillah": ""
    },
    {
      "index": 2,
      "text": "wad dawḥatil hāshimiyyatil muḍī-ah"
    },
    {
      "index": 3,
      "text": "al muthmirati binnubuwwah"
    },
    {
      "index": 4,
      "text": "al mūniqati bil imāmah"
    },
    {
      "index": 5,
      "text": "wa`alā ḍajī`ayka-ādama wanūḥ"
    },
    {
      "index": 6,
      "text": "`alayhimas salām"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka wa`alā ahli baytik"
    },
    {
      "index": 8,
      "text": "aṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka wa`alal malā-ikatil muḥdiqīna bik"
    },
    {
      "index": 10,
      "text": "walḥāffīna biqabrik"
    },
    {
      "index": 11,
      "text": "yā mawlāya yā amīralmu-minīn"
    },
    {
      "index": 12,
      "text": "hadhā yawmul aḥad"
    },
    {
      "index": 13,
      "text": "wahuwa yawmuka wabismik"
    },
    {
      "index": 14,
      "text": "wa-ana ḍayfuka fīhi wajāruk"
    },
    {
      "index": 15,
      "text": "fa-aḍifnī yā mawlāya wa-ajir-nī"
    },
    {
      "index": 16,
      "text": "fa-innaka karīmun tuḥibbuḍ ḍiyāfah"
    },
    {
      "index": 17,
      "text": "wama-mūrun bil ijārah"
    },
    {
      "index": 18,
      "text": "faf`al mā raghibtu ilayka fīh"
    },
    {
      "index": 19,
      "text": "warajawtuhu mink"
    },
    {
      "index": 20,
      "text": "bimanzilatika wa āli baytika `indallāh"
    },
    {
      "index": 21,
      "text": "wamanzilatihi `indakum"
    },
    {
      "index": 22,
      "text": "wabiḥaqqibni `ammika rasūlillāh"
    },
    {
      "index": 23,
      "text": "ṣallallāhu `alayhi wa-ālihi wasallam"
    },
    {
      "index": 24,
      "text": "wa`alayhim ajma`īn"
    },
    {
      "index": 25,
      "text": ""
    },
    {
      "index": 26,
      "text": "as salāmu `alayki yā mumtaḥanah"
    },
    {
      "index": 27,
      "text": "imtaḥanakil ladhī khalaqak"
    },
    {
      "index": 28,
      "text": "fawajadaki limamtaḥanaki ṣābirah"
    },
    {
      "index": 29,
      "text": "anā laki muṣaddiqun ṣābirun `alā mā atā bihi abūki wawaṣiyyuhu ṣalawātullāhi `alayhimā"
    },
    {
      "index": 30,
      "text": "wa-ana as-aluki in kuntu ṣaddaqtuk"
    },
    {
      "index": 31,
      "text": "illā alḥaqtinī bitaṣdīqī lahumā"
    },
    {
      "index": 32,
      "text": "litusarra nafsī"
    },
    {
      "index": 33,
      "text": "fash-hadī annī ẓāhirun biwilāyatik"
    },
    {
      "index": 34,
      "text": "wawilāyati-āli baytik"
    },
    {
      "index": 35,
      "text": "ṣalawātullāhi `alayhim ajma`īn"
    }
  ]
}