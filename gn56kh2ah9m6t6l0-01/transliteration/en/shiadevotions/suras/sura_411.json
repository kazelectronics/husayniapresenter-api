{
  "index": 411,
  "name": "Sura 411",
  "ayas": [
    {
      "index": 1,
      "text": "wa bu`dal ma`ṣiyah",
      "bismillah": "allāhummar-zuqnā tawfīqaṭ ṭā`ah"
    },
    {
      "index": 2,
      "text": "wa ṣidqan niyyah"
    },
    {
      "index": 3,
      "text": "wa `ir-fānal ḥur-mah"
    },
    {
      "index": 4,
      "text": "wa akrimnā bilhudā wal astiqāmah"
    },
    {
      "index": 5,
      "text": "wa saddid alsinatanā biṣṣawābi wal ḥikmah"
    },
    {
      "index": 6,
      "text": "wamla-qulūbanā bil `ilmi wal ma`rifah"
    },
    {
      "index": 7,
      "text": "wa ṭahhir buṭūnanā minal ḥarāmi wash shubhah"
    },
    {
      "index": 8,
      "text": "wakfuf aydiyanā `aniẓ ẓulmi was sir-qah"
    },
    {
      "index": 9,
      "text": "waghḍuḍ abṣāranā `anil fujūri wal khiyānah"
    },
    {
      "index": 10,
      "text": "wasdud asmā`anā `anil laghwi wal ghībah"
    },
    {
      "index": 11,
      "text": "wa tafaḍḍal `alā `ulamā-inā bizzuhdi wan naṣīḥah"
    },
    {
      "index": 12,
      "text": "wa `alal muta`allimīna bil juhdi war raghbah"
    },
    {
      "index": 13,
      "text": "wa `alal mustami`īna bil ittibā`i wal maw`iẓah"
    },
    {
      "index": 14,
      "text": "wa `alā mar-ḍal muslimīna bishhifā-i war rāḥah"
    },
    {
      "index": 15,
      "text": "wa `alā mawtāhum birra-fati war raḥmah"
    },
    {
      "index": 16,
      "text": "wa `alā mashāyikhinā bil waqāri was sakīnah"
    },
    {
      "index": 17,
      "text": "wa `alash shabābi bil inābati wat tawbah"
    },
    {
      "index": 18,
      "text": "wa `alan nisā-i bil ḥayā-i wal `iffah"
    },
    {
      "index": 19,
      "text": "wa `alal aghniyā-i bittawāḍu`i was sa`ah"
    },
    {
      "index": 20,
      "text": "wa `alal fuqarā-i biṣṣabri wal qanā`ah"
    },
    {
      "index": 21,
      "text": "wa `alal ghuzāti binnaṣri wal ghalabah"
    },
    {
      "index": 22,
      "text": "wa `alal usarā-i bil khalāṣi war rāḥah"
    },
    {
      "index": 23,
      "text": "wa `alal umarā-i bil `adli wash shafaqah"
    },
    {
      "index": 24,
      "text": "wa `alar ra`iyyati bil inṣāfi wa ḥusnis sīrah"
    },
    {
      "index": 25,
      "text": "wa bārik lilḥujjāji waz zuwwāri fiz zādi wan nafaqah"
    },
    {
      "index": 26,
      "text": "waqḍi mā awjabta `alayhim minal ḥajji wal `umrah"
    },
    {
      "index": 27,
      "text": "bifaḍlika wa raḥmatika yā ar-ḥamar rāḥimīn"
    }
  ]
}