{
  "index": 123,
  "name": "Sura 123",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alayka ayyuhal fāḥiṣu `an dīnillāh",
      "bismillah": "as salāmu `alayka ayyuhal bāqiru li`ilmillāh"
    },
    {
      "index": 2,
      "text": "as salāmu `alayka ayyuhal mubayyinu liḥukmillāh"
    },
    {
      "index": 3,
      "text": "as salāmu `alayka ayyuhal qā-imu biqisṭillāh"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka ayyuhan nāṣiḥu li`ibādillāh"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka ayyuhad dā`ī ilallāh"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka ayyuhad dalīlu ilallāh"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka ayyuhal faḍlul mubīn"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka ayyuhan nūrus sāṭi`"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka ayyuhal badrul lāmi`"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka ayyuhal ḥaqqul ablaj"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka ayyuhas sirājul asraj"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka ayyuhan najmul azhar"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka ayyuhal kawkabul abhar"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka ayyuhal munazzahu `anil mu`ḍilāt"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka ayyuhal ma`ṣūmu minal azzalāt"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka ayyuhaz zakiyyu fil ḥasab"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka ayyuhar rafī`u fin nasab"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka ayyuhal imāmush shafīq"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka ayyuhal qaṣrul mashīd"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka yā ḥujjatallāhi `alal khalqi ajma`īn"
    },
    {
      "index": 21,
      "text": "ash-hadu yā mawlāya annaka qad ṣada`tal ḥaqqa ṣad`ā"
    },
    {
      "index": 22,
      "text": "wabaqar-tal `ilma baqrā"
    },
    {
      "index": 23,
      "text": "wanathar-tahu nathrā"
    },
    {
      "index": 24,
      "text": "lam ta-akhudhka fillāhi lawmatu lā-im"
    },
    {
      "index": 25,
      "text": "wakunta lidīnillāhi mukātimā"
    },
    {
      "index": 26,
      "text": "waqaḍayta mā kāna `alayk"
    },
    {
      "index": 27,
      "text": "wa-akhrajta awliyā-aka min wilāyati ghayrillāhi ilā wilāyatillāh"
    },
    {
      "index": 28,
      "text": "wa-amar-ta biṭā`atillāh"
    },
    {
      "index": 29,
      "text": "wanahayta `an ma`ṣiyatillāh"
    },
    {
      "index": 30,
      "text": "ḥattā qabaḍakallāhu ilā riḍwānih"
    },
    {
      "index": 31,
      "text": "wadhahaba bika ilā dāri karāmatih"
    },
    {
      "index": 32,
      "text": "wa-ilā masākini aṣfiyā-ih"
    },
    {
      "index": 33,
      "text": "wamujāwarati awliyā-ih"
    },
    {
      "index": 34,
      "text": "was salāmu `alayka waraḥmatullāhi wabarakātuh"
    }
  ]
}