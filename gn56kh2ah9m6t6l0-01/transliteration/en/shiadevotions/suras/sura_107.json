{
  "index": 107,
  "name": "Sura 107",
  "ayas": [
    {
      "index": 1,
      "text": "ara aytal ladhī yukadhhibu bid dīn",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "fadhālikal ladhī yadu``ul yatīm"
    },
    {
      "index": 3,
      "text": "walā yaḥuḍḍu `alā ṭa`āmil miskīn"
    },
    {
      "index": 4,
      "text": "fawaylul lil-muṣallīn"
    },
    {
      "index": 5,
      "text": "alladhīna hum `an ṣalātihim sāhūn"
    },
    {
      "index": 6,
      "text": "alladhīna hum yurā-ūn"
    },
    {
      "index": 7,
      "text": "wayamna`ūnal mā`ūn"
    }
  ]
}