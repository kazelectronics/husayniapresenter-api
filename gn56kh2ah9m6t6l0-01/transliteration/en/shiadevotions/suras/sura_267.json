{
  "index": 267,
  "name": "Sura 267",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "yā man idhā sa-alahu `abdun a`ṭāh"
    },
    {
      "index": 3,
      "text": "wa-idhā ammala mā `indahu ballaghahu munāh"
    },
    {
      "index": 4,
      "text": "wa-idhā aqbala `alayhi qarrabahu wa-adnāh"
    },
    {
      "index": 5,
      "text": "wa-idhā jāharahu bil `iṣyāni satara `alā dhanbihi waghaṭṭāh"
    },
    {
      "index": 6,
      "text": "wa-idhā tawakkala `alayhi aḥsabahu wakafāh"
    },
    {
      "index": 7,
      "text": "ilāhī manil ladhī nazala bika multamisan qirāka famā qaraytah"
    },
    {
      "index": 8,
      "text": "wamanil ladhī anākha bibābika mur-tajiyan nadāka famā awlaytah"
    },
    {
      "index": 9,
      "text": "ayaḥsunu an ar-ji`a `an bābika bil khaybati maṣrūfā"
    },
    {
      "index": 10,
      "text": "walastu a`rifu siwāka mawlan bil iḥsāni mawṣūfā"
    },
    {
      "index": 11,
      "text": "kayfa ar-jū ghayraka walkhayru kulluhu biyadik"
    },
    {
      "index": 12,
      "text": "wakayfa u-ammilu siwāka walkhalqu wal-amru lak"
    },
    {
      "index": 13,
      "text": "a-aqṭa`u rajā-ī mink"
    },
    {
      "index": 14,
      "text": "waqad awlaytanī mā lam as-alhu min faḍlik"
    },
    {
      "index": 15,
      "text": "am tufqirunī ilā mithlī"
    },
    {
      "index": 16,
      "text": "wa-ana a`taṣimu biḥablik"
    },
    {
      "index": 17,
      "text": "yā man sa`ida biraḥmatihil qāṣidūn"
    },
    {
      "index": 18,
      "text": "walam yashqa binaqimatihil mustaghfirūn"
    },
    {
      "index": 19,
      "text": "kayfa ansāka walam tazal dhākirī"
    },
    {
      "index": 20,
      "text": "wakayfa alhū `anka wa-anta murāqibī"
    },
    {
      "index": 21,
      "text": "ilāhī bidhayli karamika a`laqtu yadī"
    },
    {
      "index": 22,
      "text": "walinayli `aṭāyāka basaṭtu amalī"
    },
    {
      "index": 23,
      "text": "fa-akhliṣnī bikhāliṣati tawḥīdik"
    },
    {
      "index": 24,
      "text": "waj`alnī min ṣafwati `abīdik"
    },
    {
      "index": 25,
      "text": "yā man kullu hāribin ilayhi yaltaji"
    },
    {
      "index": 26,
      "text": "wakullu ṭālibin iyyāhu yar-tajī"
    },
    {
      "index": 27,
      "text": "yā khayra mar-juww"
    },
    {
      "index": 28,
      "text": "wayā akrama mad`uww"
    },
    {
      "index": 29,
      "text": "wayā man lā yuraddu sā-iluh"
    },
    {
      "index": 30,
      "text": "walā yukhayyabu-āmiluh"
    },
    {
      "index": 31,
      "text": "yā man bābuhu maftūḥun lidā`īh"
    },
    {
      "index": 32,
      "text": "waḥijābuhu mar-fū`un lirājīh"
    },
    {
      "index": 33,
      "text": "as-aluka bikaramika an tamunna `alayy"
    },
    {
      "index": 34,
      "text": "min `aṭā-ika bimā taqarru bihi `aynī"
    },
    {
      "index": 35,
      "text": "wamin rajā-ika bimā taṭma-innu bihi nafsī"
    },
    {
      "index": 36,
      "text": "waminal yaqīni bimā tuhawwinu bihi `alayya muṣībātid dunyā"
    },
    {
      "index": 37,
      "text": "watajlū bihi `an baṣīratī ghashawātil `amā"
    },
    {
      "index": 38,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    }
  ]
}