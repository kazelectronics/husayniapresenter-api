{
  "index": 116,
  "name": "Sura 116",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alayka ayyuhal waṣiyyun nāṭiq",
      "bismillah": "as salāmu `alayka ayyuhal imāmuṣ ṣādiq"
    },
    {
      "index": 2,
      "text": "as salāmu `alayka ayyuhal fātiqur rātiq"
    },
    {
      "index": 3,
      "text": "as salāmu `alayka ayyuhas sanāmul a`ẓam"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka ayyuhaṣ ṣirāṭul aqwam"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā miftāḥal khayrāt"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā miṣbāḥal ẓulumāt"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā dāfi`al mu`ḍilāt"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yā ma`dinal barakāt"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka yā ṣāḥibal ḥujaji wad dalālāt"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā ṣāḥibal barāhīnil wāḍiḥāt"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka yā nāṣira dīnillāh"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka yā nāshira ḥukmillāh"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka yā fāṣilal khiṭābāt"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka yā kāshifal kurubāt"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka yā `amīdaṣ ṣādiqīn"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka yā lisānan nāṭiqīn"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka yā khalafal khā-ifīn"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka yā za`īmaṣ ṣāliḥīn"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka yā sayyidal muslimīn"
    },
    {
      "index": 20,
      "text": "as salāmu `alay yā kahfal mu-minīn"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka yā hādiyal muḍillīn"
    },
    {
      "index": 22,
      "text": "as salāmu `alayka yā sakanaṭ ṭā-i`īn"
    },
    {
      "index": 23,
      "text": "ash-hadu yā mawlāya annaka `alamul hudā"
    },
    {
      "index": 24,
      "text": "wal`ur-watul wuthqā, washamsuḍ ḍuḥā"
    },
    {
      "index": 25,
      "text": "wabaḥrun nadā,wakahful warā, wāl-mathalul a`lā"
    },
    {
      "index": 26,
      "text": "waṣallallāhu `alā rūḥika wabadanik"
    }
  ]
}