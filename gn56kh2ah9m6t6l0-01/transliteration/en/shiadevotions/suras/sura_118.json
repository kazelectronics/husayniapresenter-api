{
  "index": 118,
  "name": "Sura 118",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alā nūḥin nabiyyillāh",
      "bismillah": "as salāmu `alā-ādama ṣifwatillāh"
    },
    {
      "index": 2,
      "text": "as salāmu `alā ibrāhīma khalīlillāh"
    },
    {
      "index": 3,
      "text": "as salāmu `alā mūsā kalīmillāh"
    },
    {
      "index": 4,
      "text": "as salāmu `alā `īsā rūḥillāh"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā rasūlallāh"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā khayra khalqillāh"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā ṣafiyyallāh"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yā muḥammadabna `abdillāh"
    },
    {
      "index": 9,
      "text": "khātamin nabiyyīn"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā amīral mu-minīn"
    },
    {
      "index": 11,
      "text": "`aliyyabna abī ṭālib"
    },
    {
      "index": 12,
      "text": "waṣiyya rasūlillāh"
    },
    {
      "index": 13,
      "text": "as salāmu `alayki yā fāṭimah"
    },
    {
      "index": 14,
      "text": "sayyidata nisā-il `ālamīn"
    },
    {
      "index": 15,
      "text": "as salāmu `alaykumā yā sibṭay nabiyyir raḥmah"
    },
    {
      "index": 16,
      "text": "wasayyiday shabābi ahlil jjanah"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka yā `aliyyabnal ḥusayn"
    },
    {
      "index": 18,
      "text": "sayyidal `ābidīna waqurrata `aynin nāẓirīn"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka yā muḥammadabna `aliyy"
    },
    {
      "index": 20,
      "text": "bāqiral `ilmi ba`dan nabiyy"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka yā ja`farabna muḥammad"
    },
    {
      "index": 22,
      "text": "aṣ ṣādiqal bārral amīn"
    },
    {
      "index": 23,
      "text": "as salāmu `alayka yā mūsabna ja`far"
    },
    {
      "index": 24,
      "text": "aṭ ṭāhiraṭ ṭuhr"
    },
    {
      "index": 25,
      "text": "as salāmu `alayka yā `aliyyabna mūsā"
    },
    {
      "index": 26,
      "text": "ar riḍal mur-taḍā"
    },
    {
      "index": 27,
      "text": "as salāmu `alayka yā muḥammadabna `aliyyinit taqiyy"
    },
    {
      "index": 28,
      "text": "as salāmu `alayka yā `aliyyabna muḥammad"
    },
    {
      "index": 29,
      "text": "an naqiyyan nāṣiḥal amīn"
    },
    {
      "index": 30,
      "text": "as salāmu `alayka yā ḥasanabna `aliyy"
    },
    {
      "index": 31,
      "text": "as salāmu `alal waṣiyyi min ba`dih"
    },
    {
      "index": 32,
      "text": "allāhumma ṣalli `alā nūrika wasirājik"
    },
    {
      "index": 33,
      "text": "wawaliyyi waliyyik"
    },
    {
      "index": 34,
      "text": "wawaṣiyyi waṣiyyik"
    },
    {
      "index": 35,
      "text": "waḥujjatika `alā khalqik"
    },
    {
      "index": 36,
      "text": "as salāmu `alayki yā binta rasūlillāh"
    },
    {
      "index": 37,
      "text": "as salāmu `alayki yā binta fāṭimata wakhadījah"
    },
    {
      "index": 38,
      "text": "as salāmu `alayki yā binta amīril mu-minīn"
    },
    {
      "index": 39,
      "text": "as salāmu `alayki yā bintal ḥasani walḥusayn"
    },
    {
      "index": 40,
      "text": "as salāmu `alayki yā binta waliyyillāh"
    },
    {
      "index": 41,
      "text": "as salāmu `alayki yā ukhta waliyyillāh"
    },
    {
      "index": 42,
      "text": "as salāmu `alayki yā `ammata waliyyillāh"
    },
    {
      "index": 43,
      "text": "as salāmu `alayki yā binta mūsabni ja`far"
    },
    {
      "index": 44,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 45,
      "text": "as salāmu `alayk"
    },
    {
      "index": 46,
      "text": "`arrafallāhu baynanā wabaynakum fil jannah"
    },
    {
      "index": 47,
      "text": "waḥasharanā fī zumratikum"
    },
    {
      "index": 48,
      "text": "wa-awradanā ḥawḍa nabiyyikum"
    },
    {
      "index": 49,
      "text": "wasaqānā bika-si jaddikum"
    },
    {
      "index": 50,
      "text": "min yadi `aliyyibni abī ṭālib"
    },
    {
      "index": 51,
      "text": "ṣalawātullāhi `alaykum"
    },
    {
      "index": 52,
      "text": "as-alullāha an yuriyanā fīkumus surūra walfaraj"
    },
    {
      "index": 53,
      "text": "wa-an yajma`anā wa-iyyākum fī zumrati jaddikum"
    },
    {
      "index": 54,
      "text": "muḥammadin ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 55,
      "text": "wa-an lāyaslubanā ma`rifatakum"
    },
    {
      "index": 56,
      "text": "innahu waliyyun qadīr"
    },
    {
      "index": 57,
      "text": "ataqarrabu ilallāhi biḥubbikum"
    },
    {
      "index": 58,
      "text": "walbarā-ati min a`dā-ikum"
    },
    {
      "index": 59,
      "text": "wat taslīmi ilallāh"
    },
    {
      "index": 60,
      "text": "rāḍiyan bihi ghayra munkirin walā mustakbir"
    },
    {
      "index": 61,
      "text": "wa`alā yaqīni mā atā bihi muḥammad"
    },
    {
      "index": 62,
      "text": "wabihi rāḍ"
    },
    {
      "index": 63,
      "text": "naṭlubu bidhālika wajhak"
    },
    {
      "index": 64,
      "text": "yā sayyidī allāhumma wariḍāk"
    },
    {
      "index": 65,
      "text": "wad dāral-ākhirah"
    },
    {
      "index": 66,
      "text": "yā fāṭimatu ishfa`ī lī fil jannah"
    },
    {
      "index": 67,
      "text": "fa-inna laki `indallāhi sha-nan minash sha-n"
    },
    {
      "index": 68,
      "text": "allāhumma innī as-aluka an takhtima lī bissa`ādah"
    },
    {
      "index": 69,
      "text": "falā taslub minnī mā ana fīh"
    },
    {
      "index": 70,
      "text": "walā ḥawla walā quwwata illā billāhil `aliyyil `aẓīm"
    },
    {
      "index": 71,
      "text": "allāhummastajib lanā"
    },
    {
      "index": 72,
      "text": "wataqabbalhu bikaramika wa`izzatik"
    },
    {
      "index": 73,
      "text": "wabiraḥmatika wa`āfiyatik"
    },
    {
      "index": 74,
      "text": "waṣallallāhu `alā muḥammadin wa ālihi ajma`īn"
    },
    {
      "index": 75,
      "text": "wasallama taslīman yā ar-ḥamar rāḥimīn"
    }
  ]
}