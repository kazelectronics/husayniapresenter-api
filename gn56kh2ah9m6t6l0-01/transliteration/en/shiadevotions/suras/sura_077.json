{
  "index": 77,
  "name": "Sura 77",
  "ayas": [
    {
      "index": 1,
      "text": "wal mursalāti `urfā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "fal `āṣifāti `aṣfā"
    },
    {
      "index": 3,
      "text": "wan nāshirāti nashrā"
    },
    {
      "index": 4,
      "text": "fal fāriqāti farqā"
    },
    {
      "index": 5,
      "text": "fal mulqiyāti dhikrā"
    },
    {
      "index": 6,
      "text": "`udhran aw nudhrā"
    },
    {
      "index": 7,
      "text": "innamā tū`adūna lawāqi`"
    },
    {
      "index": 8,
      "text": "fa-idhan nujūmu ṭumisat"
    },
    {
      "index": 9,
      "text": "wa idhas samā-u furijat"
    },
    {
      "index": 10,
      "text": "wa idhal jibālu nusifat"
    },
    {
      "index": 11,
      "text": "wa idhar rusulu uqqitat"
    },
    {
      "index": 12,
      "text": "li-ayyi yawmin ujjilat"
    },
    {
      "index": 13,
      "text": "liyawmil faṣl"
    },
    {
      "index": 14,
      "text": "wa mā adrāka mā yawmul faṣl"
    },
    {
      "index": 15,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 16,
      "text": "alam nuhlikil-awwalīn"
    },
    {
      "index": 17,
      "text": "thumma nutbi`uhumul-ākhirīn"
    },
    {
      "index": 18,
      "text": "kadhālika naf`alu bil mujrimīn"
    },
    {
      "index": 19,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 20,
      "text": "alam nakhluqkum min mā-in mahīn"
    },
    {
      "index": 21,
      "text": "faja`alnāhu fī qarārin makīn"
    },
    {
      "index": 22,
      "text": "ilā qadarin ma`lūm"
    },
    {
      "index": 23,
      "text": "faqadarnā fani`mal qādirūn"
    },
    {
      "index": 24,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 25,
      "text": "alam naj`alil-arḍa kifātā"
    },
    {
      "index": 26,
      "text": "aḥyā-an wa-amwātā"
    },
    {
      "index": 27,
      "text": "waja`alnā fīhā rawāsiya shāmikhātin wa-asqaynākum mā-an furātā"
    },
    {
      "index": 28,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 29,
      "text": "in-ṭaliqū ilā mā kuntum bihi tukadhhibūn"
    },
    {
      "index": 30,
      "text": "in-ṭaliqū ilā ẓillin dhī thalāthi shu`ab"
    },
    {
      "index": 31,
      "text": "lā ẓalīlin walā yughnī minallāhab"
    },
    {
      "index": 32,
      "text": "innahā tarmī bishararin kal qaṣr"
    },
    {
      "index": 33,
      "text": "ka-annahu jimālatun ṣufr"
    },
    {
      "index": 34,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 35,
      "text": "hādhā yawmu lā yanṭiqūn"
    },
    {
      "index": 36,
      "text": "walā yu-dhanu lahum faya`tadhirūn"
    },
    {
      "index": 37,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 38,
      "text": "hādhā yawmul faṣl ۖ jama`nākum wal-awwalīn"
    },
    {
      "index": 39,
      "text": "fa-in kāna lakum kaydun fakīdūn"
    },
    {
      "index": 40,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 41,
      "text": "innal muttaqīna fī ẓilālin wa`uyūn"
    },
    {
      "index": 42,
      "text": "wafawākiha mimmā yashtahūn"
    },
    {
      "index": 43,
      "text": "kulū washrabū hanī-an bimā kuntum ta`malūn"
    },
    {
      "index": 44,
      "text": "innā kadhālika najzil muḥsinīn"
    },
    {
      "index": 45,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 46,
      "text": "kulū watamatta`ū qalīlan innakum mujrimūn"
    },
    {
      "index": 47,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 48,
      "text": "wa idhā qīla lahumur-ka`ū lā yarka`ūn"
    },
    {
      "index": 49,
      "text": "waylun yawma-idhin lil-mukadhhibīn"
    },
    {
      "index": 50,
      "text": "fabi-ayyi ḥadīthin ba`dahu yu-minūn"
    }
  ]
}