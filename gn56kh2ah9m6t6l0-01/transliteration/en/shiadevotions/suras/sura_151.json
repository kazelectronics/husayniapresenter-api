{
  "index": 151,
  "name": "Sura 151",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma yā mudhilla kulli jabbār"
    },
    {
      "index": 3,
      "text": "wayā mu`izzal mu-minīn"
    },
    {
      "index": 4,
      "text": "anta kahfī ḥīna tu`yīnil madhāhib"
    },
    {
      "index": 5,
      "text": "wa-anta bāri-u khalqī raḥmatan bī"
    },
    {
      "index": 6,
      "text": "waqad kunta `an khalqī ghaniyyā"
    },
    {
      "index": 7,
      "text": "walawlā raḥmatuka lakuntu minal hālikīn"
    },
    {
      "index": 8,
      "text": "wa-anta mu-ayyidī binnaṣri `alā a`dā-ī"
    },
    {
      "index": 9,
      "text": "walawlā naṣruka iyyāya lakuntu minal mafḍūḥīn"
    },
    {
      "index": 10,
      "text": "yā mur-silar raḥmati min ma`ādinihā"
    },
    {
      "index": 11,
      "text": "wamunshi-al barakati min mawāḍi`ihā"
    },
    {
      "index": 12,
      "text": "yā man khaṣṣa nafsahu bishhumūkhi war rif`ah"
    },
    {
      "index": 13,
      "text": "fa-awliya-uhu bi`izzihi yata`azzazūn"
    },
    {
      "index": 14,
      "text": "wayā man waḍa`at lahul mulūku nīral madhallati `alā a`nāqihim"
    },
    {
      "index": 15,
      "text": "fahum min saṭawātihi khā-ifūn"
    },
    {
      "index": 16,
      "text": "as-aluka bikaynūniyyatikal latishtaqaqtahā min kibriyā-ik"
    },
    {
      "index": 17,
      "text": "wa-as-aluka bikibriyā-ikal latishtaqaqtahā min `izzatik"
    },
    {
      "index": 18,
      "text": "wa-as-aluka bi`izzatikal latistawayta bihā `alā `ar-shik"
    },
    {
      "index": 19,
      "text": "fakhalaqta bihā jamī`a khalqik"
    },
    {
      "index": 20,
      "text": "fahum laka mudh`inūn"
    },
    {
      "index": 21,
      "text": "an tuṣalliya `alā muḥammadin wa-ahli baytih"
    }
  ]
}