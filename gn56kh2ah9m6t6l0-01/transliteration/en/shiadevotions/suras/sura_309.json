{
  "index": 309,
  "name": "Sura 309",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al ḥamdu lillāh"
    },
    {
      "index": 3,
      "text": "walḥamdu ḥaqquhu kamā yastaḥiqquh"
    },
    {
      "index": 4,
      "text": "ḥamdan kathīrā"
    },
    {
      "index": 5,
      "text": "wa-a`ūdhu bihi min sharri nafsī"
    },
    {
      "index": 6,
      "text": "innan nafsa la-ammāratun bissū"
    },
    {
      "index": 7,
      "text": "illā mā raḥima rabbī"
    },
    {
      "index": 8,
      "text": "wa-a`ūdhu bihi min sharrish shayṭān"
    },
    {
      "index": 9,
      "text": "al ladhī yazīdunī dhanban ilā dhanbī"
    },
    {
      "index": 10,
      "text": "wa-aḥtarizu bihi min kulli jabbārin fājir"
    },
    {
      "index": 11,
      "text": "wasulṭānin jā-ir"
    },
    {
      "index": 12,
      "text": "wa`aduwwin qāhir"
    },
    {
      "index": 13,
      "text": "allāhummaj`alnī min jundik"
    },
    {
      "index": 14,
      "text": "fa-inna jundaka humul ghālibūn"
    },
    {
      "index": 15,
      "text": "waj`alnī min ḥizbik"
    },
    {
      "index": 16,
      "text": "fa-inna ḥizbaka humul mufliḥūn"
    },
    {
      "index": 17,
      "text": "waj`alnī min awliyā-ik"
    },
    {
      "index": 18,
      "text": "fa-inna awliyā-aka lākhawfun `alayhim walā hum yaḥzanūn"
    },
    {
      "index": 19,
      "text": "allāhumma aṣliḥ lī dīnī"
    },
    {
      "index": 20,
      "text": "fa-innahu `iṣmatu amrī"
    },
    {
      "index": 21,
      "text": "wa-aṣliḥ lī-ākhiratī"
    },
    {
      "index": 22,
      "text": "fa-innahā dāru maqarrī"
    },
    {
      "index": 23,
      "text": "wa-ilayhā min mujāwaratil li-āmi mafarrī"
    },
    {
      "index": 24,
      "text": "waj`alil ḥayāta ziyādatan lī fī kulli khayr"
    },
    {
      "index": 25,
      "text": "walwafāta rāḥatan lī min kulli sharr"
    },
    {
      "index": 26,
      "text": "allāhumma ṣalli `alā muḥammad"
    },
    {
      "index": 27,
      "text": "khātamin nabiyyīn"
    },
    {
      "index": 28,
      "text": "watamāmi `iddatil mur-salīn"
    },
    {
      "index": 29,
      "text": "wa`alā-ālihiṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 30,
      "text": "wa-aṣ-ḥābihil muntajabīn"
    },
    {
      "index": 31,
      "text": "wahab lī fith thulathā-i thalāthā"
    },
    {
      "index": 32,
      "text": "lātada` lī dhanban illā ghafar-tah"
    },
    {
      "index": 33,
      "text": "walā ghamman illā adh-habtah"
    },
    {
      "index": 34,
      "text": "walā `aduwwan illā dafa`tah"
    },
    {
      "index": 35,
      "text": "bibismillāhi khayril asmā"
    },
    {
      "index": 36,
      "text": "bismillāhi rabbil ar-ḍi was samā"
    },
    {
      "index": 37,
      "text": "astadfi`u kulla makrūhin awwaluhu sakhaṭuh"
    },
    {
      "index": 38,
      "text": "wa-astajlibu kulla maḥbūbin awwaluhu riḍāh"
    },
    {
      "index": 39,
      "text": "fakhtim lī minka bil ghufrān"
    },
    {
      "index": 40,
      "text": "yā waliyyal iḥsān"
    }
  ]
}