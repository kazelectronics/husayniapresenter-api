{
  "index": 405,
  "name": "Sura 405",
  "ayas": [
    {
      "index": 1,
      "text": "walahu thiqatun warajā-un ghayruk",
      "bismillah": "allāhumma man aṣbaḥa wa-amsā"
    },
    {
      "index": 2,
      "text": "fa-anta thiqatī warajā-ī"
    },
    {
      "index": 3,
      "text": "yā ajwada man su-il"
    },
    {
      "index": 4,
      "text": "wayā ar-ḥama manistur-ḥim"
    },
    {
      "index": 5,
      "text": "ir-ḥam ḍa`fī wamaskanatī"
    },
    {
      "index": 6,
      "text": "waqillata ḥīlatī"
    }
  ]
}