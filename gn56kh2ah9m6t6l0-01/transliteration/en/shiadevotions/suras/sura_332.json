{
  "index": 332,
  "name": "Sura 332",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī aftatiḥul qawla biḥamdik"
    },
    {
      "index": 3,
      "text": "wa-anṭiqu bith thanā-i `alayk"
    },
    {
      "index": 4,
      "text": "wa umajjiduka walā ghāyata limad-ḥik"
    },
    {
      "index": 5,
      "text": "wa uthnī `alayka waman yablughu ghāyata thanā-ika wa-amada majdik"
    },
    {
      "index": 6,
      "text": "wa-annā likhalīqatika kunhu ma`rifati majdik"
    },
    {
      "index": 7,
      "text": "wa-ayya zamanin lam takun mamdūḥan bifaḍlik"
    },
    {
      "index": 8,
      "text": "mawṣūfan bimajdik"
    },
    {
      "index": 9,
      "text": "`awwādan `alal mudhnibīna biḥilmik"
    },
    {
      "index": 10,
      "text": "takhallafa sukkānu ar-ḍika `an ṭā`atik"
    },
    {
      "index": 11,
      "text": "fakunta `alayhim `aṭūfan bijūdik"
    },
    {
      "index": 12,
      "text": "jawādan bifaḍlik"
    },
    {
      "index": 13,
      "text": "`awwādan bikaramik"
    },
    {
      "index": 14,
      "text": "yā lā ilāha illā antal mannān"
    },
    {
      "index": 15,
      "text": "dhul jalāli wal-ikrām"
    }
  ]
}