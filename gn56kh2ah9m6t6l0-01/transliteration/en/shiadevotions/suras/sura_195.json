{
  "index": 195,
  "name": "Sura 195",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluka bi-anna lakal ḥamd"
    },
    {
      "index": 3,
      "text": "waḥdaka lāsharīka lak"
    },
    {
      "index": 4,
      "text": "wa-annaka wāḥidun aḥadun ṣamad"
    },
    {
      "index": 5,
      "text": "lam talid walam tūlad"
    },
    {
      "index": 6,
      "text": "walam yakun laka kufuwan aḥad"
    },
    {
      "index": 7,
      "text": "wa-anna muḥammadan `abduka warasūluk"
    },
    {
      "index": 8,
      "text": "ṣalawātuka `alayhi wa ālih"
    },
    {
      "index": 9,
      "text": "yā man huwa kulla yawmin fī sha-n"
    },
    {
      "index": 10,
      "text": "kamā kāna min sha-nika an tafaḍḍalta `alayy"
    },
    {
      "index": 11,
      "text": "bi-an ja`altanī min ahli ijābatik"
    },
    {
      "index": 12,
      "text": "wa-ahli dīnik"
    },
    {
      "index": 13,
      "text": "wa-ahli da`watik"
    },
    {
      "index": 14,
      "text": "wawaffaqtanī lidhālika fī mubtada-i khalqī"
    },
    {
      "index": 15,
      "text": "tafaḍḍulan minka wakaraman wajūdā"
    },
    {
      "index": 16,
      "text": "thumma ar-daftal faḍla faḍlā"
    },
    {
      "index": 17,
      "text": "waljūda jūdā"
    },
    {
      "index": 18,
      "text": "walkarama karamā"
    },
    {
      "index": 19,
      "text": "ra-fatan minka waraḥmah"
    },
    {
      "index": 20,
      "text": "ilā an jaddadta dhālikal `ahda lī tajdīdā"
    },
    {
      "index": 21,
      "text": "ba`da tajdīdika khalqī"
    },
    {
      "index": 22,
      "text": "wakuntu nasyan mansiyyā"
    },
    {
      "index": 23,
      "text": "nāsiyan sāhiyan ghāfilā"
    },
    {
      "index": 24,
      "text": "fa-atmamta ni`matak"
    },
    {
      "index": 25,
      "text": "bi-an dhakkar-tanī dhālik"
    },
    {
      "index": 26,
      "text": "wamananta bihi `alayy"
    },
    {
      "index": 27,
      "text": "wahadaytanī lah"
    },
    {
      "index": 28,
      "text": "falyakun min sha-nik"
    },
    {
      "index": 29,
      "text": "yā ilāhī wasayyidī wamawlāy"
    },
    {
      "index": 30,
      "text": "an tutimma lī dhālika walā taslubnīh"
    },
    {
      "index": 31,
      "text": "ḥattā tatawaffānī `alā dhālik"
    },
    {
      "index": 32,
      "text": "wa-anta `annī rāḍ"
    },
    {
      "index": 33,
      "text": "fa-innaka aḥaqqul mun`imīn"
    },
    {
      "index": 34,
      "text": "an tutimma ni`mataka `alayy"
    },
    {
      "index": 35,
      "text": "allāhumma sami`nā wa-aṭa`nā"
    },
    {
      "index": 36,
      "text": "wa-ajabnā dā`iyaka bimannik"
    },
    {
      "index": 37,
      "text": "falakal ḥamd"
    },
    {
      "index": 38,
      "text": "ghufrānaka rabbanā"
    },
    {
      "index": 39,
      "text": "wa-ilaykal maṣīr"
    },
    {
      "index": 40,
      "text": "āmannā billāhi waḥdah"
    },
    {
      "index": 41,
      "text": "lāsharīka lah"
    },
    {
      "index": 42,
      "text": "wabirasūlihi muḥammad"
    },
    {
      "index": 43,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 44,
      "text": "waṣaddaqnā wa-ajabnā dā`iyallāh"
    },
    {
      "index": 45,
      "text": "wattaba`nar rasūla fī muwālāti mawlānā"
    },
    {
      "index": 46,
      "text": "wamawlal mu-minīn"
    },
    {
      "index": 47,
      "text": "amīril mu-minīn"
    },
    {
      "index": 48,
      "text": "`aliyyibni abī ṭālib"
    },
    {
      "index": 49,
      "text": "`abdillāhi wa-akhī rasūlih"
    },
    {
      "index": 50,
      "text": "waṣ ṣiddīqil akbar"
    },
    {
      "index": 51,
      "text": "walḥujjati `alā bariyyatih"
    },
    {
      "index": 52,
      "text": "al mu-ayyidi bihi nabiyyah"
    },
    {
      "index": 53,
      "text": "wadīnahul ḥaqqal mubīn"
    },
    {
      "index": 54,
      "text": "`alaman lidīnillāh"
    },
    {
      "index": 55,
      "text": "wakhāzinan li`ilmih"
    },
    {
      "index": 56,
      "text": "wa`aybata ghaybillāh"
    },
    {
      "index": 57,
      "text": "wamawḍi`a sirrillāh"
    },
    {
      "index": 58,
      "text": "wa-amīnallāhi `alā khalqih"
    },
    {
      "index": 59,
      "text": "washāhidahu fī bariyyatih"
    },
    {
      "index": 60,
      "text": "allāhumma rabbanā innanā sami`nā munādiyā"
    },
    {
      "index": 61,
      "text": "yunādī lil-īmāni an-āminū birabbikum fa-āmannā"
    },
    {
      "index": 62,
      "text": "rabbanā faghfir lanā dhunūbanā"
    },
    {
      "index": 63,
      "text": "wakaffir `annā sayyi-ātinā"
    },
    {
      "index": 64,
      "text": "watawaffanā ma`al abrār"
    },
    {
      "index": 65,
      "text": "rabbanā wa ātinā mā wa`adtanā `alā rusulik"
    },
    {
      "index": 66,
      "text": "walā tukhzinā yawmal qiyāmah"
    },
    {
      "index": 67,
      "text": "innaka lātukhliful mī`ād"
    },
    {
      "index": 68,
      "text": "fa-innā yā rabbanā bimannika waluṭfik"
    },
    {
      "index": 69,
      "text": "ajabnā dā`iyak"
    },
    {
      "index": 70,
      "text": "wattaba`nar rasūla waṣaddaqnāh"
    },
    {
      "index": 71,
      "text": "waṣaddaqnā mawlal mu-minīn"
    },
    {
      "index": 72,
      "text": "wakafar-nā bil jibti waṭ ṭāghūt"
    },
    {
      "index": 73,
      "text": "fawallinā mā tawallaynā"
    },
    {
      "index": 74,
      "text": "waḥshur-nā ma`a a-immatinā"
    },
    {
      "index": 75,
      "text": "fa-innā bihim mu-minūna mūqinūn"
    },
    {
      "index": 76,
      "text": "walahum musallimūn"
    },
    {
      "index": 77,
      "text": "āmannā bisirrihim wa`alāniyatihim"
    },
    {
      "index": 78,
      "text": "washāhidihim waghā-ibihim"
    },
    {
      "index": 79,
      "text": "waḥayyihim wamayyitihim"
    },
    {
      "index": 80,
      "text": "waraḍīnā bihim a-immah"
    },
    {
      "index": 81,
      "text": "waqādatan wasādah"
    },
    {
      "index": 82,
      "text": "waḥasbunā bihim"
    },
    {
      "index": 83,
      "text": "baynanā wabaynallāhi dūna khalqih"
    },
    {
      "index": 84,
      "text": "lā nabtaghī bihim badalā"
    },
    {
      "index": 85,
      "text": "walā nattakhidhu min dūnihim walījah"
    },
    {
      "index": 86,
      "text": "wabari-nā ilallāh"
    },
    {
      "index": 87,
      "text": "min kulli man naṣaba lahum ḥar-bā"
    },
    {
      "index": 88,
      "text": "minal jinni wal-ins"
    },
    {
      "index": 89,
      "text": "minal awwalīna wal-ākhirīn"
    },
    {
      "index": 90,
      "text": "wakafar-nā bil jibti waṭ ṭāghūt"
    },
    {
      "index": 91,
      "text": "wal-awthānil ar-ba`ah"
    },
    {
      "index": 92,
      "text": "wa-ashyā`ihim wa-atbā`ihim"
    },
    {
      "index": 93,
      "text": "wakulli man wālāhum"
    },
    {
      "index": 94,
      "text": "minal jinni wal-ins"
    },
    {
      "index": 95,
      "text": "min awwalid dahri ilā-ākhirih"
    },
    {
      "index": 96,
      "text": "allāhumma innā nush-hiduk"
    },
    {
      "index": 97,
      "text": "annā nadīnu bimā dāna bihi muḥammadun wa ālu muḥammad"
    },
    {
      "index": 98,
      "text": "ṣallallāhu `alayhi wa`alayhim"
    },
    {
      "index": 99,
      "text": "waqawlunā mā qālū"
    },
    {
      "index": 100,
      "text": "wadīnunā mā dānū bih"
    },
    {
      "index": 101,
      "text": "mā qālū bihi qulnā"
    },
    {
      "index": 102,
      "text": "wamā dānū bihi dinnā"
    },
    {
      "index": 103,
      "text": "wamā ankarū ankar-nā"
    },
    {
      "index": 104,
      "text": "waman wālū wālaynā"
    },
    {
      "index": 105,
      "text": "waman `ādū `ādaynā"
    },
    {
      "index": 106,
      "text": "waman la`anū la`annā"
    },
    {
      "index": 107,
      "text": "waman tabarra-ū minhu tabarra-nā minh"
    },
    {
      "index": 108,
      "text": "waman taraḥḥamū `alayhi taraḥḥamnā `alayh"
    },
    {
      "index": 109,
      "text": "āmannā wasallamnā"
    },
    {
      "index": 110,
      "text": "waraḍīnā wattaba`nā mawāliyanā"
    },
    {
      "index": 111,
      "text": "ṣalawātullāhi `alayhim"
    },
    {
      "index": 112,
      "text": "allāhumma fatammim lanā dhālika walā taslubnāh"
    },
    {
      "index": 113,
      "text": "waj`alhu mustaqirran thābitan `indanā"
    },
    {
      "index": 114,
      "text": "walā taj`alhu musta`ārā"
    },
    {
      "index": 115,
      "text": "wa-aḥyinā mā aḥyaytanā `alayh"
    },
    {
      "index": 116,
      "text": "wa-amitnā idhā amattanā `alayh"
    },
    {
      "index": 117,
      "text": "ālu muḥammadin a-immatunā"
    },
    {
      "index": 118,
      "text": "fabihim na-tamm"
    },
    {
      "index": 119,
      "text": "wa-iyyāhum nulī"
    },
    {
      "index": 120,
      "text": "wa`aduwwahum `aduwwallāhi nu`ādī"
    },
    {
      "index": 121,
      "text": "faj`alnā ma`ahum fid dunyā wal-ākhirah"
    },
    {
      "index": 122,
      "text": "waminal muqarrabīn"
    },
    {
      "index": 123,
      "text": "fa-innā bidhālika rāḍūn"
    },
    {
      "index": 124,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}