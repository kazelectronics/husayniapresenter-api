{
  "index": 81,
  "name": "Sura 81",
  "ayas": [
    {
      "index": 1,
      "text": "idhash shamsu kuwwirat",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wa idhan nujūmun-kadarat"
    },
    {
      "index": 3,
      "text": "wa idhal jibālu suyyirat"
    },
    {
      "index": 4,
      "text": "wa idhal `ishāru `uṭṭilat"
    },
    {
      "index": 5,
      "text": "wa idhal wuḥūshu ḥushirat"
    },
    {
      "index": 6,
      "text": "wa idhal biḥāru sujjirat"
    },
    {
      "index": 7,
      "text": "wa idhan nufūsu zuwwijat"
    },
    {
      "index": 8,
      "text": "wa idhal maw-ū-datu su-ilat"
    },
    {
      "index": 9,
      "text": "bi-ayyi dhanbin qutilat"
    },
    {
      "index": 10,
      "text": "wa idhaṣ ṣuḥufu nushirat"
    },
    {
      "index": 11,
      "text": "wa idhas samā-u kushiṭat"
    },
    {
      "index": 12,
      "text": "wa idhal jaḥīmu su``irat"
    },
    {
      "index": 13,
      "text": "wa idhal jannatu uzlifat"
    },
    {
      "index": 14,
      "text": "`alimat nafsun mā aḥḍarat"
    },
    {
      "index": 15,
      "text": "falā uqsimu bil khunnas"
    },
    {
      "index": 16,
      "text": "al-jawāril kunnas"
    },
    {
      "index": 17,
      "text": "wal layli idhā `as`as"
    },
    {
      "index": 18,
      "text": "waṣ ṣubḥi idhā tanaffas"
    },
    {
      "index": 19,
      "text": "innahu laqawlu rasūlin karīm"
    },
    {
      "index": 20,
      "text": "dhī quwwatin `inda dhil `arshi makīn"
    },
    {
      "index": 21,
      "text": "muṭā`in thamma amīn"
    },
    {
      "index": 22,
      "text": "wa mā ṣāḥibukum bimajnūn"
    },
    {
      "index": 23,
      "text": "walaqad ra-āhu bil-ufuqil mubīn"
    },
    {
      "index": 24,
      "text": "wa mā huwa `alal-ghaybi biḍanīn"
    },
    {
      "index": 25,
      "text": "wa mā huwa biqawli shayṭānin rajīm"
    },
    {
      "index": 26,
      "text": "fa-ayna tadh-habūn"
    },
    {
      "index": 27,
      "text": "in huwa illā dhikrul lil`ālamīn"
    },
    {
      "index": 28,
      "text": "liman shā-a minkum an yastaqīm"
    },
    {
      "index": 29,
      "text": "wa mā tashā-ūna illā an yashā-allāhu rabbul `ālamīn"
    }
  ]
}