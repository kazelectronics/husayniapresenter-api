{
  "index": 314,
  "name": "Sura 314",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alayka yā ḥujjatallāhi wakhāliṣatah",
      "bismillah": "as salāmu `alayka yā waliyyallāh"
    },
    {
      "index": 2,
      "text": "as salāmu `alayka yā imāmal mu-minīn"
    },
    {
      "index": 3,
      "text": "wawārithal mur-salīn"
    },
    {
      "index": 4,
      "text": "waḥujjata rabbil `ālamīn"
    },
    {
      "index": 5,
      "text": "ṣallallāhu `alayka wa`alā-āli baytik"
    },
    {
      "index": 6,
      "text": "aṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 7,
      "text": "yā mawlāya yā abā muḥammad"
    },
    {
      "index": 8,
      "text": "al ḥasanabna `aliyy"
    },
    {
      "index": 9,
      "text": "anā mawlan laka wali-āli baytik"
    },
    {
      "index": 10,
      "text": "wahādhā yawmuka wahuwa yawmul khamīs"
    },
    {
      "index": 11,
      "text": "wa-ana ḍayfuka fīhi wamustajīrun bika fīh"
    },
    {
      "index": 12,
      "text": "fa-aḥsin ḍiyāfatī wa-ijāratī"
    },
    {
      "index": 13,
      "text": "biḥaqqi-āli baytikaṭ ṭayyibīnal ṭāhirīn"
    }
  ]
}