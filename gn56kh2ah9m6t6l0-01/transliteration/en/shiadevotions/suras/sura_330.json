{
  "index": 330,
  "name": "Sura 330",
  "ayas": [
    {
      "index": 1,
      "text": "subḥāna man ta`aṭṭafa bil majdi watakarrama bih",
      "bismillah": "subḥāna man labisal `izza walwaqār"
    },
    {
      "index": 2,
      "text": "subḥāna man lā yanbaghit tasbīḥu illā lah"
    },
    {
      "index": 3,
      "text": "subḥāna man aḥṣā kulla shay-in `ilmuh"
    },
    {
      "index": 4,
      "text": "subḥāna dhil manni wan ni`am"
    },
    {
      "index": 5,
      "text": "subḥāna dhil qudrati walkaram"
    },
    {
      "index": 6,
      "text": "allāhumma innī as-aluka bima`āqidil `izzi min `ar-shik"
    },
    {
      "index": 7,
      "text": "wamuntahar raḥmati min kitābik"
    },
    {
      "index": 8,
      "text": "wa ismikal a`ẓam"
    },
    {
      "index": 9,
      "text": "wakalimātikat tāmmah"
    },
    {
      "index": 10,
      "text": "al latī tammat ṣidqan wa`adlā"
    },
    {
      "index": 11,
      "text": "ṣalli `alā muḥammadin wa-ahli baytih"
    },
    {
      "index": 12,
      "text": ""
    }
  ]
}