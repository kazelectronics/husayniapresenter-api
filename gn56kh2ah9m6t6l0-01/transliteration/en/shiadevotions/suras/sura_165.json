{
  "index": 165,
  "name": "Sura 165",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma biḥaqqi laylatinā hādhihi wamawlūdihā"
    },
    {
      "index": 3,
      "text": "waḥujjatika wamaw`ūdihā"
    },
    {
      "index": 4,
      "text": "al latī qaranta ilā faḍlihā faḍlā"
    },
    {
      "index": 5,
      "text": "fatammat kalimatuka ṣidqan wa`adlā"
    },
    {
      "index": 6,
      "text": "lāmubaddila likalimātik"
    },
    {
      "index": 7,
      "text": "walā mu`aqqiba li-āyātik"
    },
    {
      "index": 8,
      "text": "nūrukal muta-alliq"
    },
    {
      "index": 9,
      "text": "waḍiya-ukal mushriq"
    },
    {
      "index": 10,
      "text": "wal`alamun nūru fī ṭakhyā-id dayjūr"
    },
    {
      "index": 11,
      "text": "al ghā-ibul mastūr"
    },
    {
      "index": 12,
      "text": "jalla mawliduh, wakaruma maḥtiduh"
    },
    {
      "index": 13,
      "text": "walmalā-ikatu shuhhaduh"
    },
    {
      "index": 14,
      "text": "wallāhu nāṣiruhu wamu-ayyiduh"
    },
    {
      "index": 15,
      "text": "idhā-āna mī`āduh, walmalā-ikatu amdāduh"
    },
    {
      "index": 16,
      "text": "sayfullāhil ladhī lā yanbū"
    },
    {
      "index": 17,
      "text": "wanūruhul ladhī lāyakhbū"
    },
    {
      "index": 18,
      "text": "wadhul ḥilmil ladhī lāyaṣbū"
    },
    {
      "index": 19,
      "text": "madārud dahr"
    },
    {
      "index": 20,
      "text": "wanawāmīsul `aṣr"
    },
    {
      "index": 21,
      "text": "wawulātul amr"
    },
    {
      "index": 22,
      "text": "walmunazzalu `alayhim mā yatanazzalu fī laylatil qadr"
    },
    {
      "index": 23,
      "text": "wa-aṣ-ḥābul ḥashri wan nashr"
    },
    {
      "index": 24,
      "text": "tarājimatu waḥyih"
    },
    {
      "index": 25,
      "text": "wawulātu amrihi wanahyih"
    },
    {
      "index": 26,
      "text": "allāhumma faṣalli `alā khātimihim waqā-imihim"
    },
    {
      "index": 27,
      "text": "al mastūri `an `awālimihim"
    },
    {
      "index": 28,
      "text": "allāhumma wa-adrik binā ayyāmah"
    },
    {
      "index": 29,
      "text": "waẓuhūrahu waqiyāmah"
    },
    {
      "index": 30,
      "text": "waj`alnā min anṣārih"
    },
    {
      "index": 31,
      "text": "waqrin thāranā bithārih"
    },
    {
      "index": 32,
      "text": "waktubnā fī a`wānihi wakhulaṣā-ih"
    },
    {
      "index": 33,
      "text": "wa-aḥyinā fī dawlatihi nā`imīn"
    },
    {
      "index": 34,
      "text": "wabiṣuḥbatihi ghānimīn"
    },
    {
      "index": 35,
      "text": "wabiḥaqqihi qā-imīn"
    },
    {
      "index": 36,
      "text": "waminas sū-i sālimīn"
    },
    {
      "index": 37,
      "text": "yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 38,
      "text": "walḥamdu lillāhi rabbil `ālamīn"
    },
    {
      "index": 39,
      "text": "waṣalawātuhu `alā sayyidinā muḥammad"
    },
    {
      "index": 40,
      "text": "khātamin nabiyyīna walmur-salīn"
    },
    {
      "index": 41,
      "text": "wa`alā ahli baytihiṣ ṣādiqīn"
    },
    {
      "index": 42,
      "text": "wa`itratihin nāṭiqīn"
    },
    {
      "index": 43,
      "text": "wal`an jamī`aẓ ẓālimīn"
    },
    {
      "index": 44,
      "text": "waḥkum baynanā wabaynahum"
    },
    {
      "index": 45,
      "text": "yā aḥkamal ḥākimīn"
    }
  ]
}