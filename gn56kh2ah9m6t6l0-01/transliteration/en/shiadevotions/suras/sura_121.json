{
  "index": 121,
  "name": "Sura 121",
  "ayas": [
    {
      "index": 1,
      "text": "imtaḥanakillāhul ladhī khalaqak",
      "bismillah": "yā mumtaḥanah"
    },
    {
      "index": 2,
      "text": "qabla an yakhluqak"
    },
    {
      "index": 3,
      "text": "fawajadaki limamtaḥanaki ṣābirah"
    },
    {
      "index": 4,
      "text": "waza`amnā annā laki awliyā-un wa muṣaddiqūn"
    },
    {
      "index": 5,
      "text": "waṣābirūna likulli mā atānā bihi abūk"
    },
    {
      "index": 6,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 7,
      "text": "wa-atā bihi waṣiyyuh"
    },
    {
      "index": 8,
      "text": "fa-innā nas-aluki in kunnā ṣaddaqnāk"
    },
    {
      "index": 9,
      "text": "illā alḥaqtinā bitaṣdīqinā lahumā"
    },
    {
      "index": 10,
      "text": "linubashhira anfusanā bi-annā qad ṭahur-nā biwilāyatik"
    },
    {
      "index": 11,
      "text": "as salāmu `alayki yā binta rasūlillāh"
    },
    {
      "index": 12,
      "text": "as salāmu `alayki yā binta nabiyyillāh"
    },
    {
      "index": 13,
      "text": "as salāmu `alayki yā binta ḥabībillāh"
    },
    {
      "index": 14,
      "text": "as salāmu `alayki yā binta khalīlillāh"
    },
    {
      "index": 15,
      "text": "as salāmu `alayki yā binta ṣafiyyillāh"
    },
    {
      "index": 16,
      "text": "as salāmu `alayki yā binta amīnillāh"
    },
    {
      "index": 17,
      "text": "as salāmu `alayki yā binta khayri khalqillāh"
    },
    {
      "index": 18,
      "text": "as salāmu `alayki yā binta afḍali anbiyā-illāhi warusulihi wamalā-ikatih"
    },
    {
      "index": 19,
      "text": "as salāmu `alayki yā binta khayril bariyyah"
    },
    {
      "index": 20,
      "text": "as salāmu `alayki yā sayyidata nisā-il `ālamīn"
    },
    {
      "index": 21,
      "text": "minal awwalīna wal-ākhirīn"
    },
    {
      "index": 22,
      "text": "as salāmu `alayki yā zawjata waliyyillāh"
    },
    {
      "index": 23,
      "text": "wa khayril khalqi ba`da rasūlillāh"
    },
    {
      "index": 24,
      "text": "as salāmu `alayki yā ummal ḥasani walḥusayn"
    },
    {
      "index": 25,
      "text": "sayyiday shabābi ahlil jannah"
    },
    {
      "index": 26,
      "text": "as salāmu `alayki ayyatuhaṣ ṣiddīqatush shahīdah"
    },
    {
      "index": 27,
      "text": "as salāmu `alayki ayyatuhar raḍiyyatul mar-ḍiyyah"
    },
    {
      "index": 28,
      "text": "as salāmu `alayki ayyatuhal fāḍilatuz zakiyyah"
    },
    {
      "index": 29,
      "text": "as salāmu `alayki ayyatuhal ḥawrā-ul insiyyah"
    },
    {
      "index": 30,
      "text": "as salāmu `alayki ayyatuhat taqiyyatun naqiyyah"
    },
    {
      "index": 31,
      "text": "as salāmu `alayki ayyatuhal muḥaddathatul `alīmah"
    },
    {
      "index": 32,
      "text": "as salāmu `alayki ayyatuhal maẓlūmatul maghṣūbah"
    },
    {
      "index": 33,
      "text": "as salāmu `alayki ayyatuhal muḍṭahadatul maqhūrah"
    },
    {
      "index": 34,
      "text": "as salāmu `alayki yā fāṭimatu binta rasūlillāh"
    },
    {
      "index": 35,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 36,
      "text": "ṣallallāhu `alayk"
    },
    {
      "index": 37,
      "text": "wa`alā rūḥiki wabadanik"
    },
    {
      "index": 38,
      "text": "ash-hadu annaki maḍayti `alā bayyinatin min rabbik"
    },
    {
      "index": 39,
      "text": "wa-anna man sarraki faqad sarra rasūlallāh"
    },
    {
      "index": 40,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 41,
      "text": "waman jafāki faqad jafā rasūlallāh"
    },
    {
      "index": 42,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 43,
      "text": "waman-ādhāki faqad-ādhā rasūlallāh"
    },
    {
      "index": 44,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 45,
      "text": "waman waṣalaki faqad waṣala rasūlallāh"
    },
    {
      "index": 46,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 47,
      "text": "waman qaṭa`aki faqad qaṭa`a rasūlallāh"
    },
    {
      "index": 48,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 49,
      "text": "li-annaki biḍ`atun minh"
    },
    {
      "index": 50,
      "text": "warūḥuhul ladhī bayna janbayh"
    },
    {
      "index": 51,
      "text": "ush-hidullāha warusulahu wamalā-ikatah"
    },
    {
      "index": 52,
      "text": "annī rāḍin `amman raḍīti `anh"
    },
    {
      "index": 53,
      "text": "sākhiṭun `alā man sakhiṭti `alayh"
    },
    {
      "index": 54,
      "text": "matabarri-un mimman tabarra-ti minh"
    },
    {
      "index": 55,
      "text": "muwālin liman wālayt"
    },
    {
      "index": 56,
      "text": "mu`ādin liman `ādayt"
    },
    {
      "index": 57,
      "text": "mubghiḍun liman abghaḍt"
    },
    {
      "index": 58,
      "text": "muḥibbun liman aḥbabt"
    },
    {
      "index": 59,
      "text": "wakafā billāhi shahīdan wa ḥasībā"
    },
    {
      "index": 60,
      "text": "wa jāziyan wa muthībā"
    }
  ]
}