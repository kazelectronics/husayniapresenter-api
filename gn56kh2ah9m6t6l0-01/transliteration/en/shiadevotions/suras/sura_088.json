{
  "index": 88,
  "name": "Sura 88",
  "ayas": [
    {
      "index": 1,
      "text": "hal atāka ḥadīthul ghāshiyah",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wujūhun yawma-idhin khāshi`ah"
    },
    {
      "index": 3,
      "text": "`āmilatun nāṣibah"
    },
    {
      "index": 4,
      "text": "taṣlā nāran ḥāmiyah"
    },
    {
      "index": 5,
      "text": "tusqā min `aynin āniyah"
    },
    {
      "index": 6,
      "text": "laysa lahum ṭa`āmun illā min ḍarī`"
    },
    {
      "index": 7,
      "text": "lā yusminu walā yughnī min jū`"
    },
    {
      "index": 8,
      "text": "wujūhun yawma-idhin nā`imah"
    },
    {
      "index": 9,
      "text": "lisa`yihā rāḍiyah"
    },
    {
      "index": 10,
      "text": "fī jannatin `āliyah"
    },
    {
      "index": 11,
      "text": "lā tasma`u fīhā lāghiyah"
    },
    {
      "index": 12,
      "text": "fīhā `aynun jāriyah"
    },
    {
      "index": 13,
      "text": "fīhā sururun marfū`ah"
    },
    {
      "index": 14,
      "text": "wa-akwābun mawḍū`ah"
    },
    {
      "index": 15,
      "text": "wanamāriqu maṣfūfah"
    },
    {
      "index": 16,
      "text": "wazarābiyyu mabthūthah"
    },
    {
      "index": 17,
      "text": "afalā yanẓurūna ilal-ibili kayfa khuliqat"
    },
    {
      "index": 18,
      "text": "wa ilas samā-i kayfa rufi`at"
    },
    {
      "index": 19,
      "text": "wa ilal-jibāli kayfa nuṣibat"
    },
    {
      "index": 20,
      "text": "wa ilal-arḍi kayfa suṭiḥat"
    },
    {
      "index": 21,
      "text": "fadhakkir innamā anta mudhakkir"
    },
    {
      "index": 22,
      "text": "lasta `alayhim bimuṣayṭir"
    },
    {
      "index": 23,
      "text": "illā man tawallā wakafar"
    },
    {
      "index": 24,
      "text": "fayu`adhhibuhullāhul `adhābal-akbar"
    },
    {
      "index": 25,
      "text": "inna ilaynā iyābahum"
    },
    {
      "index": 26,
      "text": "thumma inna `alaynā ḥisābahum"
    }
  ]
}