{
  "index": 219,
  "name": "Sura 219",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innā na`ūdhu bik"
    },
    {
      "index": 3,
      "text": "min nazaghātish shayṭānir rajīmi wakaydihi wamakāyidih"
    },
    {
      "index": 4,
      "text": "waminath thiqati bi-amāniyyihi wamawā`īdihi waghurūrihi wamaṣāyidih"
    },
    {
      "index": 5,
      "text": "wa-an yuṭmi`a nafsahu fī iḍlālinā"
    },
    {
      "index": 6,
      "text": "`an ṭā`atika wamtihāninā bima`ṣiyatik"
    },
    {
      "index": 7,
      "text": "aw an yaḥsuna `indanā mā ḥassana lanā"
    },
    {
      "index": 8,
      "text": "aw an yathqula `alaynā mā karraha ilaynā"
    },
    {
      "index": 9,
      "text": "allāhummakhsa-hu `annā bi`ibādatik"
    },
    {
      "index": 10,
      "text": "wakbit-hu bidu-ūbinā fī maḥabbatik"
    },
    {
      "index": 11,
      "text": "waj`al baynanā wabaynahu sitran lā yahtikuh"
    },
    {
      "index": 12,
      "text": "waradman muṣmitan lā yaftuquh"
    },
    {
      "index": 13,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 14,
      "text": "washghalhu `annā biba`ḍi a`dā-ik"
    },
    {
      "index": 15,
      "text": "wa`ṣimnā minhu biḥusni ri`āyatika wakfinā khatrah"
    },
    {
      "index": 16,
      "text": "wawallinā ẓahrahu waqṭa` `annā ithrah"
    },
    {
      "index": 17,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 18,
      "text": "wa-amti`nā minal hudā bimithli ḍalālatih"
    },
    {
      "index": 19,
      "text": "wazawwidnā minat taqwā ḍidda ghawāyatih"
    },
    {
      "index": 20,
      "text": "wasluk binā minat tuqā khilāfa sabīlihi minar radā"
    },
    {
      "index": 21,
      "text": "allāhumma lā taj`al lahu fī qulūbinā madkhalā"
    },
    {
      "index": 22,
      "text": "walā tūṭinanna lahu fīmā ladaynā manzilā"
    },
    {
      "index": 23,
      "text": "allāhumma wamā sawwala lanā min bāṭilin fa`arrifnāh"
    },
    {
      "index": 24,
      "text": "wa-idhā `arraftanāhu faqināhu wabaṣṣir-nā mā nukāyiduhu bih"
    },
    {
      "index": 25,
      "text": "wa-alhimnā mā nu`idduhu lahu wa-ayqiẓnā `an sinatil ghaflati birrukūni ilayh"
    },
    {
      "index": 26,
      "text": "wa-aḥsin bitawfīqika `awnanā `alayh"
    },
    {
      "index": 27,
      "text": "allāhumma wa-ashrib qulūbanā inkāra `amalih"
    },
    {
      "index": 28,
      "text": "walṭuf lanā fī naqḍi ḥiyalih"
    },
    {
      "index": 29,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālihi waḥawwil sulṭānahu `annā"
    },
    {
      "index": 30,
      "text": "waqṭa` rajā-ahu minnā wadra-hu `anil wulū`i binā"
    },
    {
      "index": 31,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 32,
      "text": "waj`al-ābā-anā wa-ummahātinā wa-awlādanā wa-ahāliyanā"
    },
    {
      "index": 33,
      "text": "wadhawī ar-ḥāminā waqarābātinā wajīrānanā"
    },
    {
      "index": 34,
      "text": "minal mu-minīna walmu-mināti minhu fī ḥir-zin ḥāriz"
    },
    {
      "index": 35,
      "text": "waḥiṣnin ḥāfiẓin wakahfin māni`in wa-albis-hum minhu junanan wāqiyah"
    },
    {
      "index": 36,
      "text": "wa-a`ṭihim `alayhi asliḥatan māḍiyah"
    },
    {
      "index": 37,
      "text": "allāhumma wa`mum bidhālika man shahida laka birrubūbiyyah"
    },
    {
      "index": 38,
      "text": "wa-akhlaṣa laka bil waḥdāniyyah"
    },
    {
      "index": 39,
      "text": "wa`ādāhu laka biḥaqīqatil `ubūdiyyah"
    },
    {
      "index": 40,
      "text": "wastaẓhara bika `alayhi fī ma`rifatil `ulūmir rabbāniyyah"
    },
    {
      "index": 41,
      "text": "allāhummaḥlul mā `aqada waftuq mā rataq"
    },
    {
      "index": 42,
      "text": "wafsakh mā dabbara wathabbiṭ-hu idhā `azama wanquḍ mā abram"
    },
    {
      "index": 43,
      "text": "allāhumma wahzim jundahu wa-abṭil kaydah"
    },
    {
      "index": 44,
      "text": "wahdim kahfahu wa-ar-ghim anfah"
    },
    {
      "index": 45,
      "text": "allāhummaj`alnā fī naẓmi a`dā-ih"
    },
    {
      "index": 46,
      "text": "wa`zilnā `an `idādi awliyā-ih"
    },
    {
      "index": 47,
      "text": "lā nuṭī`u lahu idhastahwānā"
    },
    {
      "index": 48,
      "text": "walā nastajību lahu idhā da`ānā"
    },
    {
      "index": 49,
      "text": "na-muru bimunāwātihi man aṭā`a amranā"
    },
    {
      "index": 50,
      "text": "wana`iẓu `an mutāba`atihi manittaba`a zajranā"
    },
    {
      "index": 51,
      "text": "allāhumma ṣalli `alā muḥammadin khātamin nabiyyīna wasayyidil mur-salīn"
    },
    {
      "index": 52,
      "text": "wa`alā ahli baytihiṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 53,
      "text": "wa-a`idhnā wa-ahāliyanā wa-ikhwānanā"
    },
    {
      "index": 54,
      "text": "wajamī`al mu-minīna walmu-mināti mimmasta`adhnā minh"
    },
    {
      "index": 55,
      "text": "wa-ajir-nā mimmastajar-nā bika min khawfih"
    },
    {
      "index": 56,
      "text": "wasma` lanā mā da`awnā bihi wa-a`ṭinā mā aghfalnāhu waḥfaẓ lanā mā nasīnāh"
    },
    {
      "index": 57,
      "text": "waṣayyir-nā bidhālika fī darajātiṣ ṣāliḥīn"
    },
    {
      "index": 58,
      "text": "wamarātibil mu-minīn"
    },
    {
      "index": 59,
      "text": "āmīna rabbal `ālamīn"
    }
  ]
}