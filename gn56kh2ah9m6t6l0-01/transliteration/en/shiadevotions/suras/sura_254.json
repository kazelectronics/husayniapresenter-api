{
  "index": 254,
  "name": "Sura 254",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "subḥānakallāhumma waḥanānayk"
    },
    {
      "index": 3,
      "text": "subḥānakallāhumma wata`ālayt"
    },
    {
      "index": 4,
      "text": "subḥānakallāhumma wal `izzu izāruk"
    },
    {
      "index": 5,
      "text": "subḥānakallāhumma wal`aẓamatu rida-uk"
    },
    {
      "index": 6,
      "text": "subḥānakallāhumma walkibriyā-u sulṭānuk"
    },
    {
      "index": 7,
      "text": "subḥānaka min `aẓīmi mā a`ẓamak"
    },
    {
      "index": 8,
      "text": "subḥānaka subbiḥta fil mala-il a`lā, tasma`u watarā mā taḥtath tharā"
    },
    {
      "index": 9,
      "text": "subḥānaka anta shāhidu kulli najwā"
    },
    {
      "index": 10,
      "text": "subḥānaka mawḍi`u kulli shakwā"
    },
    {
      "index": 11,
      "text": "subḥānaka ḥāḍiru kulli mala"
    },
    {
      "index": 12,
      "text": "subḥānaka `aẓīmur rajā"
    },
    {
      "index": 13,
      "text": "subḥānaka tarā mā fī qa`ril mā"
    },
    {
      "index": 14,
      "text": "subḥānaka tasma`u anfāsal ḥītāni fī qu`ūril biḥār"
    },
    {
      "index": 15,
      "text": "subḥānaka ta`lamu waznas samawāt"
    },
    {
      "index": 16,
      "text": "subḥānaka ta`lamu waznal araḍīn"
    },
    {
      "index": 17,
      "text": "subḥānaka ta`lamu waznash shamsi wal qamar"
    },
    {
      "index": 18,
      "text": "subḥānaka ta`lamu waznaẓ ẓulmati wan nūr"
    },
    {
      "index": 19,
      "text": "subḥānaka ta`lamu waznal fay-i walhawā"
    },
    {
      "index": 20,
      "text": "subḥānaka ta`lamu waznar rīḥi kam hiya min mithqāli dharrah"
    },
    {
      "index": 21,
      "text": "subḥānaka quddūsun quddūsun quddūs"
    },
    {
      "index": 22,
      "text": "subḥānaka `ajaban man `arafaka kayfa lā yakhāfuk"
    },
    {
      "index": 23,
      "text": "subḥānakallāhumma wabiḥamdik"
    },
    {
      "index": 24,
      "text": "subḥānakallāhul `aliyyul `aẓīm"
    }
  ]
}