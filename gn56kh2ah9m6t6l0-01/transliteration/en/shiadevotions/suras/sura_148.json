{
  "index": 148,
  "name": "Sura 148",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluka bi-annaka malik"
    },
    {
      "index": 3,
      "text": "wa-annaka `alā kulli shay-in muqtadir"
    },
    {
      "index": 4,
      "text": "wa-annaka mā tashā-u min amrin yakūn"
    },
    {
      "index": 5,
      "text": "allāhumma innī atawajjahu ilayka binabiyyika muḥammad"
    },
    {
      "index": 6,
      "text": "nabiyyir raḥmati ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 7,
      "text": "yā muḥammad"
    },
    {
      "index": 8,
      "text": "yā rasūlallāh"
    },
    {
      "index": 9,
      "text": "innī atawajjahu bika ilallāhi rabbika warabbī"
    },
    {
      "index": 10,
      "text": "liyunjiḥa lī bika ṭalibatī"
    },
    {
      "index": 11,
      "text": "allāhumma binabiyyika muḥammad"
    },
    {
      "index": 12,
      "text": "wal-a-immati min ahli baytih"
    },
    {
      "index": 13,
      "text": "ṣallallāhu `alayhi wa`alayhim"
    },
    {
      "index": 14,
      "text": "anjiḥ ṭalibatī"
    }
  ]
}