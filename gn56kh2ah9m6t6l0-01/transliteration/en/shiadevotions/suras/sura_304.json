{
  "index": 304,
  "name": "Sura 304",
  "ayas": [
    {
      "index": 1,
      "text": "ash-hadu an la-ilāha illallāh",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "waḥdahu lāsharīka lah"
    },
    {
      "index": 3,
      "text": "wa-ash-hadu annaka rasūluh"
    },
    {
      "index": 4,
      "text": "wa-annaka muḥammadubnu `abdillāh"
    },
    {
      "index": 5,
      "text": "wa-ash-hadu annaka qad ballaghta risālāti rabbik"
    },
    {
      "index": 6,
      "text": "wanaṣaḥta li-ummatik"
    },
    {
      "index": 7,
      "text": "wajāhadta fī sabīlillāh"
    },
    {
      "index": 8,
      "text": "bil ḥikmati wāl-maw`iẓatil ḥasanah"
    },
    {
      "index": 9,
      "text": "wa-addaytal ladhī `alayka minal ḥaqq"
    },
    {
      "index": 10,
      "text": "wa-annaka qad ra-ufta bil mu-minīn"
    },
    {
      "index": 11,
      "text": "waghalaẓta `alal kāfirīn"
    },
    {
      "index": 12,
      "text": "wa`abadtallāha mukhliṣā"
    },
    {
      "index": 13,
      "text": "ḥattā atyākal yaqīn"
    },
    {
      "index": 14,
      "text": "fabalaghallāhu bika ashrafa maḥallil mukarramīn"
    },
    {
      "index": 15,
      "text": "al ḥamdu lillāhil ladhistanqadhanā bik"
    },
    {
      "index": 16,
      "text": "minash shir-ki waḍ ḍalāl"
    },
    {
      "index": 17,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 18,
      "text": "waj`al ṣalawātika waṣalawāti malā-ikatik"
    },
    {
      "index": 19,
      "text": "wa-anbiyā-ika walmur-salīn"
    },
    {
      "index": 20,
      "text": "wa`ibādikaṣ ṣāliḥīn"
    },
    {
      "index": 21,
      "text": "wa-ahlis samāwāti wal-araḍīn"
    },
    {
      "index": 22,
      "text": "waman sabbaḥa laka yā rabbal `ālamīn"
    },
    {
      "index": 23,
      "text": "minal awwalīna wal-ākhirīn"
    },
    {
      "index": 24,
      "text": "`alā muḥammadin `abdika warasūlik"
    },
    {
      "index": 25,
      "text": "wanabiyyika wa-amīnik"
    },
    {
      "index": 26,
      "text": "wanajībika waḥabībik"
    },
    {
      "index": 27,
      "text": "waṣafiyyika waṣifwatik"
    },
    {
      "index": 28,
      "text": "wakhāṣṣatika wakhāliṣatik"
    },
    {
      "index": 29,
      "text": "wakhiyaratika min khalqik"
    },
    {
      "index": 30,
      "text": "wa-a`ṭihil faḍla walfaḍīlah"
    },
    {
      "index": 31,
      "text": "walwasīlata wad darajatar rafī`ah"
    },
    {
      "index": 32,
      "text": "wab`ath-hu maqāman maḥmūdā"
    },
    {
      "index": 33,
      "text": "yaghbiṭuhu bihil awwalūna wal-ākhirūn"
    },
    {
      "index": 34,
      "text": "allāhumma innaka qult"
    },
    {
      "index": 35,
      "text": "walaw annahum idh ẓalamū anfusahum"
    },
    {
      "index": 36,
      "text": "ja-uka fastaghfarullāh"
    },
    {
      "index": 37,
      "text": "wastaghfara lahumur rasūl"
    },
    {
      "index": 38,
      "text": "lawajadullāha tawwāban raḥīmā"
    },
    {
      "index": 39,
      "text": "ilāhī faqad ataytu nabiyyaka mustaghfirā"
    },
    {
      "index": 40,
      "text": "tā-iban min dhunūbī"
    },
    {
      "index": 41,
      "text": "faṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 42,
      "text": "waghfir-hā lī"
    },
    {
      "index": 43,
      "text": "yāsayyidanā"
    },
    {
      "index": 44,
      "text": "atawajjahu bika wabi-ahli baytik"
    },
    {
      "index": 45,
      "text": "ilallāhi ta`ālā rabbika warabbī"
    },
    {
      "index": 46,
      "text": "liyaghfira lī"
    },
    {
      "index": 47,
      "text": "innā lillāhi wa-innā ilayhi rāji`ūn"
    },
    {
      "index": 48,
      "text": "innā lillāhi wa-innā ilayhi rāji`ūn"
    },
    {
      "index": 49,
      "text": "innā lillāhi wa-innā ilayhi rāji`ūn"
    },
    {
      "index": 50,
      "text": "uṣibnā bika yā ḥabība qulūbinā"
    },
    {
      "index": 51,
      "text": "famā a`ẓamal muṣībata bik"
    },
    {
      "index": 52,
      "text": "ḥaythunqaṭa`a `annal waḥy"
    },
    {
      "index": 53,
      "text": "waḥaythu faqadnāk"
    },
    {
      "index": 54,
      "text": "fa-innā lillāhi wa-innā ilayhi rāji`ūn"
    },
    {
      "index": 55,
      "text": "yā sayyidanā yā rasūlallāh"
    },
    {
      "index": 56,
      "text": "ṣalawātullāhi `alayka wa`alā-āli baytikaṭ ṭāhirīn"
    },
    {
      "index": 57,
      "text": "hādhā yawmus sabti wahuwa yawmuk"
    },
    {
      "index": 58,
      "text": "wa-ana fīhi ḍayfuka wajāruk"
    },
    {
      "index": 59,
      "text": "fa-aḍifnī wa-ajir-nī"
    },
    {
      "index": 60,
      "text": "fa-innaka karīmun tuḥibbuḍ ḍiyāfah"
    },
    {
      "index": 61,
      "text": "wama-mūrun bil ijārah"
    },
    {
      "index": 62,
      "text": "fa-aḍifnī wa-aḥsin ḍiyāfatī"
    },
    {
      "index": 63,
      "text": "wa-ajir-nā wa-aḥsin ijāratanā"
    },
    {
      "index": 64,
      "text": "bimanzilatillāhi `indaka wa`inda-āli baytik"
    },
    {
      "index": 65,
      "text": "wabimanzilatihim `indah"
    },
    {
      "index": 66,
      "text": "wabimastawda`akum min `ilmih"
    },
    {
      "index": 67,
      "text": "fa-innahu akramul akramīn"
    }
  ]
}