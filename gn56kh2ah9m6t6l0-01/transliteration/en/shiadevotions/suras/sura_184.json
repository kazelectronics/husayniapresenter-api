{
  "index": 184,
  "name": "Sura 184",
  "ayas": [
    {
      "index": 1,
      "text": "subḥānal ladhī fil ar-ḍi ḥukmuh",
      "bismillah": "subḥānal ladhī fis samā-i `ar-shuh"
    },
    {
      "index": 2,
      "text": "subḥānal ladhī fil qubūri qaḍa-uh"
    },
    {
      "index": 3,
      "text": "subḥānal ladhī fil baḥri sabīluh"
    },
    {
      "index": 4,
      "text": "subḥānal ladhī fin nāri sulṭānuh"
    },
    {
      "index": 5,
      "text": "subḥānal ladhī fil jannati raḥmatuh"
    },
    {
      "index": 6,
      "text": "subḥānal ladhī fil qiyāmati `adluh"
    },
    {
      "index": 7,
      "text": "subḥānal ladhī rafa`as samā"
    },
    {
      "index": 8,
      "text": "subḥānal ladhī basaṭal ar-ḍ"
    },
    {
      "index": 9,
      "text": "subḥānal ladhī lāmalja-a walā manjā minhu illā ilayh"
    }
  ]
}