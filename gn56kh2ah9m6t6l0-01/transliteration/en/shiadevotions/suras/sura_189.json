{
  "index": 189,
  "name": "Sura 189",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "yā rabbi inna dhunūbī lā taḍurruk"
    },
    {
      "index": 3,
      "text": "wa-inna maghfirataka lī lā tanquṣuk"
    },
    {
      "index": 4,
      "text": "fa-a`ṭinī mā lā yanquṣuk"
    },
    {
      "index": 5,
      "text": "waghfir lī mā lā yaḍurruk"
    },
    {
      "index": 6,
      "text": ""
    },
    {
      "index": 7,
      "text": "allāhumma lā taḥrimnī khayra mā `indaka lisharri mā `indī"
    },
    {
      "index": 8,
      "text": "fa-in anta lam tar-ḥamnī bita`abī wanaṣabī"
    },
    {
      "index": 9,
      "text": "falā taḥrimnī ajral muṣābi `alā muṣībatih"
    }
  ]
}