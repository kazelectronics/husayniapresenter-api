{
  "index": 145,
  "name": "Sura 145",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma bārik lanā fī rajabin washa`bān"
    },
    {
      "index": 3,
      "text": "waballighnā shahra ramaḍān"
    },
    {
      "index": 4,
      "text": "wa-a`innā `alaṣ ṣiyāmi walqiyām"
    },
    {
      "index": 5,
      "text": "waḥifẓil lisān"
    },
    {
      "index": 6,
      "text": "waghaḍḍil baṣar"
    },
    {
      "index": 7,
      "text": "walā taj`al ḥaẓẓanā minhul jū`a wal`aṭash"
    }
  ]
}