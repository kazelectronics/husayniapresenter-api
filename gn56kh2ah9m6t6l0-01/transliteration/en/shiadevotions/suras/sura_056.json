{
  "index": 56,
  "name": "Sura 56",
  "ayas": [
    {
      "index": 1,
      "text": "idhā waqa`atil wāqi`ah",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "laysa liwaq`atihā kādhibah"
    },
    {
      "index": 3,
      "text": "khāfiḍatun rāfi`ah"
    },
    {
      "index": 4,
      "text": "idhā rujjatil-arḍu rajjā"
    },
    {
      "index": 5,
      "text": "wabussatil jibālu bassā"
    },
    {
      "index": 6,
      "text": "fakānat habā-an munbathhā"
    },
    {
      "index": 7,
      "text": "wakuntum azwājan thalāthah"
    },
    {
      "index": 8,
      "text": "fa-aṣ-ḥābul maymanati mā aṣ-ḥābul maymanah"
    },
    {
      "index": 9,
      "text": "wa-aṣ-ḥābul mash-amati mā aṣ-ḥābul mash-amah"
    },
    {
      "index": 10,
      "text": "was sābiqūnas sābiqūn"
    },
    {
      "index": 11,
      "text": "ulā-ikal muqarrabūn"
    },
    {
      "index": 12,
      "text": "fī jannātin na`īm"
    },
    {
      "index": 13,
      "text": "thullatun minal-awwalīn"
    },
    {
      "index": 14,
      "text": "waqalīlun minal-ākhirīn"
    },
    {
      "index": 15,
      "text": "`alā sururin mawḍūnah"
    },
    {
      "index": 16,
      "text": "muttaki-īna `alayhā mutaqābilīn"
    },
    {
      "index": 17,
      "text": "yaṭūfu `alayhim wildānun mukhalladūn"
    },
    {
      "index": 18,
      "text": "bi-akwābin wa-abārīqa waka-sin min ma`īn"
    },
    {
      "index": 19,
      "text": "lā yuṣadda`ūna `anhā walā yunzifūn"
    },
    {
      "index": 20,
      "text": "wafākihatim mimmā yatakhayyarūn"
    },
    {
      "index": 21,
      "text": "walaḥmi ṭayrin mimmā yashtahūn"
    },
    {
      "index": 22,
      "text": "waḥūrun `īn"
    },
    {
      "index": 23,
      "text": "ka-amthālillu-lu-il maknūn"
    },
    {
      "index": 24,
      "text": "jazā-an bimā kānū ya`malūn"
    },
    {
      "index": 25,
      "text": "lā yasma`ūna fīhā laghwan walā ta-thīmā"
    },
    {
      "index": 26,
      "text": "illā qīlan salāman salāmā"
    },
    {
      "index": 27,
      "text": "wa-aṣ-ḥābul yamīni mā aṣ-ḥābul yamīn"
    },
    {
      "index": 28,
      "text": "fī sidrin makhḍūd"
    },
    {
      "index": 29,
      "text": "waṭalḥin manḍūd"
    },
    {
      "index": 30,
      "text": "waẓillin mamdūd"
    },
    {
      "index": 31,
      "text": "wa mā-in maskūb"
    },
    {
      "index": 32,
      "text": "wafākihatin kathīrah"
    },
    {
      "index": 33,
      "text": "lā maqṭū`atin walā mamnū`ah"
    },
    {
      "index": 34,
      "text": "wafurushin marfū`ah"
    },
    {
      "index": 35,
      "text": "innā ansha-nāhunna inshā-ā"
    },
    {
      "index": 36,
      "text": "faja`alnāhunna abkārā"
    },
    {
      "index": 37,
      "text": "`uruban atrābā"
    },
    {
      "index": 38,
      "text": "li-aṣ-ḥābil yamīn"
    },
    {
      "index": 39,
      "text": "thullatun minal-awwalīn"
    },
    {
      "index": 40,
      "text": "wathullatun minal-ākhirīn"
    },
    {
      "index": 41,
      "text": "wa-aṣ-ḥābush shimāli mā aṣ-ḥābush shimāl"
    },
    {
      "index": 42,
      "text": "fī samūmin waḥamīm"
    },
    {
      "index": 43,
      "text": "waẓillin min yaḥmūm"
    },
    {
      "index": 44,
      "text": "lā bāridin walā karīm"
    },
    {
      "index": 45,
      "text": "innahum kānū qabla dhālika mutrafīn"
    },
    {
      "index": 46,
      "text": "wakānū yuṣirrūna `alal-ḥinthil `aẓīm"
    },
    {
      "index": 47,
      "text": "wakānū yaqūlūna a-idhā mitnā wakunnā turāban wa`iẓāman a-innā lamab`ūthūn"
    },
    {
      "index": 48,
      "text": "awa-ābā-unal-awwalūn"
    },
    {
      "index": 49,
      "text": "qul innal-awwalīna wal-ākhirīn"
    },
    {
      "index": 50,
      "text": "lamajmū`ūna ilā mīqāti yawmin ma`lūm"
    },
    {
      "index": 51,
      "text": "thumma innakum ayyuhaḍ ḍāllūnal mukadhhibūn"
    },
    {
      "index": 52,
      "text": "la-ākilūna min shajarin min zaqqūm"
    },
    {
      "index": 53,
      "text": "famāli-ūna minhal buṭūn"
    },
    {
      "index": 54,
      "text": "fashāribūna `alayhi minal ḥamīm"
    },
    {
      "index": 55,
      "text": "fashāribūna shurbal hīm"
    },
    {
      "index": 56,
      "text": "hādhā nuzuluhum yawmad dīn"
    },
    {
      "index": 57,
      "text": "naḥnu khalaqnākum falawlā tuṣaddiqūn"
    },
    {
      "index": 58,
      "text": "afara aytum mā tumnūn"
    },
    {
      "index": 59,
      "text": "a-antum takhluqūnahu am naḥnul khāliqūn"
    },
    {
      "index": 60,
      "text": "naḥnu qaddarnā baynakumul mawta wa mā naḥnu bimasbūqīn"
    },
    {
      "index": 61,
      "text": "`alā an nubaddila amthālakum wanunshi-akum fī mā lā ta`lamūn"
    },
    {
      "index": 62,
      "text": "walaqad `alimtumun nash-atal-ūlā falawlā tadhakkarūn"
    },
    {
      "index": 63,
      "text": "afara aytum mā taḥruthūn"
    },
    {
      "index": 64,
      "text": "a-antum tazra`ūnahu am naḥnuz zāri`ūn"
    },
    {
      "index": 65,
      "text": "law nashā-u laja`alnāhu ḥuṭāman faẓaltum tafakkahūn"
    },
    {
      "index": 66,
      "text": "innā lamughramūn"
    },
    {
      "index": 67,
      "text": "bal naḥnu maḥrūmūn"
    },
    {
      "index": 68,
      "text": "afara aytumul mā-al ladhī tashrabūn"
    },
    {
      "index": 69,
      "text": "a-antum anzaltumūhu minal muzni am naḥnul munzilūn"
    },
    {
      "index": 70,
      "text": "law nashā-u ja`alnāhu ujājan falawlā tashkurūn"
    },
    {
      "index": 71,
      "text": "afara aytumun nāral latī tūrūn"
    },
    {
      "index": 72,
      "text": "a-antum ansha-tum shajaratahā am naḥnul munshi-ūn"
    },
    {
      "index": 73,
      "text": "naḥnu ja`alnāhā tadhkiratan wa matā`al lil-muqwyn"
    },
    {
      "index": 74,
      "text": "fasabbiḥ bismi rabbikal `aẓīm"
    },
    {
      "index": 75,
      "text": "falā uqsimu bimawāqi`in nujūm"
    },
    {
      "index": 76,
      "text": "wa innahu laqasamul law ta`lamūna `aẓīm"
    },
    {
      "index": 77,
      "text": "innahu laqur-ānun karīm"
    },
    {
      "index": 78,
      "text": "fī kitābin maknūn"
    },
    {
      "index": 79,
      "text": "lā yamassuhu illal-muṭahharūn"
    },
    {
      "index": 80,
      "text": "tanzīlun min rabbil `ālamīn"
    },
    {
      "index": 81,
      "text": "afabihādhal ḥadīthi antum mud-hinūn"
    },
    {
      "index": 82,
      "text": "wataj`alūna rizqakum annakum tukadhhibūn"
    },
    {
      "index": 83,
      "text": "falawlā idhā balaghatil ḥulqūm"
    },
    {
      "index": 84,
      "text": "wa-antum ḥīna-idhin tanẓurūn"
    },
    {
      "index": 85,
      "text": "wanaḥnu aqrabu ilayhi minkum walākin lā tubṣirūn"
    },
    {
      "index": 86,
      "text": "falawlā in kuntum ghayra madīnīn"
    },
    {
      "index": 87,
      "text": "tarji`ūnahā in kuntum ṣādiqīn"
    },
    {
      "index": 88,
      "text": "fa-ammā in kāna minal muqarrabīn"
    },
    {
      "index": 89,
      "text": "farawḥun warayḥānun wajannatu na`īm"
    },
    {
      "index": 90,
      "text": "wa-ammā in kāna min aṣ-ḥābil yamīn"
    },
    {
      "index": 91,
      "text": "fasalāmul laka min aṣ-ḥābil yamīn"
    },
    {
      "index": 92,
      "text": "wa-ammā in kāna minal mukadhhibīnaḍ ḍāllīn"
    },
    {
      "index": 93,
      "text": "fanuzulun min ḥamīm"
    },
    {
      "index": 94,
      "text": "wataṣliyatu jaḥīm"
    },
    {
      "index": 95,
      "text": "inna hādhā lahuwa ḥaqqul yaqīn"
    },
    {
      "index": 96,
      "text": "fasabbiḥ bismi rabbikal `aẓīm"
    }
  ]
}