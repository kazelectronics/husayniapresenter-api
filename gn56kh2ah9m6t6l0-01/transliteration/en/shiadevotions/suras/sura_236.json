{
  "index": 236,
  "name": "Sura 236",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al ḥamdu lillāhi riḍan biḥukmillāh"
    },
    {
      "index": 3,
      "text": "shahidtu annallāha qasama ma`āyisha `ibādihi bil `adl"
    },
    {
      "index": 4,
      "text": "wa akhadha `alā jamī`i khalqihi bil faḍl"
    },
    {
      "index": 5,
      "text": "allāhumma ṣalli `alā muḥammadin wa-ālih"
    },
    {
      "index": 6,
      "text": "wa lā taftinnī bimā a`ṭaytahum"
    },
    {
      "index": 7,
      "text": "wa lā taftinhum bimā mana`tanī fa-aḥsuda khalqak, wa aghmaṭa ḥukmak"
    },
    {
      "index": 8,
      "text": "allāhumma ṣalli `alā muḥammadin wa-ālih"
    },
    {
      "index": 9,
      "text": "wa ṭayyib biqaḍā-ika nafsī"
    },
    {
      "index": 10,
      "text": "wa wassi` bimawāqi`i ḥukmika ṣadrī"
    },
    {
      "index": 11,
      "text": "wa hab liyath thiqata li-uqirra ma`ahā"
    },
    {
      "index": 12,
      "text": "bi-anna qaḍā-aka lam yajri illā bil khiyarah"
    },
    {
      "index": 13,
      "text": "waj`al shukrī laka `alā mā zawayta `annī"
    },
    {
      "index": 14,
      "text": "awfara min shukrī iyyāka `alā mā khawwaltanī"
    },
    {
      "index": 15,
      "text": "wa`ṣimnī min an aẓunna bidhī `adamin khasāsah"
    },
    {
      "index": 16,
      "text": "aw aẓunna biṣāḥibi thar-watin faḍlā"
    },
    {
      "index": 17,
      "text": "fa-innash sharīfa man sharrafat-hu ṭā`atuk"
    },
    {
      "index": 18,
      "text": "wal `azīza man a`azzat-hu `ibādatuk"
    },
    {
      "index": 19,
      "text": "faṣalli `alā muḥammadin wa-ālih"
    },
    {
      "index": 20,
      "text": "wa matti`nā bithar-watin lā tanfad, wa ayyidnā bi`izzin lā yufqad"
    },
    {
      "index": 21,
      "text": "wa asriḥnā fī mulkil abad"
    },
    {
      "index": 22,
      "text": "innakal wāḥidul aḥaduṣ ṣamad"
    },
    {
      "index": 23,
      "text": "al ladhī lam talid wa lam tūlad"
    },
    {
      "index": 24,
      "text": "wa lam yakun laka kufuwan aḥad"
    }
  ]
}