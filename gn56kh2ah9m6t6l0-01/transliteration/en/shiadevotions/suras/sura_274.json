{
  "index": 274,
  "name": "Sura 274",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī kasrī lā yajburuhu illā luṭfuka waḥanānuk"
    },
    {
      "index": 3,
      "text": "wafaqrī lā yughnīhi illā `aṭfuka wa-iḥsānuk"
    },
    {
      "index": 4,
      "text": "waraw`atī lā yusakkinuhā illā amānuk"
    },
    {
      "index": 5,
      "text": "wadhillatī lā yu`izzuhā illā sulṭānuk"
    },
    {
      "index": 6,
      "text": "wa-umniyatī lā yuballighunīhā illā faḍluk"
    },
    {
      "index": 7,
      "text": "wakhallatī lā yasudduhā illā ṭawluk"
    },
    {
      "index": 8,
      "text": "waḥājatī lā yaqḍīhā ghayruk"
    },
    {
      "index": 9,
      "text": "wakar-bī lā yufarrijuhu siwā raḥmatik"
    },
    {
      "index": 10,
      "text": "waḍurrī lā yakshifuhu ghayru ra-fatik"
    },
    {
      "index": 11,
      "text": "waghullatī lā yubarriduhā illā waṣluk"
    },
    {
      "index": 12,
      "text": "walaw`atī lā yuṭfīhā illā liqa-uk"
    },
    {
      "index": 13,
      "text": "washawqī ilayka lā yabulluhu illan naẓaru ilā wajhik"
    },
    {
      "index": 14,
      "text": "waqarārī lā yaqirru dūna dunuwwī mink"
    },
    {
      "index": 15,
      "text": "walahfatī lā yarudduhā illā rawḥuk"
    },
    {
      "index": 16,
      "text": "wasuqmī lā yashfīhi illā ṭibbuk"
    },
    {
      "index": 17,
      "text": "waghammī lā yuzīluhu illā qur-buk"
    },
    {
      "index": 18,
      "text": "wajur-ḥī lā yubri-uhu illā ṣafḥuk"
    },
    {
      "index": 19,
      "text": "waraynu qalbī lā yajlūhu illā `afwuk"
    },
    {
      "index": 20,
      "text": "wawaswāsu ṣadrī lā yuzīḥuhu illā amruk"
    },
    {
      "index": 21,
      "text": "fayā muntahā amalil-āmilīn"
    },
    {
      "index": 22,
      "text": "wayā ghāyata su-lis sā-ilin"
    },
    {
      "index": 23,
      "text": "wayā aqṣā ṭalibatiṭ ṭālibīn"
    },
    {
      "index": 24,
      "text": "wayā a`lā raghbatir rāghibīn"
    },
    {
      "index": 25,
      "text": "wayā waliyyaṣ ṣāliḥīn"
    },
    {
      "index": 26,
      "text": "wayā amānal khā-ifīn"
    },
    {
      "index": 27,
      "text": "wayā mujība da`watil muḍṭarrīn"
    },
    {
      "index": 28,
      "text": "wayā dhukhral mu`dimīn"
    },
    {
      "index": 29,
      "text": "wayā kanzal bā-isīn"
    },
    {
      "index": 30,
      "text": "wayā ghiyāthal mustaghīthīn"
    },
    {
      "index": 31,
      "text": "wayā qāḍiya ḥawā-ijil fuqarā-i walmasākīn"
    },
    {
      "index": 32,
      "text": "wayā akramal akramīn"
    },
    {
      "index": 33,
      "text": "wayā ar-ḥamar rāḥimīn"
    },
    {
      "index": 34,
      "text": "laka takhaḍḍu`ī wasu-ālī"
    },
    {
      "index": 35,
      "text": "wa-ilayka taḍarru`ī wabtihālī"
    },
    {
      "index": 36,
      "text": "as-aluka an tunīlanī min rawḥi riḍwānik"
    },
    {
      "index": 37,
      "text": "watudīma `alayya ni`amamtinānik"
    },
    {
      "index": 38,
      "text": "wahā ana bibābi karamika wāqif"
    },
    {
      "index": 39,
      "text": "walinafaḥāti birrika muta`arriḍ"
    },
    {
      "index": 40,
      "text": "wabiḥablikash shadīdi mu`taṣim"
    },
    {
      "index": 41,
      "text": "wabi`ur-watikal wuthqā mutamassik"
    },
    {
      "index": 42,
      "text": "ilāhī ir-ḥam `abdakadh dhalīl"
    },
    {
      "index": 43,
      "text": "dhal lisānil kalīl"
    },
    {
      "index": 44,
      "text": "wal`amalil qalīl"
    },
    {
      "index": 45,
      "text": "wamnun `alayhi biṭawlikal jazīl"
    },
    {
      "index": 46,
      "text": "waknufhu taḥta ẓillikaẓ ẓalīl"
    },
    {
      "index": 47,
      "text": "yā karīmu yā jamīl"
    },
    {
      "index": 48,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}