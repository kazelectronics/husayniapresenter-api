{
  "index": 175,
  "name": "Sura 175",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma dāḥiyal ka`bah"
    },
    {
      "index": 3,
      "text": "wafāliqal ḥabbah"
    },
    {
      "index": 4,
      "text": "waṣārifal lazbah"
    },
    {
      "index": 5,
      "text": "wakāshifa kulli kur-bah"
    },
    {
      "index": 6,
      "text": "as-aluka fī hādhal yawmi min ayyāmik"
    },
    {
      "index": 7,
      "text": "al latī a`ẓamta ḥaqqahā"
    },
    {
      "index": 8,
      "text": "wa-aqdamta sabqahā"
    },
    {
      "index": 9,
      "text": "waja`altahā `indal mu-minīna wadī`ah"
    },
    {
      "index": 10,
      "text": "wa-ilayka dharī`ah"
    },
    {
      "index": 11,
      "text": "wabiraḥmatikal wasī`ah"
    },
    {
      "index": 12,
      "text": "an tuṣalliya `alā muḥammad"
    },
    {
      "index": 13,
      "text": "`abdikal muntajabi fil mīthāqil qarīb"
    },
    {
      "index": 14,
      "text": "yawmat talāq"
    },
    {
      "index": 15,
      "text": "fātiqi kulli ratq"
    },
    {
      "index": 16,
      "text": "wadā`in ilā kulli ḥaqq"
    },
    {
      "index": 17,
      "text": "wa`alā ahli baytihil aṭ-hār"
    },
    {
      "index": 18,
      "text": "al hudātil manār"
    },
    {
      "index": 19,
      "text": "da`ā-imil jabbār"
    },
    {
      "index": 20,
      "text": "wawulātil jannati wan nār"
    },
    {
      "index": 21,
      "text": "wa-a`ṭinā fī yawminā hādhā min `aṭā-ikal makhzūn"
    },
    {
      "index": 22,
      "text": "ghayra maqṭū`in walā mamnū`"
    },
    {
      "index": 23,
      "text": "tajma`u lanā bihit tawbata waḥusnal awbah"
    },
    {
      "index": 24,
      "text": "yā khayra mad`uww"
    },
    {
      "index": 25,
      "text": "wa-akrama mar-juww"
    },
    {
      "index": 26,
      "text": "yā kafiyyu yā wafiyy"
    },
    {
      "index": 27,
      "text": "yā man luṭfuhu khafiyy"
    },
    {
      "index": 28,
      "text": "ilṭuf lī biluṭfik"
    },
    {
      "index": 29,
      "text": "wa-as`idnī bi`afwik"
    },
    {
      "index": 30,
      "text": "wa-ayyidnī binaṣrik"
    },
    {
      "index": 31,
      "text": "walā tunsinī karīma dhikrika biwulāti amrik"
    },
    {
      "index": 32,
      "text": "waḥafaẓati sirrik"
    },
    {
      "index": 33,
      "text": "waḥfaẓnī min shawā-ibid dahr"
    },
    {
      "index": 34,
      "text": "ilā yawmil ḥashri wan nashr"
    },
    {
      "index": 35,
      "text": "wa-ash-hidnī awliyā-aka `inda khurūji nafsī"
    },
    {
      "index": 36,
      "text": "waḥulūli ramsī"
    },
    {
      "index": 37,
      "text": "wanqiṭā`i `amalī"
    },
    {
      "index": 38,
      "text": "wanqiḍā-i ajalī"
    },
    {
      "index": 39,
      "text": "allāhumma wadhkur-nī `alā ṭūlil bilā"
    },
    {
      "index": 40,
      "text": "idhā ḥalaltu bayna aṭbāqith tharā"
    },
    {
      "index": 41,
      "text": "wanasiyanin nāsūna minal warā"
    },
    {
      "index": 42,
      "text": "waḥlilnī dāral muqāmah"
    },
    {
      "index": 43,
      "text": "wabawwi-nī manzilal karāmah"
    },
    {
      "index": 44,
      "text": "waj`alnī min murāfiqī awliyā-ik"
    },
    {
      "index": 45,
      "text": "wa-ahlijtibā-ika wa aṣṭifā-ik"
    },
    {
      "index": 46,
      "text": "wabārik lī fī liqā-ik"
    },
    {
      "index": 47,
      "text": "war-zuqnī ḥusnal `amali qabla ḥulūlil ajal"
    },
    {
      "index": 48,
      "text": "barī-an minaz zalali wasū-al khaṭal"
    },
    {
      "index": 49,
      "text": "allāhumma wa-awridnī ḥawḍa nabiyyika muḥammad"
    },
    {
      "index": 50,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 51,
      "text": "wasqinī minhu mashraban rawiyyā"
    },
    {
      "index": 52,
      "text": "sā-ighan hanī-ā"
    },
    {
      "index": 53,
      "text": "lā aẓma-u ba`dahu walā uḥalla-u wir-dah"
    },
    {
      "index": 54,
      "text": "walā `anhu udhād"
    },
    {
      "index": 55,
      "text": "waj`alhu lī khayra zād"
    },
    {
      "index": 56,
      "text": "wa-awfā mī`ādin yawma yaqūmul ash-hād"
    },
    {
      "index": 57,
      "text": "allāhumma wal`an jabābiratal awwalīna wal-ākhirīn"
    },
    {
      "index": 58,
      "text": "wabiḥuqūqi awliyā-ikal musta-thirīn"
    },
    {
      "index": 59,
      "text": "allāhumma waqṣim da`ā-imahum"
    },
    {
      "index": 60,
      "text": "wa-ahlik ashyā`ahum wa`āmilahum"
    },
    {
      "index": 61,
      "text": "wa`ajjil mahālikahum"
    },
    {
      "index": 62,
      "text": "waslubhum mamālikahum"
    },
    {
      "index": 63,
      "text": "waḍayyiq `alayhim masālikahum"
    },
    {
      "index": 64,
      "text": "wal`an musāhimahum wamushārikahum"
    },
    {
      "index": 65,
      "text": "allāhumma wa`ajjil faraja awliyā-ik"
    },
    {
      "index": 66,
      "text": "war-dud `alayhim maẓālimahum"
    },
    {
      "index": 67,
      "text": "wa-aẓhir bil ḥaqqi qā-imahum"
    },
    {
      "index": 68,
      "text": "waj`alhu lidīnika muntaṣirā"
    },
    {
      "index": 69,
      "text": "wabi-amrika fī a`dā-ika mu-tamirā"
    },
    {
      "index": 70,
      "text": "allāhummaḥfufhu bimalā-ikatin naṣr"
    },
    {
      "index": 71,
      "text": "wabimā alqayta ilayhi minal amr"
    },
    {
      "index": 72,
      "text": "fī laylatil qadr"
    },
    {
      "index": 73,
      "text": "muntaqiman laka ḥattā tar-ḍā"
    },
    {
      "index": 74,
      "text": "waya`ūda dīnuka bihi wa`alā yadayhi jadīdan ghaḍḍā"
    },
    {
      "index": 75,
      "text": "wayamḥaḍal ḥaqqa maḥḍā"
    },
    {
      "index": 76,
      "text": "wayar-fuḍal bāṭila rafḍā"
    },
    {
      "index": 77,
      "text": "allāhumma ṣalli `alayhi wa`alā jamī`i-ābā-ih"
    },
    {
      "index": 78,
      "text": "waj`alnā min ṣaḥbihi wa-usratih"
    },
    {
      "index": 79,
      "text": "wab`athnā fī karratih"
    },
    {
      "index": 80,
      "text": "ḥattā nakūna fī zamānihi min a`wānih"
    },
    {
      "index": 81,
      "text": "allāhumma adrik binā qiyāmah"
    },
    {
      "index": 82,
      "text": "wa-ash-hidnā ayyāmah"
    },
    {
      "index": 83,
      "text": "waṣalli `alayhi war-dud ilaynā salāmah"
    },
    {
      "index": 84,
      "text": "was salāmu `alayhi waraḥmatullāhi wabarakātuh"
    }
  ]
}