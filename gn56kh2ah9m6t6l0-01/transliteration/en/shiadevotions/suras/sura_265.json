{
  "index": 265,
  "name": "Sura 265",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī ilayka ashkū nafsā"
    },
    {
      "index": 3,
      "text": "bissū-i ammārah"
    },
    {
      "index": 4,
      "text": "wa-ilal khaṭī-ati mubādirah"
    },
    {
      "index": 5,
      "text": "wabima`āṣīka mūla`ah"
    },
    {
      "index": 6,
      "text": "walisakhaṭika muta`arriḍah"
    },
    {
      "index": 7,
      "text": "tasluka bī masālikal mahālik"
    },
    {
      "index": 8,
      "text": "wataj`alunī `indaka ahwana hālik"
    },
    {
      "index": 9,
      "text": "kathiratal `ilal"
    },
    {
      "index": 10,
      "text": "ṭawīlatal amal"
    },
    {
      "index": 11,
      "text": "in massahash sharru tajza`"
    },
    {
      "index": 12,
      "text": "wa-in massahal khayru tamna`"
    },
    {
      "index": 13,
      "text": "mayyālatan ilal la`ibi wallāhw"
    },
    {
      "index": 14,
      "text": "mamlū-atan bil ghaflati was sahw"
    },
    {
      "index": 15,
      "text": "tusri`u bī ilal ḥawbah"
    },
    {
      "index": 16,
      "text": "watusawwifunī bittawbah"
    },
    {
      "index": 17,
      "text": "ilāhī ashkū ilayk"
    },
    {
      "index": 18,
      "text": "`aduwwan yuḍillunī"
    },
    {
      "index": 19,
      "text": "washayṭānan yughwīnī"
    },
    {
      "index": 20,
      "text": "qad mala-a bil waswāsi ṣadrī"
    },
    {
      "index": 21,
      "text": "wa-aḥāṭat hawājisuhu biqalbī"
    },
    {
      "index": 22,
      "text": "yu`āḍidu liyal hawā"
    },
    {
      "index": 23,
      "text": "wayuzayyinu lī ḥubbad dunyā"
    },
    {
      "index": 24,
      "text": "wayaḥūlu baynī wabaynaṭ ṭā`ati waz zulfā"
    },
    {
      "index": 25,
      "text": "ilāhī ilayka ashkū"
    },
    {
      "index": 26,
      "text": "qalban qāsiyā"
    },
    {
      "index": 27,
      "text": "ma`al waswāsi mutaqallibā"
    },
    {
      "index": 28,
      "text": "wabirrayni waṭ ṭab`i mutalabbisā"
    },
    {
      "index": 29,
      "text": "wa`aynan `anil bukā-i min khawfika jāmidah"
    },
    {
      "index": 30,
      "text": "wa-ilā mā tasurruhā ṭāmiḥah"
    },
    {
      "index": 31,
      "text": "ilāhī lā ḥawla walā quwwata illā biqudratik"
    },
    {
      "index": 32,
      "text": "walā najāta lī min makārihid dunyā illā bi`iṣmatik"
    },
    {
      "index": 33,
      "text": "fa-as-aluka bibalāghati ḥikmatik"
    },
    {
      "index": 34,
      "text": "wanafādhi mashiyyatik"
    },
    {
      "index": 35,
      "text": "an lā taj`alnī lighayri jūdika muta`arriḍā"
    },
    {
      "index": 36,
      "text": "walā tuṣayyir-nī lilfitani gharaḍā"
    },
    {
      "index": 37,
      "text": "wakun lī `alal a`dā-i nāṣirā"
    },
    {
      "index": 38,
      "text": "wa`alal makhāzī wal`uyūbi sātirā"
    },
    {
      "index": 39,
      "text": "waminal balāyā wāqiyā"
    },
    {
      "index": 40,
      "text": "wa`anil ma`āṣī `āṣimā"
    },
    {
      "index": 41,
      "text": "bira-fatika waraḥmatik"
    },
    {
      "index": 42,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}