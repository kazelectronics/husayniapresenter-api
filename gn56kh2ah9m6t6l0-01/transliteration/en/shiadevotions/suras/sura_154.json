    {
      "index": 202,
      "text": "muḍṭali`an bi-athqālil imāmah"
    },
    {
      "index": 203,
      "text": "fanaṣaba rāyatal hudā fī `ibādik"
    },
    {
      "index": 204,
      "text": "wanashara thawbal amni fī bilādik"
    },
    {
      "index": 205,
      "text": "wabasaṭal `adla fī bariyyatik"
    },
    {
      "index": 206,
      "text": "waḥakama bikitābika fī khalīqatik"
    },
    {
      "index": 207,
      "text": "wa-aqāmal ḥudūd"
    },
    {
      "index": 208,
      "text": "waqama`al juḥūd"
    },
    {
      "index": 209,
      "text": "waqawwamaz zaygh"
    },
    {
      "index": 210,
      "text": "wasakkanal ghamrah"
    },
    {
      "index": 211,
      "text": "wa-abādal fatrah"
    },
    {
      "index": 212,
      "text": "wasaddal fur-jah"
    },
    {
      "index": 213,
      "text": "waqatalan nākithata walqāsiṭata walmāriqah"
    },
    {
      "index": 214,
      "text": "walam yazal `alā minhāji rasūlillāhi ṣallallāhu `alayhi wa ālihi wawatīratih"
    },
    {
      "index": 215,
      "text": "waluṭfi shākilatih"
    },
    {
      "index": 216,
      "text": "wajamāli sīratih"
    },
    {
      "index": 217,
      "text": "muqtadiyan bisunnatih"
    },
    {
      "index": 218,
      "text": "muta`alliqan bihimmatih"
    },
    {
      "index": 219,
      "text": "mubāshiran liṭarīqatih"
    },
    {
      "index": 220,
      "text": "wa-amthilatuhu naṣbu `aynayh"
    },
    {
      "index": 221,
      "text": "yaḥmilu `ibādaka `alayhā"
    },
    {
      "index": 222,
      "text": "wayad`ūhum ilayhā"
    },
    {
      "index": 223,
      "text": "ilā an khuḍibat shaybatuhu min dami ra-sih"
    },
    {
      "index": 224,
      "text": "allāhumma fakamā lam yu-thir fī ṭā`atika shakkan `alā yaqīn"
    },
    {
      "index": 225,
      "text": "walam yushrik bika ṭar-fata `ayn"
    },
    {
      "index": 226,
      "text": "ṣalli `alayhi ṣalātan zākiyatan nāmiyah"
    },
    {
      "index": 227,
      "text": "yalḥaqu bihā darajatan nubuwwati fī jannatik"
    },
    {
      "index": 228,
      "text": "waballigh-hu minnā taḥiyyatan wasalāmā"
    },
    {
      "index": 229,
      "text": "wa ātinā min ladunka fī muwālātihi faḍlan wa-iḥsānā"
    },
    {
      "index": 230,
      "text": "wamaghfiratan wariḍwānā"
    },
    {
      "index": 231,
      "text": "innaka dhul faḍlil jasīm"
    },
    {
      "index": 232,
      "text": "biraḥmatika yā ar-ḥmar rāḥimīn"
    },
    {
      "index": 233,
      "text": ""
    },
    {
      "index": 234,
      "text": ""
    },
    {
      "index": 235,
      "text": "allāhumma innaka bashhar-tanī"
    },
    {
      "index": 236,
      "text": "`alā lisāni nabiyyika warasūlik"
    },
    {
      "index": 237,
      "text": "muḥammadin ṣalawātuka `alayhi wa ālih"
    },
    {
      "index": 238,
      "text": "faqulta wabashhiril ladhīna-āmanū anna lahum qadama ṣidqin `inda rabbihim"
    },
    {
      "index": 239,
      "text": "allāhumma wa-innī mu-minun bijamī`i anbiyā-ika warusulika ṣalawātuka `alayhim"
    },
    {
      "index": 240,
      "text": "falā taqifnī ba`da ma`rifatihim mawqifā"
    },
    {
      "index": 241,
      "text": "tafḍaḥunī fīhi `alā ru-ūsil ash-hād"
    },
    {
      "index": 242,
      "text": "bal qifnī ma`ahum watawaffanī `alat taṣdīqi bihim"
    },
    {
      "index": 243,
      "text": "allāhumma wa-anta khaṣaṣtahum bikarāmatik"
    },
    {
      "index": 244,
      "text": "wa-amar-tanī bittibā`ihim"
    },
    {
      "index": 245,
      "text": "allāhumma wa-innī `abduka wazā-iruk"
    },
    {
      "index": 246,
      "text": "mutaqarriban ilayka biziyārati akhī rasūlik"
    },
    {
      "index": 247,
      "text": "wa`alā kulli ma-tiyyin wamazūrin ḥaqqun liman atāhu wazārah"
    },
    {
      "index": 248,
      "text": "wa-anta khayru ma-tiyy"
    },
    {
      "index": 249,
      "text": "wa-akramu mazūr"
    },
    {
      "index": 250,
      "text": "fa-as-aluka yā allāhu yā raḥmānu yā raḥīm"
    },
    {
      "index": 251,
      "text": "yā jawādu yā mājid"
    },
    {
      "index": 252,
      "text": "yā aḥadu yā ṣamad"
    },
    {
      "index": 253,
      "text": "yā man lam yalid walam yūlad"
    },
    {
      "index": 254,
      "text": "walam yakun lahu kufuwan aḥad"
    },
    {
      "index": 255,
      "text": "walam yattakhidh ṣāḥibatan walā waladā"
    },
    {
      "index": 256,
      "text": "an tuṣalliya `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 257,
      "text": "wa-an taj`ala tuḥfataka iyyāya min ziyāratī akhā rasūlik"
    },
    {
      "index": 258,
      "text": "fakāka raqabatī minan nār"
    },
    {
      "index": 259,
      "text": "wa-an taj`alanī mimman yusāri`u fil khayrāt"
    },
    {
      "index": 260,
      "text": "wayad`ūka raghaban warahabā"
    },
    {
      "index": 261,
      "text": "wataj`alanī laka minal khāshi`īn"
    },
    {
      "index": 262,
      "text": "allāhumma innaka mananta `alayya biziyārati mawlāya `aliyyibni abī ṭālib"
    },
    {
      "index": 263,
      "text": "wawilāyatihi wama`rifatih"
    },
    {
      "index": 264,
      "text": "faj`alnī mimman yanṣuruhu wayantaṣiru bih"
    },
    {
      "index": 265,
      "text": "wamunna `alayya binaṣrika lidīnik"
    },
    {
      "index": 266,
      "text": "allāhumma waj`alnī min shī`atihi watawaffanī `alā dīnih"
    },
    {
      "index": 267,
      "text": "allāhumma awjib lī minar raḥmati war riḍwān"
    },
    {
      "index": 268,
      "text": "walmaghfirati wal-iḥsān"
    },
    {
      "index": 269,
      "text": "war rizqil wāsi`il ḥalāliṭ ṭayyib"
    },
    {
      "index": 270,
      "text": "mā anta ahluhu yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 271,
      "text": "walḥamdu lillāhi rabbil `ālamīn"
    }
  ]
}