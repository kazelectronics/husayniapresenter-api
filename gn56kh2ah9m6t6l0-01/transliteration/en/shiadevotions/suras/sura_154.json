{
  "index": 154,
  "name": "Sura 154",
  "ayas": [
    {
      "index": 1,
      "text": "as salāmu `alayka yā wāritha nūḥin ṣafwatillāh",
      "bismillah": "as salāmu `alayka yā wāritha-ādama khalīfatillāh"
    },
    {
      "index": 2,
      "text": "as salāmu `alayka yā wāritha ibrāhīma khalīlillāh"
    },
    {
      "index": 3,
      "text": "as salāmu `alayka yā wāritha mūsā kalīmillāh"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka yā wāritha `īsā rūḥillāh"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā wāritha muḥammadin sayyidi rusulillāh"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā amīral mu-minīn"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā imāmal muttaqīn"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yā sayyidal waṣiyyīn"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka yā waṣiyya rasūli rabbil `ālamīn"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā wāritha `ilmil awwalīna wal-ākhirīn"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka ayyuhan naba-ul `aẓīm"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka ayyuhaṣ ṣirāṭul mustaqīm"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka ayyuhal muhadhhabul karīm"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka ayyuhal waṣiyyut taqiyy"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka ayyuhar raḍiyyuz zakiyy"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka ayyuhal badrul muḍī"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka ayyuhaṣ ṣiddīqul akbar"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka ayyuhal fārūqul a`ẓam"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka ayyuhas sirājul munīr"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka yā imāmal hudā"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka yā `alamat tuqā"
    },
    {
      "index": 22,
      "text": "as salāmu `alayka yā ḥujjatallāhil kubrā"
    },
    {
      "index": 23,
      "text": "as salāmu `alayka yā khāṣṣatallāhi wakhāliṣatah"
    },
    {
      "index": 24,
      "text": "wa-amīnallāhi waṣafwatah"
    },
    {
      "index": 25,
      "text": "wabāballāhi waḥujjatah"
    },
    {
      "index": 26,
      "text": "wama`dina ḥukmillāhi wasirrih"
    },
    {
      "index": 27,
      "text": "wa`aybata `ilmillāhi wakhāzinah"
    },
    {
      "index": 28,
      "text": "wasafīrallāhi fī khalqih"
    },
    {
      "index": 29,
      "text": "ash-hadu annaka aqamtaṣ ṣalāh"
    },
    {
      "index": 30,
      "text": "wa ātaytaz zakāh"
    },
    {
      "index": 31,
      "text": "wa-amar-ta bil ma`rūf"
    },
    {
      "index": 32,
      "text": "wanahayta `anil munkar"
    },
    {
      "index": 33,
      "text": "wattaba`tar rasūl"
    },
    {
      "index": 34,
      "text": "watalawtal kitāba ḥaqqa tilāwatih"
    },
    {
      "index": 35,
      "text": "waballaghta `anillāh"
    },
    {
      "index": 36,
      "text": "wawafayta bi`ahdillāh"
    },
    {
      "index": 37,
      "text": "watammat bika kalimātullāh"
    },
    {
      "index": 38,
      "text": "wajāhadta fillāhi ḥaqqa jihādih"
    },
    {
      "index": 39,
      "text": "wanaṣaḥta lillāhi walirasūlihi ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 40,
