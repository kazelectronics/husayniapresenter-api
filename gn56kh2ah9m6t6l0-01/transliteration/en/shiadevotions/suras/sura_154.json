    },
    {
      "index": 41,
      "text": "mujāhidan `an dīnillāh"
    },
    {
      "index": 42,
      "text": "muwaqqiyan lirasūlillāhi ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 43,
      "text": "ṭāliban mā `indallāh"
    },
    {
      "index": 44,
      "text": "rāghiban fīmā wa`adallāh"
    },
    {
      "index": 45,
      "text": "wamaḍayta lilladhī kunta `alayhi shahīdā"
    },
    {
      "index": 46,
      "text": "washāhidan wamash-hūdā"
    },
    {
      "index": 47,
