    {
      "index": 54,
      "text": "wa-a`ẓamahum `anā-ā"
    },
    {
      "index": 55,
      "text": "wa-aḥwaṭahum `alā rasūlillāhi ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 56,
      "text": "wa-afḍalahum manāqib"
    },
    {
      "index": 57,
      "text": "wa-aktharahum sawābiq"
