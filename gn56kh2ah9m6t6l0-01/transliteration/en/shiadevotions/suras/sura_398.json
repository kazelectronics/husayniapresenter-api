{
  "index": 398,
  "name": "Sura 398",
  "ayas": [
    {
      "index": 1,
      "text": "sajadtu laka khāshi`an khāḍi`an dhalīlā",
      "bismillah": "lā ilāha illā anta rabbī"
    },
    {
      "index": 2,
      "text": "faṣalli `alā muḥammadiw wa-āli muḥammad"
    },
    {
      "index": 3,
      "text": "waghfir-lī war-ḥamnī watub `alayy"
    },
    {
      "index": 4,
      "text": "innaka antat tawwābur raḥīm"
    }
  ]
}