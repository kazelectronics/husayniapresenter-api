{
  "index": 291,
  "name": "Sura 291",
  "ayas": [
    {
      "index": 1,
      "text": "wayā malikal mulūk",
      "bismillah": "yā rabbal ar-bāb"
    },
    {
      "index": 2,
      "text": "wayā sayyidas sādāt"
    },
    {
      "index": 3,
      "text": "wayā jabbāral jabābirah"
    },
    {
      "index": 4,
      "text": "wayā ilāhal-ālihah"
    },
    {
      "index": 5,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 6,
      "text": ""
    },
    {
      "index": 7,
      "text": "fa-innī `abduka nāṣiyatī fī qabḍatik"
    }
  ]
}