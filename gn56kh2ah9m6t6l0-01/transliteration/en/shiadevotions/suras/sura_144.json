{
  "index": 144,
  "name": "Sura 144",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma ahillahu `alaynā bil amni wal-īmān"
    },
    {
      "index": 3,
      "text": "was salāmati wal-islām"
    },
    {
      "index": 4,
      "text": "rabbī warabbukallāhu `azza wajall"
    }
  ]
}