{
  "index": 96,
  "name": "Sura 96",
  "ayas": [
    {
      "index": 1,
      "text": "iqra-bismi rabbikal ladhī khalaq",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "khalaqal-insāna min `alaq"
    },
    {
      "index": 3,
      "text": "iqra-warabbukal-akram"
    },
    {
      "index": 4,
      "text": "alladhī `allama bil qalam"
    },
    {
      "index": 5,
      "text": "`allamal-insāna mā lam ya`lam"
    },
    {
      "index": 6,
      "text": "kallā innal-insāna layaṭghā"
    },
    {
      "index": 7,
      "text": "an ra-āhus taghnā"
    },
    {
      "index": 8,
      "text": "inna ilā rabbikar ruj`ā"
    },
    {
      "index": 9,
      "text": "ara aytal ladhī yanhā"
    },
    {
      "index": 10,
      "text": "`abdan idhā ṣallā"
    },
    {
      "index": 11,
      "text": "ara ayta in kāna `alal-hudā"
    },
    {
      "index": 12,
      "text": "aw amara bit taqwā"
    },
    {
      "index": 13,
      "text": "ara ayta in kadhhaba watawallā"
    },
    {
      "index": 14,
      "text": "alam ya`lam bi-annallāha yarā"
    },
    {
      "index": 15,
      "text": "kallā la-in lam yantahi lanasfa`an bin nāṣiyah"
    },
    {
      "index": 16,
      "text": "nāṣiyatin kādhibatin khāṭi-ah"
    },
    {
      "index": 17,
      "text": "falyad`u nādiyah"
    },
    {
      "index": 18,
      "text": "sanad`uz zabāniyah"
    },
    {
      "index": 19,
      "text": "kallā lā tuṭi`hu wasjud waqtarib ۩"
    }
  ]
}