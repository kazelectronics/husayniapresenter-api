{
  "index": 174,
  "name": "Sura 174",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "yā muqīlal `atharāti aqilnī `athratī"
    },
    {
      "index": 3,
      "text": "yā mujībad da`awāti ajib da`watī"
    },
    {
      "index": 4,
      "text": "yā sāmi`al aṣwātisma` ṣawtī"
    },
    {
      "index": 5,
      "text": "war-ḥamnī watajāwaz `an sayyi-ātī wamā `indī"
    },
    {
      "index": 6,
      "text": "yā dhal jalāli wal-ikrām"
    }
  ]
}