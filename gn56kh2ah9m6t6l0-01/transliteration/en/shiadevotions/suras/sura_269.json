{
  "index": 269,
  "name": "Sura 269",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir rḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī adh-halanī `an iqāmati shukrika tatābu`u ṭawlik"
    },
    {
      "index": 3,
      "text": "wa-a`jazanī `an iḥṣā-i thanā-ika fayḍu faḍlik"
    },
    {
      "index": 4,
      "text": "washaghalanī `an dhikri maḥāmidika tarādufu `awā-idik"
    },
    {
      "index": 5,
      "text": "wa-a`yānī `an nashri `awārifika tawālī ayādīk"
    },
    {
      "index": 6,
      "text": "wahādhā maqāmu mani`tarafa bisubūghin na`mā"
    },
    {
      "index": 7,
      "text": "waqābalahā bittaqṣīr"
    },
    {
      "index": 8,
      "text": "washahida `alā nafsihi bil ihmāli wat taḍyī`"
    },
    {
      "index": 9,
      "text": "wa-antar ra-ūfur raḥīm"
    },
    {
      "index": 10,
      "text": "al barrul karīm"
    },
    {
      "index": 11,
      "text": "al ladhī lā yukhayyibu qāṣidīh"
    },
    {
      "index": 12,
      "text": "walā yaṭrudu `an finā-ihi-āmilīh"
    },
    {
      "index": 13,
      "text": "bisāḥatika taḥuṭṭu riḥālur rājīn"
    },
    {
      "index": 14,
      "text": "wabi`ar-ṣatika taqifu-āmālul mustar-fidīn"
    },
    {
      "index": 15,
      "text": "falā tuqābil-āmālanā bittakhyībi wal-iyās"
    },
    {
      "index": 16,
      "text": "walā tulbisnā sir-bālal qunūṭi wal-iblās"
    },
    {
      "index": 17,
      "text": "ilāhī taṣāghara `inda ta`āẓumi-ālā-ika shukrī"
    },
    {
      "index": 18,
      "text": "wataḍā-ala fī janbi ikrāmika iyyāya thanā-ī wanashrī"
    },
    {
      "index": 19,
      "text": "jallalatnī ni`amuka min anwāril īmāni ḥulalā"
    },
    {
      "index": 20,
      "text": "waḍarabat `alayya laṭā-ifu birrika minal `izzi kilalā"
    },
    {
      "index": 21,
      "text": "waqalladatnī minanuka qalā-ida lā tuḥall"
    },
    {
      "index": 22,
      "text": "waṭawwaqatnī aṭwāqan lā tufall"
    },
    {
      "index": 23,
      "text": "fa-ālā-uka jammah"
    },
    {
      "index": 24,
      "text": "ḍa`ufa lisānī `an iḥṣā-ihā"
    },
    {
      "index": 25,
      "text": "wana`ma-uka kathīrah"
    },
    {
      "index": 26,
      "text": "qaṣura fahmī `an idrākihā"
    },
    {
      "index": 27,
      "text": "faḍlan `anistiqṣā-ihā"
    },
    {
      "index": 28,
      "text": "fakayfa lī bitaḥṣīlish shukr"
    },
    {
      "index": 29,
      "text": "washukrī iyyāka yaftaqiru ilā shukr"
    },
    {
      "index": 30,
      "text": "fakullamā qultu lakal ḥamd"
    },
    {
      "index": 31,
      "text": "wajaba `alayya lidhālika an aqula lakal ḥamd"
    },
    {
      "index": 32,
      "text": "ilāhī fakamā ghadhhaytanā biluṭfik"
    },
    {
      "index": 33,
      "text": "warabbaytanā biṣun`ik"
    },
    {
      "index": 34,
      "text": "fatammim `alaynā sawābighan ni`am"
    },
    {
      "index": 35,
      "text": "wadfa` `annā makārihan niqam"
    },
    {
      "index": 36,
      "text": "wa ātinā min ḥuẓūẓid dārayn"
    },
    {
      "index": 37,
      "text": "ar-fa`ahā wa jallahā `ājilan wājilā"
    },
    {
      "index": 38,
      "text": "walakal ḥamdu `alā ḥusni balā-ik"
    },
    {
      "index": 39,
      "text": "wasubūghi na`mā-ik"
    },
    {
      "index": 40,
      "text": "ḥamdan yuwāfiqu riḍāk"
    },
    {
      "index": 41,
      "text": "wayamtaril `aẓīma min birrika wanadāk"
    },
    {
      "index": 42,
      "text": "yā `aẓīmu yā karīm"
    },
    {
      "index": 43,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    }
  ]
}