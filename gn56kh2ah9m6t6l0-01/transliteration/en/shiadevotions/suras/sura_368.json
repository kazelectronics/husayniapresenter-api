{
  "index": 368,
  "name": "Sura 368",
  "ayas": [
    {
      "index": 1,
      "text": "bī min ni`matin wa`āfiyah",
      "bismillah": "allāhumma innī ush-hiduka annahu mā aṣbaḥ"
    },
    {
      "index": 2,
      "text": "fī dīnin aw dunyā"
    },
    {
      "index": 3,
      "text": "faminka waḥdaka lā sharīka lak"
    },
    {
      "index": 4,
      "text": "lakal ḥamdu walakash shukrubihā `alayy"
    },
    {
      "index": 5,
      "text": "ḥattā tar-ḍā waba`dar riḍā"
    }
  ]
}