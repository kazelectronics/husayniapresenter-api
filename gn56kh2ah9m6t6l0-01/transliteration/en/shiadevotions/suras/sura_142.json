{
  "index": 142,
  "name": "Sura 142",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluka ṣabrash shākirīna lak"
    },
    {
      "index": 3,
      "text": "wa`amalal khā-ifīna mink"
    },
    {
      "index": 4,
      "text": "wayaqīnal `ābidīna lak"
    },
    {
      "index": 5,
      "text": "allāhumma antal `aliyyul `aẓīm"
    },
    {
      "index": 6,
      "text": "wa-ana `abdukal bā-isul faqīr"
    },
    {
      "index": 7,
      "text": "antal ghaniyyul ḥamīd"
    },
    {
      "index": 8,
      "text": "wa-anal `abdudh dhalīl"
    },
    {
      "index": 9,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 10,
      "text": "wamnun bighināka `alā faqrī"
    },
    {
      "index": 11,
      "text": "wabiḥilmika `alā jahlī"
    },
    {
      "index": 12,
      "text": "wabiquwwatika `alā ḍa`fī"
    },
    {
      "index": 13,
      "text": "yā qawiyyu yā `azīz"
    },
    {
      "index": 14,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 15,
      "text": "al awṣiyā-il mar-ḍiyyīn"
    },
    {
      "index": 16,
      "text": "wakfinī mā ahammanī min amrid dunyā wal-ākhirah"
    },
    {
      "index": 17,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}