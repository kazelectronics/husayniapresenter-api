{
  "index": 100,
  "name": "Sura 100",
  "ayas": [
    {
      "index": 1,
      "text": "wal `ādiyāti ḍabḥā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "fal mūriyāti qad-ḥā"
    },
    {
      "index": 3,
      "text": "fal mughīrāti ṣubḥā"
    },
    {
      "index": 4,
      "text": "fa-atharna bihi naq`ā"
    },
    {
      "index": 5,
      "text": "fawasaṭna bihi jam`ā"
    },
    {
      "index": 6,
      "text": "innal-insāna lirabbihi lakanūd"
    },
    {
      "index": 7,
      "text": "wa innahu `alā dhālika lashahīd"
    },
    {
      "index": 8,
      "text": "wa innahu liḥubbil khayri lashadīd"
    },
    {
      "index": 9,
      "text": "afalā ya`lamu idhā bu`thira mā fil qubūr"
    },
    {
      "index": 10,
      "text": "waḥuṣṣila mā fiṣ ṣudūr"
    },
    {
      "index": 11,
      "text": "inna rabbahum bihim yawma-idhin lakhabīr"
    }
  ]
}