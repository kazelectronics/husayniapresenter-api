{
  "index": 193,
  "name": "Sura 193",
  "ayas": [
    {
      "index": 1,
      "text": "wa-akhī nabiyyik",
      "bismillah": "allāhumma ṣalli `alā waliyyik"
    },
    {
      "index": 2,
      "text": "wawazīrihi waḥabībihi wakhalīlih"
    },
    {
      "index": 3,
      "text": "wamawḍi`i sirrih"
    },
    {
      "index": 4,
      "text": "wakhiyaratihi min usratih"
    },
    {
      "index": 5,
      "text": "wawaṣiyyihi waṣafwatihi wakhāliṣatih"
    },
    {
      "index": 6,
      "text": "wa-amīnihi wawaliyyih"
    },
    {
      "index": 7,
      "text": "wa-ashrafi `itratihil ladhīna-āmanū bih"
    },
    {
      "index": 8,
      "text": "wa-abī dhurriyyatih"
    },
    {
      "index": 9,
      "text": "wabābi ḥikmatih"
    },
    {
      "index": 10,
      "text": "wan nāṭiqi biḥujjatih"
    },
    {
      "index": 11,
      "text": "wad dā`ī ilā sharī`atih"
    },
    {
      "index": 12,
      "text": "walmāḍī `alā sunnatih"
    },
    {
      "index": 13,
      "text": "wakhalīfatihi `alā ummatih"
    },
    {
      "index": 14,
      "text": "sayyidil muslimīn"
    },
    {
      "index": 15,
      "text": "wa-amīril mu-minīn"
    },
    {
      "index": 16,
      "text": "waqā-idil ghurril muḥajjalīn"
    },
    {
      "index": 17,
      "text": "afḍala mā ṣallayta `alā aḥadin min khalqik"
    },
    {
      "index": 18,
      "text": "wa-aṣfiyā-ika wa-awṣiyā-i anbiyā-ik"
    },
    {
      "index": 19,
      "text": "allāhumma innī ash-had"
    },
    {
      "index": 20,
      "text": "annahu qad ballagha `an nabiyyika ṣallallāhu `alayhi wa ālihi mā ḥummil"
    },
    {
      "index": 21,
      "text": "wara`ā mastuḥfiẓ"
    },
    {
      "index": 22,
      "text": "waḥafiẓa mastūdi`"
    },
    {
      "index": 23,
      "text": "waḥallala ḥalālak"
    },
    {
      "index": 24,
      "text": "waḥarrama ḥarāmak"
    },
    {
      "index": 25,
      "text": "wa-aqāma aḥkāmak"
    },
    {
      "index": 26,
      "text": "wada`ā ilā sabīlik"
    },
    {
      "index": 27,
      "text": "wawālā awliyā-ak"
    },
    {
      "index": 28,
      "text": "wa`ādā a`dā-ak"
    },
    {
      "index": 29,
      "text": "wajāhadan nākithīna `an sabīlik"
    },
    {
      "index": 30,
      "text": "walqāsiṭīna walmāriqīna `an amrik"
    },
    {
      "index": 31,
      "text": "ṣābiran muḥtasibā"
    },
    {
      "index": 32,
      "text": "muqbilan ghayra mudbir"
    },
    {
      "index": 33,
      "text": "lā ta-khudhuhu fillāhi lawmatu lā-im"
    },
    {
      "index": 34,
      "text": "ḥattā balagha fī dhālikar riḍā"
    },
    {
      "index": 35,
      "text": "wasallama ilaykal qaḍā"
    },
    {
      "index": 36,
      "text": "wa`abadaka mukhliṣā"
    },
    {
      "index": 37,
      "text": "wanaṣaḥa laka mujtahidā"
    },
    {
      "index": 38,
      "text": "ḥattā atāhul yaqīn"
    },
    {
      "index": 39,
      "text": "faqabaḍtahu ilayka shahīdan sa`īdā"
    },
    {
      "index": 40,
      "text": "waliyyan taqiyyan raḍiyyā"
    },
    {
      "index": 41,
      "text": "zakiyyan hādiyan mahdiyyā"
    },
    {
      "index": 42,
      "text": "allāhumma ṣalli `alā muḥammadin wa`alayh"
    },
    {
      "index": 43,
      "text": "afḍala mā ṣallayta `alā aḥadin min anbiyā-ika wa-aṣfiyā-ik"
    },
    {
      "index": 44,
      "text": "yā rabbal `ālamīn"
    }
  ]
}