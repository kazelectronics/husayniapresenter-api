{
  "index": 255,
  "name": "Sura 255",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al ḥamdu lillāhil ladhī"
    },
    {
      "index": 3,
      "text": "tajallā lilqulūbi bil `aẓamah"
    },
    {
      "index": 4,
      "text": "waḥtajaba `anil abṣāri bil `izzah"
    },
    {
      "index": 5,
      "text": "waqtadara `alal ashyā-i bil qudrah"
    },
    {
      "index": 6,
      "text": "falal abṣāru tathbutu liru-yatih"
    },
    {
      "index": 7,
      "text": "wa lal awhāmu tablughu kunha `aẓamatih"
    },
    {
      "index": 8,
      "text": "tajabbara bil `aẓamati wal kibriyā"
    },
    {
      "index": 9,
      "text": "wa ta`aṭṭafa bil `izzi wal birri wal jalāl"
    },
    {
      "index": 10,
      "text": "wa taqaddasa bil ḥusni wal jamāl"
    },
    {
      "index": 11,
      "text": "wa tamajjada bil fakhri wal bahā"
    },
    {
      "index": 12,
      "text": "wa tajallala bil majdi wal-ālā"
    },
    {
      "index": 13,
      "text": "wastakhlaṣa binnūri waḍ ḍiyā"
    },
    {
      "index": 14,
      "text": "khāliqun lā naẓīra lah"
    },
    {
      "index": 15,
      "text": "wa aḥadun lā nidda lah"
    },
    {
      "index": 16,
      "text": "wa wāḥidun lā ḍidda lah"
    },
    {
      "index": 17,
      "text": "wa ṣamadun lā kufwa lah"
    },
    {
      "index": 18,
      "text": "wa ilāhun lā thāniya ma`ah"
    },
    {
      "index": 19,
      "text": "wa fāṭirun lā sharīka lah"
    },
    {
      "index": 20,
      "text": "wa rāziqun lā mu`īna lah"
    },
    {
      "index": 21,
      "text": "wal awwalu bilā zawāl"
    },
    {
      "index": 22,
      "text": "wad dā-imu bilā fana"
    },
    {
      "index": 23,
      "text": "wal qā-imu bilā `ana-a"
    },
    {
      "index": 24,
      "text": "wal mu-minu bilā nihāyah"
    },
    {
      "index": 25,
      "text": "wal mubdi-u bilā amad"
    },
    {
      "index": 26,
      "text": "waṣ ṣāni`u bilā aḥad"
    },
    {
      "index": 27,
      "text": "war rabbu bilā sharīk"
    },
    {
      "index": 28,
      "text": "wal fāṭiru bilā kulfah"
    },
    {
      "index": 29,
      "text": "wal fa``ālu bilā `ajz"
    },
    {
      "index": 30,
      "text": "laysa lahu ḥaddun fī makān"
    },
    {
      "index": 31,
      "text": "wa lā ghāyatun fī zamān"
    },
    {
      "index": 32,
      "text": "lam yazal wa lā yazūlu wa lan yazāla kadhālika abadā"
    },
    {
      "index": 33,
      "text": "huwal ilāhul ḥayyul qayyūm"
    },
    {
      "index": 34,
      "text": "ad dā-imul qadīm"
    },
    {
      "index": 35,
      "text": "al qādirul ḥakīm"
    },
    {
      "index": 36,
      "text": "ilāhī"
    },
    {
      "index": 37,
      "text": "`ubayduka bifinā-ik"
    },
    {
      "index": 38,
      "text": "sā-iluka bifinā-ik"
    },
    {
      "index": 39,
      "text": "faqīruka bifinā-ika"
    },
    {
      "index": 40,
      "text": "ilāhī"
    },
    {
      "index": 41,
      "text": "`ubayduka bifinā-ik"
    },
    {
      "index": 42,
      "text": "sā-iluka bifinā-ik"
    },
    {
      "index": 43,
      "text": "faqīruka bifinā-ika"
    },
    {
      "index": 44,
      "text": "ilāhī"
    },
    {
      "index": 45,
      "text": "`ubayduka bifinā-ik"
    },
    {
      "index": 46,
      "text": "sā-iluka bifinā-ik"
    },
    {
      "index": 47,
      "text": "faqīruka bifinā-ika"
    },
    {
      "index": 48,
      "text": "ilāhī"
    },
    {
      "index": 49,
      "text": "laka yar-habul mutarahhibūn"
    },
    {
      "index": 50,
      "text": "wa ilayka akhlaṣal mustahillūn"
    },
    {
      "index": 51,
      "text": "rahbatan laka wa rajā-an li`afwik"
    },
    {
      "index": 52,
      "text": "yā ilāhal ḥaqq"
    },
    {
      "index": 53,
      "text": "ir-ḥam du`ā-al mustaṣrikhīn"
    },
    {
      "index": 54,
      "text": "wa`fu `an jarā-imil ghāfilīn"
    },
    {
      "index": 55,
      "text": "wa zid fī iḥsānil munībīna yawmal wufūdi `alayk"
    },
    {
      "index": 56,
      "text": "yā karīm"
    }
  ]
}