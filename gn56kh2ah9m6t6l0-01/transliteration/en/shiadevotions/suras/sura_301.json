{
  "index": 301,
  "name": "Sura 301",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluka mūjibāti raḥmatik"
    },
    {
      "index": 3,
      "text": "wa`azā-ima maghfiratik"
    },
    {
      "index": 4,
      "text": "wan najāta minan nār"
    },
    {
      "index": 5,
      "text": "wamin kulli baliyyah"
    },
    {
      "index": 6,
      "text": "walfawza bil jannah"
    },
    {
      "index": 7,
      "text": "war riḍwāna fī dāris salām"
    },
    {
      "index": 8,
      "text": "wajiwāri nabiyyika muḥammadin `alayhi wa ālihis salām"
    },
    {
      "index": 9,
      "text": "allāhumma mā binā min ni`matin famink"
    },
    {
      "index": 10,
      "text": "la-ilāha illā ant"
    },
    {
      "index": 11,
      "text": "astaghfiruka wa-atūbu ilayk"
    }
  ]
}