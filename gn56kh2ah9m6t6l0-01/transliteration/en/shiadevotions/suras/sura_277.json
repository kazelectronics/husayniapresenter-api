{
  "index": 277,
  "name": "Sura 277",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma yā malādhal lā-idhīn"
    },
    {
      "index": 3,
      "text": "wayā ma`ādhal `ā-idhīn"
    },
    {
      "index": 4,
      "text": "wayā munjiyal hālikin"
    },
    {
      "index": 5,
      "text": "wayā `āṣimal bā-isīn"
    },
    {
      "index": 6,
      "text": "wayā rāḥimal masākīn"
    },
    {
      "index": 7,
      "text": "wayā mujībal muḍṭarrīn"
    },
    {
      "index": 8,
      "text": "wayā kanzal muftaqirīn"
    },
    {
      "index": 9,
      "text": "wayā jābiral munkasirīn"
    },
    {
      "index": 10,
      "text": "wayā ma-wal munqaṭi`īn"
    },
    {
      "index": 11,
      "text": "wayā nāṣiral mustaḍ`afīn"
    },
    {
      "index": 12,
      "text": "wayā mujīral khā-ifīn"
    },
    {
      "index": 13,
      "text": "wayā mughīthal makrūbīn"
    },
    {
      "index": 14,
      "text": "wayā ḥiṣnal lāji-īn"
    },
    {
      "index": 15,
      "text": "in lam a`udh bi`izzatika fabiman a`ūdh"
    },
    {
      "index": 16,
      "text": "wa-in lam aludh biqudratika fabiman alūdh"
    },
    {
      "index": 17,
      "text": "waqad alja-atnidh dhunūbu ilat tashabbuthi bi-adhyāli `afwik"
    },
    {
      "index": 18,
      "text": "wa-aḥwajatnil khaṭāyā ilastiftāḥi abwābi ṣafḥik"
    },
    {
      "index": 19,
      "text": "wada`atnil isā-atu ilal inākhati bifanā-i `izzik"
    },
    {
      "index": 20,
      "text": "waḥamalatnil makhāfatu min naqimatika `alat tamassuki bi`ur-wati `aṭfik"
    },
    {
      "index": 21,
      "text": "wamā aḥaqqu mani`taṣama biḥablika an yukhdhal"
    },
    {
      "index": 22,
      "text": "walā yalīqu bimanistajāra bi`izzika an yuslama aw yuhmal"
    },
    {
      "index": 23,
      "text": "ilāhī falā tukhlinā min ḥimāyatik"
    },
    {
      "index": 24,
      "text": "walā tu`rinā min ri`āyatik"
    },
    {
      "index": 25,
      "text": "wadhudnā `an mawāridil halakah"
    },
    {
      "index": 26,
      "text": "fa-innā bi`aynika wafī kanafika walak"
    },
    {
      "index": 27,
      "text": "as-aluka bi-ahli khāṣṣatik"
    },
    {
      "index": 28,
      "text": "min malā-ikatika waṣ ṣāliḥīna min bariyyatik"
    },
    {
      "index": 29,
      "text": "an taj`ala `alaynā wāqiyah"
    },
    {
      "index": 30,
      "text": "tunjīnā minal halakāt"
    },
    {
      "index": 31,
      "text": "watujannibunā minal-āfāt"
    },
    {
      "index": 32,
      "text": "watukinnunā min dawāhil muṣībāt"
    },
    {
      "index": 33,
      "text": "wa-an tunzila `alaynā min sakīnatik"
    },
    {
      "index": 34,
      "text": "wa-an tughashhiya wujūhanā bi-anwāri maḥabbatik"
    },
    {
      "index": 35,
      "text": "wa-an tu-wiyanā ilā shadīdi ruknik"
    },
    {
      "index": 36,
      "text": "wa-an taḥwiyanā fī aknāfi `iṣmatik"
    },
    {
      "index": 37,
      "text": "bira-fatika waraḥmatik"
    },
    {
      "index": 38,
      "text": "yā ar-ḥamar rāḥimīn"
    }
  ]
}