{
  "index": 85,
  "name": "Sura 85",
  "ayas": [
    {
      "index": 1,
      "text": "was samā-i dhātil burūj",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wal yawmil maw`ūd"
    },
    {
      "index": 3,
      "text": "washāhidin wa mash-hūd"
    },
    {
      "index": 4,
      "text": "qutila aṣ-ḥābul-ukhdūd"
    },
    {
      "index": 5,
      "text": "an-nāri dhātil waqūd"
    },
    {
      "index": 6,
      "text": "idh hum `alayhā qu`ūd"
    },
    {
      "index": 7,
      "text": "wahum `alā mā yaf`alūna bil mu-minīna shuhūd"
    },
    {
      "index": 8,
      "text": "wa mā naqamū minhum illā an yu-minū billāhil `azīzil ḥamīd"
    },
    {
      "index": 9,
      "text": "alladhī lahu mulkus samāwāti wal-arḍ ۚ wallāhu `alā kulli shay-in shahīd"
    },
    {
      "index": 10,
      "text": "innal ladhīna fatanul mu-minīna wal mu-mināti thumma lam yatūbū falahum `adhābu jahannama walahum `adhābul ḥarīq"
    },
    {
      "index": 11,
      "text": "innal ladhīna āmanū wa`amiluṣ ṣāliḥāti lahum jannātun tajrī min taḥtihal-anhār ۚ dhālikal fawzul kabīr"
    },
    {
      "index": 12,
      "text": "inna baṭsha rabbika lashadīd"
    },
    {
      "index": 13,
      "text": "innahu huwa yubdi-u wayu`īd"
    },
    {
      "index": 14,
      "text": "wahuwal ghafūrul wadūd"
    },
    {
      "index": 15,
      "text": "dhul `arshil majīd"
    },
    {
      "index": 16,
      "text": "fa``ālul limā yurīd"
    },
    {
      "index": 17,
      "text": "hal atāka ḥadīthul junūd"
    },
    {
      "index": 18,
      "text": "fir`awna wathamūd"
    },
    {
      "index": 19,
      "text": "balil ladhīna kafarū fī takdhīb"
    },
    {
      "index": 20,
      "text": "wallāhu min warā-ihim muḥīṭ"
    },
    {
      "index": 21,
      "text": "bal huwa qur-ānun majīd"
    },
    {
      "index": 22,
      "text": "fī lawḥin maḥfūẓ"
    }
  ]
}