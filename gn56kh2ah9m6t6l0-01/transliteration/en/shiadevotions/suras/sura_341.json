{
  "index": 341,
  "name": "Sura 341",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī ta`arraḍa laka fī hādhal laylil muta`arriḍūn"
    },
    {
      "index": 3,
      "text": "wa qaṣadaka fīhil qāṣidūn"
    },
    {
      "index": 4,
      "text": "wa ammala faḍlaka wa ma`rūfakaṭ ṭālibūn"
    },
    {
      "index": 5,
      "text": "wa laka fī hādhal layli nafaḥāt"
    },
    {
      "index": 6,
      "text": "wa jawā-izu wa `aṭāyā wa mawāhib"
    },
    {
      "index": 7,
      "text": "tamunnu bihā `alā man tashā-u min `ibādik"
    },
    {
      "index": 8,
      "text": "wa tamna`uhā man lam tasbiqu lahul `ināyatu mink"
    },
    {
      "index": 9,
      "text": "wa hā ana dhā `ubaydukal faqīru ilayk"
    },
    {
      "index": 10,
      "text": "al mu-ammilu faḍlaka wa ma`rūfak"
    },
    {
      "index": 11,
      "text": "fa-in kunta yā mawlāya tafaḍḍalta fī hādhihil laylah"
    },
    {
      "index": 12,
      "text": "`alā aḥadin min khalqik"
    },
    {
      "index": 13,
      "text": "wa `udta `alayhi bi`ā-idatin min `aṭfik"
    },
    {
      "index": 14,
      "text": "faṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 15,
      "text": "aṭ ṭayyibīnaṭ ṭāhirīnal khayyirīnal fāḍilīn"
    },
    {
      "index": 16,
      "text": "wa jud `alayya biṭawlika wa ma`rūfik"
    },
    {
      "index": 17,
      "text": "yā rabbal `ālamīna wa ṣallallāhu `alā muḥammad"
    },
    {
      "index": 18,
      "text": "khātamin nabiyyīna wa-ālihiṭ ṭāhirīna wa sallama taslīmā"
    },
    {
      "index": 19,
      "text": "innallāha ḥamīdun majīd"
    },
    {
      "index": 20,
      "text": "allāhumma innī ad`ūka kamā amar-ta fastajib lī kamā wa`adt"
    },
    {
      "index": 21,
      "text": "innaka lā tukhliful mī`ād"
    }
  ]
}