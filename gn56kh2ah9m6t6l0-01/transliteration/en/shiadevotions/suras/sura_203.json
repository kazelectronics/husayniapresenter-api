{
  "index": 203,
  "name": "Sura 203",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al ḥamdu lillāhil awwali bilā awwalin kāna qablah"
    },
    {
      "index": 3,
      "text": "wal-ākhiri bilā-ākhirin yakūnu ba`dah"
    },
    {
      "index": 4,
      "text": "al ladhī qaṣurat `an ru-yatihi abṣārun nāẓirīn"
    },
    {
      "index": 5,
      "text": "wa `ajazat `an na`tihi awhāmul wāṣifīn"
    },
    {
      "index": 6,
      "text": "ibtada`a biqudratihil khalqabtidā`ā"
    },
    {
      "index": 7,
      "text": "wakhtara`ahum `alā mashiyyatihikhtirā`ā"
    },
    {
      "index": 8,
      "text": "thumma salaka bihim ṭarīqa irādatih"
    },
    {
      "index": 9,
      "text": "wa ba`athahum fī sabīli maḥabbatih"
    },
    {
      "index": 10,
      "text": "lā yamlikūna ta-khīran `ammā qaddamahum ilayh"
    },
    {
      "index": 11,
      "text": "wa lā yastaṭī`ūna taqadduman ilā mā akhharahum `anh"
    },
    {
      "index": 12,
      "text": "wa ja`ala likulli rūḥin minhum"
    },
    {
      "index": 13,
      "text": "qūtan ma`lūman maqsūman min rizqih"
    },
    {
      "index": 14,
      "text": "lā yanquṣu man zādahu nāqiṣ"
    },
    {
      "index": 15,
      "text": "wa lā yazīdu man naqaṣa minhum zā-id"
    },
    {
      "index": 16,
      "text": "thumma ḍaraba lahu fil ḥayāti ajalan mawqūtā"
    },
    {
      "index": 17,
      "text": "wa naṣaba lahu amadan maḥdūdā"
    },
    {
      "index": 18,
      "text": "yatakhaṭṭa-u ilayhi bi-ayyāmi `umurih"
    },
    {
      "index": 19,
      "text": "wa yar-haquhu bi-a`wāmi dahrih"
    },
    {
      "index": 20,
      "text": "ḥattā idhā balagha aqṣā atharih"
    },
    {
      "index": 21,
      "text": "wastaw`aba ḥisāba `umurih"
    },
    {
      "index": 22,
      "text": "qabaḍahu ilā mā nadabahu ilayh"
    },
    {
      "index": 23,
      "text": "min mawfūri thawābih, aw maḥdhūri `iqābih"
    },
    {
      "index": 24,
      "text": "liyajziyal ladhīna asā-ū bimā `amilū"
    },
    {
      "index": 25,
      "text": "wa yajziyal ladhīna aḥsanū bil ḥusnā"
    },
    {
      "index": 26,
      "text": "`adlan minh, taqaddasat asma-uh"
    },
    {
      "index": 27,
      "text": "wa taẓāharat-āla-uh"
    },
    {
      "index": 28,
      "text": "lā yus-alu `ammā yaf`alu wa hum yus-alūn"
    },
    {
      "index": 29,
      "text": "wal ḥamdu lillāhil ladhī law ḥabasa `an `ibādihi ma`rifah"
    },
    {
      "index": 30,
      "text": "ḥamdihi `alā mā ablāhum min minanihil mutatābi`ah"
    },
    {
      "index": 31,
      "text": "wa asbagha `alayhim min ni`amihil mutaẓāhirah"
    },
    {
      "index": 32,
      "text": "lataṣarrafū fī minanihi falam yaḥmadūh"
    },
    {
      "index": 33,
      "text": "wa tawassa`ū fī rizqihi falam yashkurūh"
    },
    {
      "index": 34,
      "text": "wa law kānū kadhalika lakharajū min ḥudūdil insāniyyah"
    },
    {
      "index": 35,
      "text": "ilā ḥaddil bahīmiyyah"
    },
    {
      "index": 36,
      "text": "fakānū kamā waṣafa fī muḥkami kitābih"
    },
    {
      "index": 37,
      "text": "in hum illā kal-an`āmi bal hum aḍallu sabilā"
    },
    {
      "index": 38,
      "text": "wal ḥamdu lillāhi `alā mā `arrafanā min nafsih"
    },
    {
      "index": 39,
      "text": "wa alhamanā min shukrih"
    },
    {
      "index": 40,
      "text": "wa fataḥa lanā min abwābil `ilmi birubūbiyyatih"
    },
    {
      "index": 41,
      "text": "wa dallanā `alayhi minal ikhlāṣi lahu fī tawḥīdih"
    },
    {
      "index": 42,
      "text": "wa jannabanā minal ilḥādi wash shakki fī amrih"
    },
    {
      "index": 43,
      "text": "ḥamdan nu`ammaru bihi fīman ḥamidahu min khalqih"
    },
    {
      "index": 44,
      "text": "wa nasbiqu bihi man sabaqa ilā riḍāhu wa `afwih"
    },
    {
      "index": 45,
      "text": "ḥamdan yuḍī-u lanā bihi ẓulumātil bar-zakh"
    },
    {
      "index": 46,
      "text": "wa yusahhilu `alaynā bihi sabīlal mab`ath"
    },
    {
      "index": 47,
      "text": "wa yusharrifu bihi manāzilanā `inda mawāqifil ash-hād"
    },
    {
      "index": 48,
      "text": "yawma tujzā kullu nafsin bimā kasabat wa hum lā yuẓlamūn"
    },
    {
      "index": 49,
      "text": "yawma lā yughnī mawlan `an mawlan shay-an wa lā hum yunṣarūn"
    },
    {
      "index": 50,
      "text": "ḥamdan yar-tafi`u minnā ilā a`lā `illiyyīn"
    },
    {
      "index": 51,
      "text": "fī kitābin mar-qūmin yash-haduhul muqarrabūn"
    },
    {
      "index": 52,
      "text": "ḥamdan taqarru bihi `uyūnunā idhā bariqatil abṣār"
    },
    {
      "index": 53,
      "text": "wa tabyaḍḍu bihi wujūhunā idhaswaddatil abshār"
    },
    {
      "index": 54,
      "text": "ḥamdan nu`taqu bihi min alīmi nārillāh"
    },
    {
      "index": 55,
      "text": "ilā karīmi jiwārillāh"
    },
    {
      "index": 56,
      "text": "ḥamdan nuzāḥimu bihi malā-ikatahul muqarrabīn"
    },
    {
      "index": 57,
      "text": "wa nuḍāmmu bihi anbiyā-ahul mur-salīn"
    },
    {
      "index": 58,
      "text": "fī dāril muqāmatil latī lā tazūl"
    },
    {
      "index": 59,
      "text": "wa maḥalli karāmatihil latī lā taḥūl"
    },
    {
      "index": 60,
      "text": "wal ḥamdu lillāhil ladhikhtāra lanā maḥāsinal khalq"
    },
    {
      "index": 61,
      "text": "wa ajrā `alaynā ṭayyibātir rizq"
    },
    {
      "index": 62,
      "text": "wa ja`ala lanal faḍīlata bil malakati `alā jamī`il khalq"
    },
    {
      "index": 63,
      "text": "fakullu khalīqatihi munqādatun lanā biqudratih"
    },
    {
      "index": 64,
      "text": "wa ṣā-iratun ilā ṭā`atinā bi`izzatih"
    },
    {
      "index": 65,
      "text": "wal ḥamdu lillāhil ladhī aghlaqa `annā bābal ḥājati illā ilayh"
    },
    {
      "index": 66,
      "text": "fakayfa nuṭīqu ḥamdahu am matā nu-addī shukrahu lā, matā"
    },
    {
      "index": 67,
      "text": "wal ḥamdu lillāhil ladhī rakkaba fīnā-ālātil basṭ"
    },
    {
      "index": 68,
      "text": "wa ja`ala lanā adawātil qabḍ"
    },
    {
      "index": 69,
      "text": "wa matta`anā bi-ar-wāḥil ḥayāh"
    },
    {
      "index": 70,
      "text": "wa athbata fīnā jawāriḥal a`māl"
    },
    {
      "index": 71,
      "text": "wa ghadhhānā biṭayyibātir rizq"
    },
    {
      "index": 72,
      "text": "wa aghnānā bifaḍlih"
    },
    {
      "index": 73,
      "text": "wa aqnānā bimannih"
    },
    {
      "index": 74,
      "text": "thumma amaranā liyakhtabira ṭā`atanā"
    },
    {
      "index": 75,
      "text": "wa nahānā liyabtaliya shukranā"
    },
    {
      "index": 76,
      "text": "fakhālafnā `an ṭarīqi amrih"
    },
    {
      "index": 77,
      "text": "wa rakibnā mutūna zajrih"
    },
    {
      "index": 78,
      "text": "falam yabtadir-nā bi`uqūbatih"
    },
    {
      "index": 79,
      "text": "wa lam yu`ājilnā biniqmatih"
    },
    {
      "index": 80,
      "text": "bal ta-annānā biraḥmatihi takarrumā"
    },
    {
      "index": 81,
      "text": "wantaẓara murāja`atanā bira-fatihi ḥilmā"
    },
    {
      "index": 82,
      "text": "wal ḥamdu lillāhil ladhī dallanā `alat tawbah"
    },
    {
      "index": 83,
      "text": "al latī lam nufid-hā illā min faḍlih"
    },
    {
      "index": 84,
      "text": "falaw lam na`tadid min faḍlih"
    },
    {
      "index": 85,
      "text": "illā bihā laqad ḥasuna bala-uhu `indanā"
    },
    {
      "index": 86,
      "text": "wa jalla iḥsānuhu ilaynā"
    },
    {
      "index": 87,
      "text": "wa jasuma faḍluhu `alaynā"
    },
    {
      "index": 88,
      "text": "famā hakadhā kānat sunnatuhu fit tawbah"
    },
    {
      "index": 89,
      "text": "liman kāna qablanā"
    },
    {
      "index": 90,
      "text": "laqad waḍa`a `annā"
    },
    {
      "index": 91,
      "text": "mā lā ṭāqata lanā bih"
    },
    {
      "index": 92,
      "text": "wa lam yukallifnā illā wus`ā"
    },
    {
      "index": 93,
      "text": "wa lam yujashhimnā illā yusrā"
    },
    {
      "index": 94,
      "text": "wa lam yada` li-aḥadin minnā ḥujjatan wa lā `udhrā"
    },
    {
      "index": 95,
      "text": "falhāliku minnā man halaka `alayh"
    },
    {
      "index": 96,
      "text": "was sa`īdu minnā man raghiba ilayh"
    },
    {
      "index": 97,
      "text": "wal ḥamdu lillāhi bikulli mā ḥamidahu bih"
    },
    {
      "index": 98,
      "text": "adnā malā-ikatihi ilayh"
    },
    {
      "index": 99,
      "text": "wa akramu khalīqatihi `alayh"
    },
    {
      "index": 100,
      "text": "wa ar-ḍā ḥāmidīhi ladayh"
    },
    {
      "index": 101,
      "text": "ḥamdan yafḍulu sā-iral ḥamd"
    },
    {
      "index": 102,
      "text": "kafaḍli rabbinā `alā jamī`i khalqih"
    },
    {
      "index": 103,
      "text": "thumma lahul ḥamd"
    },
    {
      "index": 104,
      "text": "makāna kulli ni`matin lahu `alaynā"
    },
    {
      "index": 105,
      "text": "wa `alā jamī`i `ibādihil māḍīna wal bāqīn"
    },
    {
      "index": 106,
      "text": "`adada mā aḥāṭa bihi `ilmuhu min jamī`il ashyā"
    },
    {
      "index": 107,
      "text": "wa makāna kulli wāḥidatin minhā `adaduhā"
    },
    {
      "index": 108,
      "text": "aḍ`āfan muḍā`afatan abadan sar-madan ilā yawmil qiyāmah"
    },
    {
      "index": 109,
      "text": "ḥamdan lā muntahā liḥaddih"
    },
    {
      "index": 110,
      "text": "wa lā ḥisāba li`adadih"
    },
    {
      "index": 111,
      "text": "wa lā mablagha lighāyatih"
    },
    {
      "index": 112,
      "text": "wa lanqiṭā`a li-amadih"
    },
    {
      "index": 113,
      "text": "ḥamdan yakūnu wuṣlatan ilā ṭā`atihi wa `afwih"
    },
    {
      "index": 114,
      "text": "wa sababan ilā riḍwānih"
    },
    {
      "index": 115,
      "text": "wa dharī`atan ilā maghfiratih"
    },
    {
      "index": 116,
      "text": "wa ṭarīqan ilā jannatih"
    },
    {
      "index": 117,
      "text": "wa khafīran min naqimatih"
    },
    {
      "index": 118,
      "text": "wa amnan min ghaḍabih"
    },
    {
      "index": 119,
      "text": "wa ẓahīran `alā ṭā`atih"
    },
    {
      "index": 120,
      "text": "wa ḥājizan `an ma`ṣiyatih"
    },
    {
      "index": 121,
      "text": "wa `awnan `alā ta-diyati ḥaqqihi wa waẓā-ifih"
    },
    {
      "index": 122,
      "text": "ḥamdan nas`adu bihi fis su`adā-i min awliyā-ih"
    },
    {
      "index": 123,
      "text": "wa naṣīru bihi fī naẓmish shuhadā-i bisuyūfi a`dā-ih"
    },
    {
      "index": 124,
      "text": "innahu waliyyun ḥamīd"
    }
  ]
}