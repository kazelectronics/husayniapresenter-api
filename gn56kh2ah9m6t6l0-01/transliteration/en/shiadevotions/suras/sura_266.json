{
  "index": 266,
  "name": "Sura 266",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī atarāka ba`dal īmāni bika tu`adhhibunī"
    },
    {
      "index": 3,
      "text": "am ba`da ḥubbī iyyāka tuba``idunī"
    },
    {
      "index": 4,
      "text": "am ma`a rajā-ī liraḥmatika waṣafḥika taḥrimunī"
    },
    {
      "index": 5,
      "text": "am ma`astijāratī bi`afwika tuslimunī"
    },
    {
      "index": 6,
      "text": "ḥāshā liwajhikal karīmi an tukhayyibanī"
    },
    {
      "index": 7,
      "text": "layta shi`rī alishhaqā-i waladatnī ummī"
    },
    {
      "index": 8,
      "text": "am lil`anā-i rabbatnī"
    },
    {
      "index": 9,
      "text": "falaytahā lam talidnī walam turabbinī"
    },
    {
      "index": 10,
      "text": "walaytanī `alimt"
    },
    {
      "index": 11,
      "text": "amin ahlis sa`ādati ja`altanī"
    },
    {
      "index": 12,
      "text": "wabiqur-bika wajiwārika khaṣaṣtanī"
    },
    {
      "index": 13,
      "text": "fataqarra bidhālika `aynī"
    },
    {
      "index": 14,
      "text": "wataṭma-inna lahu nafsī"
    },
    {
      "index": 15,
      "text": "ilāhī hal tusawwidu wujūhan kharrat sājidatan li`aẓamatik"
    },
    {
      "index": 16,
      "text": "aw tukhrisu alsinatan naṭaqat bilthhanā-i `alā majdika wajalālatik"
    },
    {
      "index": 17,
      "text": "aw taṭba`u `alā qulūbininṭawat `alā maḥabbatik"
    },
    {
      "index": 18,
      "text": "aw tuṣimmu asmā`an taladhhadhat bisamā`i dhikrika fī irādatik"
    },
    {
      "index": 19,
      "text": "aw taghullu akuffan rafa`at-hal-āmālu ilayka rajā-a ra-fatik"
    },
    {
      "index": 20,
      "text": "aw tu`āqibu abdānan `amilat biṭā`atika ḥattā naḥilat fī mujāhadatik"
    },
    {
      "index": 21,
      "text": "aw tu`adhhibu ar-julan sa`at fī `ibādatik"
    },
    {
      "index": 22,
      "text": "ilāhī lā tughliq `alā muwaḥḥidīka abwāba raḥmatik"
    },
    {
      "index": 23,
      "text": "walā taḥjub mushtāqīka `anin naẓari ilā jamīli ru-yatik"
    },
    {
      "index": 24,
      "text": "ilāhī nafsun a`zaztahā bitawḥīdik"
    },
    {
      "index": 25,
      "text": "kayfa tudhilluhā bimahānati hijrānik"
    },
    {
      "index": 26,
      "text": "waḍamīrunin`aqada `alā mawaddatik"
    },
    {
      "index": 27,
      "text": "kayfa tuḥriquhu biḥarārati nīrānik"
    },
    {
      "index": 28,
      "text": "ilāhī ajir-nī min alīmi ghaḍabik"
    },
    {
      "index": 29,
      "text": "wa`aẓīmi sakhaṭik"
    },
    {
      "index": 30,
      "text": "yā ḥannānu yā mannān"
    },
    {
      "index": 31,
      "text": "yā raḥīmu yā raḥmān"
    },
    {
      "index": 32,
      "text": "yā jabbāru yā qahhār"
    },
    {
      "index": 33,
      "text": "yā ghaffāru yā sattār"
    },
    {
      "index": 34,
      "text": "najjinī biraḥmatika min `adhābin nār"
    },
    {
      "index": 35,
      "text": "wafaḍīḥatil `ār"
    },
    {
      "index": 36,
      "text": "idhamtāzal akhyāru minal ashrār"
    },
    {
      "index": 37,
      "text": "waḥālatil aḥwāl"
    },
    {
      "index": 38,
      "text": "wahālatil ahwāl"
    },
    {
      "index": 39,
      "text": "waqarubal muḥsinūn"
    },
    {
      "index": 40,
      "text": "waba`udal musī-ūn"
    },
    {
      "index": 41,
      "text": "wawuffiyat kullu nafsin mā kasabat"
    },
    {
      "index": 42,
      "text": "wahum lā yuẓlamūn"
    }
  ]
}