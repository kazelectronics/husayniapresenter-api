{
  "index": 259,
  "name": "Sura 259",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī innah"
    },
    {
      "index": 3,
      "text": "laysa yaruddu ghaḍabaka illā ḥilmuk"
    },
    {
      "index": 4,
      "text": "walā yunjī min `iqābika illā `afwuk"
    },
    {
      "index": 5,
      "text": "walā yukhalliṣu minka illā raḥmatuka wat taḍarru`u ilayk"
    },
    {
      "index": 6,
      "text": "fahab lī yā ilāhī faraḥan bil qudratil latī"
    },
    {
      "index": 7,
      "text": "bihā tuḥyī maytal bilād"
    },
    {
      "index": 8,
      "text": "wa bihā tanshuru ar-wāḥal `ibād"
    },
    {
      "index": 9,
      "text": "walā tuhliknī wa `arrifnil ijābah"
    },
    {
      "index": 10,
      "text": "yārabbi war-fa`nī wa lā taḍa`nī"
    },
    {
      "index": 11,
      "text": "wanṣur-nī war-zuqnī"
    },
    {
      "index": 12,
      "text": "wa `āfinī minal-āfāt"
    },
    {
      "index": 13,
      "text": "yā rabb"
    },
    {
      "index": 14,
      "text": "in tar-fa`nī faman yaḍa`unī"
    },
    {
      "index": 15,
      "text": "wa in taḍa`nī faman yar-fa`unī"
    },
    {
      "index": 16,
      "text": "waqad `alimtu yā ilāhī an"
    },
    {
      "index": 17,
      "text": "laysa fī ḥukmika ẓulm"
    },
    {
      "index": 18,
      "text": "walā fī naqimatika `ajalah"
    },
    {
      "index": 19,
      "text": "innamā ya`jalu man yakhāful fawt"
    },
    {
      "index": 20,
      "text": "wa yaḥtāju ilaẓ ẓulmiḍ ḍa`īf"
    },
    {
      "index": 21,
      "text": "wa qad ta`ālayta `an dhālika yā sayyidī `uluwwan kabīrā"
    },
    {
      "index": 22,
      "text": "rabbi lā taj`alnī"
    },
    {
      "index": 23,
      "text": "lilbalā-i gharaḍā"
    },
    {
      "index": 24,
      "text": "wa lā linaqimatika naṣabā"
    },
    {
      "index": 25,
      "text": "wa mahhilnī wanaffisnī"
    },
    {
      "index": 26,
      "text": "wa aqilnī `athratī"
    },
    {
      "index": 27,
      "text": "wa lā tutbi`nī bil balā"
    },
    {
      "index": 28,
      "text": "faqad tarā ḍa`fī wa qillata ḥīlatī"
    },
    {
      "index": 29,
      "text": ""
    },
    {
      "index": 30,
      "text": ""
    },
    {
      "index": 31,
      "text": "faṣabbir-nī fa-innī yā rabbi ḍa`īfun mutaḍarri`un ilayka yā rabb"
    },
    {
      "index": 32,
      "text": "wa a`ūdhu bika minka fa-a`idhnī"
    },
    {
      "index": 33,
      "text": "wa antal `aẓīmu a`ẓamu min kulli `aẓīm"
    },
    {
      "index": 34,
      "text": "bika bika bikastatar-t"
    },
    {
      "index": 35,
      "text": "yā allāhu yā allāhu yā allāhu yā allāhu yā allāhu yā allāhu yā allāhu yā allāhu yā allāhu yā allāh"
    },
    {
      "index": 36,
      "text": "ṣalli `alā muḥammadin wa-ālihiṭ ṭayyibīnaṭ ṭāhirīn"
    }
  ]
}