{
  "index": 105,
  "name": "Sura 105",
  "ayas": [
    {
      "index": 1,
      "text": "alam tara kayfa fa`ala rabbuka bi-aṣ-ḥābil fīl",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "alam yaj`al kaydahum fī taḍlīl"
    },
    {
      "index": 3,
      "text": "wa-arsala `alayhim ṭayran abābīl"
    },
    {
      "index": 4,
      "text": "tarmīhim biḥijāratim min sijjīl"
    },
    {
      "index": 5,
      "text": "faja`alahum ka`aṣfin ma-kūl"
    }
  ]
}