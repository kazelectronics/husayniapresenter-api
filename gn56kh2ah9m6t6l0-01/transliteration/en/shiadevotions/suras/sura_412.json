{
  "index": 412,
  "name": "Sura 412",
  "ayas": [
    {
      "index": 1,
      "text": "waḥdahu lā sharīka lah",
      "bismillah": "lā ilāha illallāh"
    },
    {
      "index": 2,
      "text": "lahul mulku walahul ḥamd"
    },
    {
      "index": 3,
      "text": "yuḥyī wayumīt"
    },
    {
      "index": 4,
      "text": "wayumītu wayuḥyī"
    },
    {
      "index": 5,
      "text": "wahuwa ḥayyun lā yamūt"
    },
    {
      "index": 6,
      "text": "biyadihil khayr"
    },
    {
      "index": 7,
      "text": "wahuwa `alā kulli shay-in qadīr"
    }
  ]
}