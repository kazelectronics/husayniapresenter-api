{
  "index": 141,
  "name": "Sura 141",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "khābal wāfidūna `alā ghayrik"
    },
    {
      "index": 3,
      "text": "wakhasiral muta`arriḍūna illā lak"
    },
    {
      "index": 4,
      "text": "waḍā`al mulimmūna illā bik"
    },
    {
      "index": 5,
      "text": "wa-ajdabal muntaji`ūna illā manintaja`a faḍlak"
    },
    {
      "index": 6,
      "text": "bābuka maftūḥun lirrāghibīn"
    },
    {
      "index": 7,
      "text": "wakhayruka mabdhūlun liṭṭālibīn"
    },
    {
      "index": 8,
      "text": "wafaḍluka mubāḥun lissā-ilīn"
    },
    {
      "index": 9,
      "text": "wanayluka mutāḥun lil-āmilīn"
    },
    {
      "index": 10,
      "text": "warizquka mabsūṭun liman `aṣāk"
    },
    {
      "index": 11,
      "text": "waḥilmuka mu`tariḍun liman nāwāk"
    },
    {
      "index": 12,
      "text": "`ādatukal iḥsānu ilal musī-īn"
    },
    {
      "index": 13,
      "text": "wasabīlukal ibqā-u `alal mu`tadīn"
    },
    {
      "index": 14,
      "text": "allāhumma fahdinī hudal muhtadīn"
    },
    {
      "index": 15,
      "text": "war-zuqnijtihādal mujtahidīn"
    },
    {
      "index": 16,
      "text": "walā taj`alnī minal ghāfilīnal mub`adīn"
    },
    {
      "index": 17,
      "text": "waghfir lī yawmad dīn"
    }
  ]
}