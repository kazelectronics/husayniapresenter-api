{
  "index": 311,
  "name": "Sura 311",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "al ḥamdu lillāhil ladhī ja`alal layla libāsā"
    },
    {
      "index": 3,
      "text": "wan nawma subātā"
    },
    {
      "index": 4,
      "text": "waja`alan nahāra nushūrā"
    },
    {
      "index": 5,
      "text": "lakal ḥamdu an ba`athtanī min mar-qadī"
    },
    {
      "index": 6,
      "text": "walaw shi-ta ja`altahu sar-madā"
    },
    {
      "index": 7,
      "text": "ḥamdan dā-iman lāyanqaṭi`u abadā"
    },
    {
      "index": 8,
      "text": "walā yuḥṣī lahul khalā-iqu `adadā"
    },
    {
      "index": 9,
      "text": "allāhumma lakal ḥamdu an khalaqta fasawwayt"
    },
    {
      "index": 10,
      "text": "waqaddar-ta waqaḍayt"
    },
    {
      "index": 11,
      "text": "wa amatta wa aḥyayt"
    },
    {
      "index": 12,
      "text": "wa-amraḍta washafayt"
    },
    {
      "index": 13,
      "text": "wa`āfayta wa ablayt"
    },
    {
      "index": 14,
      "text": "wa`alal `ar-shistawayt"
    },
    {
      "index": 15,
      "text": "wa`alal mulkiḥtawayt"
    },
    {
      "index": 16,
      "text": "ad`ūka du`ā-a man ḍa`ufat wasīlatuh"
    },
    {
      "index": 17,
      "text": "wanqaṭa`at ḥīlatuh"
    },
    {
      "index": 18,
      "text": "waqtaraba ajaluh"
    },
    {
      "index": 19,
      "text": "watadānā fid dunyā amaluh"
    },
    {
      "index": 20,
      "text": "washtaddat ilā raḥmatika fāqatuh"
    },
    {
      "index": 21,
      "text": "wa`aẓumat litafrīṭihi ḥasratuh"
    },
    {
      "index": 22,
      "text": "wakathurat zallatuhu wa`athratuh"
    },
    {
      "index": 23,
      "text": "wakhaluṣat liwajhika tawbatuh"
    },
    {
      "index": 24,
      "text": "faṣalli `alā muḥammad"
    },
    {
      "index": 25,
      "text": "khātamin nabiyyīn"
    },
    {
      "index": 26,
      "text": "wa`alā ahli baytihiṭ ṭayyibīnaṭ ṭāhirīn"
    },
    {
      "index": 27,
      "text": "war-zuqnī shafā`ata muḥammad"
    },
    {
      "index": 28,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 29,
      "text": "walā taḥrimnī ṣuḥbatah"
    },
    {
      "index": 30,
      "text": "innaka anta ar-ḥamur rāḥimīn"
    },
    {
      "index": 31,
      "text": "allāhummaqḍi lī fil ar-ba`ā-i ar-ba`ā"
    },
    {
      "index": 32,
      "text": "ij`al quwwatī fī ṭā`atik"
    },
    {
      "index": 33,
      "text": "wanashāṭī fī `ibādatik"
    },
    {
      "index": 34,
      "text": "waraghbatī fī thawābik"
    },
    {
      "index": 35,
      "text": "wazuhdī fīmā yūjibu lī alīma `iqābik"
    },
    {
      "index": 36,
      "text": "innaka laṭīfun limā tashā"
    }
  ]
}