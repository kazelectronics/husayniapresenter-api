{
  "index": 93,
  "name": "Sura 93",
  "ayas": [
    {
      "index": 1,
      "text": "waḍ ḍuḥā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wal layli idhā sajā"
    },
    {
      "index": 3,
      "text": "mā wadda`aka rabbuka wa mā qalā"
    },
    {
      "index": 4,
      "text": "walal-ākhiratu khayrul laka minal-ūlā"
    },
    {
      "index": 5,
      "text": "walasawfa yu`ṭīka rabbuka fatarḍā"
    },
    {
      "index": 6,
      "text": "alam yajidka yatīman fa-āwā"
    },
    {
      "index": 7,
      "text": "wawajadaka ḍāllan fahadā"
    },
    {
      "index": 8,
      "text": "wawajadaka `ā-ilan fa-aghnā"
    },
    {
      "index": 9,
      "text": "fa-ammal yatīma falā taqhar"
    },
    {
      "index": 10,
      "text": "wa-ammas sā-ila falā tanhar"
    },
    {
      "index": 11,
      "text": "wa-ammā bini`mati rabbika faḥaddith"
    }
  ]
}