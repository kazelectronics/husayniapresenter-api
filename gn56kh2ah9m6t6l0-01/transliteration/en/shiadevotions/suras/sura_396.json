{
  "index": 396,
  "name": "Sura 396",
  "ayas": [
    {
      "index": 1,
      "text": "wa-adbāri laylik",
      "bismillah": "allāhumma innī as-aluka bi-iqbāli nahārik"
    },
    {
      "index": 2,
      "text": "waḥuḍūri ṣalawātik"
    },
    {
      "index": 3,
      "text": "wa-aṣwāti du`ā-ik"
    },
    {
      "index": 4,
      "text": "watasbīḥa malā-ikatik"
    },
    {
      "index": 5,
      "text": "an tuṣalliya `alā muḥammadiw wa-āli muḥammad"
    },
    {
      "index": 6,
      "text": "wa-an tatūba `alayy"
    },
    {
      "index": 7,
      "text": "innaka antat tawwābur raḥīm"
    }
  ]
}