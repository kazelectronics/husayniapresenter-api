{
  "index": 357,
  "name": "Sura 357",
  "ayas": [
    {
      "index": 1,
      "text": "rabbi adkhilnī fī lujjati baḥri aḥadiyyatik",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "waṭamṭāmi yammi waḥdāniyyatik"
    },
    {
      "index": 3,
      "text": "waqawwinī biquwwati saṭwati sulṭāni far-dāniyyatik"
    },
    {
      "index": 4,
      "text": "ḥattā akhruja ilā faḍā-i sa`ati raḥmatik"
    },
    {
      "index": 5,
      "text": "wafī wajhī lama`ātu bar-qil qur-bi min-āthāri ḥimāyatik"
    },
    {
      "index": 6,
      "text": "mahīban bihaybatik"
    },
    {
      "index": 7,
      "text": "`azīzan bi`ināyatik"
    },
    {
      "index": 8,
      "text": "mutajallilan mukarramā"
    },
    {
      "index": 9,
      "text": "bita`līmika watazkiyatik"
    },
    {
      "index": 10,
      "text": "wa-albisnī khila`al `izzati walqabūl"
    },
    {
      "index": 11,
      "text": "wasahhil lī manāhijal wuṣlati walwuṣūl"
    },
    {
      "index": 12,
      "text": "watawwijnī bitājil karāmati walwaqār"
    },
    {
      "index": 13,
      "text": "wa-allif baynī wabayna aḥibbā-ik"
    },
    {
      "index": 14,
      "text": "fī dārid dunyā wadāril qarār"
    },
    {
      "index": 15,
      "text": "war-zuqnī min nūrismika haybatan wasaṭwah"
    },
    {
      "index": 16,
      "text": "tanqādu liyal qulūbu wal-ar-wāḥ"
    },
    {
      "index": 17,
      "text": "watakhḍa`u ladayyan nufūsu wal-ashbāḥ"
    },
    {
      "index": 18,
      "text": "yā man dhallat lahu riqābul jabābirah"
    },
    {
      "index": 19,
      "text": "wakhaḍa`at ladayhi a`nāqul akāsirah"
    },
    {
      "index": 20,
      "text": "lāmalja-a walā manjā minka illā ilayk"
    },
    {
      "index": 21,
      "text": "walā i`ānata illā bik"
    },
    {
      "index": 22,
      "text": "walattikā-a illā `alayk"
    },
    {
      "index": 23,
      "text": "idfa` `annī kaydal ḥāsidīn"
    },
    {
      "index": 24,
      "text": "waẓulumāti sharril mu`ānidīn"
    },
    {
      "index": 25,
      "text": "war-ḥamnī taḥta surādiqāti `ar-shik"
    },
    {
      "index": 26,
      "text": "yā akramal akramīn"
    },
    {
      "index": 27,
      "text": "ayyid ẓāhirī fī taḥṣīli marāḍīk"
    },
    {
      "index": 28,
      "text": "wanawwir qalbī wasirrī bilāṭṭilā`i `alā manāhiji masā`īk"
    },
    {
      "index": 29,
      "text": "ilāhī kayfa aṣduru `an bābika bikhaybatin mink"
    },
    {
      "index": 30,
      "text": "waqad waradtuhu `alā thiqatin bik"
    },
    {
      "index": 31,
      "text": "wakayfa tu-yisunī min `aṭā-ik"
    },
    {
      "index": 32,
      "text": "waqad amar-tanī bidu`ā-ik"
    },
    {
      "index": 33,
      "text": "wahā ana muqbilun `alayk"
    },
    {
      "index": 34,
      "text": "multaji-un ilayk"
    },
    {
      "index": 35,
      "text": "bā`id baynī wabayna a`dā-ī"
    },
    {
      "index": 36,
      "text": "kamā bā`adta bayna a`dā-ī"
    },
    {
      "index": 37,
      "text": "ikhtaṭif abṣārahum `annī"
    },
    {
      "index": 38,
      "text": "binūri qudsik"
    },
    {
      "index": 39,
      "text": "wajalāli majdik"
    },
    {
      "index": 40,
      "text": "innaka antallāh"
    },
    {
      "index": 41,
      "text": "al mu`ṭī jalā-ilan ni`amil mukarramah"
    },
    {
      "index": 42,
      "text": "liman nājāka bilaṭā-ifi raḥmatik"
    },
    {
      "index": 43,
      "text": "yā ḥayyu yā qayyūm"
    },
    {
      "index": 44,
      "text": "yā dhal jalāli wal-ikrām"
    },
    {
      "index": 45,
      "text": "waṣallallāhu `alā sayyidinā wanabiyyinā muḥammad"
    },
    {
      "index": 46,
      "text": "wa ālihi ajma`īnaṭ ṭayyibīnaṭ ṭāhirīn"
    }
  ]
}