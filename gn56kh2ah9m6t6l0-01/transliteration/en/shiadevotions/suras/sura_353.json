{
  "index": 353,
  "name": "Sura 353",
  "ayas": [
    {
      "index": 1,
      "text": "wal-ajsādil bāliyah",
      "bismillah": "allāhumma rabba hādhihil ar-wāḥil fāniyah"
    },
    {
      "index": 2,
      "text": "wal`iẓāmin nakhirah"
    },
    {
      "index": 3,
      "text": "al latī kharajat minad dunyā wahiya bika mu-minah"
    },
    {
      "index": 4,
      "text": "adkhil `alayhim rawḥan minka wasalāman minnī"
    }
  ]
}