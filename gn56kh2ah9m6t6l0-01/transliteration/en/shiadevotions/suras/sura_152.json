{
  "index": 152,
  "name": "Sura 152",
  "ayas": [
    {
      "index": 1,
      "text": "`aliyyabna muḥammad",
      "bismillah": "as salāmu `alayka yā abal ḥasan"
    },
    {
      "index": 2,
      "text": "az zakiyyar rāshid"
    },
    {
      "index": 3,
      "text": "an nūrath thāqib"
    },
    {
      "index": 4,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā ṣafiyyallāh"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā sirrallāh"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā ḥablallāh"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yā-ālallāh"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka yā khiyaratallāh"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yā ṣafwatallāh"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka yā amīnallāh"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka yā ḥaqqallāh"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka yā ḥabīballāh"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka yā nūral anwār"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka yā zaynal abrār"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka yā salīlal akhyār"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka yā `unṣural aṭ-hār"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka yā ḥujjatar raḥmān"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka yā ruknal īmān"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka yā mawlal mu-minīn"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka yā waliyyaṣ ṣāliḥīn"
    },
    {
      "index": 22,
      "text": "as salāmu `alayka yā `alamal hudā"
    },
    {
      "index": 23,
      "text": "as salāmu `alayka yā ḥalīfat tuqā"
    },
    {
      "index": 24,
      "text": "as salāmu `alayka yā `amūdad dīn"
    },
    {
      "index": 25,
      "text": "as salāmu `alayka yabna khātamin nabiyyīn"
    },
    {
      "index": 26,
      "text": "as salāmu `alayka yabna sayyidil waṣiyyīn"
    },
    {
      "index": 27,
      "text": "as salāmu `alayka yabna fāṭimataz zahrā"
    },
    {
      "index": 28,
      "text": "sayyidati nisā-il `ālamīn"
    },
    {
      "index": 29,
      "text": "as salāmu `alayka ayyuhal amīnul wafiyy"
    },
    {
      "index": 30,
      "text": "as salāmu `alayka ayyuhal `alamur raḍiyy"
    },
    {
      "index": 31,
      "text": "as salāmu `alayka ayyuhaz zāhidut taqiyy"
    },
    {
      "index": 32,
      "text": "as salāmu `alayka ayyuhal ḥujjatu `alal khalqi ajma`īn"
    },
    {
      "index": 33,
      "text": "as salāmu `alayka ayyuhat tālī lilqur-ān"
    },
    {
      "index": 34,
      "text": "as salāmu `alayka ayyuhal mubayyinu lilḥalāli minal ḥarām"
    },
    {
      "index": 35,
      "text": "as salāmu `alayka ayyuhal waliyyun nāṣiḥ"
    },
    {
      "index": 36,
      "text": "as salāmu `alayka ayyuhaṭ ṭarīqul wāḍiḥ"
    },
    {
      "index": 37,
      "text": "as salāmu `alayka ayyuhan najmul lā-iḥ"
    },
    {
      "index": 38,
      "text": "ash-hadu yā mawlāya yā abal ḥasan"
    },
    {
      "index": 39,
      "text": "annaka ḥujjatullāhi `alā khalqih"
    },
    {
      "index": 40,
      "text": "wakhalīfatuhu fī bariyyatih"
    },
    {
      "index": 41,
      "text": "wa-amīnuhu fī bilādih"
    },
    {
      "index": 42,
      "text": "washāhiduhu `alā `ibādih"
    },
    {
      "index": 43,
      "text": "wa-ash-hadu annaka kalimatut taqwā"
    },
    {
      "index": 44,
      "text": "wabābul hudā"
    },
    {
      "index": 45,
      "text": "wal`ur-watul wuthqā"
    },
    {
      "index": 46,
      "text": "walḥujjatu `alā man fawqal ar-ḍ"
    },
    {
      "index": 47,
      "text": "waman taḥtath tharā"
    },
    {
      "index": 48,
      "text": "wa-ash-hadu annakal muṭahharu minadh dhunūb"
    },
    {
      "index": 49,
      "text": "al mubarra-u minal `uyūb"
    },
    {
      "index": 50,
      "text": "walmukhtaṣṣu bikarāmatillāh"
    },
    {
      "index": 51,
      "text": "walmaḥbuwwu biḥujjatillāh"
    },
    {
      "index": 52,
      "text": "walmawhūbu lahu kalimatullāh"
    },
    {
      "index": 53,
      "text": "war ruknul ladhī yalja-u ilayhil `ibād"
    },
    {
      "index": 54,
      "text": "watuḥyā bihil bilād"
    },
    {
      "index": 55,
      "text": "wa-ash-hadu yā mawlāy"
    },
    {
      "index": 56,
      "text": "annī bika wabi-ābā-ika wa-abnā-ika mūqinun muqirr"
    },
    {
      "index": 57,
      "text": "walakum tābi`un fī dhāti nafsī"
    },
    {
      "index": 58,
      "text": "washarāyi`i dīnī"
    },
    {
      "index": 59,
      "text": "wakhātimati `amalī"
    },
    {
      "index": 60,
      "text": "wamunqalabī wamathwāy"
    },
    {
      "index": 61,
      "text": "wa-annī waliyyun liman wālākum"
    },
    {
      "index": 62,
      "text": "wa`aduwwun liman `ādākum"
    },
    {
      "index": 63,
      "text": "mu-minun bisirrikum wa`alāniyatikum"
    },
    {
      "index": 64,
      "text": "wa-awwalikum wākhirikum"
    },
    {
      "index": 65,
      "text": "bi-abī anta wa-ummī"
    },
    {
      "index": 66,
      "text": "was salāmu `alayka waraḥmatullāhi wabarakātuh"
    }
  ]
}