{
  "index": 258,
  "name": "Sura 258",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "ilāhī"
    },
    {
      "index": 3,
      "text": "lā tushmit bī `aduwwī"
    },
    {
      "index": 4,
      "text": "walā tafja` bī ḥamīmī wa ṣadīqī"
    },
    {
      "index": 5,
      "text": "ilāhī hab lī laḥẓatan min laḥaẓātik"
    },
    {
      "index": 6,
      "text": "takshifu `annī mabtalaytanī bih"
    },
    {
      "index": 7,
      "text": "watu`īdunī ilā aḥsani `ādātika `indī"
    },
    {
      "index": 8,
      "text": "wastajib du`ā-ī wadu`ā-a man akhlaṣa laka du`ā-ahu faqad"
    },
    {
      "index": 9,
      "text": "ḍa`ufat quwwatī"
    },
    {
      "index": 10,
      "text": "waqallat ḥīlatī"
    },
    {
      "index": 11,
      "text": "washtaddat ḥālī"
    },
    {
      "index": 12,
      "text": "wa-ayistu mimmā `inda khalqik"
    },
    {
      "index": 13,
      "text": "falam yabqa lī illā raja-uka `alayy"
    },
    {
      "index": 14,
      "text": "ilāhī"
    },
    {
      "index": 15,
      "text": "inna qudrataka `alā kashfi mā ana fīhi kaqudratika `alā mabtalaytanī bih"
    },
    {
      "index": 16,
      "text": "wa inna dhikra `awā-idika yūnisunī"
    },
    {
      "index": 17,
      "text": "war rajā-u fī in`āmika wafaḍlika yuqawwīnī"
    },
    {
      "index": 18,
      "text": "li-annī lam akhlu min ni`matika mundhu khalaqtanī"
    },
    {
      "index": 19,
      "text": "wa anta ilāhī"
    },
    {
      "index": 20,
      "text": "mafza`ī wamalja-ī"
    },
    {
      "index": 21,
      "text": "walḥāfiẓu lī wadh dha-ābbu `annī"
    },
    {
      "index": 22,
      "text": "al mutaḥanninu `alayyar raḥīmu bī"
    },
    {
      "index": 23,
      "text": "al mutakaffilu birizqī"
    },
    {
      "index": 24,
      "text": "fī qaḍā-ika kāna mā ḥalla bī"
    },
    {
      "index": 25,
      "text": "wa bi`ilmika mā ṣir-tu ilayh"
    },
    {
      "index": 26,
      "text": "faj`al yā waliyyī wa sayyidī fīmā qaddar-ta wa qaḍayta `alayya waḥatamt"
    },
    {
      "index": 27,
      "text": "`āfiyatī wa mā fīhi ṣalāḥī wa khalāṣī mimmā ana fīh"
    },
    {
      "index": 28,
      "text": "fa-innī lā ar-jū lidaf`i dhālika ghayrak"
    },
    {
      "index": 29,
      "text": "walā a`tamidu fīhi illā `alayk"
    },
    {
      "index": 30,
      "text": "fakun yā dhal jalāli wal-ikrāmi `inda aḥsani ẓannī bik"
    },
    {
      "index": 31,
      "text": "war-ḥam ḍa`fī waqillata ḥīlatī"
    },
    {
      "index": 32,
      "text": "wakshif kur-batī"
    },
    {
      "index": 33,
      "text": "wastajib da`watī"
    },
    {
      "index": 34,
      "text": "wa aqilnī `athratī"
    },
    {
      "index": 35,
      "text": "wamnun `alayya bidhālika wa `alā kulli dā`in lak"
    },
    {
      "index": 36,
      "text": "amar-tanī yā sayyidī biddu`ā-i wa takaffalta bil ijābah"
    },
    {
      "index": 37,
      "text": "wawa`dukal ḥaqqul ladhī lā khulfa fīhi walā tabdīl"
    },
    {
      "index": 38,
      "text": "faṣalli `alā muḥammadin nabiyyika wa `abdik"
    },
    {
      "index": 39,
      "text": "wa`alaṭ ṭāhirīna min ahli baytih"
    },
    {
      "index": 40,
      "text": "wa aghithnī fa-innak"
    },
    {
      "index": 41,
      "text": "ghiyāthu man lā ghiyātha lah"
    },
    {
      "index": 42,
      "text": "waḥir-zu man lā ḥir-za lah"
    },
    {
      "index": 43,
      "text": "wa anal muḍṭarrul ladhī awjabt"
    },
    {
      "index": 44,
      "text": "ijābatahu wakashfa mā bihi minas sū"
    },
    {
      "index": 45,
      "text": "fa-ajibnī wakshif hammī"
    },
    {
      "index": 46,
      "text": "wafarrij ghammī"
    },
    {
      "index": 47,
      "text": "wa a`id ḥālī ilā aḥsani mā kānat `alayh"
    },
    {
      "index": 48,
      "text": "wa lā tujāzinī bil istiḥqāqi wa lākin biraḥmatikal latī wasi`at kulla shay"
    },
    {
      "index": 49,
      "text": "yā dhal jalāli wal ikrām"
    },
    {
      "index": 50,
      "text": "ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 51,
      "text": "wasma` wa ajib yā `azīz"
    }
  ]
}