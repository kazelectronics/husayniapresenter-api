{
  "index": 400,
  "name": "Sura 400",
  "ayas": [
    {
      "index": 1,
      "text": "ḥanīfan muslimā",
      "bismillah": "wajjahtu wajhiya lilladhī faṭaras samāwāti wal-ar-ḍ"
    },
    {
      "index": 2,
      "text": "wamā ana minal mushrikīn"
    },
    {
      "index": 3,
      "text": "inna ṣalātī wanusukī"
    },
    {
      "index": 4,
      "text": "wamaḥyāya wamamātī"
    },
    {
      "index": 5,
      "text": "lillāhi rabbil `ālamīn"
    },
    {
      "index": 6,
      "text": "lā sharīka lah"
    },
    {
      "index": 7,
      "text": "wabidhālika umir-t"
    },
    {
      "index": 8,
      "text": "wa-ana minal muslimīn"
    }
  ]
}