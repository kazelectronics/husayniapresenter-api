{
  "index": 55,
  "name": "Sura 55",
  "ayas": [
    {
      "index": 1,
      "text": "ar-raḥmān",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "`allamal qur-ān"
    },
    {
      "index": 3,
      "text": "khalaqal-insān"
    },
    {
      "index": 4,
      "text": "`allamahul bayān"
    },
    {
      "index": 5,
      "text": "ash-shamsu wal qamaru biḥusbān"
    },
    {
      "index": 6,
      "text": "wan najmu wash shajaru yasjudān"
    },
    {
      "index": 7,
      "text": "was samā-a rafa`ahā wawaḍa`al mīzān"
    },
    {
      "index": 8,
      "text": "allā taṭghaw fil mīzān"
    },
    {
      "index": 9,
      "text": "wa-aqīmul wazna bil qisṭi walā tukhsirul mīzān"
    },
    {
      "index": 10,
      "text": "wal-arḍa waḍa`ahā lil-anām"
    },
    {
      "index": 11,
      "text": "fīhā fākihatun wan nakhlu dhātul-akmām"
    },
    {
      "index": 12,
      "text": "wal ḥabbu dhul `aṣfi war rayḥān"
    },
    {
      "index": 13,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 14,
      "text": "khalaqal-insāna min ṣalṣālin kal fakhhār"
    },
    {
      "index": 15,
      "text": "wakhalaqal jānna min mārijin min nār"
    },
    {
      "index": 16,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 17,
      "text": "rabbul mashriqayni warabbul maghribayn"
    },
    {
      "index": 18,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 19,
      "text": "marajal baḥrayni yaltaqiyān"
    },
    {
      "index": 20,
      "text": "baynahumā barzakhul lā yabghiyān"
    },
    {
      "index": 21,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 22,
      "text": "yakhruju minhumallu-lu-u wal marjān"
    },
    {
      "index": 23,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 24,
      "text": "walahul jawāril munsha-ātu fil baḥri kal-a`lām"
    },
    {
      "index": 25,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 26,
      "text": "kullu man `alayhā fān"
    },
    {
      "index": 27,
      "text": "wayabqā wajhu rabbika dhul jalāli wal-ikrām"
    },
    {
      "index": 28,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 29,
      "text": "yas-aluhu man fis samāwāti wal-arḍ ۚ kulla yawmin huwa fī sha-n"
    },
    {
      "index": 30,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 31,
      "text": "sanafrughu lakum ayyuhath thaqalān"
    },
    {
      "index": 32,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 33,
      "text": "yāma`sharal jinni wal-insi inis-taṭa`tum an tanfudhū min aqṭāris samāwāti wal-arḍi fanfudhū ۚ lā tanfudhūna illā bisulṭān"
    },
    {
      "index": 34,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 35,
      "text": "yursalu `alaykumā shuwāẓun min nārin wanuḥāsun falā tantaṣirān"
    },
    {
      "index": 36,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 37,
      "text": "fa-idhan-shaqqatis samā-u fakānat wardatan kad dihān"
    },
    {
      "index": 38,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 39,
      "text": "fayawma-idhin lā yus-alu `an dhanbihi insun walā jānn"
    },
    {
      "index": 40,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 41,
      "text": "yu`raful mujrimūna bisīmāhum fayu-khadhu bin nawāṣī wal-aqdām"
    },
    {
      "index": 42,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 43,
      "text": "hādhihi jahannamul latī yukadhhibu bihal mujrimūn"
    },
    {
      "index": 44,
      "text": "yaṭūfūna baynahā wabayna ḥamīmin ān"
    },
    {
      "index": 45,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 46,
      "text": "waliman khāfa maqāma rabbihi jannatān"
    },
    {
      "index": 47,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 48,
      "text": "dhawātā afnān"
    },
    {
      "index": 49,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 50,
      "text": "fīhimā `aynāni tajriyān"
    },
    {
      "index": 51,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 52,
      "text": "fīhimā min kulli fākihatin zawjān"
    },
    {
      "index": 53,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 54,
      "text": "muttaki-īna `alā furushin baṭā-inuhā min istabraq ۚ wajanal jannatayni dān"
    },
    {
      "index": 55,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 56,
      "text": "fīhinna qāṣirātuṭ ṭarfi lam yaṭmith-hunna insun qablahum walā jānn"
    },
    {
      "index": 57,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 58,
      "text": "ka-annahunnal yāqūtu wal marjān"
    },
    {
      "index": 59,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 60,
      "text": "hal jazā-ul-iḥsāni illal-iḥsān"
    },
    {
      "index": 61,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 62,
      "text": "wa min dūnihimā jannatān"
    },
    {
      "index": 63,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 64,
      "text": "mud-hāmmatān"
    },
    {
      "index": 65,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 66,
      "text": "fīhimā `aynāni naḍḍākhatān"
    },
    {
      "index": 67,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 68,
      "text": "fīhimā fākihatun wanakhlun warummān"
    },
    {
      "index": 69,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 70,
      "text": "fīhinna khayrātun ḥisān"
    },
    {
      "index": 71,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 72,
      "text": "ḥūrun maqṣūrātun fil khiyām"
    },
    {
      "index": 73,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 74,
      "text": "lam yaṭmith-hunna insun qablahum walā jānn"
    },
    {
      "index": 75,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 76,
      "text": "muttaki-īna `alā rafrafin khuḍrin wa`abqariyyin ḥisān"
    },
    {
      "index": 77,
      "text": "fabi-ayyi ālā-i rabbikumā tukadhhibān"
    },
    {
      "index": 78,
      "text": "tabārakas-mu rabbika dhil jalāli wal-ikrām"
    }
  ]
}