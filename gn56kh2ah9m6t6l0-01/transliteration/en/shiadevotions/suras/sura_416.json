{
  "index": 416,
  "name": "Sura 416",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluka min bahā-ika bi-abhāh"
    },
    {
      "index": 3,
      "text": "wakullu bahā-ika bahiyy"
    },
    {
      "index": 4,
      "text": "allāhumma innī as-aluka bibahā-ika kullih"
    },
    {
      "index": 5,
      "text": "allāhumma innī as-aluka min jamālika bi-ajmalih"
    },
    {
      "index": 6,
      "text": "wakullu jamālika jamīl"
    },
    {
      "index": 7,
      "text": "allāhumma innī as-aluka bijamālika kullih"
    },
    {
      "index": 8,
      "text": "allāhumma innī as-aluka min jalālika bi-ajallih"
    },
    {
      "index": 9,
      "text": "wakullu jalālika jalīl"
    },
    {
      "index": 10,
      "text": "allāhumma innī as-aluka bijalālika kullih"
    },
    {
      "index": 11,
      "text": "allāhumma innī as-aluka min `aẓamatika bi-a`ẓamihā"
    },
    {
      "index": 12,
      "text": "wakullu `aẓamatika `aẓīmah"
    },
    {
      "index": 13,
      "text": "allāhumma innī as-aluka bi`aẓamatika kullihā"
    },
    {
      "index": 14,
      "text": "allāhumma innī as-aluka min nūrika bi-anwarih"
    },
    {
      "index": 15,
      "text": "wakullu nūrika nayyir"
    },
    {
      "index": 16,
      "text": "allāhumma innī as-aluka binūrika kullih"
    },
    {
      "index": 17,
      "text": "allāhumma innī as-aluka min raḥmatika bi-awsa`ihā"
    },
    {
      "index": 18,
      "text": "wakullu raḥmatika wāsi`ah"
    },
    {
      "index": 19,
      "text": "allāhumma innī as-aluka biraḥmatika kullihā"
    },
    {
      "index": 20,
      "text": "allāhumma innī as-aluka min kalimātika bi-atammihā"
    },
    {
      "index": 21,
      "text": "wakullu kalimātika tāmmah"
    },
    {
      "index": 22,
      "text": "allāhumma innī as-aluka bikalimātika kullihā"
    },
    {
      "index": 23,
      "text": "allāhumma innī as-aluka min kamālika bi-akmalih"
    },
    {
      "index": 24,
      "text": "wakullu kamālika kāmil"
    },
    {
      "index": 25,
      "text": "allāhumma innī as-aluka bikamālika kullih"
    },
    {
      "index": 26,
      "text": "allāhumma innī as-aluka min asmā-ika bi-akbarihā"
    },
    {
      "index": 27,
      "text": "wakullu asmā-ika kabīrah"
    },
    {
      "index": 28,
      "text": "allāhumma innī as-aluka bi-asmā-ika kullihā"
    },
    {
      "index": 29,
      "text": "allāhumma innī as-aluka min `izzatika bi-a`azzihā"
    },
    {
      "index": 30,
      "text": "wakullu `izzatika `azīzah"
    },
    {
      "index": 31,
      "text": "allāhumma innī as-aluka bi`izzitika kullihā"
    },
    {
      "index": 32,
      "text": "allāhumma innī as-aluka min mashī-atika bi-amḍāhā"
    },
    {
      "index": 33,
      "text": "wakullu mashī-atika māḍiyah"
    },
    {
      "index": 34,
      "text": "allāhumma innī as-aluka bimashī-atika kullihā"
    },
    {
      "index": 35,
      "text": "allāhumma innī as-aluka min qudratika bil qudratil latistaṭalta bihā `alā kulli shay"
    },
    {
      "index": 36,
      "text": "wakullu qudratika mustaṭīlah"
    },
    {
      "index": 37,
      "text": "allāhumma innī as-aluka biqudratika kullihā"
    },
    {
      "index": 38,
      "text": "allāhumma innī as-aluka min `ilmika bi-anfadhih"
    },
    {
      "index": 39,
      "text": "wakullu `ilmika nāfidh"
    },
    {
      "index": 40,
      "text": "allāhumma innī as-aluka bi`ilmika kullih"
    },
    {
      "index": 41,
      "text": "allāhumma innī as-aluka min qawlika bi-ar-ḍāh"
    },
    {
      "index": 42,
      "text": "wakullu qawlika raḍiyy"
    },
    {
      "index": 43,
      "text": "allāhumma innī as-aluka biqawlika kullih"
    },
    {
      "index": 44,
      "text": "allāhumma innī as-aluka min masā-ilika bi-aḥabbihā ilayk"
    },
    {
      "index": 45,
      "text": "wakullu masā-ilika ilayka ḥabībah"
    },
    {
      "index": 46,
      "text": "allāhumma innī as-aluka bimasā-ilika kullihā"
    },
    {
      "index": 47,
      "text": "allāhumma innī as-aluka min sharafika bi-ashrafih"
    },
    {
      "index": 48,
      "text": "wakullu sharafika sharīf"
    },
    {
      "index": 49,
      "text": "allāhumma innī as-aluka bisharafika kullih"
    },
    {
      "index": 50,
      "text": "allāhumma innī as-aluka min sulṭānika bi-adwamih"
    },
    {
      "index": 51,
      "text": "wakullu sulṭānika dā-im"
    },
    {
      "index": 52,
      "text": "allāhumma innī as-aluka bisulṭānika kullih"
    },
    {
      "index": 53,
      "text": "allāhumma innī as-aluka min mulkika bi-afkharih"
    },
    {
      "index": 54,
      "text": "wakullu mulkika fākhir"
    },
    {
      "index": 55,
      "text": "allāhumma innī as-aluka bimulkika kullih"
    },
    {
      "index": 56,
      "text": "allāhumma innī as-aluka min `uluwwika bi-a`lāh"
    },
    {
      "index": 57,
      "text": "wakullu `uluwwika `āl"
    },
    {
      "index": 58,
      "text": "allāhumma innī as-aluka bi`uluwwika kullih"
    },
    {
      "index": 59,
      "text": "allāhumma innī as-aluka min mannika bi-aqdamih"
    },
    {
      "index": 60,
      "text": "wakullu mannika qadīm"
    },
    {
      "index": 61,
      "text": "allāhumma innī as-aluka bimannika kullih"
    },
    {
      "index": 62,
      "text": "allāhumma innī as-aluka min-āyātika bi-akramihā"
    },
    {
      "index": 63,
      "text": "wakullu-āyātika karīmah"
    },
    {
      "index": 64,
      "text": "allāhumma innī as-aluka bi-āyātika kullihā"
    },
    {
      "index": 65,
      "text": "allāhumma innī as-aluka bimā anta fīhi minash sha-ni waljabarūt"
    },
    {
      "index": 66,
      "text": "wa-as-aluka bikulli sha-nin waḥdahu wajabarūtin waḥdahā"
    },
    {
      "index": 67,
      "text": "allāhumma innī as-aluka bimā tujībunī bihi ḥīna as-aluka fa-ajibnī yā allāh"
    }
  ]
}