{
  "index": 95,
  "name": "Sura 95",
  "ayas": [
    {
      "index": 1,
      "text": "wat tīni waz zaytūn",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "waṭūri sīnīn"
    },
    {
      "index": 3,
      "text": "wahādhal baladil-amīn"
    },
    {
      "index": 4,
      "text": "laqad khalaqnal-insāna fī aḥsani taqwym"
    },
    {
      "index": 5,
      "text": "thumma radadnāhu asfala sāfilīn"
    },
    {
      "index": 6,
      "text": "illal ladhīna āmanū wa`amiluṣ ṣāliḥāti falahum ajrun ghayru mamnūn"
    },
    {
      "index": 7,
      "text": "famā yukadhhibuka ba`du bid dīn"
    },
    {
      "index": 8,
      "text": "alaysallāhu bi-aḥkamil ḥākimīn"
    }
  ]
}