{
  "index": 366,
  "name": "Sura 366",
  "ayas": [
    {
      "index": 1,
      "text": "wawassi` `alayya ḍīqal maḍja`",
      "bismillah": "allāhumma a`innī `alā hawlil muṭṭala`"
    },
    {
      "index": 2,
      "text": "war-zuqnī khayra mā qablal mawt"
    },
    {
      "index": 3,
      "text": "war-zuqnī khayra mā ba`dal mawt"
    }
  ]
}