{
  "index": 404,
  "name": "Sura 404",
  "ayas": [
    {
      "index": 1,
      "text": "allāhummaghfir lī war-ḥamnī",
      "bismillah": "astaghfirullāha rabbī wa-atūbu ilayh"
    },
    {
      "index": 2,
      "text": "wajbur-nī wadfa` `annī wa`āfinī"
    },
    {
      "index": 3,
      "text": "wa innī limā anzalta ilayya min khayrin faqīr"
    },
    {
      "index": 4,
      "text": "tabārakallāhu rabbal `ālamīn"
    }
  ]
}