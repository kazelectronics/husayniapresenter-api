{
  "index": 289,
  "name": "Sura 289",
  "ayas": [
    {
      "index": 1,
      "text": "anta rabbī ḥaqqan ḥaqqā",
      "bismillah": "subḥānakallāhumma"
    },
    {
      "index": 2,
      "text": "sajadtu laka yā rabbi ta`abbudan wariqqā"
    },
    {
      "index": 3,
      "text": "allāhumma inna `amalī ḍa`īfun faḍā`ifhu lī"
    },
    {
      "index": 4,
      "text": "allāhumma qinī `adhābaka yawma tab`athu `ibādak"
    },
    {
      "index": 5,
      "text": "watub `alayya innaka antat tawwābur raḥīm"
    }
  ]
}