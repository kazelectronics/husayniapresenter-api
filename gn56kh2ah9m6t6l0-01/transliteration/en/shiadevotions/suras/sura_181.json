{
  "index": 181,
  "name": "Sura 181",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma yā shāhida kulli najwā"
    },
    {
      "index": 3,
      "text": "wamawḍi`a kulli shakwā"
    },
    {
      "index": 4,
      "text": "wa`ālima kulli khafiyyah"
    },
    {
      "index": 5,
      "text": "wamuntahā kulli ḥājah"
    },
    {
      "index": 6,
      "text": "yā mubtadi-an binni`ami `alal `ibād"
    },
    {
      "index": 7,
      "text": "yā karīmal `afw"
    },
    {
      "index": 8,
      "text": "yā ḥasanat tajāwuz"
    },
    {
      "index": 9,
      "text": "yā jawād"
    },
    {
      "index": 10,
      "text": "yā man lāyuwārī minhu laylun dāj"
    },
    {
      "index": 11,
      "text": "walā baḥrun `ajjāj"
    },
    {
      "index": 12,
      "text": "walā samā-un dhātu abrāj"
    },
    {
      "index": 13,
      "text": "walā ẓulamun dhātur-titāj"
    },
    {
      "index": 14,
      "text": "yā maniẓ ẓulmatu `indahu ḍiyā"
    },
    {
      "index": 15,
      "text": "as-aluka binūri wajhikal karīm"
    },
    {
      "index": 16,
      "text": "al ladhī tajallayta bihi liljabali faja`altahu dakkā"
    },
    {
      "index": 17,
      "text": "wakharra mūsā ṣa`iqā"
    },
    {
      "index": 18,
      "text": "wabismikal ladhī rafa`ta bihis samāwāti bilā `amad"
    },
    {
      "index": 19,
      "text": "wasaṭaḥta bihil ar-ḍa `alā wajhi mā-in jamad"
    },
    {
      "index": 20,
      "text": "wabismikal makhzūnil maknūn"
    },
    {
      "index": 21,
      "text": "al maktūbiṭ ṭāhir"
    },
    {
      "index": 22,
      "text": "al ladhī idhā du`īta bihi ajabt"
    },
    {
      "index": 23,
      "text": "wa-idhā su-ilta bihi a`ṭayt"
    },
    {
      "index": 24,
      "text": "wabismikas subbūḥil quddūsil bur-hān"
    },
    {
      "index": 25,
      "text": "al ladhī huwa nūrun `alā kulli nūr"
    },
    {
      "index": 26,
      "text": "wanūrun min nūr"
    },
    {
      "index": 27,
      "text": "yuḍī-u minhu kullu nūr"
    },
    {
      "index": 28,
      "text": "idhā balaghal ar-ḍanshaqqat"
    },
    {
      "index": 29,
      "text": "wa-idhā balaghas samāwāti futiḥat"
    },
    {
      "index": 30,
      "text": "wa-idhā balaghal `ar-shahtazz"
    },
    {
      "index": 31,
      "text": "wabismikal ladhī tar-ta`idu minhu farā-iṣu malā-ikatik"
    },
    {
      "index": 32,
      "text": "wa-as-aluka biḥaqqi jabra-īla wamīkā-īla wa-isrāfīl"
    },
    {
      "index": 33,
      "text": "wabiḥaqqi muḥammadinil muṣṭafā"
    },
    {
      "index": 34,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 35,
      "text": "wa`alā jamī`il anbiyā-i wajamī`il malā-ikah"
    },
    {
      "index": 36,
      "text": "wabilasmil ladhī mashā bihil khiḍru `alā qulalil mā"
    },
    {
      "index": 37,
      "text": "kamā mashā bihi `alā jadadil ar-ḍ"
    },
    {
      "index": 38,
      "text": "wabismikal ladhī falaqta bihil baḥra limūsā"
    },
    {
      "index": 39,
      "text": "wa-aghraqta fir-`awna waqawmah"
    },
    {
      "index": 40,
      "text": "wa-anjayta bihi mūsābna `imrān"
    },
    {
      "index": 41,
      "text": "min jānibiṭ ṭūril ayman"
    },
    {
      "index": 42,
      "text": "fastajabta lahu wa-alqayta `alayhi maḥabbatan mink"
    },
    {
      "index": 43,
      "text": "wabismikal ladhī bihi aḥyā `īsābnu mar-yamal mawtā"
    },
    {
      "index": 44,
      "text": "watakallama fil mahdi ṣabiyyā"
    },
    {
      "index": 45,
      "text": "wa-abra-al akmaha wal-abraṣa bi-idhnik"
    },
    {
      "index": 46,
      "text": "wabismikal ladhī da`āka bihi ḥamalatu `ar-shik"
    },
    {
      "index": 47,
      "text": "wajabra-īlu wamīkā-īlu wa-isrāfīl"
    },
    {
      "index": 48,
      "text": "waḥabībuka muḥammadun ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 49,
      "text": "wamalā-ikatukal muqarrabūn"
    },
    {
      "index": 50,
      "text": "wa-anbiya-ukal mur-salūn"
    },
    {
      "index": 51,
      "text": "wa`ibādukaṣ ṣāliḥūna min ahlis samāwāti wal-araḍīn"
    },
    {
      "index": 52,
      "text": "wabismikal ladhī da`āka bihi dhun nūn"
    },
    {
      "index": 53,
      "text": "idh dhahaba mughāḍiban faẓanna an lan taqdira `alayh"
    },
    {
      "index": 54,
      "text": "fanādā fiẓ ẓulumāti an lā-ilāha illā anta subḥānak"
    },
    {
      "index": 55,
      "text": "innī kuntu minaẓ ẓālimīn"
    },
    {
      "index": 56,
      "text": "fastajabta lahu wanajjaytahu minal ghamm"
    },
    {
      "index": 57,
      "text": "wakadhālika tunjil mu-minīn"
    },
    {
      "index": 58,
      "text": "wabismikal `aẓīmil ladhī da`āka bihi dāwud"
    },
    {
      "index": 59,
      "text": "wakharra laka sājidan faghafar-ta lahu dhanbah"
    },
    {
      "index": 60,
      "text": "wabismikal ladhī da`atka bihi-āsiyatumra-atu fir-`awn"
    },
    {
      "index": 61,
      "text": "idh qālat rabbibni lī `indaka baytan fil jannah"
    },
    {
      "index": 62,
      "text": "wanajjinī min fir-`awna wa`amalih"
    },
    {
      "index": 63,
      "text": "wanajjinī minal qawmiẓ ẓalimīn"
    },
    {
      "index": 64,
      "text": "fastajabta lahā du`ā-ahā"
    },
    {
      "index": 65,
      "text": "wabismikal ladhī da`āka bihi ayyūbu idh ḥalla bihil balā"
    },
    {
      "index": 66,
      "text": "fa`āfaytahu wa ātaytahu ahlahu wamithlahum ma`ahum"
    },
    {
      "index": 67,
      "text": "raḥmatan min `indika wadhikrā lil`ābidīn"
    },
    {
      "index": 68,
      "text": "wabismikal ladhī da`āka bihi ya`qūb"
    },
    {
      "index": 69,
      "text": "faradadta `alayhi baṣarahu waqurrata `aynihi yūsufa wajama`ta shamlah"
    },
    {
      "index": 70,
      "text": "wabismikal ladhī da`āka bihi sulaymān"
    },
    {
      "index": 71,
      "text": "fawahabta lahu mulkan lā yanbaghī li-aḥadin min ba`dih"
    },
    {
      "index": 72,
      "text": "innaka antal wahhāb"
    },
    {
      "index": 73,
      "text": "wabismikal ladhī sakhhar-ta bihil burāqa limuḥammad"
    },
    {
      "index": 74,
      "text": "ṣallallāhu `alayhi wa ālihi wasallam"
    },
    {
      "index": 75,
      "text": "idh qāla ta`ālā subḥānal ladhī asrā bi`abdihi laylā"
    },
    {
      "index": 76,
      "text": "minal masjidil ḥarāmi ilal masjidil aqṣā"
    },
    {
      "index": 77,
      "text": "waqawluhu subḥānal ladhī sakhhara lanā hādhā wamā kunnā lahu muqrinīn"
    },
    {
      "index": 78,
      "text": "wa-innā ilā rabbinā lamunqalibūn"
    },
    {
      "index": 79,
      "text": "wabismikal ladhī tanazzala bihi jabra-īlu `alā muḥammad"
    },
    {
      "index": 80,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 81,
      "text": "wabismikal ladhī da`āka bihi-ādam"
    },
    {
      "index": 82,
      "text": "faghafar-ta lahu dhanbahu wa-askantahu jannatak"
    },
    {
      "index": 83,
      "text": "wa-as-aluka biḥaqqil qur-ānil `aẓīm"
    },
    {
      "index": 84,
      "text": "wabiḥaqqi muḥammadin khātamin nabiyyīn"
    },
    {
      "index": 85,
      "text": "wabiḥaqqi ibrāhīm"
    },
    {
      "index": 86,
      "text": "wabiḥaqqi faṣlika yawmal qaḍā"
    },
    {
      "index": 87,
      "text": "wabiḥaqqil mawāzīni idhā nuṣibat"
    },
    {
      "index": 88,
      "text": "waṣ ṣuḥufi idhā nushirat"
    },
    {
      "index": 89,
      "text": "wabiḥaqqil qalami wamā jarā"
    },
    {
      "index": 90,
      "text": "wal lawḥi wamā aḥṣā"
    },
    {
      "index": 91,
      "text": "wabiḥaqqil ismil ladhī katabtahu `alā surādiqil `ar-sh"
    },
    {
      "index": 92,
      "text": "qabla khalqikal khalqa wad dunyā"
    },
    {
      "index": 93,
      "text": "wash shamsa walqamara bi-alfay `ām"
    },
    {
      "index": 94,
      "text": "wa-ash-hadu an lā ilāha illallāh"
    },
    {
      "index": 95,
      "text": "waḥdahu lā sharīka lah"
    },
    {
      "index": 96,
      "text": "wa-anna muḥammadan `abduhu warasūluh"
    },
    {
      "index": 97,
      "text": "wa-as-aluka bismikal makhzūni fī khazā-inik"
    },
    {
      "index": 98,
      "text": "al ladhista-thar-ta bihi fī `ilmil ghaybi `indak"
    },
    {
      "index": 99,
      "text": "lam yaẓhar `alayhi aḥadun min khalqik"
    },
    {
      "index": 100,
      "text": "lā malakun muqarrab"
    },
    {
      "index": 101,
      "text": "walā nabiyyun mur-sal"
    },
    {
      "index": 102,
      "text": "walā `abdun muṣṭafā"
    },
    {
      "index": 103,
      "text": "wa-as-aluka bismikal ladhī shaqaqta bihil biḥār"
    },
    {
      "index": 104,
      "text": "waqāmat bihil jibāl"
    },
    {
      "index": 105,
      "text": "wakhtalafa bihil laylu wan nahār"
    },
    {
      "index": 106,
      "text": "wabiḥaqqis sab`il mathānī walqur-ānil `aẓīm"
    },
    {
      "index": 107,
      "text": "wabiḥaqqil kirāmil kātibīn"
    },
    {
      "index": 108,
      "text": "wabiḥaqqi ṭāhā wa yāsīn wa kāf-hā-yā-`aīn-ṣād"
