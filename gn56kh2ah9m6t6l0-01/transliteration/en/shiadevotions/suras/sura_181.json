    {
      "index": 191,
      "text": "waṣ-ḥabnī fī laylī wanahārī"
    },
    {
      "index": 192,
      "text": "wamin bayni yadayya wamin khalfī"
    },
    {
      "index": 193,
      "text": "wa`an yamīnī wa`an shimālī"
    },
    {
      "index": 194,
      "text": "wamin fawqī wamin taḥtī"
    },
    {
      "index": 195,
      "text": "wayassir liyas sabīl"
    },
    {
      "index": 196,
      "text": "wa-aḥsin liyat taysīr"
    },
    {
      "index": 197,
      "text": "walā takhdhulnī fil `asīr"
    },
    {
      "index": 198,
      "text": "wahdinī yā khayra dalīl"
    },
    {
      "index": 199,
      "text": "walā takilnī ilā nafsī fil umūr"
    },
    {
      "index": 200,
      "text": "walaqqinī kulla surūr"
    },
    {
      "index": 201,
      "text": "waqlibnī ilā ahlī bil falāḥi wan najāḥ"
    },
    {
      "index": 202,
      "text": "maḥbūran fil `ājili wal-ājil"
    },
    {
      "index": 203,
      "text": "innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 204,
      "text": "war-zuqnī min faḍlik"
    },
    {
      "index": 205,
      "text": "wa-awsi` `alayya min ṭayyibāti rizqik"
    },
    {
      "index": 206,
      "text": "wasta`milnī fī ṭā`atik"
    },
    {
      "index": 207,
      "text": "wa-ajir-nī min `adhābika wanārik"
    },
    {
      "index": 208,
      "text": "waqlibnī idhā tawaffaytanī ilā jannatika biraḥmatik"
    },
    {
      "index": 209,
      "text": "allāhumma innī a`ūdhu bika min zawāli ni`matik"
    },
    {
      "index": 210,
      "text": "wamin taḥwīli `āfiyatik"
    },
    {
      "index": 211,
      "text": "wamin ḥulūli naqimatik"
    },
    {
      "index": 212,
      "text": "wamin nuzūli `adhābik"
    },
    {
      "index": 213,
      "text": "wa-a`ūdhu bika min jahdil balā"
    },
    {
      "index": 214,
      "text": "wadarakish shaqā"
    },
    {
      "index": 215,
      "text": "wamin sū-il qaḍā"
    },
    {
      "index": 216,
      "text": "washamātatil a`dā"
    },
    {
      "index": 217,
      "text": "wamin sharri mā yanzilu minas samā"
    },
    {
      "index": 218,
      "text": "wamin sharri mā fil kitābil munzal"
    },
    {
      "index": 219,
      "text": "allāhumma lā taj`alnī minal ashrār"
    },
    {
      "index": 220,
      "text": "walā min aṣ-ḥābin nār"
    },
    {
      "index": 221,
      "text": "walā taḥrimnī ṣuḥbatal akhyār"
    },
    {
      "index": 222,
      "text": "wa-aḥyinī ḥayātan ṭayyibah"
    },
    {
      "index": 223,
      "text": "watawaffanī wafātan ṭayyibatan tulḥiqunī bil abrār"
    },
    {
      "index": 224,
      "text": "war-zuqnī murāfaqatal anbiyā"
    },
    {
      "index": 225,
      "text": "fī maq`adi ṣidqin `inda malīkin muqtadir"
    },
    {
      "index": 226,
      "text": "allāhumma lakal ḥamdu `alā ḥusni balā-ika waṣun`ik"
    },
    {
      "index": 227,
      "text": "walakal ḥamdu `alal islāmi wattibā`is sunnah"
    },
    {
      "index": 228,
      "text": "yā rabbi kamā hadaytahum lidīnik"
    },
    {
      "index": 229,
      "text": "wa`allamtahum kitābaka fahdinā wa`allimnā"
    },
    {
      "index": 230,
      "text": "walakal ḥamdu `alā ḥusni balā-ika waṣun`ika `indī khāṣṣah"
    },
    {
      "index": 231,
      "text": "kamā khalaqtanī fa-aḥsanta khalqī"
    },
    {
      "index": 232,
      "text": "wa`allamtanī fa-aḥsanta ta`līmī"
    },
    {
      "index": 233,
      "text": "wahadaytanī fa-aḥsanta hidāyatī"
    },
    {
      "index": 234,
      "text": "falakal ḥamdu `alā in`āmika `alayya qadīman waḥadīthā"
    },
    {
      "index": 235,
      "text": "fakam min kar-bin yā sayyidī qad farrajtah"
    },
    {
      "index": 236,
      "text": "wakam min ghammin yā sayyidī qad naffastah"
    },
    {
      "index": 237,
      "text": "wakam min hammin yā sayyidī qad kashaftah"
    },
    {
      "index": 238,
      "text": "wakam min balā-in yā sayyidī qad ṣaraftah"
    },
    {
      "index": 239,
      "text": "wakam min `aybin yā sayyidī qad satar-tah"
    },
    {
      "index": 240,
      "text": "falakal ḥamdu `alā kulli ḥāl"
    },
    {
      "index": 241,
      "text": "fī kulli mathwan wazamān"
    },
    {
      "index": 242,
      "text": "wamunqalabin wamaqām"
    },
    {
      "index": 243,
      "text": "wa`alā hādhihil ḥāli wakulli ḥāl"
    },
    {
      "index": 244,
      "text": "allāhummaj`alnī min afḍali `ibādika naṣībā"
    },
    {
      "index": 245,
      "text": "fī hādhal yawmi min khayrin taqsimuh"
    },
    {
      "index": 246,
      "text": "aw ḍurrin takshifuh"
    },
    {
      "index": 247,
      "text": "aw sū-in taṣrifuh"
    },
    {
      "index": 248,
      "text": "aw balā-in tadfa`uh"
    },
    {
      "index": 249,
      "text": "aw khayrin tasūquh"
    },
    {
      "index": 250,
      "text": "aw raḥmatin tanshuruhā"
    },
    {
      "index": 251,
      "text": "aw `āfiyatin tulbisuhā"
    },
    {
      "index": 252,
      "text": "fa-innaka `alā kulli shay-in qadīr"
    },
    {
      "index": 253,
      "text": "wabiyadika khazā-inus samāwāti wal-ar-ḍ"
    },
    {
      "index": 254,
      "text": "wa-antal wāḥidul karīmul mu`ṭī"
    },
    {
      "index": 255,
      "text": "al ladhī lā yuraddu sā-iluh"
    },
    {
      "index": 256,
      "text": "walā yukhayyabu-āmiluh"
    },
    {
      "index": 257,
      "text": "walā yanquṣu nā-iluh"
    },
    {
      "index": 258,
      "text": "walā yanfadu mā `indah"
    },
    {
      "index": 259,
      "text": "bal yazdādu kathratan waṭībā"
    },
    {
      "index": 260,
      "text": "wa`aṭā-an wajūdā"
    },
    {
      "index": 261,
      "text": "war-zuqnī min khazā-inikal latī lā tafnā"
    },
    {
      "index": 262,
      "text": "wamin raḥmatikal wāsi`ah"
    },
    {
      "index": 263,
      "text": "inna `aṭā-aka lam yakun maḥẓūrā"
    },
    {
      "index": 264,
      "text": "wa-anta `alā kulli shay-in qadīr"
    },
    {
      "index": 265,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    }
  ]
}