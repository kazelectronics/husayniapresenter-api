    {
      "index": 118,
      "text": "fawqa jabali ṭūri saynā"
    },
    {
      "index": 119,
      "text": "wa-as-aluka bismikal ladhī `allamtahu malakal mawti liqabḍil ar-wāḥ"
    },
    {
      "index": 120,
      "text": "wa-as-aluka bismikal ladhī kutiba `alā waraqiz zaytūn"
    },
    {
      "index": 121,
      "text": "fakhaḍa`atin nīrānu litilkal waraqah"
    },
    {
      "index": 122,
      "text": "faqulta yā nāru kūnī bar-dan wasalāmā"
    },
    {
      "index": 123,
      "text": "wa-as-aluka bismikal ladhī katabtahu `alā surādiqil majdi walkarāmah"
    },
    {
      "index": 124,
      "text": "yā man lā yuḥfīhi sā-il"
