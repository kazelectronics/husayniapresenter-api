    {
      "index": 111,
      "text": "wa-injīli `īsā"
    },
    {
      "index": 112,
      "text": "wazabūri dāwud"
    },
    {
      "index": 113,
      "text": "wafur-qāni muḥammad"
    },
    {
      "index": 114,
      "text": "ṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 115,
      "text": "wa`alā jamī`ir rusuli wabāhiyyan sharāhiyyā"
    },
    {
      "index": 116,
      "text": "allāhumma innī as-aluka biḥaqqi tilkal munājāh"
    },
    {
      "index": 117,
      "text": "al latī kānat baynaka wabayna mūsābni `imrān"
