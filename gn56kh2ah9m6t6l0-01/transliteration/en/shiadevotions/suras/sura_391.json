{
  "index": 391,
  "name": "Sura 391",
  "ayas": [
    {
      "index": 1,
      "text": "walā min warā-i ẓahrī",
      "bismillah": "allāhumma lā tu`ṭinī kitābī bishimālī"
    },
    {
      "index": 2,
      "text": "walā taj`alhā maghlūlatan ilā `unuqī"
    },
    {
      "index": 3,
      "text": "wa-a`ūdhu bika min muqaṭṭa`ātin nīrān"
    }
  ]
}