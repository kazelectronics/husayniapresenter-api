{
  "index": 99,
  "name": "Sura 99",
  "ayas": [
    {
      "index": 1,
      "text": "idhā zulzilatil-arḍu zilzālahā",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wa-akhrajatil-arḍu athqālahā"
    },
    {
      "index": 3,
      "text": "waqālal-insānu mā lahā"
    },
    {
      "index": 4,
      "text": "yawma-idhin tuḥaddithu akhbārahā"
    },
    {
      "index": 5,
      "text": "bi-anna rabbaka awḥā lahā"
    },
    {
      "index": 6,
      "text": "yawma-idhiy yaṣdurun nāsu ashtātal liyuraw a`mālahum"
    },
    {
      "index": 7,
      "text": "faman ya`mal mithqāla dharratin khayran yarah"
    },
    {
      "index": 8,
      "text": "wa man ya`mal mithqāla dharratin sharran yarah"
    }
  ]
}