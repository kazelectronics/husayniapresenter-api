{
  "index": 97,
  "name": "Sura 97",
  "ayas": [
    {
      "index": 1,
      "text": "innā anzalnāhu fī laylatil qadr",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wa mā adrāka mā laylatul qadr"
    },
    {
      "index": 3,
      "text": "laylatul qadri khayrun min alfi shahr"
    },
    {
      "index": 4,
      "text": "tanazzalul malā-ikatu war rūḥu fīhā bi-idhni rabbihim min kulli amr"
    },
    {
      "index": 5,
      "text": "salāmun hiya ḥattā maṭla`il fajr"
    }
  ]
}