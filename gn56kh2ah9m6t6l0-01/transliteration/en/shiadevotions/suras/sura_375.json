{
  "index": 375,
  "name": "Sura 375",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmanir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innī as-aluka fī aklī wa shur-bis salāmata min wa`kah"
    },
    {
      "index": 3,
      "text": "wal quwwata bihi `alā ṭā`atik"
    },
    {
      "index": 4,
      "text": "wa dhikrika wa shukrik"
    },
    {
      "index": 5,
      "text": "fīmā abqaytahu fī badanī"
    },
    {
      "index": 6,
      "text": "wa an tashajji`nī biquwwatihā `alā `ibādatik"
    },
    {
      "index": 7,
      "text": "wa an tulhimanī ḥusnat taḥriri min ma`ṣiyatik"
    }
  ]
}