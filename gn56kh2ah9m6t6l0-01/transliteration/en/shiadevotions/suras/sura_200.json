{
  "index": 200,
  "name": "Sura 200",
  "ayas": [
    {
      "index": 1,
      "text": "watamāma ni`matih",
      "bismillah": "al ḥamdu lillāhil ladhī ja`ala kamāla dīnih"
    },
    {
      "index": 2,
      "text": "biwilāyati amīril mu-minīn"
    },
    {
      "index": 3,
      "text": "`aliyyibni abī ṭālibin `alayhis salām"
    }
  ]
}