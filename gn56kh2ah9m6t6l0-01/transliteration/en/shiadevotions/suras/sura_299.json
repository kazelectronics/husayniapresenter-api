{
  "index": 299,
  "name": "Sura 299",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma in `aẓumat dhunūbī fa-anta a`ẓam"
    },
    {
      "index": 3,
      "text": "wa-in kabura tafrīṭī fa-anta akbar"
    },
    {
      "index": 4,
      "text": "wa-in dāma bukhlī fa-anta ajwad"
    },
    {
      "index": 5,
      "text": "allāhummaghfir lī `aẓīma dhunūbī bi`aẓīmi `afwik"
    },
    {
      "index": 6,
      "text": "wakathīra tafrīṭī biẓāhiri karamik"
    },
    {
      "index": 7,
      "text": "waqma` bukhlī bifaḍli jūdik"
    },
    {
      "index": 8,
      "text": "allāhumma mā binā min ni`matin famink"
    },
    {
      "index": 9,
      "text": "la-ilāha illā ant"
    },
    {
      "index": 10,
      "text": "astaghfiruka wa-atūbu ilayk"
    }
  ]
}