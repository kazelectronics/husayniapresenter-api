{
  "index": 233,
  "name": "Sura 233",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma yā dhal mulkil muta-abbidi bil khulūdi wassulṭān"
    },
    {
      "index": 3,
      "text": "al mumtani`i bighayri junūdin walā a`wān"
    },
    {
      "index": 4,
      "text": "wal`izzil bāqī `alā marrid duhūr"
    },
    {
      "index": 5,
      "text": "wakhawālil a`wām, wamawāḍil azmāni wal-ayyām"
    },
    {
      "index": 6,
      "text": "`azza sulṭānuka `izzan lā ḥadda lahu bi-awwaliyyatin walā muntahā lahu bi-ākhiriyyah"
    },
    {
      "index": 7,
      "text": "wasta`lā mulkuka `uluwwan saqaṭatil ashyā-u dūna bulūghi amadih"
    },
    {
      "index": 8,
      "text": "walā yablughu adnā masta-thar-ta bihi min dhālika aqṣā na`tin na`itīn"
    },
    {
      "index": 9,
      "text": "ḍallat fīkaṣ ṣifātu watafassakhat dūnakan nu`ūt"
    },
    {
      "index": 10,
      "text": "waḥārat fī kibriyā-ika laṭā-iful awhām"
    },
    {
      "index": 11,
      "text": "kadhālika antallāhul awwalu fī awwaliyyatik"
    },
    {
      "index": 12,
      "text": "wa`alā dhālika anta dā-imun lā tazūl"
    },
    {
      "index": 13,
      "text": "wa-anal `abduḍ ḍa`īfu `amalanil jasīmu amalā"
    },
    {
      "index": 14,
      "text": "kharajat min yadī asbābul wuṣulāti illā mā waṣalahu raḥmatuk"
    },
    {
      "index": 15,
      "text": "wataqaṭṭa`at `annī `iṣamul-āmāl"
    },
    {
      "index": 16,
      "text": "illā mā ana mu`taṣimun bihi min `afwik"
    },
    {
      "index": 17,
      "text": "qalla `indī mā a`taddu bihi min ṭā`atik"
    },
    {
      "index": 18,
      "text": "wakathura `alayya mā abū-u bihi min ma`ṣiyatik"
    },
    {
      "index": 19,
      "text": "walan yaḍīqa `alayka `afwun `an `abdika wa in asā-a fa`fu `annī"
    },
    {
      "index": 20,
      "text": "allāhumma waqad ashrafa `alā khafāyal a`māli `ilmuk"
    },
    {
      "index": 21,
      "text": "wankashafa kullu mastūrin dūna khubrik"
    },
    {
      "index": 22,
      "text": "walā tanṭawī `anka daqā-iqul umūr"
    },
    {
      "index": 23,
      "text": "walā ta`zubu `anka ghayyibātus sarā-ir"
    },
    {
      "index": 24,
      "text": "waqadistaḥwadha `alayya `aduwwukal ladhistanẓaraka lighawāyatī fa-anẓar-tah"
    },
    {
      "index": 25,
      "text": "wastamhalaka ilā yawmid dīni li-aḍlālī fa-amhaltah"
    },
    {
      "index": 26,
      "text": "fa-awqa`anī waqad harabtu ilayk"
    },
    {
      "index": 27,
      "text": "min ṣaghā-iri dhunūbin mūbiqah"
    },
    {
      "index": 28,
      "text": "wakabā-iri a`mālin mur-diyah"
    },
    {
      "index": 29,
      "text": "ḥattā idhā qāraftu ma`ṣiyatak"
    },
    {
      "index": 30,
      "text": "wastawjabtu bisū-i sa`yī sakhṭatak"
    },
    {
      "index": 31,
      "text": "fatala `annī `idhāra ghadrih"
    },
    {
      "index": 32,
      "text": "watalaqqānī bikalimati kufrih"
    },
    {
      "index": 33,
      "text": "watawallal barā-ata minnī wa-adbara muwalliyan `annī"
    },
    {
      "index": 34,
      "text": "fa-aṣ-ḥarnī lighaḍabika farīdā"
    },
    {
      "index": 35,
      "text": "wa-akhrajanī ilā finā-i naqimatika ṭarīdā"
    },
    {
      "index": 36,
      "text": "lā shafī`un yashfa`u lī ilayk"
    },
    {
      "index": 37,
      "text": "walā khafīrun yu-minunī `alayk"
    },
    {
      "index": 38,
      "text": "walā ḥiṣnun yaḥjubunī `ank"
    },
    {
      "index": 39,
      "text": "walā mala-adhun alja-u ilayhi mink"
    },
    {
      "index": 40,
      "text": "fahadhā maqāmul `ā-idhi bik, wamaḥallul mu`tarifi lak"
    },
    {
      "index": 41,
      "text": "falā yaḍīqanna `annī faḍluk, walā yaqṣuranna dunī `afwuk"
    },
    {
      "index": 42,
      "text": "walā kun akhyaba `ibādikat tā-ibīn"
    },
    {
      "index": 43,
      "text": "walā aqnaṭa ufūdikal-āmilīn"
    },
    {
      "index": 44,
      "text": "waghfir lī innaka khayrul ghāfirīn"
    },
    {
      "index": 45,
      "text": "allāhumma innaka amar-tanī fatarakt, wanahaytanī farakibt"
    },
    {
      "index": 46,
      "text": "wasawwala liyal khaṭa-a khāṭirus sū-i fafarraṭt"
    },
    {
      "index": 47,
      "text": "walā astash-hidu `alā ṣiyāmī naharā"
    },
    {
      "index": 48,
      "text": "walā astajīru bitahajjudī laylā"
    },
    {
      "index": 49,
      "text": "walā tuthnī `alayya bi-iḥyā-ihā sunnah"
    },
    {
      "index": 50,
      "text": "ḥashā furūḍikal latī man ḍayya`ahā halak"
    },
    {
      "index": 51,
      "text": "walastu atawassalu ilayka bifaḍli nāfilah"
    },
    {
      "index": 52,
      "text": "ma`a kathīri mā aghfaltu min waẓā-ifi furūḍik"
    },
    {
      "index": 53,
      "text": "wata`addaytu `an maqāmāti ḥudūdik"
    },
    {
      "index": 54,
      "text": "ilā ḥurumātinitahaktuhā, wakabā-iri dhunūbinijtaraḥtuhā"
    },
    {
      "index": 55,
      "text": "kānat `āfiyatuka lī min faḍā-iḥihā sitrā"
    },
    {
      "index": 56,
      "text": "wahādhā maqāmu manistaḥyā linafsihi mink"
    },
    {
      "index": 57,
      "text": "wasakhiṭa `alayhā, waraḍiya `ank"
    },
    {
      "index": 58,
      "text": "fatalaqqāka binafsin khāshi`at, waraqabatin khāḍi`at, waẓahrin muthqalin minal khaṭāyā"
    },
    {
      "index": 59,
      "text": "wāqifan baynar raghbati ilayka war rahbati mink"
    },
    {
      "index": 60,
      "text": "wa-anta awlā man rajah"
    },
    {
      "index": 61,
      "text": "wa-aḥaqqu man khashiyahu wattaqāh"
    },
    {
      "index": 62,
      "text": "fa`ṭinī yā rabbi mā rajawt, wa āminnī mā ḥadhir-t"
    },
    {
      "index": 63,
      "text": "wa`ud `alayya bi`ā-idati raḥmatika innaka akramul mas-ūlīn"
    },
    {
      "index": 64,
      "text": "allāhumma wa-idh satar-tanī bi`afwika wataghammadtanī bifaḍlik"
    },
    {
      "index": 65,
      "text": "fī dāril fanā-i biḥaḍratil akfā"
    },
    {
      "index": 66,
      "text": "fa-ajir-nī min faḍīḥāti dāril baqā"
    },
    {
      "index": 67,
      "text": "`inda mawāqifil ash-hādi minal malā-ikatil muqarrabīn"
    },
    {
      "index": 68,
      "text": "war rusulil mukarramīna wash shuhadā-i waṣ ṣāliḥīn"
    },
    {
      "index": 69,
      "text": "min jārin kuntu ukātimuhu sayyi-ātī"
    },
    {
      "index": 70,
      "text": "wamin dhī raḥimin kuntu aḥtashimu minhu fī sarīrātī"
    },
    {
      "index": 71,
      "text": "lam athiq bihim rabbi fis sitri `alayy"
    },
    {
      "index": 72,
      "text": "wawathiqtu bika rabbi fil maghfirati lī"
    },
    {
      "index": 73,
      "text": "wa-anta awlā man wuthiqa bihi wa-a`ṭā man rughiba ilayh"
    },
    {
      "index": 74,
      "text": "wa-ar-afu manistur-ḥima far-ḥamnī"
    },
    {
      "index": 75,
      "text": "allāhumma wa-anta ḥadar-tanī mā-an mahīnan min ṣulb"
    },
    {
      "index": 76,
      "text": "mutaḍā-iqil `iẓāmi ḥarijil masālik"
    },
    {
      "index": 77,
      "text": "ilā raḥim ḍayyiqat satar-tahā bil ḥujub"
    },
    {
      "index": 78,
      "text": "tuṣarrifunī ḥālana `an ḥāl ḥattantahayta bī ilā tamāmiṣ ṣūrah"
    },
    {
      "index": 79,
      "text": "wa-athbatta fiyyal jawārḥ"
    },
    {
      "index": 80,
      "text": "kamā na`atta fī kitābik"
    },
    {
      "index": 81,
      "text": "nuṭfatan thumma `alaqatan thumma muḍghatan thumma `iẓāmā"
    },
    {
      "index": 82,
      "text": "thumma kasawtal `iẓāma laḥman thumma ansha-tanī khalqan-ākhara kamā shi-t"
    },
    {
      "index": 83,
      "text": "ḥattā idhaḥtajtu ilā rizqik"
    },
    {
      "index": 84,
      "text": "walam astaghni `an ghiyathi faḍlik"
    },
    {
      "index": 85,
      "text": "ja`alta lī quwwatan min faḍli ṭa`ām washarāb"
    },
    {
      "index": 86,
      "text": "ajraytahu li-amatikal latī askantanī jawfahā wa-awda`tanī qarāra raḥimihā"
    },
    {
      "index": 87,
      "text": "walaw takilunī yā rabbi fī tilkal ḥalāti ilā ḥawlī"
    },
    {
      "index": 88,
      "text": "aw taḍṭarrunī ilā quwwti"
    },
    {
      "index": 89,
      "text": "lakānal ḥawlu `annī mu`tazilā, walakānatil quwwatu minnī ba`īdah"
    },
    {
      "index": 90,
      "text": "faghadhawtanī bifaḍlika ghidhā-al barril laṭīf"
    },
    {
      "index": 91,
      "text": "taf`alu dhālika bī taṭawwulan `alayya ilā ghāyatī hadhih"
    },
    {
      "index": 92,
      "text": "lā a`damu birraka walā yubṭi-u bī ḥusnu ṣanī`ik"
    },
    {
      "index": 93,
      "text": "walā tata-akkadu ma`a dhālika thiqatī"
    },
    {
      "index": 94,
      "text": "fa-atafarragha limā huwa aḥẓā lī `indak"
    },
    {
      "index": 95,
      "text": "qad malakash shayṭānu `inānī fī sū-iẓ ẓanni waḍa`fil yaqīn"
    },
    {
      "index": 96,
      "text": "fa-ana ashkū sū-a mujāwaratihi lī waṭa`ata nafsī lah"
    },
    {
      "index": 97,
      "text": "wa-asta`ṣimuka min malakatih, wa-ataḍarra`u ilayka fī ṣar-fi kaydihi `annī"
    },
    {
      "index": 98,
      "text": "wa as-aluka fī an tusahhila ilā rizqī sabīlā"
    },
    {
      "index": 99,
      "text": "falakal ḥamdu `alabtidā-ika binni`amil jisām"
    },
    {
      "index": 100,
      "text": "wa-ilhāmikash shukra `alal iḥsāni wal-in`ām"
    },
    {
      "index": 101,
      "text": "faṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 102,
      "text": "wasahhil `alayya rizqī wa-an tuqanni`anī bitaqdīrika lī"
    },
    {
      "index": 103,
      "text": "wa-an tur-ḍiyanī biḥiṣṣatī fīmā qasamta lī"
    },
    {
      "index": 104,
      "text": "wa-an taj`ala mā dhahaba min jismī wa`umurī"
    },
    {
      "index": 105,
      "text": "fī sabīli ṭā`atika innaka khayrur rāziqīn"
    },
    {
      "index": 106,
      "text": "allahumma innī a`ūdhu bika man nārin taghallaẓta bihā `alā man `aṣāk"
    },
    {
      "index": 107,
      "text": "watawa``adta bihā man ṣadafa `an riḍāk"
    },
    {
      "index": 108,
      "text": "wamin nārin nuruhā ẓulmat wahayyinuhā alīm, waba`īduhā qarīb"
    },
    {
      "index": 109,
      "text": "wamin nārin ya-kulu ba`ḍahā ba`ḍ"
    },
    {
      "index": 110,
      "text": "wayaṣūlu ba`ḍuhā `alā ba`ḍ"
    },
    {
      "index": 111,
      "text": "wamin nārin tadharul `iẓāma ramīmā"
    },
    {
      "index": 112,
      "text": "watasqī ahlahā ḥamīmā"
    },
    {
      "index": 113,
      "text": "wamin nārin lā tubqī `alā man taḍarra`a ilayhā"
    },
    {
      "index": 114,
      "text": "walā tar-ḥamu manista`ṭafahā"
    },
    {
      "index": 115,
      "text": "walā taqdiru `alat takhfīfi `amman khasha`a lahā wastaslama ilayhā"
    },
    {
      "index": 116,
      "text": "talqā sukkānahā bi-aḥarri mā ladayhā min alīmin nakāli washadīdil wabāl"
    },
    {
      "index": 117,
      "text": "wa-a`ūdhu bika min `aqāribihal fāghirati afwāhuhā"
    },
    {
      "index": 118,
      "text": "waḥayyātihaṣ ṣāliqati bi-anyābihā"
    },
    {
      "index": 119,
      "text": "washarābihal ladhī yuqaṭṭi`u am`ā-a wa-af-idata sukkānihā, wayanzi`u qulūbahum"
    },
    {
      "index": 120,
      "text": "wa-astahdīka limā bā`ada minhā wa-akhhara `anhā"
    },
    {
      "index": 121,
      "text": "allāhumma ṣalli `alā muḥammad wa ālih"
    },
    {
      "index": 122,
      "text": "wa-ajir-nī minhā bifaḍli raḥmatik"
    },
    {
      "index": 123,
      "text": "wa-aqilnī `atharātī biḥusni iqālatik"
    },
    {
      "index": 124,
      "text": "walā takhdhulnī yā khayral mujirīn"
    },
    {
      "index": 125,
      "text": "allāhumma innaka taqil karīhah, watu`ṭil ḥasanah"
    },
    {
      "index": 126,
      "text": "wataf`alu mā turīdu wa-anta `alā kulli shay-in qadīr"
    },
    {
      "index": 127,
      "text": "allāhumma ṣalli `alā muḥammadin wa ālih, idhā dhukiral abrār"
    },
    {
      "index": 128,
      "text": "waṣalli `alā muḥammadin wa ālihi makhtalafal laylu wan nahār"
    },
    {
      "index": 129,
      "text": "ṣalātan lā yanqaṭi`u madaduhā"
    },
    {
      "index": 130,
      "text": "walā yuḥṣā `adaduhā ṣalātan tash-ḥanul hawā"
    },
    {
      "index": 131,
      "text": "watamla-ul ar-ḍa was samā"
    },
    {
      "index": 132,
      "text": "ṣallallāhu `alayhi ḥattā yar-ḍā"
    },
    {
      "index": 133,
      "text": "waṣallallāhu `alayhi wa ālih"
    },
    {
      "index": 134,
      "text": "ba`dar riḍā ṣalātan lā ḥadda lahā"
    },
    {
      "index": 135,
      "text": "walā muntahā yā ar-ḥamar rāḥimīn"
    }
  ]
}