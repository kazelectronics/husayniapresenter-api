{
  "index": 376,
  "name": "Sura 376",
  "ayas": [
    {
      "index": 1,
      "text": "wasaqānā, wakafānā",
      "bismillah": "al ḥamdu lillāhil ladhī aṭ`amanā"
    },
    {
      "index": 2,
      "text": "wa-ayyadanā, wa āwānā"
    },
    {
      "index": 3,
      "text": "wa-an`ama `alaynā, wa-afḍal"
    },
    {
      "index": 4,
      "text": "al ḥamdu lillāhil ladhī yuṭ`imu walā yuṭ`am"
    }
  ]
}