{
  "index": 401,
  "name": "Sura 401",
  "ayas": [
    {
      "index": 1,
      "text": "wabika-āmantu wa`alayka tawakkalt",
      "bismillah": "allāhumma laka raka`tu walaka aslamt"
    },
    {
      "index": 2,
      "text": "wa-anta rabbī khasha`a laka qalbī"
    },
    {
      "index": 3,
      "text": "wasam`ī wabaṣarī"
    },
    {
      "index": 4,
      "text": "wabasharī washa`rī"
    },
    {
      "index": 5,
      "text": "walaḥmī wadamī"
    },
    {
      "index": 6,
      "text": "wamukhhī wa`aṣabī wa`iẓāmī"
    },
    {
      "index": 7,
      "text": "wajamī`i jawāriḥī ghayr"
    },
    {
      "index": 8,
      "text": "mustankifin walā mustakbirin walā mustaḥsir"
    }
  ]
}