{
  "index": 119,
  "name": "Sura 119",
  "ayas": [
    {
      "index": 1,
      "text": "yā abā muḥammadinil ḥasanibna `aliyy",
      "bismillah": "as salāmu `alayka yā mawlāy"
    },
    {
      "index": 2,
      "text": "al hādil muhtadī"
    },
    {
      "index": 3,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka yā waliyyallāhi wabna awliyā-ih"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā ḥujjatallāhi wabna ḥujajih"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā ṣafiyyallāhi wabna aṣfiyā-ih"
    },
    {
      "index": 7,
      "text": "as salāmu `alayka yā khalīfatallāhi wabna khulafā-ihi wa-abā khalīfatih"
    },
    {
      "index": 8,
      "text": "as salāmu `alayka yabna khātamin nabiyyīn"
    },
    {
      "index": 9,
      "text": "as salāmu `alayka yabna sayyidil waṣiyyīn"
    },
    {
      "index": 10,
      "text": "as salāmu `alayka yabna amīril mu-minīn"
    },
    {
      "index": 11,
      "text": "as salāmu `alayka yabna sayyidati nisā-il `ālamīn"
    },
    {
      "index": 12,
      "text": "as salāmu `alayka yabnal a-immatil hādīn"
    },
    {
      "index": 13,
      "text": "as salāmu `alayka yabnal awṣiyā-ir rāshidīn"
    },
    {
      "index": 14,
      "text": "as salāmu `alayka yā `iṣmatal muttaqīn"
    },
    {
      "index": 15,
      "text": "as salāmu `alayka yā imāmal fā-izīn"
    },
    {
      "index": 16,
      "text": "as salāmu `alayka yā ruknal mu-minīn"
    },
    {
      "index": 17,
      "text": "as salāmu `alayka yā farajal malhūfīn"
    },
    {
      "index": 18,
      "text": "as salāmu `alayka yā wārithal anbiyā-il muntajabīn"
    },
    {
      "index": 19,
      "text": "as salāmu `alayka yā khāzina `ilmi waṣiyyi rasūlillāh"
    },
    {
      "index": 20,
      "text": "as salāmu `alayka ayyuhad dā`ī biḥukmillāh"
    },
    {
      "index": 21,
      "text": "as salāmu `alayka ayyuhan nāṭiqu bikitābillāh"
    },
    {
      "index": 22,
      "text": "as salāmu `alayka yā ḥujjatal ḥujaj"
    },
    {
      "index": 23,
      "text": "as salāmu `alayka yā hādiyal umam"
    },
    {
      "index": 24,
      "text": "as salāmu `alayka yā waliyyan ni`am"
    },
    {
      "index": 25,
      "text": "as salāmu `alayka yā `aybatal `ilm"
    },
    {
      "index": 26,
      "text": "as salāmu `alayka yā safīnatal ḥilm"
    },
    {
      "index": 27,
      "text": "as salāmu `alayka yā abal imāmil muntaẓar"
    },
    {
      "index": 28,
      "text": "aẓ ẓāhirati lil`āqili ḥujjatuh"
    },
    {
      "index": 29,
      "text": "wath thābitati fil yaqīni ma`rifatuh"
    },
    {
      "index": 30,
      "text": "al muḥtajabi `an a`yuniẓ ẓālimīn"
    },
    {
      "index": 31,
      "text": "walmughayyabi `an dawlatil fāsiqīn"
    },
    {
      "index": 32,
      "text": "walmu`īdi rabbunā bihil islāma jadīdan ba`dal anṭimās"
    },
    {
      "index": 33,
      "text": "walqur-āna ghaḍḍan ba`dal andirās"
    },
    {
      "index": 34,
      "text": "ash-hadu yāmawlāya annaka aqamtaṣ ṣalāh"
    },
    {
      "index": 35,
      "text": "wa ātaytaz zakāh"
    },
    {
      "index": 36,
      "text": "wa-amar-ta bil ma`rūf"
    },
    {
      "index": 37,
      "text": "wanahayta `anil munkar"
    },
    {
      "index": 38,
      "text": "wada`awta ilā sabīli rabbik"
    },
    {
      "index": 39,
      "text": "bil ḥikmati walmaw`iẓatil ḥasanah"
    },
    {
      "index": 40,
      "text": "wa`abadtallāha mukhliṣā"
    },
    {
      "index": 41,
      "text": "ḥattā atākal yaqīn"
    },
    {
      "index": 42,
      "text": "as-alullāha bishha-nil ladhī lakum `indah"
    },
    {
      "index": 43,
      "text": "an yataqabbala ziyāratī lakum"
    },
    {
      "index": 44,
      "text": "wayashkura sa`yī ilaykum"
    },
    {
      "index": 45,
      "text": "wayastajība du`ā-ī bikum"
    },
    {
      "index": 46,
      "text": "wayaj`alanī min anṣāril ḥaqq"
    },
    {
      "index": 47,
      "text": "wa-atbā`ihi wa-ashyā`ih"
    },
    {
      "index": 48,
      "text": "wamawālīhi wamuḥibbīh"
    },
    {
      "index": 49,
      "text": "was salāmu `alayka waraḥmatullāhi wabarakātuh"
    }
  ]
}