{
  "index": 350,
  "name": "Sura 350",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "lā ilāha illallāhul jalīlul jabbār"
    },
    {
      "index": 3,
      "text": "lā ilāha illallāhul `azīzul ghaffār"
    },
    {
      "index": 4,
      "text": "lā ilāha illallāhul wāḥidul qahhār"
    },
    {
      "index": 5,
      "text": "lā ilāha illallāhu waḥdahu lā sharīka lah"
    },
    {
      "index": 6,
      "text": "ilāhan wāḥidan wa naḥnu lahu muslimūn"
    },
    {
      "index": 7,
      "text": "lā ilāha illallāhu waḥdahu lā sharīka lah"
    },
    {
      "index": 8,
      "text": "ilāhan wāḥidan wa naḥnu lahu mukhliṣūn"
    },
    {
      "index": 9,
      "text": "lā ilāha illallāhu waḥdahu lā sharīka lah"
    },
    {
      "index": 10,
      "text": "ilāhan wāḥidan wa naḥnu lahu `ābidūn"
    },
    {
      "index": 11,
      "text": "lā ilāha illallāh"
    },
    {
      "index": 12,
      "text": "muḥammadun rasūlullāh"
    },
    {
      "index": 13,
      "text": "`aliyyun waliyyullāh"
    },
    {
      "index": 14,
      "text": "wa ṣallallāhu `alā khayri khalqih"
    },
    {
      "index": 15,
      "text": "wa maẓhari luṭfih"
    },
    {
      "index": 16,
      "text": "muḥammadin wa-ālihi ajma`īnaṭ ṭayyibīnal ṭṭāhirīn"
    },
    {
      "index": 17,
      "text": "biraḥmatika yā ar-ḥamar rāḥimīn"
    },
    {
      "index": 18,
      "text": "ḥasbunallāhu wa ni`mal wakīl"
    },
    {
      "index": 19,
      "text": "wa ni`mal mawlā wa ni`man naṣīr"
    }
  ]
}