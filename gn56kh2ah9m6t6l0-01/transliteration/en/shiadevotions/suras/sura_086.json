{
  "index": 86,
  "name": "Sura 86",
  "ayas": [
    {
      "index": 1,
      "text": "was samā-i waṭ ṭāriq",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "wa mā adrāka maṭ ṭāriq"
    },
    {
      "index": 3,
      "text": "an-najmuth thāqib"
    },
    {
      "index": 4,
      "text": "in kullu nafsin lammā `alayhā ḥāfiẓ"
    },
    {
      "index": 5,
      "text": "falyanẓuril-insānu mimma khuliq"
    },
    {
      "index": 6,
      "text": "khuliqa min mā-in dāfiq"
    },
    {
      "index": 7,
      "text": "yakhruju min bayniṣ ṣulbi wat tarā-ib"
    },
    {
      "index": 8,
      "text": "innahu `alā raj`ihi laqādir"
    },
    {
      "index": 9,
      "text": "yawma tublas sarā-ir"
    },
    {
      "index": 10,
      "text": "famā lahu min quwwatin walā nāṣir"
    },
    {
      "index": 11,
      "text": "was samā-i dhātir raj`"
    },
    {
      "index": 12,
      "text": "wal-arḍi dhātiṣ ṣad`"
    },
    {
      "index": 13,
      "text": "innahu laqawlun faṣl"
    },
    {
      "index": 14,
      "text": "wa mā huwa bil hazl"
    },
    {
      "index": 15,
      "text": "innahum yakīdūna kaydā"
    },
    {
      "index": 16,
      "text": "wa-akīdu kaydā"
    },
    {
      "index": 17,
      "text": "famahhilil kāfirīna amhilhum ruwaydā"
    }
  ]
}