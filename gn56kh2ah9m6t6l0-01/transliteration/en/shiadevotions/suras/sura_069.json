{
  "index": 69,
  "name": "Sura 69",
  "ayas": [
    {
      "index": 1,
      "text": "al-ḥāqqah",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "mal ḥāqqah"
    },
    {
      "index": 3,
      "text": "wa mā adrāka mal ḥāqqah"
    },
    {
      "index": 4,
      "text": "kadhhabat thamūdu wa`ādun bil qāri`ah"
    },
    {
      "index": 5,
      "text": "fa-ammā thamūdu fa-uhlikū biṭ ṭāghiyah"
    },
    {
      "index": 6,
      "text": "wa-ammā `ādun fa-uhlikū birīḥin ṣarṣarin `ātiyah"
    },
    {
      "index": 7,
      "text": "sakhharahā `alayhim sab`a layālin wathamāniyata ayyāmin ḥusūman fataral qawma fīhā ṣar`ā ka-annahum a`jāzu nakhlin khāwiyah"
    },
    {
      "index": 8,
      "text": "fahal tarā lahum min bāqiyah"
    },
    {
      "index": 9,
      "text": "wajā-a fir`awnu wa man qablahu wal mu-tafikātu bil khāṭi-ah"
    },
    {
      "index": 10,
      "text": "fa`aṣaw rasūla rabbihim fa-akhadhahum akhdhatan rābiyah"
    },
    {
      "index": 11,
      "text": "innā lammā ṭaghal mā-u ḥamalnākum fil jāriyah"
    },
    {
      "index": 12,
      "text": "linaj`alahā lakum tadhkiratan wata`iyahā udhunun wā`iyah"
    },
    {
      "index": 13,
      "text": "fa-idhā nufikha fiṣ ṣūri nafkhatun wāḥidah"
    },
    {
      "index": 14,
      "text": "waḥumilatil-arḍu wal jibālu fadukkatā dakkatan wāḥidah"
    },
    {
      "index": 15,
      "text": "fayawma-idhin waqa`atil wāqi`ah"
    },
    {
      "index": 16,
      "text": "wanshaqqatis samā-u fahiya yawma-idhin wāhiyah"
    },
    {
      "index": 17,
      "text": "wal malaku `alā arjā-ihā ۚ wayaḥmilu `arsha rabbika fawqahum yawma-idhin thamāniyah"
    },
    {
      "index": 18,
      "text": "yawma-idhin tu`raḍūna lā takhfā minkum khāfiyah"
    },
    {
      "index": 19,
      "text": "fa-ammā man ūtiya kitābahu biyamīnihi fayaqūlu hā-umuq-ra-ū kitābiyah"
    },
    {
      "index": 20,
      "text": "innī ẓanantu annī mulāqin ḥisābiyah"
    },
    {
      "index": 21,
      "text": "fahuwa fī `īshatin rāḍiyah"
    },
    {
      "index": 22,
      "text": "fī jannatin `āliyah"
    },
    {
      "index": 23,
      "text": "quṭūfuhā dāniyah"
    },
    {
      "index": 24,
      "text": "kulū washrabū hanī-an bimā aslaftum fil-ayyāmil khāliyah"
    },
    {
      "index": 25,
      "text": "wa-ammā man ūtiya kitābahu bishimālihi fayaqūlu yālaytanī lam ūta kitābiyah"
    },
    {
      "index": 26,
      "text": "walam adri mā ḥisābiyah"
    },
    {
      "index": 27,
      "text": "yālaytahā kānatil qāḍiyah"
    },
    {
      "index": 28,
      "text": "mā aghnā `annī māliyah ۜ"
    },
    {
      "index": 29,
      "text": "halaka `annī sulṭāniyah"
    },
    {
      "index": 30,
      "text": "khudhūhu faghullūh"
    },
    {
      "index": 31,
      "text": "thummal jaḥīma ṣallūh"
    },
    {
      "index": 32,
      "text": "thumma fī silsilatin dhar`uhā sab`ūna dhirā`an faslukūh"
    },
    {
      "index": 33,
      "text": "innahu kāna lā yu-minu billāhil `aẓīm"
    },
    {
      "index": 34,
      "text": "walā yaḥuḍḍu `alā ṭa`āmil miskīn"
    },
    {
      "index": 35,
      "text": "falaysa lahul yawma hāhunā ḥamīm"
    },
    {
      "index": 36,
      "text": "walā ṭa`āmun illā min ghislīn"
    },
    {
      "index": 37,
      "text": "lā ya-kuluhu illal-khāṭi-ūn"
    },
    {
      "index": 38,
      "text": "falā uqsimu bimā tubṣirūn"
    },
    {
      "index": 39,
      "text": "wa mā lā tubṣirūn"
    },
    {
      "index": 40,
      "text": "innahu laqawlu rasūlin karīm"
    },
    {
      "index": 41,
      "text": "wa mā huwa biqawli shā`ir ۚ qalīlan mā tu-minūn"
    },
    {
      "index": 42,
      "text": "walā biqawli kāhin ۚ qalīlan mā tadhakkarūn"
    },
    {
      "index": 43,
      "text": "tanzīlun min rabbil `ālamīn"
    },
    {
      "index": 44,
      "text": "walaw taqawwala `alaynā ba`ḍal-aqāwyl"
    },
    {
      "index": 45,
      "text": "la-akhadhnā minhu bil yamīn"
    },
    {
      "index": 46,
      "text": "thumma laqaṭa`nā minhul watīn"
    },
    {
      "index": 47,
      "text": "famā minkum min aḥadin `anhu ḥājizīn"
    },
    {
      "index": 48,
      "text": "wa innahu latadhkiratun lil-muttaqīn"
    },
    {
      "index": 49,
      "text": "wa innā lana`lamu anna minkum mukadhhibīn"
    },
    {
      "index": 50,
      "text": "wa innahu laḥasratun `alal-kāfirīn"
    },
    {
      "index": 51,
      "text": "wa innahu laḥaqqul yaqīn"
    },
    {
      "index": 52,
      "text": "fasabbiḥ bismi rabbikal `aẓīm"
    }
  ]
}