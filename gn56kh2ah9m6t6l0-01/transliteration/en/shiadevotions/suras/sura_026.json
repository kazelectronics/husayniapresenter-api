{
  "index": 26,
  "name": "Sura 26",
  "ayas": [
    {
      "index": 1,
      "text": "ṭā sīn mīm",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "tilka āyātul kitābil mubīn"
    },
    {
      "index": 3,
      "text": "la`allaka bākhi`un nafsaka allā yakūnū mu-minīn"
    },
    {
      "index": 4,
      "text": "in nasha-nunazzil `alayhim minas samā-i āyatan faẓallat a`nāquhum lahā khāḍi`īn"
    },
    {
      "index": 5,
      "text": "wa mā ya-tīhim min dhikrin minar raḥmāni muḥdathin illā kānū `anhu mu`riḍīn"
    },
    {
      "index": 6,
      "text": "faqad kadhhabū fasaya-tīhim anbā-u mā kānū bihi yastahzi-ūn"
    },
    {
      "index": 7,
      "text": "awalam yaraw ilal-arḍi kam anbatnā fīhā min kulli zawjin karīm"
    },
    {
      "index": 8,
      "text": "inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 9,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 10,
      "text": "wa idh nādā rabbuka mūsā ani-til qawmaẓ ẓālimīn"
    },
    {
      "index": 11,
      "text": "qawma fir`awn ۚ alā yattaqūn"
    },
    {
      "index": 12,
      "text": "qāla rabbi innī akhāfu an yukadhhibūn"
    },
    {
      "index": 13,
      "text": "wayaḍīqu ṣadrī walā yanṭaliqu lisānī fa-arsil ilā hārūn"
    },
    {
      "index": 14,
      "text": "walahum `alayya dhanbun fa-akhāfu an yaqtulūn"
    },
    {
      "index": 15,
      "text": "qāla kallā ۖ fadh-habā bi-āyātinā ۖ innā ma`akum mustami`ūn"
    },
    {
      "index": 16,
      "text": "fa-tiyā fir`awna faqūlā innā rasūlu rabbil `ālamīn"
    },
    {
      "index": 17,
      "text": "an arsil ma`anā banī isrā-iyl"
    },
    {
      "index": 18,
      "text": "qāla alam nurabbika fīnā walīdan walabithta fīnā min `umurika sinīn"
    },
    {
      "index": 19,
      "text": "wafa`alta fa`latakal latī fa`alta wa-anta minal kāfirīn"
    },
    {
      "index": 20,
      "text": "qāla fa`altuhā idhan wa-ana minaḍ ḍāllīn"
    },
    {
      "index": 21,
      "text": "fafarartu minkum lammā khiftukum fawahaba lī rabbī ḥukman waja`alanī minal mursalīn"
    },
    {
      "index": 22,
      "text": "watilka ni`matun tamunnuhā `alayya an `abbadta banī isrā-iyl"
    },
    {
      "index": 23,
      "text": "qāla fir`awnu wa mā rabbul `ālamīn"
    },
    {
      "index": 24,
      "text": "qāla rabbus samāwāti wal-arḍi wa mā baynahumā ۖ in kuntum mūqinīn"
    },
    {
      "index": 25,
      "text": "qāla liman ḥawlahu alā tastami`ūn"
    },
    {
      "index": 26,
      "text": "qāla rabbukum warabbu ābā-ikumul-awwalīn"
    },
    {
      "index": 27,
      "text": "qāla inna rasūlakumul ladhī ursila ilaykum lamajnūn"
    },
    {
      "index": 28,
      "text": "qāla rabbul mashriqi wal maghribi wa mā baynahumā ۖ in kuntum ta`qilūn"
    },
    {
      "index": 29,
      "text": "qāla la-init-takhadhta ilāhan ghayrī la-aj`alannaka minal masjūnīn"
    },
    {
      "index": 30,
      "text": "qāla awalaw ji-tuka bishay-in mubīn"
    },
    {
      "index": 31,
      "text": "qāla fa-ti bihi in kunta minaṣ ṣādiqīn"
    },
    {
      "index": 32,
      "text": "fa-alqā `aṣāhu fa-idhā hiya thu`bānun mubīn"
    },
    {
      "index": 33,
      "text": "wanaza`a yadahu fa-idhā hiya bayḍā-u linnāẓirīn"
    },
    {
      "index": 34,
      "text": "qāla lil-mala-i ḥawlahu inna hādhā lasāḥirun `alīm"
    },
    {
      "index": 35,
      "text": "yurīdu an yukhrijakum min arḍikum bisiḥrihi famādhā ta-murūn"
    },
    {
      "index": 36,
      "text": "qālū arjih wa-akhāhu wab`ath fil madā-ini ḥāshirīn"
    },
    {
      "index": 37,
      "text": "ya-tūka bikulli saḥḥārin `alīm"
    },
    {
      "index": 38,
      "text": "fajumi`as saḥaratu limīqāti yawmin ma`lūm"
    },
    {
      "index": 39,
      "text": "waqīla linnāsi hal antum mujtami`ūn"
    },
    {
      "index": 40,
      "text": "la`allanā nattabi`us saḥarata in kānū humul ghālibīn"
    },
    {
      "index": 41,
      "text": "falammā jā-as saḥaratu qālū lifir`awna a-inna lanā la-ajran in kunnā naḥnul ghālibīn"
    },
    {
      "index": 42,
      "text": "qāla na`am wa innakum idhal laminal muqarrabīn"
    },
    {
      "index": 43,
      "text": "qāla lahum mūsā alqū mā antum mulqūn"
    },
    {
      "index": 44,
      "text": "fa-alqaw ḥibālahum wa`iṣiyyahum waqālū bi`izzati fir`awna innā lanaḥnul ghālibūn"
    },
    {
      "index": 45,
      "text": "fa-alqā mūsā `aṣāhu fa-idhā hiya talqafu mā ya-fikūn"
    },
    {
      "index": 46,
      "text": "fa-ulqiyas saḥaratu sājidīn"
    },
    {
      "index": 47,
      "text": "qālū āmannā birabbil `ālamīn"
    },
    {
      "index": 48,
      "text": "rabbi mūsā wahārūn"
    },
    {
      "index": 49,
      "text": "qāla āmantum lahu qabla an ādhana lakum ۖ innahu lakabīrukumul ladhī `allamakumus siḥra falasawfa ta`lamūn ۚ la-uqaṭṭi`anna aydiyakum wa-arjulakum min khilāfin wala-uṣallibannakum ajma`īn"
    },
    {
      "index": 50,
      "text": "qālū lā ḍayr ۖ innā ilā rabbinā munqalibūn"
    },
    {
      "index": 51,
      "text": "innā naṭma`u an yaghfira lanā rabbunā khaṭāyānā an kunnā awwalal-mu-minīn"
    },
    {
      "index": 52,
      "text": "wa-awḥaynā ilā mūsā an asri bi`ibādī innakum muttaba`ūn"
    },
    {
      "index": 53,
      "text": "fa-arsala fir`awnu fil madā-ini ḥāshirīn"
    },
    {
      "index": 54,
      "text": "inna hā-ulā-i lashirdhimatun qalīlūn"
    },
    {
      "index": 55,
      "text": "wa innahum lanā laghā-iẓūn"
    },
    {
      "index": 56,
      "text": "wa innā lajamī`un ḥādhirūn"
    },
    {
      "index": 57,
      "text": "fa-akhrajnāhum min jannātin wa`uyūn"
    },
    {
      "index": 58,
      "text": "wakunūzin wa maqāmin karīm"
    },
    {
      "index": 59,
      "text": "kadhālika wa-awrathnāhā banī isrā-iyl"
    },
    {
      "index": 60,
      "text": "fa-atba`ūhum mushriqīn"
    },
    {
      "index": 61,
      "text": "falammā tarā al jam`āni qāla aṣ-ḥābu mūsā innā lamudrakūn"
    },
    {
      "index": 62,
      "text": "qāla kallā ۖ inna ma`iya rabbī sayahdīn"
    },
    {
      "index": 63,
      "text": "fa-awḥaynā ilā mūsā aniḍ-rib bi`aṣākal baḥr ۖ fanfalaqa fakāna kullu firqin kaṭ ṭawdil `aẓīm"
    },
    {
      "index": 64,
      "text": "wa-azlafnā thammal-ākharīn"
    },
    {
      "index": 65,
      "text": "wa-anjaynā mūsā wa man ma`ahu ajma`īn"
    },
    {
      "index": 66,
      "text": "thumma aghraqnal-ākharīn"
    },
    {
      "index": 67,
      "text": "inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 68,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 69,
      "text": "watlu `alayhim naba-a ibrāhīm"
    },
    {
      "index": 70,
      "text": "idh qāla li-abīhi waqawmihi mā ta`budūn"
    },
    {
      "index": 71,
      "text": "qālū na`budu aṣnāman fanaẓallu lahā `ākifīn"
    },
    {
      "index": 72,
      "text": "qāla hal yasma`ūnakum idh tad`ūn"
    },
    {
      "index": 73,
      "text": "aw yanfa`ūnakum aw yaḍurrūn"
    },
    {
      "index": 74,
      "text": "qālū bal wajadnā ābā-anā kadhālika yaf`alūn"
    },
    {
      "index": 75,
      "text": "qāla afara aytum mā kuntum ta`budūn"
    },
    {
      "index": 76,
      "text": "antum wa-ābā-ukumul-aqdamūn"
    },
    {
      "index": 77,
      "text": "fa-innahum `aduwwul lī illā rabbal `ālamīn"
    },
    {
      "index": 78,
      "text": "alladhī khalaqanī fahuwa yahdīn"
    },
    {
      "index": 79,
      "text": "wal ladhī huwa yuṭ`imunī wayasqīn"
    },
    {
      "index": 80,
      "text": "wa idhā mariḍtu fahuwa yashfīn"
    },
    {
      "index": 81,
      "text": "wal ladhī yumītunī thumma yuḥyīn"
    },
    {
      "index": 82,
      "text": "wal ladhī aṭma`u an yaghfira lī khaṭī-atī yawmad dīn"
    },
    {
      "index": 83,
      "text": "rabbi hab lī ḥukman wa-alḥiqnī biṣ ṣāliḥīn"
    },
    {
      "index": 84,
      "text": "waj`al lī lisāna ṣidqin fil-ākhirīn"
    },
    {
      "index": 85,
      "text": "waj`alnī min warathati jannatin na`īm"
    },
    {
      "index": 86,
      "text": "waghfir li-abī innahu kāna minaḍ ḍāllīn"
    },
    {
      "index": 87,
      "text": "walā tukhzinī yawma yub`athūn"
    },
    {
      "index": 88,
      "text": "yawma lā yanfa`u mālun walā banūn"
    },
    {
      "index": 89,
      "text": "illā man atallāha biqalbin salīm"
    },
    {
      "index": 90,
      "text": "wa-uzlifatil jannatu lil-muttaqīn"
    },
    {
      "index": 91,
      "text": "waburrizatil jaḥīmu lilghāwyn"
    },
    {
      "index": 92,
      "text": "waqīla lahum ayna mā kuntum ta`budūn"
    },
    {
      "index": 93,
      "text": "min dūnillāhi hal yanṣurūnakum aw yantaṣirūn"
    },
    {
      "index": 94,
      "text": "fakubkibū fīhā hum wal ghāwūn"
    },
    {
      "index": 95,
      "text": "wajunūdu iblīsa ajma`ūn"
    },
    {
      "index": 96,
      "text": "qālū wahum fīhā yakhtaṣimūn"
    },
    {
      "index": 97,
      "text": "tallāhi in kunnā lafī ḍalālin mubīn"
    },
    {
      "index": 98,
      "text": "idh nusawwīkum birabbil `ālamīn"
    },
    {
      "index": 99,
      "text": "wa mā aḍallanā illal-mujrimūn"
    },
    {
      "index": 100,
      "text": "famā lanā min shāfi`īn"
    },
    {
      "index": 101,
      "text": "walā ṣadīqin ḥamīm"
    },
    {
      "index": 102,
      "text": "falaw anna lanā karratan fanakūna minal mu-minīn"
    },
    {
      "index": 103,
      "text": "inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 104,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 105,
      "text": "kadhhabat qawmu nūḥinil-mursalīn"
    },
    {
      "index": 106,
      "text": "idh qāla lahum akhūhum nūḥun alā tattaqūn"
    },
    {
      "index": 107,
      "text": "innī lakum rasūlun amīn"
    },
    {
      "index": 108,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 109,
      "text": "wa mā as-alukum `alayhi min ajr ۖ in ajriya illā `alā rabbil `ālamīn"
    },
    {
      "index": 110,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 111,
      "text": "qālū anu-minu laka wattaba`akal-ardhalūn"
    },
    {
      "index": 112,
      "text": "qāla wa mā `il-mī bimā kānū ya`malūn"
    },
    {
      "index": 113,
      "text": "in ḥisābuhum illā `alā rabbī ۖ law tash`urūn"
    },
    {
      "index": 114,
      "text": "wa mā ana biṭāridil mu-minīn"
    },
    {
      "index": 115,
      "text": "in ana illā nadhīrun mubīn"
    },
    {
      "index": 116,
      "text": "qālū la-in lam tantahi yānūḥu latakūnanna minal marjūmīn"
    },
    {
      "index": 117,
      "text": "qāla rabbi inna qawmī kadhhabūn"
    },
    {
      "index": 118,
      "text": "faftaḥ baynī wabaynahum fat-ḥan wanajjinī wa man ma`iya minal mu-minīn"
    },
    {
      "index": 119,
      "text": "fa-anjaynāhu wa man ma`ahu fil fulkil mashḥūn"
    },
    {
      "index": 120,
      "text": "thumma aghraqnā ba`dul bāqīn"
    },
    {
      "index": 121,
      "text": "inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 122,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 123,
      "text": "kadhhabat `ādunil-mursalīn"
    },
    {
      "index": 124,
      "text": "idh qāla lahum akhūhum hūdun alā tattaqūn"
    },
    {
      "index": 125,
      "text": "innī lakum rasūlun amīn"
    },
    {
      "index": 126,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 127,
      "text": "wa mā as-alukum `alayhi min ajr ۖ in ajriya illā `alā rabbil `ālamīn"
    },
    {
      "index": 128,
      "text": "atabnūna bikulli rī`in āyatan ta`bathūn"
    },
    {
      "index": 129,
      "text": "watattakhidhūna maṣāni`a la`allakum takhludūn"
    },
    {
      "index": 130,
      "text": "wa idhā baṭashtum baṭashtum jabbārīn"
    },
    {
      "index": 131,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 132,
      "text": "wattaqul ladhī amaddakum bimā ta`lamūn"
    },
    {
      "index": 133,
      "text": "amaddakum bi-an`āmin wabanīn"
    },
    {
      "index": 134,
      "text": "wajannātin wa`uyūn"
    },
    {
      "index": 135,
      "text": "innī akhāfu `alaykum `adhāba yawmin `aẓīm"
    },
    {
      "index": 136,
      "text": "qālū sawā-un `alaynā awa`aẓta am lam takun minal wā`iẓīn"
    },
    {
      "index": 137,
      "text": "in hādhā illā khuluqul-awwalīn"
    },
    {
      "index": 138,
      "text": "wa mā naḥnu bimu`adhhabīn"
    },
    {
      "index": 139,
      "text": "fakadhhabūhu fa-ahlaknāhum ۗ inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 140,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 141,
      "text": "kadhhabat thamūdul mursalīn"
    },
    {
      "index": 142,
      "text": "idh qāla lahum akhūhum ṣāliḥun alā tattaqūn"
    },
    {
      "index": 143,
      "text": "innī lakum rasūlun amīn"
    },
    {
      "index": 144,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 145,
      "text": "wa mā as-alukum `alayhi min ajr ۖ in ajriya illā `alā rabbil `ālamīn"
    },
    {
      "index": 146,
      "text": "atutrakūna fī mā hāhunā āminīn"
    },
    {
      "index": 147,
      "text": "fī jannātin wa`uyūn"
    },
    {
      "index": 148,
      "text": "wazurū`in wanakhlin ṭal`uhā haḍīm"
    },
    {
      "index": 149,
      "text": "watanḥitūna minal jibāli buyūtan fārihīn"
    },
    {
      "index": 150,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 151,
      "text": "walā tuṭī`ū amral musrifīn"
    },
    {
      "index": 152,
      "text": "alladhīna yufsidūna fil-arḍi walā yuṣliḥūn"
    },
    {
      "index": 153,
      "text": "qālū innamā anta minal musaḥḥarīn"
    },
    {
      "index": 154,
      "text": "mā anta illā basharun mithlunā fa-ti bi-āyatin in kunta minaṣ ṣādiqīn"
    },
    {
      "index": 155,
      "text": "qāla hādhihi nāqatun lahā shirbun walakum shirbu yawmin ma`lūm"
    },
    {
      "index": 156,
      "text": "walā tamassūhā bisū-in faya-khudhakum `adhābu yawmin `aẓīm"
    },
    {
      "index": 157,
      "text": "fa`aqarūhā fa-aṣbaḥū nādimīn"
    },
    {
      "index": 158,
      "text": "fa-akhadhahumul `adhāb ۗ inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 159,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 160,
      "text": "kadhhabat qawmu lūṭinil-mursalīn"
    },
    {
      "index": 161,
      "text": "idh qāla lahum akhūhum lūṭun alā tattaqūn"
    },
    {
      "index": 162,
      "text": "innī lakum rasūlun amīn"
    },
    {
      "index": 163,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 164,
      "text": "wa mā as-alukum `alayhi min ajr ۖ in ajriya illā `alā rabbil `ālamīn"
    },
    {
      "index": 165,
      "text": "ata-tūnadh dhukrāna minal `ālamīn"
    },
    {
      "index": 166,
      "text": "watadharūna mā khalaqa lakum rabbukum min azwājikum ۚ bal antum qawmun `ādūn"
    },
    {
      "index": 167,
      "text": "qālū la-in lam tantahi yālūṭu latakūnanna minal mukhrajīn"
    },
    {
      "index": 168,
      "text": "qāla innī li`amalikum minal qālīn"
    },
    {
      "index": 169,
      "text": "rabbi najjinī wa-ahlī mimmā ya`malūn"
    },
    {
      "index": 170,
      "text": "fanajjaynāhu wa-ahlahu ajma`īn"
    },
    {
      "index": 171,
      "text": "illā `ajūzan fil ghābirīn"
    },
    {
      "index": 172,
      "text": "thumma dammarnal-ākharīn"
    },
    {
      "index": 173,
      "text": "wa-amṭarnā `alayhim maṭarā ۖ fasā-a maṭarul mundharīn"
    },
    {
      "index": 174,
      "text": "inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 175,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 176,
      "text": "kadhhaba aṣ-ḥābul-aykatil mursalīn"
    },
    {
      "index": 177,
      "text": "idh qāla lahum shu`aybun alā tattaqūn"
    },
    {
      "index": 178,
      "text": "innī lakum rasūlun amīn"
    },
    {
      "index": 179,
      "text": "fattaqullāha wa-aṭī`ūn"
    },
    {
      "index": 180,
      "text": "wa mā as-alukum `alayhi min ajr ۖ in ajriya illā `alā rabbil `ālamīn"
    },
    {
      "index": 181,
      "text": "awful kayla walā takūnū minal mukhsirīn"
    },
    {
      "index": 182,
      "text": "wazinū bil qisṭāsil mustaqīm"
    },
    {
      "index": 183,
      "text": "walā tabkhasun nāsa ashyā-ahum walā ta`thaw fil-arḍi mufsidīn"
    },
    {
      "index": 184,
      "text": "wattaqul ladhī khalaqakum wal jibillatal-awwalīn"
    },
    {
      "index": 185,
      "text": "qālū innamā anta minal musaḥḥarīn"
    },
    {
      "index": 186,
      "text": "wa mā anta illā basharun mithlunā wa in naẓunnuka laminal kādhibīn"
    },
    {
      "index": 187,
      "text": "fa-asqiṭ `alaynā kisafan minas samā-i in kunta minaṣ ṣādiqīn"
    },
    {
      "index": 188,
      "text": "qāla rabbī a`lamu bimā ta`malūn"
    },
    {
      "index": 189,
      "text": "fakadhhabūhu fa-akhadhahum `adhābu yawmiẓ ẓullah ۚ innahu kāna `adhāba yawmin `aẓīm"
    },
    {
      "index": 190,
      "text": "inna fī dhālika la-āyah ۖ wa mā kāna aktharuhum mu-minīn"
    },
    {
      "index": 191,
      "text": "wa inna rabbaka lahuwal `azīzur raḥīm"
    },
    {
      "index": 192,
      "text": "wa innahu latanzīlu rabbil `ālamīn"
    },
    {
      "index": 193,
      "text": "nazala bihir rūḥul-amīn"
    },
    {
      "index": 194,
      "text": "`alā qalbika litakūna minal mundhirīn"
    },
    {
      "index": 195,
      "text": "bilisānin `arabiyyin mubīn"
    },
    {
      "index": 196,
      "text": "wa innahu lafī zuburil-awwalīn"
    },
    {
      "index": 197,
      "text": "awalam yakun lahum āyatan an ya`lamahu `ulamā-u banī isrā-iyl"
    },
    {
      "index": 198,
      "text": "walaw nazzalnāhu `alā ba`ḍil-a`jamīn"
    },
    {
      "index": 199,
      "text": "faqara-ahu `alayhim mā kānū bihi mu-minīn"
    },
    {
      "index": 200,
      "text": "kadhālika salaknāhu fī qulūbil mujrimīn"
    },
    {
      "index": 201,
      "text": "lā yu-minūna bihi ḥattā yarawul `adhābal-alīm"
    },
    {
      "index": 202,
      "text": "faya-tiyahum baghtatan wahum lā yash`urūn"
    },
    {
      "index": 203,
      "text": "fayaqūlū hal naḥnu munẓarūn"
    },
    {
      "index": 204,
      "text": "afabi`adhābinā yasta`jilūn"
    },
    {
      "index": 205,
      "text": "afara ayta in matta`nāhum sinīn"
    },
    {
      "index": 206,
      "text": "thumma jā-ahum mā kānū yū`adūn"
    },
    {
      "index": 207,
      "text": "mā aghnā `anhum mā kānū yumatta`ūn"
    },
    {
      "index": 208,
      "text": "wa mā ahlaknā min qaryatin illā lahā mundhirūn"
    },
    {
      "index": 209,
      "text": "dhikrā wa mā kunnā ẓālimīn"
    },
    {
      "index": 210,
      "text": "wa mā tanazzalat bihish shayāṭīn"
    },
    {
      "index": 211,
      "text": "wa mā yanbaghī lahum wa mā yastaṭī`ūn"
    },
    {
      "index": 212,
      "text": "innahum `anis sam`i lama`zūlūn"
    },
    {
      "index": 213,
      "text": "falā tad`u ma`allāhi ilāhan ākhara fatakūna minal mu`adhhabīn"
    },
    {
      "index": 214,
      "text": "wa-andhir `ashīratakal-aqrabīn"
    },
    {
      "index": 215,
      "text": "wakhfiḍ janāḥaka limanit-taba`aka minal mu-minīn"
    },
    {
      "index": 216,
      "text": "fa-in `aṣawka faqul innī barī-un mimmā ta`malūn"
    },
    {
      "index": 217,
      "text": "watawakkal `alal-`azīzir raḥīm"
    },
    {
      "index": 218,
      "text": "alladhī yarāka ḥīna taqūm"
    },
    {
      "index": 219,
      "text": "wataqallubaka fis sājidīn"
    },
    {
      "index": 220,
      "text": "innahu huwas samī`ul `alīm"
    },
    {
      "index": 221,
      "text": "hal unabbi-ukum `alā man tanazzalush shayāṭīn"
    },
    {
      "index": 222,
      "text": "tanazzalu `alā kulli affākin athīm"
    },
    {
      "index": 223,
      "text": "yulqūnas sam`a wa-aktharuhum kādhibūn"
    },
    {
      "index": 224,
      "text": "wash shu`arā-u yattabi`uhumul ghāwūn"
    },
    {
      "index": 225,
      "text": "alam tara annahum fī kulli wādiy yahīmūn"
    },
    {
      "index": 226,
      "text": "wa-annahum yaqūlūna mā lā yaf`alūn"
    },
    {
      "index": 227,
      "text": "illal ladhīna āmanū wa`amiluṣ ṣāliḥāti wadhakarullāha kathīran wantaṣarū min ba`di mā ẓulimū ۗ wasaya`lamul ladhīna ẓalamū ayya munqalabiy yanqalibūn"
    }
  ]
}