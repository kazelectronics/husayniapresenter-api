{
  "index": 320,
  "name": "Sura 320",
  "ayas": [
    {
      "index": 1,
      "text": "waraḥmatullāhi wabarakātuh",
      "bismillah": "as salāmu `alayka yā rasūlallāh"
    },
    {
      "index": 2,
      "text": "as salāmu `alayka yā muḥammadabna `abdilllāh"
    },
    {
      "index": 3,
      "text": "as salāmu `alayka yā khiyaratallāh"
    },
    {
      "index": 4,
      "text": "as salāmu `alayka yā ḥabīballāh"
    },
    {
      "index": 5,
      "text": "as salāmu `alayka yā ṣifwatallāh"
    },
    {
      "index": 6,
      "text": "as salāmu `alayka yā amīnallāh"
    },
    {
      "index": 7,
      "text": "ash-hadu annaka rasūlullāh"
    },
    {
      "index": 8,
      "text": "wa-ash-hadu annaka muḥammadubnu `abdillāh"
    },
    {
      "index": 9,
      "text": "wa-ash-hadu annaka qad naṣaḥta li-ummatik"
    },
    {
      "index": 10,
      "text": "wajāhadta fī sabīli rabbik"
    },
    {
      "index": 11,
      "text": "wa`abadtahu ḥattā atākal yaqīn"
    },
    {
      "index": 12,
      "text": "fajazākallāhu yā rasūlallāh"
    },
    {
      "index": 13,
      "text": "afḍala mā jazā nabiyyan `an ummatih"
    },
    {
      "index": 14,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad"
    },
    {
      "index": 15,
      "text": "afḍala mā ṣallayt"
    },
    {
      "index": 16,
      "text": "`alā ibrāhīma wa āli ibrāhīm"
    },
    {
      "index": 17,
      "text": "innaka ḥamīdun majīd"
    }
  ]
}