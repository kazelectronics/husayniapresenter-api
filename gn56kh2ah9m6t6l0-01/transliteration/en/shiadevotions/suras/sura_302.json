{
  "index": 302,
  "name": "Sura 302",
  "ayas": [
    {
      "index": 1,
      "text": "allāhumma ṣalli `alā muḥammadin wa āli muḥammad",
      "bismillah": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "text": "allāhumma innahu laysa lī `ilmun bimawḍi`i rizqī"
    },
    {
      "index": 3,
      "text": "wa-innamā aṭlubuhu bikhaṭarātin takhṭuru `alā qalbī"
    },
    {
      "index": 4,
      "text": "fa-ajūlu fī ṭalabihil buldān"
    },
    {
      "index": 5,
      "text": "fa-ana fīmā ana ṭālibun kalḥayrān"
    },
    {
      "index": 6,
      "text": "lā-adrī afī sahlin huwa am fī jabal"
    },
    {
      "index": 7,
      "text": "am fī ar-ḍin am fī sama"
    },
    {
      "index": 8,
      "text": "am fī barrin am fī baḥr"
    },
    {
      "index": 9,
      "text": "wa`alā yaday man"
    },
    {
      "index": 10,
      "text": "wamin qibali man"
    },
    {
      "index": 11,
      "text": "waqad `alimtu anna `ilmahu `indak"
    },
    {
      "index": 12,
      "text": "wa-asbābahu biyadik"
    },
    {
      "index": 13,
      "text": "wa-antal ladhī taqsimuhu biluṭfik"
    },
    {
      "index": 14,
      "text": "watusabbibuhu biraḥmatik"
    },
    {
      "index": 15,
      "text": "allāhumma faṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 16,
      "text": "waj`al yā rabbi rizqaka lī wāsi`ā"
    },
    {
      "index": 17,
      "text": "wamaṭlabahu sahlā"
    },
    {
      "index": 18,
      "text": "wama-khadhahu qarībā"
    },
    {
      "index": 19,
      "text": "walā tu`anninī biṭalbi mā lam tuqaddir lī fīhi rizqā"
    },
    {
      "index": 20,
      "text": "fa-innaka ghaniyyun `an `adhābī"
    },
    {
      "index": 21,
      "text": "wa-ana faqīrun ilā raḥmatik"
    },
    {
      "index": 22,
      "text": "faṣalli `alā muḥammadin wa ālih"
    },
    {
      "index": 23,
      "text": "wajud `alā `abdika bifaḍlik"
    },
    {
      "index": 24,
      "text": "innaka dhū faḍlin `aẓīm"
    }
  ]
}