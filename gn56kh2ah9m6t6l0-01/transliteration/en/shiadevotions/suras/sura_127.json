{
  "index": 127,
  "name": "Sura 127",
  "ayas": [
    {
      "index": 1,
      "text": "al mutaṣāghiri li`aẓamatihi jabābiratuṭ ṭāghīn",
      "bismillah": "al ḥamdu lillāhil malikil ḥaqqil mubīn"
    },
    {
      "index": 2,
      "text": "al mu`tarifi birubūbiyyatihi jamī`u ahlis samāwāti wal-araḍīn"
    },
    {
      "index": 3,
      "text": "al muqirri bitawḥīdihi sā-irul khalqi ajma`īn"
    },
    {
      "index": 4,
      "text": "waṣallallāhu `alā sayyidil anām"
    },
    {
      "index": 5,
      "text": "wa-ahli baytihil kirām"
    },
    {
      "index": 6,
      "text": "ṣalātan taqarru bihā a`yunuhum"
    },
    {
      "index": 7,
      "text": "wayar-ghamu bihā anfu shāni-ihim"
    },
    {
      "index": 8,
      "text": "minal jinni wal-insi ajma`īn"
    },
    {
      "index": 9,
      "text": "salāmullāhil `aliyyil `aẓīm"
    },
    {
      "index": 10,
      "text": "wasalāmu malā-ikatihil muqarrabīn"
    },
    {
      "index": 11,
      "text": "wa-anbiyā-ihil mur-salīn"
    },
    {
      "index": 12,
      "text": "wa-a-immatihil muntajabīn"
    },
    {
      "index": 13,
      "text": "wa`ibādihiṣ ṣāliḥīn"
    },
    {
      "index": 14,
      "text": "wajamī`ish shuhadā-i waṣ ṣiddīqīn"
    },
    {
      "index": 15,
      "text": "waz zākiyātuṭ ṭayyibāt"
    },
    {
      "index": 16,
      "text": "fīmā taghtadī watarūḥ"
    },
    {
      "index": 17,
      "text": "`alayka yā muslimabna `aqīlibni abī ṭālib"
    },
    {
      "index": 18,
      "text": "waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 19,
      "text": "ash-hadu annaka aqamtaṣ ṣalāh"
    },
    {
      "index": 20,
      "text": "wa ātaytaz zakāh"
    },
    {
      "index": 21,
      "text": "wa-amar-ta bil ma`rūf"
    },
    {
      "index": 22,
      "text": "wanahayta `anil munkar"
    },
    {
      "index": 23,
      "text": "wajāhadta fillāhi ḥaqqa jihādih"
    },
    {
      "index": 24,
      "text": "waqutilta `alā minhājil mujāhidīna fī sabīlih"
    },
    {
      "index": 25,
      "text": "ḥattā laqītallāha `azzawajalla wahuwa `anka rāḍ"
    },
    {
      "index": 26,
      "text": "wa-ash-hadu annaka wafayta bi`ahdillāh"
    },
    {
      "index": 27,
      "text": "wabadhalta nafsaka fī nuṣrati ḥujjatillāhi wabni ḥujjatih"
    },
    {
      "index": 28,
      "text": "ḥattā atākal yaqīn"
    },
    {
      "index": 29,
      "text": "ash-hadu laka bittaslīmi walwafā"
    },
    {
      "index": 30,
      "text": "wan naṣīḥati likhalafin nabiyyil mur-sal"
    },
    {
      "index": 31,
      "text": "was sibṭil muntajab"
    },
    {
      "index": 32,
      "text": "wad dalīlil `ālim"
    },
    {
      "index": 33,
      "text": "walwaṣiyyil muballigh"
    },
    {
      "index": 34,
      "text": "walmaẓlūmil muhtaḍam"
    },
    {
      "index": 35,
      "text": "fajazākallāhu `an rasūlih"
    },
    {
      "index": 36,
      "text": "wa`an amīril mu-minīn"
    },
    {
      "index": 37,
      "text": "wa`anil ḥasani walḥusayn"
    },
    {
      "index": 38,
      "text": "afḍalal jazā"
    },
    {
      "index": 39,
      "text": "bimā ṣabar-ta waḥtasabta wa-a`ant"
    },
    {
      "index": 40,
      "text": "fani`ma `uqbad dār"
    },
    {
      "index": 41,
      "text": "la`anallāhu man qatalak"
    },
    {
      "index": 42,
      "text": "wala`anallāhu man amara biqatlik"
    },
    {
      "index": 43,
      "text": "wala`anallāhu man ẓalamak"
    },
    {
      "index": 44,
      "text": "wala`anallāhu maniftarā `alayk"
    },
    {
      "index": 45,
      "text": "wala`anallāhu man jahila ḥaqqak"
    },
    {
      "index": 46,
      "text": "wastakhaffa biḥur-matik"
    },
    {
      "index": 47,
      "text": "wala`anallāhu man bāya`aka waghashhak"
    },
    {
      "index": 48,
      "text": "wakhadhalaka wa-aslamak"
    },
    {
      "index": 49,
      "text": "waman allaba `alayka walam yu`ink"
    },
    {
      "index": 50,
      "text": "al ḥamdu lillāhil ladhī ja`alan nāra mathwāhum"
    },
    {
      "index": 51,
      "text": "wabi-sal wir-dul mawrūd"
    },
    {
      "index": 52,
      "text": "ash-hadu annaka qutilta maẓlūmā"
    },
    {
      "index": 53,
      "text": "wa-annallāha munjizun lakum mā wa`adakum"
    },
    {
      "index": 54,
      "text": "ji-tuka zā-iran `ārifan biḥaqqikum"
    },
    {
      "index": 55,
      "text": "musalliman lakum"
    },
    {
      "index": 56,
      "text": "tābi`an lisunnatikum"
    },
    {
      "index": 57,
      "text": "wanuṣratī lakum mu`addah"
    },
    {
      "index": 58,
      "text": "ḥattā yaḥkumallāhu wahuwa khayrul ḥākimīn"
    },
    {
      "index": 59,
      "text": "fama`akum ma`akum"
    },
    {
      "index": 60,
      "text": "lāma`a `aduwwikum"
    },
    {
      "index": 61,
      "text": "ṣalawātullāhi `alaykum"
    },
    {
      "index": 62,
      "text": "wa`alā ar-wāḥikum wa-ajsādikum"
    },
    {
      "index": 63,
      "text": "washāhidikum waghā-ibikum"
    },
    {
      "index": 64,
      "text": "was salāmu `alaykum waraḥmatullāhi wabarakātuh"
    },
    {
      "index": 65,
      "text": "qatalallāhu ummatan qatalatkum bil aydī wal-alsun"
    }
  ]
}