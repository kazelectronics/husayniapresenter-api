{
  "index": 108,
  "name": "الكوثر",
  "arabic_name": "الكَوْثَر",
  "english_name": "Al-Kawthar",
  "english_meaning": "Abundance",
  "ayas": [
    {
      "index": 1,
      "arabic": "إِنَّآ أَعْطَيْنَـٰكَ ٱلْكَوْثَرَ",
      "translation": "To thee have We granted the Fount (of Abundance).",
      "transliteration": "innā a`ṭaynākal kawthar",
      "bismillah": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ"
    },
    {
      "index": 2,
      "arabic": "فَصَلِّ لِرَبِّكَ وَٱنْحَرْ",
      "translation": "Therefore to thy Lord turn in Prayer and Sacrifice.",
      "transliteration": "faṣalli lirabbika wanḥar"
    },
    {
      "index": 3,
      "arabic": "إِنَّ شَانِئَكَ هُوَ ٱلْأَبْتَرُ",
      "translation": "For he who hateth thee, he will be cut off (from Future Hope).",
      "transliteration": "inna shāni-aka huwal-abtar"
    }
  ]
}