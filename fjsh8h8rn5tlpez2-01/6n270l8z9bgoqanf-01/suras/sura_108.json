{
  "index": 108,
  "name": "الكوثر",
  "arabic_name": "الكَوْثَر",
  "english_name": "Al-Kawthar",
  "english_meaning": "Abundance",
  "ayas": [
    {
      "index": 1,
      "arabic": "إِنَّآ أَعْطَيْنَـٰكَ ٱلْكَوْثَرَ",
      "translation": "Lo! We have given thee Abundance;",
      "transliteration": "innā a`ṭaynākal kawthar",
      "bismillah": {
        "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
        "translation": "In the name of Allah, the Beneficent, the Merciful.",
        "transliteration": "bismillāhir raḥmānir raḥīm"
      }
    },
    {
      "index": 2,
      "arabic": "فَصَلِّ لِرَبِّكَ وَٱنْحَرْ",
      "translation": "So pray unto thy Lord, and sacrifice.",
      "transliteration": "faṣalli lirabbika wanḥar"
    },
    {
      "index": 3,
      "arabic": "إِنَّ شَانِئَكَ هُوَ ٱلْأَبْتَرُ",
      "translation": "Lo! it is thy insulter (and not thou) who is without posterity.",
      "transliteration": "inna shāni-aka huwal-abtar"
    }
  ]
}