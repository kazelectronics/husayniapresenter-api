{
  "index": 113,
  "name": "الفلق",
  "arabic_name": "الفَلَق",
  "english_name": "Al-Falaq",
  "english_meaning": "The Daybreak",
  "rows": [
    {
      "index": 1,
      "id": 0,
      "show_id": "",
      "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "translation": "In the name of Allah, most benevolent, ever-merciful.",
      "transliteration": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "id": 1,
      "show_id": "1",
      "arabic": "قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ",
      "translation": "SAY: \"I SEEK refuge with the Lord of rising day",
      "transliteration": "qul a`ūdhu birabbil falaq"
    },
    {
      "index": 3,
      "id": 2,
      "show_id": "2",
      "arabic": "مِن شَرِّ مَا خَلَقَ",
      "translation": "From the evil of what He has created,",
      "transliteration": "min sharri mā khalaq"
    },
    {
      "index": 4,
      "id": 3,
      "show_id": "3",
      "arabic": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "And the evil of evening darkness when it overspreads,",
      "transliteration": "wa min sharri ghāsiqin idhā waqab"
    },
    {
      "index": 5,
      "id": 4,
      "show_id": "4",
      "arabic": "وَمِن شَرِّ ٱلنَّفَّـٰثَـٰتِ فِى ٱلْعُقَدِ",
      "translation": "From the evil of sorceresses who blow incantations on knots,",
      "transliteration": "wa min sharrin naffāthāti fil `uqad"
    },
    {
      "index": 6,
      "id": 5,
      "show_id": "5",
      "arabic": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "From the evil of the envier when he envies.",
      "transliteration": "wa min sharri ḥāsidin idhā ḥasad"
    }
  ]
}