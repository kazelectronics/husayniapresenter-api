  "arabic_name": "المَسَد",
  "english_name": "Al-Masad",
  "english_meaning": "The Palm Fiber",
  "rows": [
    {
      "index": 1,
      "id": 0,
      "show_id": "",
      "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "translation": "In the name of Allah, Most Gracious, Most Merciful.",
      "transliteration": "bismillāhir raḥmānir raḥīm"
    },
    {
      "index": 2,
      "id": 1,
      "show_id": "1",
      "arabic": "تَبَّتْ يَدَآ أَبِى لَهَبٍ وَتَبَّ",
      "translation": "Perish the hands of the Father of Flame! Perish he!",
      "transliteration": "tabbat yadā abī lahabin watabb"
    },
    {
      "index": 3,
      "id": 2,
      "show_id": "2",
      "arabic": "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ",
      "translation": "No profit to him from all his wealth, and all his gains!",
      "transliteration": "mā aghnā `anhu māluhu wa mā kasab"
    },
    {
      "index": 4,
      "id": 3,
      "show_id": "3",
      "arabic": "سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ",
      "translation": "Burnt soon will he be in a Fire of Blazing Flame!",
      "transliteration": "sayaṣlā nāran dhāta lahab"
    },
    {
      "index": 5,
      "id": 4,
      "show_id": "4",
      "arabic": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ",
      "translation": "His wife shall carry the (crackling) wood - As fuel!-",
      "transliteration": "wamra-atuhu ḥammālatal ḥaṭab"
    },
    {
      "index": 6,
      "id": 5,
      "show_id": "5",
      "arabic": "فِى جِيدِهَا حَبْلٌ مِّن مَّسَدٍۭ",
      "translation": "A twisted rope of palm-leaf fibre round her (own) neck!",
      "transliteration": "fī jīdihā ḥablun min masad"
    }
  ]
