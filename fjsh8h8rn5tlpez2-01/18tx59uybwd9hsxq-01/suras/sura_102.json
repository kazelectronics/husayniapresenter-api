{
  "index": 102,
  "name": "التكاثر",
  "arabic_name": "التَّكَاثر",
  "english_name": "At-Takāthur",
  "english_meaning": "The Rivalry in World Increase",
  "ayas": [
    {
      "index": 0,
      "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "translation": "In the name of Allah, Most Gracious, Most Merciful.",
      "transliteration": "bismillāhir raḥmānir raḥīm",
      "is_bismillah": true
    },
    {
      "index": 1,
      "arabic": "أَلْهَىٰكُمُ ٱلتَّكَاثُرُ",
      "translation": "The mutual rivalry for piling up (the good things of this world) diverts you (from the more serious things),",
      "transliteration": "alhākumut takāthur"
    },
    {
      "index": 2,
      "arabic": "حَتَّىٰ زُرْتُمُ ٱلْمَقَابِرَ",
      "translation": "Until ye visit the graves.",
      "transliteration": "ḥattā zurtumul maqābir"
    },
    {
      "index": 3,
      "arabic": "كَلَّا سَوْفَ تَعْلَمُونَ",
      "translation": "But nay, ye soon shall know (the reality).",
      "transliteration": "kallā sawfa ta`lamūn"
    },
    {
      "index": 4,
      "arabic": "ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَ",
      "translation": "Again, ye soon shall know!",
      "transliteration": "thumma kallā sawfa ta`lamūn"
    },
    {
      "index": 5,
      "arabic": "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ ٱلْيَقِينِ",
      "translation": "Nay, were ye to know with certainty of mind, (ye would beware!)",
      "transliteration": "kallā law ta`lamūna `il-mal yaqīn"
    },
    {
      "index": 6,
      "arabic": "لَتَرَوُنَّ ٱلْجَحِيمَ",
      "translation": "Ye shall certainly see Hell-Fire!",
      "transliteration": "latarawunnal jaḥīm"
    },
    {
      "index": 7,
      "arabic": "ثُمَّ لَتَرَوُنَّهَا عَيْنَ ٱلْيَقِينِ",
      "translation": "Again, ye shall see it with certainty of sight!",
      "transliteration": "thumma latarawunnahā `aynal yaqīn"
    },
    {
      "index": 8,
      "arabic": "ثُمَّ لَتُسْـَٔلُنَّ يَوْمَئِذٍ عَنِ ٱلنَّعِيمِ",
      "translation": "Then, shall ye be questioned that Day about the joy (ye indulged in!).",
      "transliteration": "thumma latus-alunna yawma-idhin `anin na`īm"
    }
  ]
}