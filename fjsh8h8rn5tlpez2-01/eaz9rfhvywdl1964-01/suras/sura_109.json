{
  "index": 109,
  "name": "الكافرون",
  "arabic_name": "الكَافِرُون",
  "english_name": "Al-Kāfirūn",
  "english_meaning": "The Disbelievers",
  "rows": [
    {
      "index": 1,
      "arabic": "قُلْ يَـٰٓأَيُّهَا ٱلْكَـٰفِرُونَ",
      "translation": "Say: O unbelievers!",
      "transliteration": "qul yā-ayyuhal kāfirūn",
      "bismillah": {
        "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
        "translation": "In the name of Allah, the Beneficent, the Merciful.",
        "transliteration": "bismillāhir raḥmānir raḥīm"
      }
    },
    {
      "index": 2,
      "arabic": "لَآ أَعْبُدُ مَا تَعْبُدُونَ",
      "translation": "I do not serve that which you serve,",
      "transliteration": "lā a`budu mā ta`budūn"
    },
    {
      "index": 3,
      "arabic": "وَلَآ أَنتُمْ عَـٰبِدُونَ مَآ أَعْبُدُ",
      "translation": "Nor do you serve Him Whom I serve:",
      "transliteration": "walā antum `ābidūna mā a`bud"
    },
    {
      "index": 4,
      "arabic": "وَلَآ أَنَا۠ عَابِدٌ مَّا عَبَدتُّمْ",
      "translation": "Nor am I going to serve that which you serve,",
      "transliteration": "walā ana `ābidun mā `abadtum"
    },
    {
      "index": 5,
      "arabic": "وَلَآ أَنتُمْ عَـٰبِدُونَ مَآ أَعْبُدُ",
      "translation": "Nor are you going to serve Him Whom I serve:",
      "transliteration": "walā antum `ābidūna mā a`bud"
    },
    {
      "index": 6,
      "arabic": "لَكُمْ دِينُكُمْ وَلِىَ دِينِ",
      "translation": "You shall have your religion and I shall have my religion.",
      "transliteration": "lakum dīnukum waliya dīn"
    }
  ]
}