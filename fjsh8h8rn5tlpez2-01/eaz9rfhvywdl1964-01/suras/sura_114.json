{
  "index": 114,
  "name": "الناس",
  "arabic_name": "النَّاس",
  "english_name": "An-Nās",
  "english_meaning": "Mankind",
  "ayas": [
    {
      "index": 0,
      "arabic": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "translation": "In the name of Allah, the Beneficent, the Merciful.",
      "transliteration": "bismillāhir raḥmānir raḥīm",
      "is_bismillah": true
    },
    {
      "index": 1,
      "arabic": "قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "Say: I seek refuge in the Lord of men,",
      "transliteration": "qul a`ūdhu birabbin nās"
    },
    {
      "index": 2,
      "arabic": "مَلِكِ ٱلنَّاسِ",
      "translation": "The King of men,",
      "transliteration": "malikin nās"
    },
    {
      "index": 3,
      "arabic": "إِلَـٰهِ ٱلنَّاسِ",
      "translation": "The god of men,",
      "transliteration": "ilāhin nās"
    },
    {
      "index": 4,
      "arabic": "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ",
      "translation": "From the evil of the whisperings of the slinking (Shaitan),",
      "transliteration": "min sharril waswāsil khannās"
    },
    {
      "index": 5,
      "arabic": "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ",
      "translation": "Who whispers into the hearts of men,",
      "transliteration": "alladhī yuwaswisu fī ṣudūrin nās"
    },
    {
      "index": 6,
      "arabic": "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ",
      "translation": "From among the jinn and the men.",
      "transliteration": "minal jinnati wan nās"
    }
  ]
}