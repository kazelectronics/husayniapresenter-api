  "content": {
    "arabic": [
      "اَللَّهُ اَللَّهُ رَبِّي",
      "لَا اُشْرِكُ بِهِ شَيْئًا",
      "وَلَا اتَّخِذُ مِنْ دُونِهِ وَلِيًّا"
    ],
    "translation": [
      "Allah, Allah is my Lord.",
      "I do not associate anyone with Him",
      "and I do not betake any Master other than Him."
    ],
    "transliteration": [
      "allāhu allāhu rabbī",
      "lā ushriku bihi shay-ā",
      "walattakhidhu min dūnihi waliyyā"
    ]
  }
