  "content": {
    "arabic": [
      "اَللَّهُمَّ اجْعَلْ قَلْبِيْ بَارًّا",
      "وَعَيْشِيْ قَارًّا",
      "وَرِزْقِيْ دَارًّا",
      "وَاجْعَلْ لِيْ عِنْدَ قَبْرِ رَسُوْلِكَ",
      "صَلَّى اللَّهُ عَلَيْهِ وَ آلِهِ",
      "مُسْتَقَرًّا وَقَرَارًا"
    ],
    "translation": [
      "O Allah, (please] make my heart dutiful,",
      "my livelihood delightful,",
      "my sustenance profuse,",
      "and make for me at the tomb of Your Messenger,",
      "may Allah bless him and his Household,",
      "an abode and a dwelling."
    ],
    "transliteration": [
      "allāhummaj`al qalbī bārrā",
      "wa`ayshī qārrā",
      "warizqī dārrā",
      "waj`al lī `inda qabri rasūlik",
      "ṣallallāhu `alayhi wa ālih",
      "mustaqarran waqarārā"
    ]
  }
}