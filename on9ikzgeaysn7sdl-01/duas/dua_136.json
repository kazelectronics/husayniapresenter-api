  "content": {
    "arabic": [
      "اَسْتَغْفِرُ اللَّهَ ذَا الْجَلاَلِ وَالْاِكْرَامِ",
      "مِنْ جَمِيْعِ الذُّنُوبِ وَالْآثَامِ"
    ],
    "translation": [
      "I implore Allah, the Lord of majesty and honor,",
      "to forgive my all sins and offenses."
    ],
    "transliteration": [
      "astaghfirullāha dhal jalāli wal-ikrām",
      "min jamī`idh dhunūbi wal-āthām"
    ]
  }
